  /** Colors **/
.unileverApp .btn {
  margin: 0;
}
body input.btn {
  padding: 12px 40px;
}
body .btn {
  font-family: 'Arial';
  font-weight: normal;
  border: none;
  font-size: 13px;
  color: #ffffff;
  -webkit-border-radius: 0px;
  -moz-border-radius: 0px;
  border-radius: 0px;
}
body .btn.btn-cancel {
  background: #666a6b;
}
body .btn-create-bet {
	background: #3F3F3F;
}
body .btn.btn-save {
  background: #be236d;
}
body button.clearButton {
  background: #be236d;
  border: none;
  padding: 12px 20px;
  font-family: 'Rockwell';
  font-weight: normal;
  margin: 4px 0px;
  line-height: 1;
  font-size: 17px;
  color: #ffffff;
  -webkit-border-radius: 0px;
  -moz-border-radius: 0px;
  border-radius: 0px;
}
.rich-tab-bottom-line {
  border-bottom: 3px solid #be236d;
}
.rich-tabpanel-content {
  font-family: 'Rockwell';
  background: #f4f4f5;
  border-left: 3px solid #be236d;
  border-right: 3px solid #be236d;
  border-bottom: 3px solid #be236d;
  padding: 14px;
}
.rich-tabpanel-content label {
  color: #4d5051;
  font-size: 17px;
}
.form-item {
	margin-left: 40px;
}
.form-item .item-label {
  margin-bottom: 4px;
}

.form-item .item-label label {
  font-family: "Arial-BoldMT","Arial Bold","Arial";
  font-weight: 700;
  font-style: normal;
  font-size: 13px;
  color: #333;
}

.form-item .item-input {
  position: relative;
  left: 175px;
  top: -30px;
}

.form-item .item-input-date {
  position: relative;
  left: 175px;
  top: -30px;
}

.form-item .item-input-checkbox {
  position: relative;
  left: 175px;
  top: -20px;
}

.form-item .item-input input {
  width: 506px;
  height: 15px;
  font-family: "ArialMT","Arial";
  font-weight: 400;
  font-style: normal;
  font-size: 13px;
  border: 2px solid #dcdcdc;
  padding: 8px;
  color: #4d5051;
}

.form-item .item-input-date input {
  width: 270px;
  height: 15px;
  font-family: "ArialMT","Arial";
  font-weight: 400;
  font-style: normal;
  font-size: 13px;
  border: 2px solid #dcdcdc;
  padding: 8px;
  color: #4d5051;
}

.form-item .item-input select {
  width: 526px;
  height: 35px;
  display: block;
  font-family: "ArialMT","Arial";
  font-weight: 400;
  font-style: normal;
  font-size: 13px;
  padding: 7px;
  margin-bottom: 8px;
  color: #4d5051;
}
.form-item .item-input textarea {
  width: 506px;
  min-height: 50px;
}
.form-item .item-input table {
  width: 100%;
}
.form-item .item-input table select {
  width: 90%;
}
.form-item .item-input .multiSelectPicklistCell {
  width: 10%;
}
.form-item .item-input .lookupIcon,
.form-item .item-input .lookupIconOn {
  background-position: center center;
  width: 36px;
  height: 35px;
  position: absolute;
  margin-left: -36px;
  background-image: url('data:image/gif;base64,');
}

.form-item .item-input-date .dateInput .dateFormat {
  display: inline-block;
  height: 20px;
  width: 19px;
  position: absolute;
  top: 10px;
  margin-left: -30px;
  background-repeat: no-repeat;
  background-position: left center;
  background-image: url('data:image/png;base64,');
  font-size: 0px;
}
.form-item .item-input-date .dateInput .dateFormat a {
  height: 20px;
  display: block;
  opacity: 0;
}
.rich-tabhdr-side-border {
  display: none;
}
.rich-tab-header {
  font-family: 'Rockwell';
  width: 208px;
  padding: 10px 0px;
  border: none;
  font-size: 17px;
  cursor: pointer;
}
.rich-tabhdr-cell-active .rich-tabhdr-side-cell {
  border: none;
}
.rich-tabhdr-cell-active .activeTab {
  color: #ffffff;
  background-position: center bottom;
  background-color: transparent;
  background-image: url('data:image/png;base64,');
}
.rich-tabhdr-cell-inactive {
  padding: 0px;
}
.rich-tabhdr-cell-inactive .rich-tabhdr-side-cell {
  border: none;
}
.rich-tabhdr-cell-inactive .inactiveTab {
  color: #be236d;
  background-position: center bottom;
  background-color: transparent;
  background-image: url('data:image/png;base64,');
}

.form-item .item-input .hideOwner select {
    display: none;
}

.question-icon, .question-icon-left {
    display: inline-block;
    width: 16px;
    height: 16px;
    margin-left: 5px;
    background: url({!$Resource.BETApp}/images/question-mark.png) center center/100% no-repeat;
    cursor: pointer;
}
.question-icon-large {
    width: 32px;
    height: 32px;
    vertical-align: middle;
}
.qtip {
position: absolute;
left: -28000px;
top: -28000px;
display: none;
max-width: 280px;
min-width: 50px;
background-color: #eeeeed;
color: gray;
font-family: Arial;
font-size: 12px;
line-height: 12px;
direction: ltr;
box-shadow: none;
padding: 0;
border: 1px solid #333333;
}
.qtip-content {
line-height: 1.5;
position: relative;
padding: 5px;
overflow: hidden;
text-align: left;
word-wrap: break-word;
}
body .bPageBlock .requiredBlock {
    display: none;
}
body .bPageBlock .requiredInput {
    display: inline-block;
    vertical-align: middle;
    height: auto;
}

.form-item-wrapper{
	margin-top: 50px;
}

.form-header{
	background-color: #D3D3D3;
	margin-top: 50px;
}

.form-header .form-item .item-label label{
  font-family: "ArialMT","Arial";
  font-weight: 400;
  font-style: normal;
  font-size: 26px;
  color: #6B6B6B;
}
.form-mandatory .item-label{
	font-family: "ArialMT","Arial";
  font-weight: 400;
  font-style: normal;
  font-size: 13px;
  color: #333;
  text-align: right;
  line-height: normal;
}

.form-item-brandtip .item-label label{
	font-family: "Calibri-Italic","Calibri Italic","Calibri";
  font-weight: 400;
  font-style: italic;
  color: #515151;
	font-size: 13px;
	margin-left: 215px;
}

.form-item-brandtip {
	margin-top: -30px;
}

.betActionButtons {
  margin-left: 215px;
  padding-bottom: 40px;
}

.spinnerclass{
  position: relative;
  left: 175px;
  top: -30px;
  font-size: 13px;
  font-family: "ArialMT","Arial";
  font-weight: 400;
  font-style: normal;
}
