        <isOutput>false</isOutput>
    </variables>
    <variables>
        <description>To store the SMS Incoming Id</description>
        <name>IncomingSMSId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <description>To store the senderOwner Id</description>
        <name>SenderOwnerId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
    </variables>
