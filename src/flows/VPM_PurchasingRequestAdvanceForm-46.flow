    <formulas>
        <description>VPM - Setting Finance Approval Required to true if Finance Details are entered.</description>
        <name>VPM_IsFinanceRequired</name>
        <dataType>Boolean</dataType>
        <expression>OR(LEN(TRIM({!VPM_LocationVar}))&gt;0, LEN(TRIM( {!VPM_CityVar} ))&gt;0)</expression>
    </formulas>
    <formulas>
        <name>VPM_IsProcurementRequired</name>
        <dataType>Boolean</dataType>
        <expression>OR(LEN(TRIM({!VPM_CommodityNameVar}))&gt;0, LEN(TRIM( {!VPM_OrderingEmailAddressVar} ))&gt;0)</expression>
    </formulas>
    <formulas>
        <description>VPM - Used to Get the Status Values of the record</description>
        <name>VPM_Picklits_Status_Text</name>
        <dataType>String</dataType>
        <expression>{!VPM_StatusVar}</expression>
    </formulas>
    <formulas>
        <description>This formula field will help us to determine , the purchasing record exist is of which recordType</description>
        <name>VPM_RecordTypeNameFormula</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBlank({!VPM_PurchasingRequest.RecordTypeId})),
CASE({!VPM_PurchasingRequest.RecordTypeId},
{!VPM_VendorCreateRecordTypeID}, VPM_CreateWordConstant,
{!VPM_VendorMaintainRecordTypeID},VPM_MaintainWordConstant,
{!VPM_VendorExtendRecordTypeID}, VPM_ExtendWordConstant,&apos;CASE&apos;), &apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>VPM_ScreenNotVisited</name>
        <dataType>String</dataType>
        <expression>IF({!VPM_RequestBriefScreenVar}==&apos;&apos;,&apos;Request Brief&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_BankDetailsScreenVar}==&apos;&apos;,&apos;Bank Details&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_PaymentTermScreenVar}==&apos;&apos;,&apos;Payment Terms&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_VendorDetailsScreenVar}==&apos;&apos;,&apos;Vendor Details&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_FinanciallySenitiveFieldsScreenVar}==&apos;&apos;,&apos;Financially Senitive Fields&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_RegionalSpecificFieldScreenVar}==&apos;&apos;,&apos;Regional Specific Fields&lt;br/&gt;&apos;,&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>VPM_ScreenVisited</name>
        <dataType>String</dataType>
        <expression>IF({!VPM_RequestBriefScreenVar}==&apos;&apos;,&apos;&apos;,&apos;Request Brief&lt;br/&gt;&apos;)+IF({!VPM_BankDetailsScreenVar}==&apos;&apos;,&apos;&apos;,&apos;Bank Details&lt;br/&gt;&apos;)+IF({!VPM_PaymentTermScreenVar}==&apos;&apos;,&apos;&apos;,&apos;Payment Terms&lt;br/&gt;&apos;)+IF({!VPM_VendorDetailsScreenVar}==&apos;&apos;,&apos;&apos;,&apos;Vendor Details&lt;br/&gt;&apos;)+IF({!VPM_FinanciallySenitiveFieldsScreenVar}==&apos;&apos;,&apos;&apos;,&apos;Financially Senitive Fields&lt;br/&gt;&apos;)+IF({!VPM_RegionalSpecificFieldScreenVar}==&apos;&apos;,&apos;&apos;,&apos;Regional Specific Fields&lt;br/&gt;&apos;)</expression>
    </formulas>
    <formulas>
        <description>VPM -  formula field show status value  of Purchasing object otherwise as Draft Request</description>
        <name>VPMStatusFormula</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBlank({!VPM_Picklits_Status_Text})), {!VPM_Picklits_Status_Text}, &apos;Draft Request&apos;)</expression>
    </formulas>
