            <field>mdm_Name1__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>VPM_Vendors.mdm_Postcode__c</assignToReference>
            <field>mdm_Postcode__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>VPM_Vendors.mdm_Street__c</assignToReference>
