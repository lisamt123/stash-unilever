    <formulas>
        <description>VPM - Setting Finance Approval Required to true if Finance Details are entered.</description>
        <name>VPM_IsFinanceRequired</name>
        <dataType>Boolean</dataType>
        <expression>OR(LEN(TRIM({!VPM_FinanceDetail1} ))&gt;0, LEN(TRIM( {!VPM_FinanceDetail2} ))&gt;0)</expression>
    </formulas>
    <formulas>
        <name>VPM_IsProcurementRequired</name>
        <dataType>Boolean</dataType>
        <expression>OR(LEN(TRIM({!VPM_ProcurementDetail1} ))&gt;0, LEN(TRIM( {!VPM_ProcurementDetail2} ))&gt;0)</expression>
    </formulas>
