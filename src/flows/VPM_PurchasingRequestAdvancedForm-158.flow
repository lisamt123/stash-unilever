        <expression>IF({!VPM_RequestBriefScreen}==&apos;&apos;,&apos;Request Brief&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_BankDetailsScreen}==&apos;&apos;,&apos;Bank Details&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_PaymentTermScreen}==&apos;&apos;,&apos;Payment Terms&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_VendorDetailsScreen}==&apos;&apos;,&apos;Vendor Details&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_FinanciallySenitiveFieldsScreen}==&apos;&apos;,&apos;Financially Senitive Fields&lt;br/&gt;&apos;,&apos;&apos;)+IF({!VPM_RegionalSpecificFieldScreen}==&apos;&apos;,&apos;Regional Specific Fields&lt;br/&gt;&apos;,&apos;&apos;)</expression>
    </formulas>
    <formulas>
