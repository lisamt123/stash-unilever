    <formulas>
        <description>This formula field  will help us determine which recortype to be selected based on Launch screen of Create, Manitain  or Extend</description>
        <name>VPM_RecodTypeIdFormula</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBlank({!VPM_MenuRoutingDecisionFormula})),
CASE({!VPM_MenuRoutingDecisionFormula},
&apos;Create&apos;, {!VPM_VendorCreateRecordTypeID},
&apos;Extend&apos;,{!VPM_VendorExtendRecordTypeID},
&apos;Maintain1&apos;, {!VPM_VendorMaintainRecordTypeID},
&apos;Maintain2&apos;,{!VPM_VendorMaintainRecordTypeID},&apos;&apos;), &apos;&apos;)</expression>
    </formulas>
    <formulas>
        <description>This formula field will help us to determine , the purchasing record exist is of which recordType</description>
        <name>VPM_RecordTypeNameFormula</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBlank({!VPM_PurchasingRequest.RecordTypeId})),
CASE({!VPM_PurchasingRequest.RecordTypeId},
{!VPM_VendorCreateRecordTypeID}, VPM_CreateWordConstant,
{!VPM_VendorMaintainRecordTypeID},VPM_MaintainWordConstant,
{!VPM_VendorExtendRecordTypeID}, VPM_ExtendWordConstant,&apos;CASE&apos;), &apos;IF&apos;)</expression>
    </formulas>
    <formulas>
        <name>VPM_RequestBriefFormula</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!VPM_RequestBriefChoice})), &apos;Request Brief&apos;, &apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>VPM_ScreenNotVisited</name>
