            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EmailValid1</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SendWebFormRequest</targetReference>
            </connector>
            <label>Invalid Outcome</label>
        </rules>
        <rules>
            <name>VPM_VaildOutcome</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EmailValid1</leftValueReference>
                <operator>EqualTo</operator>
