    <decisions>
        <name>Email_ValidationDecision</name>
        <label>Email Validation Decision</label>
        <locationX>24</locationX>
        <locationY>162</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Valid_Outcome</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EmailValid</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assignment_1</targetReference>
            </connector>
            <label>Valid Outcome</label>
        </rules>
        <rules>
            <name>InVaildOutcome</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EmailValid</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SendWebFormRequest</targetReference>
            </connector>
            <label>In Vaild Outcome</label>
        </rules>
    </decisions>
    <decisions>
        <name>VPM_EmailValidationDecision2</name>
