<apex:page controller="IPM_RolloutPlansTableController" sidebar="false" showHeader="false" >
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/jquery-ui.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/bootstrap.min.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_Buttons.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'assets/font-awesome/css/font-awesome.min.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_DropDown.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_Modal1.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_Table.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_Header-new.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_Table.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_RolloutPlans.css')}"/>
    <apex:form >
        <c:IPM_LoadingStatus ></c:IPM_LoadingStatus>
        <apex:outputPanel id="addRolloutUrlStrPanel" styleClass="addRolloutUrlStrClass" >{!addRolloutUrlStr}</apex:outputPanel>
        <apex:outputPanel id="deleteMCORolloutUrlStrPanel" styleClass="deleteMCORolloutUrlStrClass">{!deleteMCORolloutUrlStr}</apex:outputPanel>
        <apex:outputPanel id="cancelRolloutUrlStrPanel" styleClass="cancelRolloutUrlStrClass">{!cancelRolloutUrlStr}</apex:outputPanel>
        <apex:outputPanel id="saveRolloutUrlStrPanel" styleClass="saveRolloutUrlStrClass">{!saveRolloutUrlStr}</apex:outputPanel>
        <apex:outputPanel id="editRolloutUrlStrPanel" styleClass="editRolloutUrlStrClass">{!editRolloutUrlStr}</apex:outputPanel>
        <apex:pageMessages />
        
        <apex:outputPanel rendered="{!isError}">
            <div class="finGreyBG">
                <apex:outputText value="{!errorMessage}" />
            </div>
        </apex:outputPanel> 
        
        <apex:actionStatus startText="" id="Loading2" startStyleClass="proInitLoader"/>  
        <apex:outputPanel id="ParentPanel" rendered="{!NOT(isError)}">
            <div class="row">
                <apex:outputPanel id="topButtonPanel"> 
                    <div class="col-sm-6 pull-left mSpacing">
                        <apex:commandLink reRender="addRolloutUrlStrPanel" styleClass="ipmButton" value="{!$Label.IPM_ADD_EDIT_ROLLOUTS}" action="{!addRollout}" rendered="{!AND(isEditMode,isEditable)}" status="loadingStatus" oncomplete="movetoaddrollouts()" />
                    </div>
                    <div class="col-sm-6 pull-right mSpacing">
                        <apex:commandLink oncomplete="editRolloutFunc();" reRender="editRolloutUrlStrPanel" styleClass="ipmButton primary pull-right" value="{!$Label.IPM_EDIT}" action="{!editRollout}" rendered="{! AND(NOT(isEditMode),isEditable)}" status="loadingStatus">       
                        </apex:commandLink>
                        
                        <apex:outputPanel rendered="{!AND(isEditMode,projectType.IPM_CreateLocalProjects__c ) }" layout="block"> 
                            <a class="ipmButton primary pull-right focusRolloutmodal" data-toggle="modal" data-target="#ipmRegionalRollout" data-backdrop="static" data-keyboard="false">{!$Label.IPM_SAVE}</a>
                        </apex:outputPanel>
                         
                        <apex:commandLink oncomplete="saveRolloutFuncJs();" reRender="validationErrorPanel,saveRolloutUrlStrPanel" styleClass="ipmButton primary pull-right" value="{!$Label.IPM_SAVE}" action="{!saveRollout}" rendered="{!AND(isEditMode,isEditable,NOT(projectType.IPM_CreateLocalProjects__c))}" status="loadingStatus" onclick="skipValidation();">
                            <apex:param name="p1" assignTo="{!isEditMode}" value="{!$Label.IPM_FALSE}"/>
                        </apex:commandLink>
                        
                        <apex:commandLink reRender="cancelRolloutUrlStrPanel" styleClass="ipmButton pull-right mr10" value="{!$Label.IPM_CANCEL}" action="{!cancelRollout}" rendered="{!AND(isEditable,isEditMode)}" onComplete="aftercancel();" status="loadingStatus" onclick="skipValidation();">
                            <apex:param name="p1" assignTo="{!isEditMode}" value="{!$Label.IPM_FALSE}"/>
                        </apex:commandLink>
                    </div>
                </apex:outputPanel>
            </div>

            <apex:outputPanel id="validationErrorMsg" rendered="{!AND(NOT(ISNULL(validationError)),NOT(ISBLANK(validationError)))}" > 
                <apex:outputText styleClass="rolloutErrorBox" value="{!validationError}" />
            </apex:outputPanel>
            
            <!-- ---- Table Heading for Non GKM3 Models starts here  ---------->
            <apex:outputPanel >
                <table class="ipmTable table table-bordered myListTable">
                    <thead class="">
                        <tr>
                            <th width="34px" scope="col" colspan="1"> 
                                <div>
                                    <div class="colCompleted incHeight"><span> </span></div>
                                </div>
                            </th>
                            <th scope="col" width="150px" colspan="1">
                                <div>
                                    <div class="colProject incHeight"><span>{!$Label.IPM_ROLLOUT_NAME}</span></div>
                                </div>              
                            </th>
                            <th scope="col" width="60px" colspan="1">
                                <div>
                                    <div class="colCategory incHeight"><span>{!$Label.IPM_KEY}</span></div>
                                </div>
                            </th>
                            <th scope="col" width="190px" colspan="1">
                                <div>
                                    <div class="colCategory incHeight"><span>{!$Label.IPM_TARGET_LAUNCH_DATE}</span></div>
                                </div>
                            </th>
                            <th scope="col" width="100px" colspan="1">
                                <div>
                                    <div class="colPhase incHeight"><span>{!$Label.IPM_Status}</span></div>
                                </div>
                            </th>
                            <th scope="col" width="180px" colspan="1">
                                <div>
                                    <div class="colTasks incHeight"><span>{!$Label.IPM_MCO_Rollout_Leader}</span></div>  
                                </div>
                            </th>
                            <th scope="col" width="120px" colspan="1">
                                <div>
                                    <div class="colSection incHeight"><span>{!$Label.IPM_FINANACE_LEADER}</span></div>
                                </div>
                            </th> 
                            
                        </tr>
                    </thead>
                </table>
            </apex:outputPanel>
            <!-- ---- Table Heading for Non GKM3 Models ends here  ---------->
            
            <apex:outputPanel id="rolloutsPanel" >
                <apex:outputPanel id="rolloutPanel">
                    <div class="tableAccrdion">
                        <div class="ipmAccordion"> 
                            <!-- REGIONAL PROJECTS -->
                            <!-- REGIONAL & LOCAL ROLLOUTS -->
                            <apex:repeat value="{!rolloutTolocalRolloutToCountryMap}" var="regionalRolloutID">
                                
                                <apex:variable var="regionalRollout" value="{!lstcompleteRollout[regionalRolloutID]}" /> 
                                    <!--  <apex:repeat value="{!lstRegionalRollouts}" var="regionalRollout"> --> 
                                    <div class="projectContainer">
                                        <!-- Regional Rollout Header -->
                                        <div class="pHead">
                                            <table class="rollouttable">   
                                                <thead class="">
                                                    <tr>
                                                        <th scope="col" width="34px" colspan=""> 
                                                            <div class="incHeight">
                                                                <span class="expico fa fa-minus"></span>
                                                            </div>
                                                        </th>
                                                        <th scope="col" width="150px" colspan="">                                                    
                                                            <span class="rollsubhead">
                                                                <strong>
                                                                    <apex:outputPanel rendered="{!project.IPM_ProjectGKM__c = 'GKM3'}">
                                                                        {!regionalRollout.Market_Cluster_Name__c} 
                                                                    </apex:outputPanel>
                                                                </strong>
                                                            </span>                                        
                                                        </th> 
                                                        <th scope="col" width="60px" colspan="">                                                
                                                            <span class="rollsubhead">
                                                                <strong></strong>
                                                            </span>                                        
                                                        </th>
                                        <th scope="col" width="190px" colspan="1"></th>
                                        <th scope="col" width="100px" colspan="1"></th>
                                        <th scope="col" width="180px" colspan="1"></th>
                                        <th scope="col" width="120px" colspan="1"></th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                        
                        <!-- Local Rollouts -->
                        <div class="ipmAcrdnExpand">
                            <apex:repeat value="{!rolloutTolocalRolloutToCountryMap[regionalRolloutID]}" var="localRolloutId" >  
                            <apex:variable var="localRollout" value="{!lstcompleteRollout[localRolloutId]}" />
                            
                            <apex:variable var="nonKeyMcoTLDMisaligned" value="{!nonKeyMcoTLDMisalignedMap[lstcompleteRollout[localRolloutId].Regional_Rollout_External_Id__c]}" />
                                <div class="pt20">
                                    <table class="rollouttable editable">                                 
                                        <tr>
                                            <td scope="col" width="34px">
                                                <div class="incHeight">
                                                    <span class="rollsubhead">
                                                    </span>
                                                </div>
                                            </td>
                                            <td scope="col" width="150px">
                                                <div>               
                                                    <span class="rollsubhead wordwrap" title="{!IF(AND(project.IPM_ProjectGKM__c = 'GKM3',NOT(lstcompleteRollout[localRolloutId].KeyMCO__c),project.IPM_Phase__c != 'Market Ready',project.IPM_Phase__c != 'Market Deployment',project.IPM_Phase__c != 'PLE'),$Label.IPM_Non_Key_Project_Creation_GKM3, '')}">
                                                        <apex:outputLink value="{!URLFOR($Page.IPM_ProjectOverview,null,[id=lstcompleteRollout[localRolloutId].Local_Project__c])}" styleClass="noDecoration" target="_blank" rendered="{!IF(lstcompleteRollout[localRolloutId].Local_Project__c != null, true, false)}"><!-- {!IF(lstcompleteRollout[localRolloutId].Local_Project__c != null, true, false)} -->
                                                            {!lstcompleteRollout[localRolloutId].Name}
                                                        </apex:outputLink>
                                                        <apex:outputText rendered="{!IF(lstcompleteRollout[localRolloutId].Local_Project__c != null, false, true)}"> <!-- {!IF(lstcompleteRollout[localRolloutId].Local_Project__c != null, false, true)} --> 
                                                            {!lstcompleteRollout[localRolloutId].Name} 
                                                        </apex:outputText>
                                                    </span>
                                                </div>
                                            </td>
                                            <td scope="col" width="60px">
                                                <div class="incHeight">              
                                                    <span class="key wordwrap">
                                                        <apex:image styleClass="wid26" value="{!URLFOR($Resource.IPM_Resource, 'images/flag.svg')}" rendered="{!lstcompleteRollout[localRolloutId].KeyMCO__c}"/>
                                                    </span>
                                                </div>
                                            </td>
                                            <td scope="col" width="190px">
                                                <div class="incHeight">
                                              		<apex:outputField rendered="{!AND(lstcompleteRollout[localRolloutId].Local_Project__c != null,OR(NOT(isEditMode),AND(isEditMode,NOT(isEditable) ) ) )}" value="{!lstcompleteProjects[lstcompleteRollout[localRolloutId].Local_Project__c].Rollout_Plan_Target_Launch_Date__c}"></apex:outputField>
                                              		<apex:inputField value="{!lstcompleteProjects[lstcompleteRollout[localRolloutId].Local_Project__c].Rollout_Plan_Target_Launch_Date__c}" styleClass="dateInputBox calendar{!IF(AND(nonKeyMcoTLDMisaligned, NOT(localRollout.KeyMCO__c)), ' misaligned', '')}" rendered="{!AND(isEditMode,isEditable,lstcompleteRollout[localRolloutId].Local_Project__c != null)}" onchange="alignTLD(this);"/>
                                              	  
                                                  	<apex:outputField value="{!lstcompleteRollout[localRolloutId].Target_Launch_Date__c}" rendered="{!AND(lstcompleteRollout[localRolloutId].Local_Project__c == null,OR(NOT(isEditMode),AND(isEditMode,NOT(isEditable) ) ) )}"/>
                                                  	<apex:inputField value="{!lstcompleteRollout[localRolloutId].Target_Launch_Date__c}" styleClass="dateInputBox calendar{!IF(AND(nonKeyMcoTLDMisaligned, NOT(lstcompleteRollout[localRolloutId].KeyMCO__c)), ' misaligned', '')}" rendered="{!AND(lstcompleteRollout[localRolloutId].Local_Project__c == null,isEditMode,isEditable)}" onchange="alignTLD(this);"/>                                            
                                                </div>
                                            </td>
                                            <td scope="col" width="100px">
                                                <!-- This render condition will check if rollout is eidtable and edit mode and rollout status is leader -->
                                                <apex:outputField value="{!lstcompleteRollout[localRolloutId].IPM_Status__c}" rendered="{!IF(AND(isStatusEditable,isEditMode,lstcompleteRollout[localRolloutId].IPM_Status__c=='Leader'), false, true)}"/>
                                                <apex:inputField value="{!lstcompleteRollout[localRolloutId].IPM_Status__c}" rendered="{!IF(AND(isStatusEditable,isEditable,isEditMode,lstcompleteRollout[localRolloutId].IPM_Status__c=='Leader'), true, false)}"/>
                                            </td>
                                            <td scope="col" width="180px">
                                                <div class="incHeight">
                                                    <apex:outputPanel styleClass="pull-left proLeaderImg" layout="block">
                                                            <apex:image styleClass="rounded" url="{!lstcompleteRollout[localRolloutId].IPM_Regional_PL__r.SmallPhotoUrl}" rendered="{!NOT(isEditMode)}"/>
                                                    </apex:outputPanel>
                                                    
                                                    <apex:outputPanel styleClass="pull-left proLeaderColumn" layout="block">
                                                                                               
                                                        <apex:outputPanel id="countryLeaderPanel" rendered="{!AND(isEditMode,lstcompleteRollout[localRolloutId].Local_Project__c==Null)}">
                                                            <div class="selectproLeader" id="selectproLeader" data-toggle="modal" html-text="{!$LABEL.IPM_MCO_Rollout_Leader}" data-target="#ipmAddMemberModal" title="{!$LABEL.IPM_Select_MCO_Rollout_Leader}" value="{!$page.IPM_Resource_Allocation}?id={!project.Id}&Category={!project.IPM_Category_Text__c}&invokedFrom=Rollouts&RolloutLeaderType=Project&RolloutId={!lstcompleteRollout[localRolloutId].Id}&RolloutSpan=Local" data-backdrop="static" data-keyboard="false" >
                                                                <span class="" >{!$Label.IPM_Select_MCO_Rollout_Leader}</span>
                                                            </div>
                                                        </apex:outputPanel>
                                                        <apex:outputField rendered="{!lstcompleteRollout[localRolloutId].Local_Project__c==Null}"  value="{!lstcompleteRollout[localRolloutId].IPM_Regional_PL__r.Name}"/>
                                                        <apex:outputField rendered="{!lstcompleteRollout[localRolloutId].Local_Project__c!=Null}" value="{!lstcompleteRollout[localRolloutId].Local_Project__r.IPM_Project_Leader__r.Name}"/>
                                                    </apex:outputPanel>
                                                </div>
                                            </td> 
                                            <td scope="col" width="120px">
                                                <div>
                                                <apex:outputPanel styleClass="pull-left proLeaderImg" layout="block">
                                                        <apex:image styleClass="rounded" url="{!lstcompleteRollout[localRolloutId].Finance_Member__r.SmallPhotoUrl}" rendered="{!NOT(isEditMode)}"/>
                                                </apex:outputPanel>
                                                <apex:outputPanel id="financeLeaderPanel" rendered="{!AND(isEditMode,isEditable,lstcompleteRollout[localRolloutId].Local_Project__c==Null)}"> 
                                                        <div class="selectproLeader" id="selectproLeader" data-toggle="modal" html-text="{!$LABEL.IPM_Local_Finance_Leader}" data-target="#ipmAddMemberModal" title="{!$LABEL.IPM_Select_Finance_Leader}" value="{!$page.IPM_Resource_Allocation}?id={!project.Id}&Category={!project.IPM_Category_Text__c}&invokedFrom=Rollouts&RolloutLeaderType=Finance&RolloutId={!lstcompleteRollout[localRolloutId].Id}&RolloutSpan=Local" data-backdrop="static" data-keyboard="false" >
                                                            <span class="">{!$Label.IPM_Select_Finance_Leader}</span>
                                                        </div>
                                                    </apex:outputPanel>
                                                    <apex:outputField rendered="{!lstcompleteRollout[localRolloutId].Local_Project__c==Null}" value="{!lstcompleteRollout[localRolloutId].Finance_Member__r.Name}"  />
                                                    <apex:outputField rendered="{!lstcompleteRollout[localRolloutId].Local_Project__c!=Null}" value="{!lstcompleteRollout[localRolloutId].Local_Project__r.IPM_Finance_Lead__r.Name}"  />
                                                
                                                </div>
                                            </td>                
                                        </tr> 
                                    </table>
                                    <table class="rollouttable editable">                                   
                                        <tr class="secrow">
                                            <td width="3%"> </td>
                                            <td width="15%"> 
                                                <strong>MCO: </strong>{!lstcompleteRollout[localRolloutId].MCO_Name__c}
                                            </td>
                                            <td scope="col" colspan="3" width="40%" class="noBorder">
                                                <span class="separator">
                                                    <strong>{!$Label.IPM_INCLUDED_COUNTRIES}: </strong>
                                                    <apex:variable var="countryString" value="" />
                                                    <apex:repeat value="{!rolloutTolocalRolloutToCountryMap[regionalRolloutID][localRolloutId]}" var="country"> 
                                                        <apex:variable var="countryString" value="{!countryString +','+country.Country_Name__c}" rendered="{!LEN(countryString) > 0}"/>
                                                        <apex:variable var="countryString" value="{!country.Country_Name__c}" rendered="{!LEN(countryString) == 0}"/>
                                                    </apex:repeat>
                                                    <apex:outputText value="{!countryString}"/> 
                                                </span>
                                            </td>
                                            <td scope="col" class="" colspan="3">
                                            <strong>{!$Label.IPM_Rollout_Brand}: </strong>
                                                <apex:outputText styleClass="lclNoselect" value="{!localRolloutBrandMap[localRolloutId].IPM_Brand__c}" rendered="{!OR(!AND(isEditMode,isEditable),AND(isEditMode,isEditable,lstcompleteRollout[localRolloutId].Local_Project__c != null,OR(project.IPM_Phase__c = 'Market Ready',project.IPM_Phase__c = 'Market Deployment',project.IPM_Phase__c = 'PLE')))}"/>
                                                <apex:outputPanel styleClass="localSelectList" rendered="{!OR(AND(isEditMode,isEditable,lstcompleteRollout[localRolloutId].Local_Project__c = null),AND(isEditMode,isEditable,lstcompleteRollout[localRolloutId].Local_Project__c != null,OR(project.IPM_Phase__c = 'Ideas',project.IPM_Phase__c = 'Feasibility',project.IPM_Phase__c = 'Capability')))}">
                                                    <span class="hideDupfield" data-holder="{!localRolloutId}" value="{!localRolloutBrandMap[localRolloutId].IPM_Brand__c}"></span>
                                                    <select class="rollSelectList" id="">  
                                                        <apex:repeat value="{!holdRolloutidBrandMap[localRolloutId]}" var="brandVal">
                                                            <option value="{!brandVal}">{!brandVal}</option>
                                                        </apex:repeat>
                                                    </select>
                                                </apex:outputPanel> 
                                            </td>
                                            <td scope="col" class="text-right" width="6%">
                                                <div class="mr15 inlineBlk">
                                                    <div class="SecButtonSet">                                                       
                                                        <apex:commandLink reRender="deleteMCORolloutUrlStrPanel" onComplete="afterdeleterollout();" styleClass="icoButton delete" action="{!deleteMCORollout}" rendered="{!AND(isEditable,isEditMode)}" status="loadingStatus">
                                                            <apex:param name="p1" assignTo="{!selectedMCORollout}" value="{!localRollout.Id}"/>
                                                        </apex:commandLink>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                                <apex:variable value="{!rolloutCount+1}" var="rolloutCount"/> 
                            </apex:repeat>
                        </div>
                    </div>            
                    </apex:repeat>
                    
                </div>
            </div>
        </apex:outputPanel>
        </apex:outputPanel> 
            <apex:actionFunction name="holdRolloutsBrand" action="{!holdLocalRolloutBrandVal}" reRender="topButtonPanel" status="Loading2">
                <apex:param name="localRolloutIdBrand" value="" assignTo="{!localRolloutIdBrand}"/>
                <apex:param name="localRolloutBrandVal" value="" assignTo="{!localRolloutBrand}"/>
            </apex:actionFunction>
            <div class="row"> 
                <apex:outputPanel id="bottomButtonPanel">     
                    <div class="col-sm-6 pull-left mSpacing"> 
                    </div>
                    <div class="col-sm-6 pull-right mSpacing">
                        <apex:outputPanel rendered="{!AND(isEditMode,projectType.IPM_CreateLocalProjects__c) }" layout="block"> 
                            <a class="ipmButton primary pull-right" data-toggle="modal" data-target="#ipmRegionalRollout" data-backdrop="static" data-keyboard="false">{!$Label.IPM_SAVE}</a>
                        </apex:outputPanel> 
                        
                        <apex:commandLink oncomplete="saveRolloutFuncJs();" reRender="validationErrorPanel,saveRolloutUrlStrPanel" styleClass="ipmButton primary pull-right" value="{!$Label.IPM_SAVE}" action="{!saveRollout}" rendered="{!AND(isEditMode,isEditable,NOT(projectType.IPM_CreateLocalProjects__c))}" status="loadingStatus" onclick="skipValidation();">
                            <apex:param name="p1" assignTo="{!isEditMode}" value="{!$Label.IPM_FALSE}"/>
                        </apex:commandLink>
                        
                        <apex:commandLink oncomplete="editRolloutFunc();" reRender="editRolloutUrlStrPanel" styleClass="ipmButton primary pull-right" value="{!$Label.IPM_EDIT}" action="{!editRollout}" rendered="{!AND(NOT(isEditMode),isEditable)}" status="loadingStatus">
                            <apex:param name="p1" assignTo="{!isEditMode}" value="{!$Label.IPM_TRUE}"/>
                        </apex:commandLink>
                        
                        <apex:commandLink reRender="cancelRolloutUrlStrPanel" onComplete="aftercancel();" styleClass="ipmButton pull-right mr10" value="{!$Label.IPM_CANCEL}" action="{!cancelRollout}" rendered="{!AND(isEditable,isEditMode)}" status="loadingStatus" onclick="skipValidation();">
                            <apex:param name="p1" assignTo="{!isEditMode}" value="{!$Label.IPM_FALSE}"/>
                        </apex:commandLink>
                    </div>
                </apex:outputPanel>
            </div>
            <apex:actionFunction name="reRenderPanels" reRender="topButtonPanel,ParentPanel,bottomButtonPanel,hideUser,reRenderPanels,validationErrorMsg" status="loadingStatus" oncomplete="selectDrpdown();"/>
            
        </apex:outputPanel>
        
        <div id="ipmRegionalRollout" class="modal fade ipmRegionalRollout">  
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <div class="modal-title">{!$Label.IPM_CNFRM_CREATE_LOCAL_ROLLOUTS}</div> 
                    </div>
                    
                    <div class="modal-body">
                        <div class="container-fluid">
                            <div class="row">    
                                <div class="col-sm-12"> 
                                    <span class="skipDesc">{!$Label.IPM_PhaseChangeBKProcessWarning_Local}</span> 
                                </div>            
                                <div class="skipButtoncontainer txtAlignright pull-right">
                                    <apex:outputPanel id="SkipBossC">
                                        <div class="buttonMarginRight noPadright pull-left" >
                                            <input type="button" data-dismiss="modal" class="ipmButton" value="{!$Label.IPM_CANCEL}" />
                                        </div> 
                                    </apex:outputPanel>
                                    <apex:actionStatus startText="" id="Loading1" startStyleClass="proInit"/>
                                    <div class="pull-left noPadright">                                  
                                        
                                        <apex:commandLink value="{!$Label.IPM_SAVE}" styleClass="ipmButton primary" onComplete="saveRolloutFuncJs();" reRender="validationErrorPanel,saveRolloutUrlStrPanel,createNewProject" status="loadingStatus" html-data-dismiss="modal" action="{!saveRollout}" onclick="skipValidation();">
                                            <apex:param name="p1" assignTo="{!isEditMode}" value="{!$Label.IPM_FALSE}"/>
                                        </apex:commandLink>    
                                    </div> 
                                </div>
                            </div>  
                        </div>          
                    </div>
                </div>  
            </div>
        </div>
        <!-- Modal -->
        <div id="tldWarningDialog" tabindex="-1" data-backdrop-limit="1" class="modal fade">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <div class="modal-title">{!$Label.Business_Case_Update_Needed}</div>
                    </div>
                    <div class="modal-body">
                        <div class="removeapprpopup">
                            <span class="removeApprText">{!$Label.Business_Case_Update_Needed_Value}</span>
                            <apex:outputpanel rendered="{!showAllignAllMCOChoice}">
                                <p><span class="removeApprText">{!$Label.IPM_Align_AllMCO_TLD}</span></p>
                                <apex:selectRadio value="{!alignAllMCOTLDs}" styleClass="ipmRadioButton">  
                                    <apex:selectoption itemvalue="True"  itemlabel="Yes"/> 
                                    <apex:selectoption itemvalue="False"  itemlabel="No"/> 
                                </apex:selectRadio> 
                            </apex:outputpanel>
                            <div class="remvButtonContainer pull-right">  
                                <div class="pull-left buttonMarginRight noPadright">
                                    <input type="button" class="ipmButton" data-dismiss="modal" value="{!$Label.Cancel_and_Revert}" onClick="revertTLDValue();"/>
                                </div>
                                <div class="buttonMarginRight noPadright pull-left" >
                                    <!-- <span class="ipmButton primary" data-dismiss="modal" >Agree and Proceed</span> -->
