<apex:page controller="PQN_GlobalGeographyReport" showHeader="false" readOnly="true">
 <body>
    <div id="wrapper">
        <c:PQN_Navigation navigationName="Reports" subNavName="geography"/>
        <div id="page-wrapper">
            <div class="container-fluid">
 <apex:form >
 
      <apex:pageBlock >
          <apex:pageblockTable value="{!xSUWRAPLIST}" var="xD" columnsWidth="24%, 6%, 6%, 6%, 6%, 6%, 6%, 6%, 6%, 6%, 6%, 6%" >
              <apex:column headerValue="" >
                  <apex:outputText value="{!xD.Region}" style="font-weight:800" rendered="{!xD.child}"/>
                  <apex:outputText value="{!empty}" rendered="{!xD.child}" />
              
                  &nbsp; &nbsp; &nbsp; &nbsp;<apex:outputText value="{!xD.Cluster}" rendered="{!xD.childCluster}"/>
                  &nbsp; &nbsp; &nbsp; &nbsp;<apex:outputText value="{!empty}" rendered="{!xD.childCluster}" />
                  
                  <apex:outputText value="{!xD.SubCluster}" />
              </apex:column>              
              <apex:column headerValue="PY SU" value="{!xD.preYeardata}" />
              <apex:column headerValue="PY SU%" value="{!xD.preYearPercent}" />
              <apex:column headerValue="PY total" value="{!xD.preYeardatatot }" />
              <apex:column headerValue="CM SU data" value="{!xD.cumnthdata }" />
              <apex:column headerValue="CM SU %" value="{!xD.cumnthPercent }" />
              <apex:column headerValue="CM DC" value="{!xD.cumnthdcdata }" />
              <apex:column headerValue="CM IM/EX" value="{!xD.cumnthIEdata }" />
              <apex:column headerValue="CM Total" value="{!xD.cumnthtotdata }" />
              <apex:column headerValue="CY SU" value="{!xD.cuYeardata}" />
              <apex:column headerValue="CY SU %" value="{!xD.cuYearPercent}" />
              <apex:column headerValue="CY Total" value="{!xD.cuYeardatatot }" />              
          </apex:pageblockTable>
      </apex:pageBlock>
      
</apex:form>
</div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
    </div>
</body> 
</apex:page>