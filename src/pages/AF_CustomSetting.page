<apex:page controller="AF_CustomSetting" sidebar="false" >
<apex:includeScript value="{!URLFOR($Resource.FancyJquery17, 'jquery.min.js')}"/>
<apex:includeScript value="{!URLFOR($Resource.ulw_brief_2_static_resources, 'static_resources/js/jquery.colorbox-min.js')}"/> 



<script>
    $(document).ready(function(){
    $(".pg1").hover(function(){
        $(".pg1").prop('title', 'Hi');
    }
    );
    });
   
    </script>
<apex:form >

    <apex:pageblock title="QuarterlyBonus" id="pg2" >
        <apex:outputText >{!$Label.AF_QuarterlyBonus}</apex:outputText>
