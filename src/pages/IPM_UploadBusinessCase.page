<apex:page controller="IPM_UploadBusinessCase" sidebar="false" showHeader="false" docType="html-5.0" cache="false">
  <apex:form id="fom"> 
      <apex:pageMessages />                  
      <apex:inputfile id="Fn"  accept="xls, xlsx" value="{!filebody}" filename="{!fileName}" ></apex:inputfile>
      <apex:actionRegion >
          <apex:commandButton value="Save" action="{!saveBusinessCase}" />
      </apex:actionRegion>
  </apex:form>
</apex:page>