        function changeColor(id) {    
            var tabs = document.getElementsByClassName('tabcolorchange')
            for (var i = 0; i < tabs.length; ++i) {
                var item = tabs[i];
                item.style.color = (item.id == id) ? "white" : "#999999";
                item.style.backgroundColor = (item.id == id) ? "#999999" : "white";
