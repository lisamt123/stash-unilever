<apex:page sidebar="false" showHeader="false" >
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_GateDocument.css')}"/>  
    <apex:composition template="IPM_TemplateForPlainStyling">  
        <apex:define name="body">
            <apex:form >
                <c:IPM_Risk_QuestionnaireView getProjectId="{!$CurrentPage.parameters.getProjectId}"
                getProjectSectionId="{!$CurrentPage.parameters.getProjectSectionId}"
                editSection="{!$CurrentPage.parameters.editSection}"
                gateType="{!$CurrentPage.parameters.gateType}">
                </c:IPM_Risk_QuestionnaireView>
            </apex:form>
        </apex:define>
    </apex:composition>
    <script src="{!URLFOR($Resource.IPM_Resource, 'js/iframeResizer.contentWindow.min.js')}" type="text/javascript"></script>
</apex:page>