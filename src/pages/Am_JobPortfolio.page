        <script type="text/javascript">
                $j = jQuery.noConflict();
                function transferValue(projName)
                {        
                        if(projName!=="--None--") { 
                                $j('.projecttextProjDetail').val(projName);           
                        }
                }     
                
                
                $j(function(){
                        
                        $j('td.empty').remove();
                        
                    $j('.commonall').click(function(){   
                       var dataId = $j(this).attr('data-id');
                       
                       if($j(this).hasClass('minusclass')) {
                            $j(this).attr('class', 'commonall plusclass');
                            $j('.'+dataId).slideToggle(500);
                       } else if($j(this).hasClass('plusclass')) {
                            $j(this).attr('class', 'commonall minusclass');
                            $j('.'+dataId).slideToggle(500);
                       }                                   
                  });
                    
                   $j('.addlicon').click(function(){                
                       $j('.job-list-search-section').slideToggle(500);
                   });
                   
                   $j('.addliconAlert').click(function(){                
                       $j('.job-list-search-sectionAlert').slideToggle(500);
                   });
                   
                   
                   var stringArray = JSON.parse('{!MethodToPassProjNames}');
                        for(var i=0;i<stringArray.length;i++){
                        //console.log(stringArray);
                             var newOption = $j('<option value="'+stringArray[i]+'">'+stringArray[i]+'</option>');
                             $j('#projectList').append(newOption);
                        }
                       //console.log('options'+newOption );
                
                   $j('.footer_section .footer:first').addClass('top');
                  var change = function(){
                      var curr = $j('.footer_section .footer.top');
                            var nextsl = curr.next();
                            if(!nextsl.length){
                                nextsl = $j('.footer_section .footer:first');                   
                                nextsl.addClass('top');
                                curr.removeClass('top');
                            }
                            else{
                                nextsl.addClass('top'); 
                                curr.removeClass('top');
                            }
                  
                  }
                             setInterval(change, 5000); 
                     
                     $j('.close').click(function() {
                         //alert('YOOOOO');
                         var currDiv = $j('.footer_section .footer.top');
                            
                           var selectedAlert =  $j('.footer_section .footer.top input').val();
                         //  alert(selectedAlert);                           
                         var next = currDiv.next();
                         if(!next.length){
                             currDiv.remove();
                             $j('.footer_section .footer:first').addClass('top');
                             hideAlert(selectedAlert);
                         }
                         else{
                             currDiv.remove();
                             next.addClass('top');  
                           hideAlert(selectedAlert);
                        }                        
                        
                        if( $j('.footer').length ==0 ) {
                            $j('.alert_div').remove();
                        }
                        
                     }); 
                     
                     var strArr = {!CategoryList};
                     var none = $j('<option value="none">--None--</option>');
                      $j('#categoryList').append(none);
                         ///alert(strArr.length);
                      for(var j=0;j<strArr.length;j++){
                         console.log(strArr[j]);
                         var newOp = $j('<option value="'+strArr[j]+'">'+strArr[j]+'</option>');
                                    // alert(newOp);
                         $j('#categoryList').append(newOp);
                        }            
                        console.log('options'+newOp); 
                       // alert(newOp);
                          
                });
                
                
                 function callActionFunctionStartDate(startDate) {
                
                    callContrlStartDate(startDate);
                   }  
                 function callActionFunctionEndDate(endDate) {
                
                   callContrlEndDate(endDate);
                
                }
                 function callFuctionForPrioritySort(prioritySortOption){
                
                    callContrlPrioritySort(prioritySortOption);
                }
                function callFuctionForPendingSort(pendingSortOption){
                
                    callContrlPendingSort(pendingSortOption);
                }
                
                 function callFuctionForDormantSort(dormantSortOption){
                
                    callContrlDormantSort(dormantSortOption);
                }
                
                 function callFuctionForLiveSort(liveSortOption){
                
                    callContrlLiveSort(liveSortOption);
                }
                   function transferCategoryValue(categoryValue){
                sendCategory(categoryValue);
                      $j('.loading').show();
             
        }
                           
        </script>             
