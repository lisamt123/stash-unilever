   <script>
   $(document).ready(function(){

        $('tr.data, tr.btt').show().find('td').wrapInner('<div />');
        $('tr.data, tr.btt').find('td > div').hide();
        $('table tr.Oblix_heading').click(function(){
            var data = $(this).next('tr');
            var btt = data.next('tr');
            data.find('td > div').slideToggle('slow');
            btt.find('td > div').slideToggle('slow');
            $(this).toggleClass('show');
        });
         $('table tr.Oblix_recordTypeth').click(function(){
            var data = $(this).next('tr');
            var btt = data.next('tr');
            data.find('td > div').slideToggle('slow');
            btt.find('td > div').slideToggle('slow');
            $(this).toggleClass('show');
        });
        var IdsProject = {!IdsProject};
        console.dir(IdsProject);
        for (var i=0; i<IdsProject.length;i++) {
            $('#'+IdsProject[i]+'_Audio_tabs').tabs();
            $('#'+IdsProject[i]+'_Audio_tabs1').tabs();
            $('#'+IdsProject[i]+'_Audio_tabs2').tabs();
            $('#'+IdsProject[i]+'_Static_tabs').tabs();
            $('#'+IdsProject[i]+'_Static_tabs1').tabs();
            $('#'+IdsProject[i]+'_Static_tabs2').tabs();
            $('#'+IdsProject[i]+'_Exper_tabs').tabs();
            $('#'+IdsProject[i]+'_Exper_tabs1').tabs();
            $('#'+IdsProject[i]+'_Exper_tabs2').tabs();
            $('#'+IdsProject[i]+'_Value_tabs').tabs();
            $('#'+IdsProject[i]+'_Value_tabs1').tabs();
            $('#'+IdsProject[i]+'_Value_tabs2').tabs();
            $('#'+IdsProject[i]+'_Digital_tabs').tabs();
            $('#'+IdsProject[i]+'_Digital_tabs1').tabs();
            $('#'+IdsProject[i]+'_Digital_tabs2').tabs();
            $('#'+IdsProject[i]+'_Research_tabs').tabs();
            $('#'+IdsProject[i]+'_Research_tabs1').tabs();
            $('#'+IdsProject[i]+'_Research_tabs2').tabs();
        }
   });
   
