<div class="truncate">
                            <span class="sub-branding" style="padding-left: 6px;" >{!USCategory},</span>
                            <span class="sub-branding">{!USlevel1},</span>
                            <span class="sub-branding">{!USlevel2}</span>
                            </div>
                    <span class="sub-header-change">
                          <!--  <apex:commandButton value="Change" action="{!openBrandSwitching}" style="border: none;background: 0;"/> -->
                <a href="javascript:void(0);" class="modal-btn-change" style="font-size:10.5px; position: absolute;right: 5px;border: none;background:0;" onclick="showModal();">Change</a>
                    </span>
                        
                </div>
            </div>
        </div>
    </div>
    <div class="navigationSection">
        <div class="slds-page-header footerSection" role="banner" style="height: 4.4em;">
        <div class="slds-grid">
            <div style="margin-left: -13px;">
                <apex:commandButton image="/resource/AIW_Static_Resource/icons/standard/user_60.png" action="{!openHBM}" style="margin-bottom: -14px;opacity:0.5; border: none;background: 0;width: 2.3em;margin-top: -1em;margin-left: 3em;padding-bottom: 0px;"/>
                <span id="keys" style="margin-left: 35px;" class="disabled">My Profile</span>
            </div>
            <div>
                <apex:commandButton image="/resource/AIW_Static_Resource/icons/project-specific/icon_heartbeat.png" action="{!openHBM}" style="margin-bottom: -8px; border: none;background: 0;width: 1.7em;margin-top: -0.8em;margin-left: 2em;padding-bottom: 0px;"/>
                <span id="heartbeat">Heartbeat</span>
            </div>
            <div>
                <apex:commandButton image="/resource/AIW_Static_Resource/icons/project-specific/icon_competition.png" action="{!openCompetition}"  style="margin-bottom: -8px;opacity:0.5; border: none;background: 0;width: 1.7em;margin-top: -0.5em;margin-left: 2.6em;padding-bottom: 0px;" />
                <span id="competition" class="disabled">Competition</span>
            </div>
            <div>
                <apex:commandButton image="/resource/AIW_Static_Resource/icons/project-specific/icon_trending.png" action="{!openSocial}" style="margin-bottom: -5px;opacity:0.5; border: none;background: 0;width: 1.5em;margin-top: -0.3em;margin-left: 1.7em;padding-bottom: 0px;"/>
                <span id="trending" class="disabled">Social</span>
            </div>
        </div>
    </div>
    </div>
    <div class="tabBadges" role="banner" style="margin-bottom: 12px;">
        <div class="slds-grid">
            <div class="slds-col slds-has-flexi-truncate">
                <div class="slds-media other-slds-media" style="width:28em;">
                    <span id = "tabcolorchange1" class="tabcolorchange slds-badge slds-theme--shade top" style="color:#999999;height: 26px;width: 20% !important;"><apex:commandButton value="4W" action="{!getKantarData}"  onClick="changeColor('tabcolorchange1')" reRender="metricsdata,metricsdata2,metricsdata3,metricsdata4" style=" border: none;background: 0;">                       
            <apex:param name="4wbuttonparam" value="4W" assignTo="{!timeperiod}"/>
                  </apex:commandButton></span>
                    <span id = "tabcolorchange2" class="tabcolorchange slds-badge slds-theme--shade middle" style="color:#999999;height: 26px;width: 20% !important;"> <apex:commandButton value="12W" action="{!getKantarData}" onClick="changeColor('tabcolorchange2')" reRender="metricsdata,metricsdata2,metricsdata3,metricsdata4" style=" border: none;background: 0;">                       
            <apex:param name="12wbuttonparam" value="12W" assignTo="{!timeperiod}"/>
                  </apex:commandButton></span>
                    <span id = "tabcolorchange3" class="tabcolorchange slds-badge slds-theme--shade middle" style="color:#999999;height: 26px;width: 20% !important;"> <apex:commandButton value="YTD" action="{!getKantarData}"  onClick="changeColor('tabcolorchange3')" reRender="metricsdata,metricsdata2,metricsdata3,metricsdata4"  style=" border: none;background: 0;">                       
            <apex:param name="YTDbuttonparam" value="YTD" assignTo="{!timeperiod}"/>
                  </apex:commandButton></span>
                    <span id = "tabcolorchange4" class="tabcolorchange slds-badge last" style="color:white;background-color: #999999;height: 26px;width: 20% !important;"><apex:commandButton value="MAT" action="{!getKantarData}"  onClick="changeColor('tabcolorchange4')" reRender="metricsdata,metricsdata2,metricsdata3,metricsdata4"  style=" border: none;background: 0;">                       
            <apex:param name="MATbuttonparam" value="MAT" assignTo="{!timeperiod}"/>
                  </apex:commandButton></span>
                </div>
            </div>
        </div>
    </div>
   <apex:outputPanel id="metricsdata">
     <apex:commandButton image="/resource/AIW_Static_Resource/icons/project-specific/icon_back.png" action="{!openMKS}"  style="  width: 1.65em;font-weight: bolder; color: #1766A6; border: none;background: 0;margin-left: 7px; margin-top: 0px;"/>
        <div style="width: 23em;margin-top: -29px;margin-left: 2em;font-weight: bolder;font-size: 16px;color: #1766A6;">
            Shopper Expenditure<span style='font-size:12px;'> (Kantar)</span>
        </div>
     <div class="buttonBlock">  
            <apex:commandButton value="All Channels" disabled="true" style="font-size: 13px;height: 36px;border:1px solid light-grey;text-align: left;padding-left: 12px;background: white !important;border-radius: 6px;width: 93.6%;color: #1766A6;font-weight: bold;margin-left: 10px;margin-right: 2.5em;margin-top: 10px;"/>
            <img src="/resource/AIW_Static_Resource/icons/project-specific/icon_dropdown_arrow.png" id="slider" style="width: 10%;margin-top: -57px;border-radius: 6px;"/>
    </div>
    <div class="expenditureChange" style="font-size: 10px;font-weight: bold; padding-left: 0.5em;">
        <div class="expenditureChange-Left" style="padding-left: 5px;font-size: 13px;float:left;text-align: left;color: #1766A6;font-weight: bold;width: 50%;">
            Expenditure Change 
        </div>
        <div class="expenditureChange-Right" style="float:right;">
        <div style="margin-top: 3px;width: 6em;float:left;margin-left: -28px;padding-left:20px;">
            <apex:outputPanel rendered="{!IF (expchange < 0,'true','false')}">
                <div class="leftText redFont">{!ROUND((expchange),1)} %</div>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!IF (expchange > 0 || expchange = 0,'true','false')}">
                <div class="leftText greenFont">{!ROUND((expchange),1)}%</div>
            </apex:outputPanel>
        </div>
        <div style="font-weight: bold;font-size: 13px;float:left;margin-top: 0px;">  
            <apex:outputPanel rendered="{!IF (expvalue < 0,'true','false')}">
                <div class="rightText redFont">{!ROUND((expvalue),1)}<span style='font-size:9px;color:grey;'>(£K)</span></div>
                
            </apex:outputPanel>
            <apex:outputPanel rendered="{!IF (expvalue> 0 || expvalue = 0,'true','false')}">
                <div class="rightText greenFont">+{!ROUND((expvalue),1)}(£K)</div>
                
            </apex:outputPanel>
        </div>
         <apex:outputPanel rendered="{!IF (expvalue < 0, 'true','false')}">
            <div style='float:right;    '><!--user_icon arrow_up-->
                  <img class="Arrow" src="/resource/AIW_Static_Resource/icons/project-specific/icon_arrow_down.png"/>
            </div>
         </apex:outputPanel> 
         
         <apex:outputPanel rendered="{!IF (expvalue > 0 || expvalue = 0, 'true','false')}">
            <div style='float:left;'><!--user_icon arrow_up-->
                  <img class="Arrow" src="/resource/AIW_Static_Resource/icons/project-specific/icon_arrow_up.png"/>
            </div>
         </apex:outputPanel> 
              
                
        <div style='clear:both;'></div>
      </div><!--end of right div-->
      <div style='clear:both;'></div>
    </div>
    </apex:outputPanel>
     
    <apex:outputPanel id="metricsdata2">
    <div class="shopperExpenditureChange" style="height: 19.5em;">
    
        <div class="header" style="padding-left: 0.5em;">
            <span class="blueFont" style="font-size: 13px;float:left;padding-bottom:21px;color: #1766A6;font-weight: bold;padding-top: 2px;">
                Drivers of Shopper Expenditure Change
            </span>
        </div>
        <div class="container">
  <br />
  <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="false">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
       
       <div class="item active">
             <apex:chart height="200" width="300" data="{!VCdata}" theme="Green" colorSet="#6BAE4A,#FF9123,#6BAE4A,#424242,#A4A4A4">         
                   <apex:axis type="Category" position="left" fields="name" />                        
                    <apex:axis type="Numeric" position="bottom" fields="data" maximum="{!tcmax}"/>    
                    <apex:barSeries orientation="horizontal" axis="left" xField="data" yField="name" colorSet="{!colorSet}" colorsProgressWithinSeries="true"> 
                   <apex:chartTips height="20" width="120"/>
  </apex:barSeries>  
          </apex:chart> 
      </div>

      <div class="item" style='width:98%; padding-left: 7px;'>
  
 
 <table class="slds-table slds-table--bordered" style="padding-right:50px;padding-left:10px;border:0;">
  
  <tr>
 
 <td style="width:60%;border-top:0 !important;"> </td>
 <td style="font-weight:bold;width:20%;border-top:0 !important;text-align: left !important;padding-left: 4px;">Value</td>
 <td style="font-weight:bold;width:20%;border-top:0 !important;text-align: left !important;">% Change</td>
</tr>

 <apex:repeat value="{!kantarList}" var="kantar" rendered="true"> 
  <tr class="slds-hint-parent">
    <apex:outputText rendered="{!IF(kantar.Measures__c = 'Penetration','true','false')}">
    <td width="40%" style="font-weight:bold;"><span>{!kantar.Measures__c}</span></td>
      
    <td width="30%" style="font-weight:bold;"><span>{!ROUND((kantar.Value__c),1)}%</span></td>
 </apex:outputText>
 
 <apex:outputText rendered="{!IF(kantar.Measures__c = 'Price per pack','true','false')}">
    <td width="40%" style="font-weight:bold;"><span>{!kantar.Measures__c}</span></td>
      
    <td width="30%" style="font-weight:bold;"><span>£{!ROUND((kantar.Value__c),1)}</span></td>
 </apex:outputText>
 
 <apex:outputText rendered="{!IF(kantar.Measures__c != 'Penetration' && kantar.Measures__c != 'Price per pack','true','false')}">
    <td width="40%" style="font-weight:bold;"><span>{!kantar.Measures__c}</span></td>
      
