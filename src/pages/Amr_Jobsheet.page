   <body>
   <apex:form >
      
     
      <div class="container">
         <div class="jobportpoliosection">
            <h1>Jobs Sheet</h1>
         </div>
         <div class="admangersheet">
            <h2>AD Manager &nbsp;<apex:outputField value="{!joblist.AdManagerText__c}" /></h2>
         </div>
         
         
         
        <apex:tabPanel switchType="client" selectedTab="job_sheet" id="theTabPanel">
            <apex:tab label="Job Sheet" name="job_sheet" id="job_sheet">
                <div class="filter_section width30">
                <div class="filters width20">
                  
                  
                <div id = "newDiv" style="display:{!IF(proFileName != 'CAP' , 'None', '')}"> 
                   <div class="box_header">
                      <h3>Allocation<span class="caret2"></span></h3>
                   </div>
                   <div class="box_content filteralert">
                      <div class="subbox" style="height:64px;">
                        <div class="subbox-first" style="float:left;width:221px;" >
                            <apex:selectList label="Select the user Name" multiselect="true" size="2" value="{!selectusernamelst}" >

                             <apex:selectOptions value="{!usernam}"></apex:selectOptions>
                             </apex:selectList>
                        </div>                  
                        <div class="subbox-second" style="float:left;margin-top:14px;font-size: 15px;">
                            <apex:commandButton styleClass="allocationbtn" value="Allocate" action="{!Reallocate}"  reRender="msgs"/>
                        </div>
                        <div class="subbox-third" style="float:left;margin-top:17px;">
                            <span class="canceltxt">
                            <apex:commandLink value="Cancel this job" action="{!cancelJob}" reRender="msgs" />
                         </span>
                        </div>                                           
                      </div>
                   </div>
                </div> 
                 
                 
                 
                 
           
           
           
           
                <div id="rapDiv" style="display:{!IF(proFileName != 'RAP' , 'None', '')}">
          <apex:pageBlock >
           <table class = "alloc_table" width = "100%" style="text-align:center">
               <tr>
                    <td>
                        <apex:outputPanel >
                            <apex:outputLabel value="Accept" style="font-weight:bold;"/> <br/><br/><br/><br/><br/><br/>
                            <apex:commandButton styleClass="allocationbtn" value="Accept" action="{!acceptJob}" />
                        </apex:outputPanel>
                    </td>
                   <td> 
                        <apex:outputPanel >
                            <apex:outputLabel value="Re - Allocate" style="font-weight:bold;"/><br/><br/>

                            <apex:selectList label="Select the user Name" multiselect="true" size="2" value="{!selectedRapName}" >
                                <apex:selectOptions value="{!usernam}"></apex:selectOptions>
                            </apex:selectList> <br/>
                <apex:commandButton value="Change owner" styleClass="allocationbtn" action="{!reallocateRAP}" reRender="pgbt,msgs"/> 
                        </apex:outputPanel>
                   </td> 
                      <td  style="display:{!IF(jobStatus != 'Accepted' , 'None', '')}">  <!-- display:{!IF(jobStatus != 'Accepted' , 'None', '')} -->
                       <apex:outputPanel >
                          <apex:outputLabel value="Record Share" style="font-weight:bold;"/><br/>
                          
                            <apex:selectList id="sel1" value="{!leftselected}" multiselect="true" style="width:150px" size="3">
                                <apex:selectOptions value="{!usernam}"/>
                            </apex:selectList> <br/>
                     
                          <apex:commandButton action="{!recordShare}" value="Share" styleClass="allocationbtn"  reRender="msgs" /> 
                    </apex:outputPanel>     
                   </td>
                   <td  style="display:{!IF(primaryRap != true , 'None', '')}">     
                        <apex:outputPanel >
                            <apex:outputLabel value="Cancel" style="font-weight:bold;" /> <br/><br/><br/><br/><br/><br/>
                            <apex:commandButton value="Submit" action="{!cancelRapJob}"  styleClass="allocationbtn" reRender="msgs"/>
                        </apex:outputPanel>
                   </td>
   
                     <td>
                        <apex:outputPanel >
                            <apex:outputLabel value="Priority" style="font-weight:bold;"/><br/>
                            <apex:selectRadio layout="pageDirection" styleClass="radio_pick" value="{!priority}">
                                <apex:selectOption itemLabel="Priority" itemValue="Priority"   />
                                <apex:selectOption itemLabel="Live" itemValue="Live" />
                                <apex:selectOption itemLabel="Dormant" itemValue="Dormant" />
                            </apex:selectRadio> 
                            <apex:commandButton value="Submit" styleClass="allocationbtn" action="{!changePriority}"/>
                        </apex:outputPanel>
                   </td>
               </tr>
           </table>
        </apex:pageBlock>
     </div> 
                 
                 
                 
                 
                 
                 
                 
                
                
                
                
                
                
                </div>
             </div>
             <div class="filter_section width30">
            <div class="filters width20">
               <div class="box_header">
                  <h3>Jobsheet</h3>
               </div>
               <div class="box_content filteralert">
                  <div class="bottom_border">
                     <div class="starttxt">
                        <h3>Before you start...</h3>
                     </div>
                     <div class="paratext" style="margin-top:15px;">
                        <label>Have you checked with CMI if there is an existing asset in the MilWard Brown TV add Vitality bank that could be utilised for this JTBD</label>
                        <span class="starimg"></span>
                     </div>                    
                     <apex:inputField value="{!joblist.Check_With_CMI__c}" />
                     <section style="margin-top:25px;">
                        <div class="starttxt">
                           <h3>Unilever Information</h3>
                        </div>
                        <div class="form-group">
                           <label>Your Email Address</label><span class="starimg"></span>
                           <br/>                           
                           <apex:inputField value="{!joblist.Your_Email_Address__c}" styleClass="your_email_address" />
                        </div>
                        <div class="form-group">
                           <label>Unilever project leader email (if different) </label>
                           <br/>
                           <apex:inputField value="{!joblist.Unilever_Project_Leader_Email__c}" styleClass="your_email_address" />
                        </div>
                        <div class="form-group">
                           <label>Location of Unilever project leader </label><span class="starimg"></span>
                           <br/>                                                      
                           <apex:inputField value="{!joblist.Project_Leader_Cluster__c}" />&nbsp;&nbsp;&nbsp;&nbsp;
                           <apex:inputField value="{!joblist.Project_Leader_Country__c}" />                                                                          
                        </div>
                        <div class="form-group"> 
                           <label>Unilever Finance Member Email</label><span class="starimg"></span><br/>
                           <apex:inputField value="{!joblist.Unilever_Finance_Member_Email__c}" html-placeholder="Email Address" />
                        </div>
                        <div class="form-group"> 
                           <label>Final approver of material email</label><span class="starimg"></span>
                           <br/>
                           <apex:inputField value="{!joblist.Final_Approver_Of_Material_Email__c}" html-placeholder="Email Address" />
                        </div>
                        <div class="form-group">
                           <label>Which function are you part of (BD/BB/Other)</label>
                           <span class="starimg"></span><br/>
                           <apex:inputField value="{!joblist.Function_You_Are_Part__c}" />
                        </div>
                        <div class="form-group">
                           <label>Budget responsibility (BD, BB, Other)</label><span class="starimg"></span>
                           <br/>
                           <apex:inputField value="{!joblist.Budget_Responsibility__c}" />
                        </div>
                        <div class="form-group form-group">
                           <label>Budget allocated for this activity (enter all digits)</label>
                           <span class="starimg"></span><br/>
                           <div style="float:left;width:345px;">
                            <div class="form-field">
                              <apex:selectList value="{!selectedCountry }" size="1" id="selectedId" onchange="Calculate()" styleClass="selectedIdClass">
                                 <apex:selectOptions value="{!currencyRecords }" />
                              </apex:selectList>
                             
                           </div>
                           </div>
                           <div style="float:left;width:345px;">
                            <apex:inputField value="{!joblist.Budget_Allocated_Input_Value__c}" styleClass="agencypick inputIdClass" onkeypress="return isNumber(event)" onkeyup="Calculate()"/>
                           </div>
                           <div style="float:left;width:345px;">
                            <apex:inputField value="{!joblist.Allocated_Budget_euro__c}" styleClass="agencypick outputIdClass"/>
                           </div>
                           <div style="clear:both;">
                           </div>
                        </div>
                        <div class="form-inline">
                           <div class="form-group">
                              <label>Category >Brand Positioning > Brand</label><span class="starimg"></span><br/>
                              
                           </div>
                         <div class="form-group">
                         <!--   <div style="float:left;width:345px;">
                            <apex:inputField value="{!joblist.Category__c}"/>
                           </div>
                            <div style="float:left;width:345px;">
                            <apex:inputField value="{!joblist.Brand_Positioning__c}" />
                           </div>
                            <div style="float:left;width:345px;">
                            <apex:inputField value="{!joblist.Brand_Name__c}" />
                             <div style="clear:both;">
                           </div>     -->
                             
                                 
                                <apex:actionRegion >
                              
                               <apex:selectList value="{!categoryName}" size="1">
                                    <apex:actionSupport event="onchange" reRender="brandPosPick" />
                                    <apex:selectOptions value="{!CategoryList}" />
                                 </apex:selectList>
                              </apex:actionRegion>
                              <apex:actionRegion >
                                 <apex:selectList id="brandPosPick" value="{!brandPosName}"
                                    size="1">
                                    <apex:selectOptions value="{!BrandPositioningList}" />
                                    <apex:actionSupport event="onchange" reRender="brandPick" />
                                 </apex:selectList>
                              </apex:actionRegion>
                              <apex:actionRegion >
                                 <apex:selectList id="brandPick" value="{!brandNameFromPage}"
                                    size="1">
                                    <apex:selectOptions value="{!BrandList}" />
                                 </apex:selectList>
                              </apex:actionRegion> 
                           </div>
                           <div class="form-group">
                              
                              
                           </div>
                        </div>
                     </section>
                     <section style="margin-top:25px;">
                        <div class="starttxt">
                           <h3>Agency Information</h3>
                        </div>
                        <div class="form-group">
                           <label>Lead Agency Name</label>
                           <span class="starimg"></span><br/>
                           <apex:inputField value="{!joblist.Lead_Creative_Agency_name__c}" styleClass="agencypick" />
                           <span style="display:none;"><a href="">Cancel</a></span>
                        </div>
                        <div class="form-group">
                           <label>Lead Agency Location</label><span class="starimg"></span><br/>
                           <apex:inputField value="{!joblist.Lead_Agency_Location_Cluster__c}" />&nbsp;&nbsp;&nbsp;&nbsp;
                           <apex:inputField value="{!joblist.Lead_Agency_Location_Country__c}" />
                        </div>
                        <div class="form-group">
                           <label>Agency contact or account director email<span class="starimg"></span></label>
                           <br/>
                           <apex:inputField value="{!joblist.Agency_Contact_Account_Director_Email__c}" html-placeholder="Email Address" />
                        </div>
                        
                        <div class="form-group">
                           <label>Digital Agency Name (if applicable)</label>
                           <span class="starimg"></span><br/>
                           <apex:inputField value="{!joblist.Digital_Agency_Name__c}" html-placeholder="Digital Agency" />                           
                        </div>
                        
                        <div class="form-group">
                           <label>Any other agencies involved?</label>
                           <span class="starimg"></span><br/>
                           <apex:inputField value="{!joblist.Any_other_agencies_involved__c}" html-placeholder="Other Agency" />                           
                        </div>
                        
                        
                     </section>
                     <section style="margin-top:25px;">
                        <div class="starttxt">
                           <h3>Overview of project</h3>
                        </div>
                        <div class="form-group">
                           <label>Project name (e.g. Apollo)<span class="starimg"></span></label><br/>
                            <apex:selectList value="{!selectedProjectName}" size="1">
                                <apex:selectOptions value="{!ProjectNameList}"></apex:selectOptions>
                            </apex:selectList>
                       <!--    <apex:inputField value="{!joblist.Project_Name__c}" html-placeholder="Project Name" />  -->
                           <span style="display:none;">
                           <a href="">Cancel</a></span>
                        </div>
                        <div class="form-group">
                           <label>Project location<span class="starimg"></span></label><br/>                           
                           <apex:inputField value="{!joblist.Project_Location_Cluster__c}" html-placeholder="Cluster" />&nbsp;&nbsp;&nbsp;&nbsp;
                           <apex:inputField value="{!joblist.Project_Location_Country__c}" html-placeholder="Country" />
                        </div>
                        <div class="form-group">
                           <label>Broadcast Market</label><span class="starimg"></span><br/>
                           <apex:inputField value="{!joblist.Brodcast_Market__c}" />
                        </div>
                        
