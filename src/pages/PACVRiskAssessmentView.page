<apex:page controller="PACVRiskAssesmentRecords_CC"> 
    <apex:pageBlock title="PACV Risk Assesment Records"> 
        <apex:pageBlockTable value="{!Records}" var="Record"> 
            <apex:column > 
                <apex:facet name="header">Account Name</apex:facet> 
                <apex:outputText value="{!Record.Name}"/> 
            </apex:column> 
            <apex:column > 
                <apex:facet name="header">Project Name</apex:facet> 
                <apex:outputText value="{!Record.PACV_Project_Name__c}"/> 
            </apex:column> 
            <apex:column > 
                <apex:facet name="header">Solution Name</apex:facet> 
                <apex:outputText value="{!Record.PACV_Solution_Name__c}"/> 
            </apex:column> 
        </apex:pageBlockTable> 
    </apex:pageBlock> 
</apex:page>