        <script type="text/javascript">                     
            var agencyFlag      = true;
            var projectFlag     = true;         
            
            //    $ = jQuery.noConflict();
                        
            function transferValue(projName)
            {           
              if(projName!=="--None--") {  
                    $('.projecttextProjDetail').val(projName);          
                }else{
            $('.projecttextProjDetail').val('');
            }
            }
            
            function callActionFunctionProjectName(projectName) {       
                callContrlProjectName(projectName);
            }  
            
            
            function Calculate() {                  
                var selectedValue   = $('.selectedIdClass').val();
                var localValue      = $('.inputIdClass').val();   
              
                
                    if(localValue!==null && localValue !=="") {  
                        if(selectedValue  !=='--None--'){
                    localValue = parseInt(localValue);    
                    var finalValue =localValue/selectedValue;
                    finalValue=finalValue.toFixed(2);   
                    $('.outputIdClass').val(finalValue);
                    }else{
                    $('.outputIdClass').val(0);
                    }
                }else{
                    $('.outputIdClass').val(0);
                }               
            }
            
            function isNumber(evt) {
                evt = (evt) ? evt : window.event;
                var charCode = (evt.which) ? evt.which : evt.keyCode;
                if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                    return false;
                }
                return true;
            }
            
            function enableText() {                         
                $('.outputIdClass').removeAttr('disabled');             
                $('.defaultTv').removeAttr('disabled');
                $('.defaultCinema').removeAttr('disabled');
                $('.defaultRadio').removeAttr('disabled');
                $('.defaultPrint').removeAttr('disabled');
                $('.defaultInternalVideo').removeAttr('disabled');                               
            }
            
            $(document).ready(function() {
                alert('There');
                //transferValue();
                            
                $('.defaultTv').attr('disabled','disabled');
                $('.defaultCinema').attr('disabled','disabled');
                $('.defaultRadio').attr('disabled','disabled');
                $('.defaultPrint').attr('disabled','disabled');
                $('.defaultInternalVideo').attr('disabled','disabled');
                          
            
            
            
                //Default popup for your email address                
                $('.your_email_address').val( $('.your_email_address').attr('default-value') );
                
                //Auto calculation                                                
                $('.outputIdClass').attr('disabled', 'disabled');
                $('.outputIdClass').css('background', '#EFEFEF');
                
                // Agency logic
                                
                if($('.agencytext').val().length>0) {
                      $('.agencytext').show();
                      $('.agencypick').hide();
                      $('.agencypick option').prop('selected', false);
                      $('.agencyother').text('Cancel');
                      agencyFlag = false;
                } else {
                 $('.agencytext').val('');
                 $('.agencytext').hide();
                }
                            
                $('.agencyother').click(function(e) {
                    e.preventDefault();
                     if(agencyFlag == true){
                      $('.agencypick').hide();
                      $('.agencypick option').prop('selected', false);
                      $('.agencytext').show();
                      $('.agencyother').text('Cancel');
                      agencyFlag = false;
                 } else {
                      $('.agencytext').val('');
                      $('.agencytext').hide();
                      $('.agencypick').show(); 
                      $('.agencyother').text('Other');
                      agencyFlag = true;
                 }
                });  
                
                
                
                // Project info logic
                if($('.projecttext').val().length>0) {
                 $('.projecttext').show();
                 $('.projectpick').hide();
                 $('.projectpick option').prop('selected', false);
                 $('.projectotherClass').text('Cancel');
                 projectFlag = false;
                } else { 
                    $('.projecttext').val('');
                    $('.projecttext').hide();
                    $('.projectpick').show(); 
                    $('.projectotherClass').text('Other');
                    projectFlag = true;   
                }
              
                $('.projectotherClass').click(function(e) {
                 e.preventDefault();                    
                 if(projectFlag == true) {                   
                      $('.projectpick').hide();
                      $('.projectpick option').prop('selected', false);
                      $('.projecttext').show();
                      $('.projectotherClass').text('Cancel');
                      projectFlag = false;
                 } else {
                      $('.projecttext').val('');
                      $('.projecttext').hide();
                      $('.projectpick').show(); 
                      $('.projectotherClass').text('Other');
                      projectFlag = true;
                 }
                });
                
                // production activity logic 
                //1. if none selected, dont show anything and only one master can be selected
                
                var masterselection = true;
                $('.master').click(function() {
                    if(this.checked) {
                    
                        var selected_parent = $(this).attr('data-id');
                        $('.secondary-parent').hide();
                        $('.'+selected_parent).show();
                        
                        
                       // $('.dfn-selected').attr('class','');
                      //  $(this).parent().children('label').children('dfn').attr('class','dfn-selected'); //sunil
                          removeOnlyOneClass('dfn-selected'); //sunil
                          $(this).parent().addClass('dfn-selected'); //Sunil
                        
                        
                        masterselection = true;
                        
                        if($(this).hasClass('original_master')) {
                            $('.other_master').attr('disabled', 'disabled');
                        } else {
                            $('.original_master').attr('disabled', 'disabled');
                        }                        
                        
                    } else {
                    
                        $('.secondary-parent').hide();//Sunil
                        
                        //uncheck associates
                        var selected_parent = $(this).attr('data-id');
                        console.log('parent is'+selected_parent+'__');                    
                        $('.'+selected_parent).find('input:checkbox').prop('checked', false);
                        
                        //start swarup comments
                            $('.'+selected_parent).hide(); 
                        //  $(this).parent().find('dfn').attr('class',''); //sunil
                            removeOnlyOneClass('dfn-selected'); //sunil
                            
                            var flip_to_selection = false;
                            var flip_to;
                            $('.master').each(function() {                                                      
                                if(this.checked) {                                  
                                    flip_to = $(this).attr('data-id');
                                   // alert(flip_to);
                                    flip_to_selection = true;                                         
                                }
                            }); 
                            
                            if(flip_to_selection == true) {                             
                                $('.'+flip_to).show();
                                //$('input[data-id='+flip_to+']').parent().children('label').children('dfn').attr('class','dfn-selected'); //sunil
                                $('input[data-id='+flip_to+']').parent().addClass('dfn-selected'); //sunil
                            }
                            
                        //end swarup comments
                        // end
                                                                    
                        masterselection = false;
                        $('.master').each(function() {                                                      
                            if(this.checked) {
                                masterselection = true;     
                            }
                        });
                        
                        if( masterselection == false ) {
                            
                          //  $('.dfn-selected').attr('class',''); //Sunil
                          removeOnlyOneClass('dfn-selected'); //sunil
                            $('.original_master').removeAttr('disabled');
                            $('.other_master').removeAttr('disabled');
                            $('.summary').html('');
                            
                            $('.secondary-parent').hide();
                            
                            $('.mediamasterdiv input:checkbox').each( function() {
                                $(this).prop('checked', false);
                            });
                            
                            $('.submediamasterdiv input:checkbox').each( function() {
                                $(this).prop('checked', false);
                            });
                            
                            disableDependent();           
                        }                                                                                             
                    }
                    getSummary();
                    
                });
                
                
                // 2. default value selected
                
                $('.additional').click(function() {
                    selectedDefault = $(this).attr('data-id');
                    if(this.checked) {                    
                      //  $('.'+selectedDefault).prop('checked', true);
                      $(this).closest(".secondary-parent").find('.'+selectedDefault).prop('checked', true);                   
                    } else {                
                      //  $('.'+selectedDefault).prop('checked', false);
                      $(this).closest(".secondary-parent").find('.'+selectedDefault).prop('checked', false);
                    }
                });
                
                $('.additional2').click(function() {                    
                    selectedDependent = $(this).attr('media');                    
                    if(this.checked) {                                                                 
                       // $(this).closest(".secondary-parent").find('.dependent'+selectedDependent).find('input:checkbox').prop('checked', true);
                        $(this).closest(".secondary-parent").find('.dependent'+selectedDependent).find('input:checkbox').removeAttr('disabled');               
                    } else {                                        
                        $(this).closest(".secondary-parent").find('.dependent'+selectedDependent).find('input:checkbox').prop('checked', false);
                        $(this).closest(".secondary-parent").find('.dependent'+selectedDependent).find('input:checkbox').attr('disabled','disabled'); 
                    }
                }); 
                
                $('.master_parent label').click(function(){
                    
                    var flag = false;
                    
                    $(this).parent('div').find('input:checkbox').each(function() {                 
                        if(this.checked) {
                            flag = true;
                        }
                    });
                    
                    if( flag == true ) {
                        var siblingCheckboxMedia = $(this).parent('div').children('input:checkbox').attr('data-id');                    
                        $('.secondary-parent').hide();
                        $('.'+siblingCheckboxMedia).show();
                       // $('.dfn-selected').attr('class',''); //Sunil
                        // $(this).children('dfn').attr('class','dfn-selected'); //Sunil
                        removeOnlyOneClass('dfn-selected'); //sunil
                        $(this).parent().addClass('dfn-selected'); //Sunil   
                    }                                                                              
                });
                
                $('.mediamasterdiv input:checkbox').click(function() {                  
                    if(this.checked) {
                        var required_parent = $(this).closest('.secondary-parent').attr('class');
                        required_parent = required_parent.replace('secondary-parent','').trim();
                        
                        $('.master').each(function(){
                            var required_parent1 = $(this).attr('data-id');
                            if( required_parent == required_parent1 ) {
                                $(this).prop('checked', true);
                            }
                        });                                             
                    }                                   
                });
                
                //Summary logic
                $('.mediamasterdiv input:checkbox, .submediamasterdiv input:checkbox').click(function() {
                    getSummary();                                       
                });                     
                                
                disableDependent();   
                
                /*Start of repeated code*/
                
                var selected_master = false; 
                
                $('.master').each(function(){
                    if(this.checked) {
                    
                        if($(this).hasClass('original_master')) {
                            $('.other_master').attr('disabled', 'disabled');
                        } else {
                            $('.original_master').attr('disabled', 'disabled');
                        } 
                    
                        var selected_parent = $(this).attr('data-id');
                        $('.secondary-parent').hide();
                        $('.'+selected_parent).show();
                        
                        
                        //$('.dfn-selected').attr('class',''); //sunil
                        //$(this).parent().children('label').children('dfn').attr('class','dfn-selected'); //sunil
                        
                        removeOnlyOneClass('dfn-selected'); //sunil
                          $(this).parent().addClass('dfn-selected'); //Sunil
                        
                        selected_master = true;
                    }       
                });
                
                if( selected_master == true ) {
                    getSummary();
                }               
                
                $('.additional2').each(function(){
                    selectedDependent = $(this).attr('media');                    
                    if(this.checked) {                                                                                     
                        $(this).closest(".secondary-parent").find('.dependent'+selectedDependent).find('input:checkbox').removeAttr('disabled');               
                    } else {                                        
                        $(this).closest(".secondary-parent").find('.dependent'+selectedDependent).find('input:checkbox').prop('checked', false);
                        $(this).closest(".secondary-parent").find('.dependent'+selectedDependent).find('input:checkbox').attr('disabled','disabled'); 
                    }               
                });                             
                
                /*End of repeated code*/            
                                
            });
            
            function removeOnlyOneClass(selected) {
                $('.'+selected).each(function(){
                    var otherClasses         = $(this).attr('class');
                    var modifiedOtherClasses = otherClasses.replace(selected,'');
                    $(this).attr('class',modifiedOtherClasses);
                });             
            }
            function disableDependent() {
                $('.dependentOut_Of_Home input:checkbox').attr('disabled','disabled');
                $('.dependentIn_Store input:checkbox').attr('disabled','disabled');
                $('.dependentDigital input:checkbox').attr('disabled','disabled');
            }
            
            function getSummary() {
                $('.summary').hide();
                $('.summary').html('');
                
                $('.sub-master').each(function() {                                                      
                    if(this.checked) {
                        $('.summary').html('<div class="summary-title">Summary</div><div class="clear"></div>');     
                    }
                });
                
                $('.secondary-parent').each(function() {
                    var level1  = $(this).closest('.secondary-parent').attr('summaryText');
                    level1      = level1.replace('secondary-parent','').trim();
                        $(this).find('.mediamasterdiv').each(function(){                                                        
                            if($(this).find('input:checkbox').is(':checked')) {
                                if( $(this).find('input:checkbox').hasClass('additional') ) {
                                    level2 = $(this).find('input:checkbox').attr('summary-text');
                                    $('.summary').show();
                                    var classname = level1.replace(/\s/g, '-');                                 
                                    if($('.'+classname).length) {
                                        $('.'+classname).append(', '+level2);
                                    } else {                                        
                                        $('.summary').append('<div class="summary-detail">'+level1+'</div><div class="summary-detail1 '+classname+'">'+level2+'</div><div class="clear"></div>');
                                    }                                   
                                }                                                                                                                                       
                            }
                                                                        
                        });
                        
                        /*start new code*/
                        
                        $(this).find('.submediamasterdiv').each(function(){                                                     
                            if($(this).find('input:checkbox').is(':checked')) {                             
                                
                                $(this).find('input:checkbox').each(function(){
                                    if(this.checked) {
                                        level2 = $(this).attr('labelText');                                     
                                        if(level2) {
                                            //$('.summary').append('<div class="summary-detail">'+level1+'</div><div class="summary-detail1">'+level2+'</div><div class="clear"></div>');
                                            $('.summary').show();
                                            var classname = level1.replace(/\s/g, '-');                                 
                                            if($('.'+classname).length) {
                                                $('.'+classname).append(', '+level2);
                                            } else {
                                                $('.summary').append('<div class="summary-detail">'+level1+'</div><div class="summary-detail1 '+classname+'">'+level2+'</div><div class="clear"></div>');
                                            }
                                            
                                        }                                       
                                    }                                   
                                });                                                                                                                                                                                                 
                            }
                                                                       
                        });
                        
                        /* end new code*/
                       
                });         
            }
                     
     
        </script>
      
