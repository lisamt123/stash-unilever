<apex:page standardController="IPM_Project_Document_Section__c" sidebar="false" showHeader="false"> 
<apex:includeScript value="{!URLFOR($Resource.ipmResourceRevamp, 'js/jquery.js')}"/>
  <c:IPM_ObjectHistoryComponent myObjectName="{!IPM_Project_Document_Section__c}" recordLimitCount="50"></c:IPM_ObjectHistoryComponent>
 <script>
    var jq = jQuery.noConflict();  
    jq(document).ready(function(){
    jq('#ipmSecHistoryTab .ipmTabContent').show();
    jq('#ipmSecHistoryTab .ipmTabContent:last').hide();
    jq('#ipmSecHistoryTab .ipmTabs li:first').addClass('active');                
    jq('#ipmSecHistoryTab .ipmTabs li').on('click', function(e){ 
        e.preventDefault();
        jq('#ipmSecHistoryTab .ipmTabs li').removeClass('active');                   
        var getId = jq(this).attr('class');
        jq('#ipmSecHistoryTab .ipmTabContent').hide();
        jq(this).addClass('active');
        jq('#'+getId).fadeIn("fast");                    
    }); 
    });     
</script>  
<apex:outputPanel ></apex:outputPanel>
</apex:page>