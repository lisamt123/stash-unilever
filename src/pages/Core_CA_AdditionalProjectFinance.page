<apex:page docType="html-5.0" showHeader="false" standardStylesheets="false" sidebar="false">
    <meta http-equiv="content-type" content="text/html; charset=UTF8" />
    <div class="iPhoneHomeScreenHeight" id="pageTop">
        <div id="homePage1" class="headerBarBackground">
            <table class="headerBarWidth">
                <tr>
                    <td class="headerBarRowAlign" >
                        <img src="{!URLFOR($Resource.MyApprovalsSR, 'img/back.svg')}" ng-click="gotoDetailPage()" alt="back" class="homePageFilter" />
                    </td>
                </tr>
            </table>
        </div>
        
        <div class="defaultHeadding defaultBottomSpace defaultTopSpace rowTableWidth defaultLeftSpace ">   
            &nbsp; &nbsp; &nbsp;
            <label class="detailSectionHeader"  >{{'LBL_CONTRACT_SOA' | translate}}</label>
        </div>
        <div class="template">            
            <table class="homePageMenuWidth smallHeight">
                <tr class="defaultBottomBorder">
                    <td class="mediumWidth alignTop">                          
                        <span class="detailSectionHeader"> {{'LBL_PROPOSED_BUDGET' | translate}} </span>
                    </td>
                    <td class="mediumWidth alignTop">
                        <span class="detailSectionHeader" ng-show="('{{approvalRequestData.ApprovalType}}'=='APPTYPE4' || '{{approvalRequestData.ApprovalType}}'== 'APPTYPE6' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE9' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE11')"> {{'LBL_PROJECT_FINANCES_BY_YEAR'| translate}} </span>
                    </td>
                </tr>
            </table>
            <div class="responsiveRow" >
                    <div class="responsiveColumn">
                        <table class="responsiveDetailSectionTable">
                            <tr>
                                <td class="responsiveDetailSectionTableRowLabel"> {{'LBL_TRADE_ID' | translate}}</td>
                            </tr>
                            <tr>
                                <td class="responsiveDetailSectionTableRowValue"><label ng-if="approvalRequestData.CompanyCode != null && approvalRequestData.CompanyCode != ''"> {{approvalRequestData.Currencies}}</label> {{approvalRequestData.CompanyCode | replaceNullValue}} </td>
                            </tr>
                        </table>
                    </div>
                    <div class="responsiveColumn" ng-show="('{{approvalRequestData.ApprovalType}}'=='APPTYPE4' || '{{approvalRequestData.ApprovalType}}'== 'APPTYPE6' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE9' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE11')">
                        <table class="responsiveDetailSectionTable">
                            <tr>
                                <td class="responsiveDetailSectionTableRowLabel">{{approvalRequestData.PreTripDetails.PassengerInfo.split(',')[0]}} </td>
                            </tr>
                            <tr>
                                <td class="responsiveDetailSectionTableRowValue"><label ng-if="approvalRequestData.PreTripDetails.PassengerInfo.split(',')[1] != null && approvalRequestData.PreTripDetails.PassengerInfo.split(',')[1] != ''"> {{approvalRequestData.Currencies}}</label>   {{approvalRequestData.PreTripDetails.PassengerInfo.split(',')[1] | replaceNullValue}}</td>
                            </tr>
                        </table>
                    </div>
                </div>
                <div class="responsiveRow">
                    <div class="responsiveColumn">
                        <table class="responsiveDetailSectionTable">
                            <tr>
                                <td class="responsiveDetailSectionTableRowLabel"> {{'LBL_CAPEX' | translate}}</td>
                            </tr>
                            <tr>
                                <td class="responsiveDetailSectionTableRowValue"> <label ng-if="approvalRequestData.Capex != null && approvalRequestData.Capex != ''"> {{approvalRequestData.Currencies}}</label> {{approvalRequestData.Capex | replaceNullValue}}</td>
                            </tr>
                        </table>
                    </div>
                    <div class="responsiveColumn" ng-show="('{{approvalRequestData.ApprovalType}}'=='APPTYPE4' || '{{approvalRequestData.ApprovalType}}'== 'APPTYPE6' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE9' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE11')">
                        <table class="responsiveDetailSectionTable">
                            <tr>
                                <td class="responsiveDetailSectionTableRowLabel"> {{approvalRequestData.Currentyear1.split(',')[0]}}</td>
                            </tr>
                            <tr>
                                <td class="responsiveDetailSectionTableRowValue"><label ng-if="approvalRequestData.Currentyear1.split(',')[1] != null && approvalRequestData.Currentyear1.split(',')[1] != ''"> {{approvalRequestData.Currencies}}</label>  {{approvalRequestData.Currentyear1.split(',')[1] | replaceNullValue}}</td>
                            </tr>
                        </table>
                    </div>
                </div>
                <div class="responsiveRow">
                    <div class="responsiveColumn">
                        <table class="responsiveDetailSectionTable">
                            <tr>
                                <td class="responsiveDetailSectionTableRowLabel"> {{'LBL_RESTRUCTURING' | translate}}</td>
                            </tr>
                            <tr>
                                <td class="responsiveDetailSectionTableRowValue"><label ng-if="approvalRequestData.PreTripDetails.Id != null && approvalRequestData.PreTripDetails.Id != ''"> {{approvalRequestData.Currencies}}</label>  {{approvalRequestData.PreTripDetails.Id | replaceNullValue}}</td>
                            </tr>
                        </table>
                    </div>
                    <div class="responsiveColumn" ng-show="('{{approvalRequestData.ApprovalType}}'=='APPTYPE4' || '{{approvalRequestData.ApprovalType}}'== 'APPTYPE6' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE9' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE11')">
                        <table class="responsiveDetailSectionTable">
                            <tr>
                                <td class="responsiveDetailSectionTableRowLabel"> {{'LBL_ALL_OTHER_YEARS' | translate}} </td>
                            </tr>
                            <tr>
                                <td class="responsiveDetailSectionTableRowValue"><label ng-if="approvalRequestData.YearAllOther != null && approvalRequestData.YearAllOther != ''"> {{approvalRequestData.Currencies}}</label>  {{approvalRequestData.YearAllOther | replaceNullValue}}</td>
                            </tr>
                        </table>
                    </div>
                </div>
                <div class="responsiveRow">
                    <div class="responsiveColumn">
                        <table class="responsiveDetailSectionTable">
                            <tr>
                                <td class="responsiveDetailSectionTableRowLabel"> {{'LBL_TOTAL' | translate}}</td>
                            </tr>
                            <tr>
                                <td class="responsiveDetailSectionTableRowValue"> <label ng-if="approvalRequestData.PreTripDetails.LocationDetails != null && approvalRequestData.PreTripDetails.LocationDetails != ''"> {{approvalRequestData.Currencies}}</label>  {{approvalRequestData.PreTripDetails.LocationDetails | replaceNullValue}}</td>
                            </tr>
                        </table>
                    </div>
                    <div class="responsiveColumn">
                        <table class="responsiveDetailSectionTable" ng-show="('{{approvalRequestData.ApprovalType}}'=='APPTYPE4' || '{{approvalRequestData.ApprovalType}}'== 'APPTYPE6' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE9' || '{{approvalRequestData.ApprovalType}}'==  'APPTYPE11')">
                            <tr>
                                <td class="responsiveDetailSectionTableRowLabel"> {{'LBL_TOTAL' | translate}}</td>
                            </tr>
                            <tr>
                                <td class="responsiveDetailSectionTableRowValue"><label ng-if="approvalRequestData.Toatl2 != null && approvalRequestData.Toatl2 != ''"> {{approvalRequestData.Currencies}}</label>  {{approvalRequestData.Toatl2 | replaceNullValue}}</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    <br/>
    <!--editing  to fix the allignment Ss-->
</apex:page>