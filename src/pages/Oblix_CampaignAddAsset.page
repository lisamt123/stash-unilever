                                                                                           <apex:outputPanel rendered="{!IF(hasEditAccess || $Profile.Name =='System Administrator',true,false)}">
                                                                                            <input value="UPDATE" onclick="updateAsset(this, {!selectionCount}); if(setMendatory($(this))) blockme(); return false;" class="btn btn-primary hide_button" style="width:70px" type="submit"  />  <!--return false;reRender="assetPart,campaignFeeId,estimateId,Oblix_card" -->
                                                                                            </apex:outputPanel>
                                                                                        </div>

                                                                                    </apex:actionRegion>
                                                                           
                                                                                </apex:outputPanel>
                                                                                <apex:variable var="selectionCount" value="{!selectionCount + 1}" />
                                                                            </div>
