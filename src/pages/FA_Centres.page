                    <script type="text/javascript">  
                     $(document).ready(function() {                        
                        $=$.noConflict(); 
                       $('div.sidebar').css("display","none");
                       $("a").click(function(){                       
                        if(this.href.indexOf('mapPage')!=-1){
                         var ContentID=this.href.split("#")[1].split('-')[0] + '-map';
                         // alert(ContentID);
                         buildMap(ContentID);
                         }
                            });                 
                         }) ;     
                                function buildMap(divmap) {
                                
                                var ContentID=divmap.split("-");
                                var addressID= 'address'+ContentID[0];
                                var locationAdd=$('#'+addressID).val();
                                    //alert(locationAdd);
                              var myOptions = {
                                zoom: 15,
                                mapTypeId: google.maps.MapTypeId.ROADMAP,
                                mapTypeControl: false
                              }
                            
                              var map;
                              var marker;                             
                              
                            
                              var geocoder = new google.maps.Geocoder();                            
                                    // var address ="George Road,Kingston-upon-Thames,United Kingdom,KT2 7PD";
                              var address =locationAdd;
                              var infowindow = new google.maps.InfoWindow({                              
                              content: locationAdd
                              });
                            
                              geocoder.geocode( { address: address}, function(results, status) {
                                if (status == google.maps.GeocoderStatus.OK && results.length) {
                                  if (status != google.maps.GeocoderStatus.ZERO_RESULTS) {
                                  //Initializing the map div size 
                                          $('#'+ divmap).css(
                                                  {
                                                     'height' : $(window).height() +'px',
                                                      'width' : $(window).width()  +'px'
                                                  });
                                    //create map
                                    map = new google.maps.Map(document.getElementById(divmap), myOptions);
                            
                                    //center map
                                    map.setCenter(results[0].geometry.location);
                                    var currCenter = map.getCenter();
                            
                                    //create marker
                                    marker = new google.maps.Marker({
                                        position: results[0].geometry.location,
                                        map: map,
                                   
                                       title: "Unilever Four Acres"
                                       
                                       
                                    });
                            
                                    //add listeners
                                    google.maps.event.addListener(marker, 'click', function() {
                                      infowindow.open(map,marker);
                                    });
                                    google.maps.event.addListener(infowindow, 'closeclick', function() {
                                      map.setCenter(marker.getPosition()); 
                                    });
                                    setTimeout(function() {
                                                  google.maps.event.trigger(map,'resize');
                                                   map.setCenter(currCenter);
                                                   infowindow.open(map,marker);
                                                 }, 500);
                            
                                  }
                            
                                } else {
                                  $('#'+ divmap).css({'height' : '15px'});
                                  $('#'+divmap).html("Oops! 's address could not be found, please make sure the address is correct.");
                                  resizeIframe();
                                }
                              }); 
                                    
                             
                              $('#'+ divmap + 'page' ).live( 'pagebeforeshow',function(event, ui){alert('fef');
                                         // alert( 'This page was just hidden: '+ ui.prevPage);
                                        });
                             
                              function resizeIframe() {
                                var me = window.name;
                                if (me) {
                                  var iframes = parent.document.getElementsByName(me);
                                  if (iframes && iframes.length == 1) {
                                    height = document.body.offsetHeight;
                                    iframes[0].style.height = height + "px";
                                  }
                                }
                              }
                            
                            }
                                             
                              
                    
                    </script>
                         
                        
                  

