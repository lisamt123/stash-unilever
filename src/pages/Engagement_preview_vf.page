     <apex:outputPanel id="panel2" rendered="{!q.Rating__c==7}">
     <div class="checkbox good">
    <div class="questext col-md-10">
    <p style="font-weight:bold; font-size:18px"><apex:outputText value="{!q.Answer__c}" style="color:black"></apex:outputText></p>
    </div>
    </div>
    </apex:outputPanel>
    
