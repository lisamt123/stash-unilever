    <script type="text/javascript">
        var stringArraySavingSubmitted = JSON.parse('{!lstcov }');


        function PCEuroAmountAccepted() {

            var PCCurrency = $j('.PCCurrencyAccepted').val();
            var PCLocalAmount = $j('.PCLocalAmountAccepted').val();

            PCEuroAmountAcceptedCall(stringArraySavingSubmitted, PCCurrency, PCLocalAmount);
        }



        function PPCEuroAmountAccepted() {
            var PPCCurrency = $j('.PPCCurrencyAccepted').val();
            var PPCLocalAmount = $j('.PPCLocalAmountAccepted').val();

            PPCEuroAmountAcceptedCall(stringArraySavingSubmitted, PPCCurrency, PPCLocalAmount);

        }


        function CAEuroAmountAccepted() {
            var CACurrency = $j('.CACurrencyAccepted').val();
            var CALocalAmount = $j('.CALocalAmountAccepted').val();

            CAEuroAmountAcceptedCall(stringArraySavingSubmitted, CACurrency, CALocalAmount);
        }



        function commonCurrencyRelatedNew() {

            var PCLocalAmountNew = $j('.PCLocalAmountNew').val();
            var PCEuroAmountNew = $j('.PCEuroAmountNew').val();
            var PCCurrencyNew = $j('.commonCurrencyRelatedNew').val();

            commonCurrencyRelatedNewCall(stringArraySavingSubmitted, PCLocalAmountNew, PCEuroAmountNew, PCCurrencyNew)
        }

        function commonCurrencyRelatedPPCNewFun() {
            var PPCLocalAmountNew = $j('.PPCLocalAmountNew').val();
            var PPCEuroAmountNew = $j('.PPCEuroAmountNew').val();
            var PPCCurrencyNew = $j('.commonCurrencyRelatedPPCNew').val();
            commonCurrencyRelatedPPCNewFunCall(stringArraySavingSubmitted, PPCLocalAmountNew, PPCEuroAmountNew, PPCCurrencyNew);
        }

        function commonCurrencyRelatedCANewFun() {

            var CALocalAmountNew = $j('.CALocalAmountNew').val();
            var CAEuroAmountNew = $j('.CAEuroAmountNew').val();
            var CACurrencyNew = $j('.commonCurrencyRelatedCANew').val();

            commonCurrencyRelatedCANewFunCall(stringArraySavingSubmitted, CALocalAmountNew, CAEuroAmountNew, CACurrencyNew);
        }        
        
        $j(function() {
            var starRatingData = {!SelectedRatingStar};
            starRatingProcess(starRatingData);
        });
        
        $j(function(){
        	
        	if( $j('.noOfMastersMain').length ) {
	        	loadMasterProcess();
