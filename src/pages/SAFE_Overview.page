<apex:page docType="html-5.0" sidebar="false" showHeader="true">
   <html lang="en">
      <head>
         <meta name="viewport" content="width=device-width, initial-scale=1 " charset="utf-8" />
         <title>Overview</title>
        <apex:stylesheet value="{!URLFOR($Resource.SAFEStaticResource, '/SAFEStaticResource/css/SAFE_Tabs.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.SAFEStaticResource, '/SAFEStaticResource/css/jquery-ui.css')}"/>
        <apex:includeScript value="{!URLFOR($Resource.SAFEStaticResource, '/SAFEStaticResource/jsfile/jquery-1.12.4.js')}"/> 
        <apex:includeScript value="{!URLFOR($Resource.SAFEStaticResource, '/SAFEStaticResource/jsfile/jquery-ui.js')}"/> 
        
        <script>
            $( function() { $("#tab-solution").hide();var answered=0;var icons = { header: "iconOpen", activeHeader: "iconClosed"};$("#ovrTab ").click(function(){ window.location.reload(true);});
            /*Overview Tab*/
            $("#tab-1").find(".accordion").accordion({collapsible:true,active:false,icons:icons});
            /*Questionairre Tab Styles...*/
              $("#tab-2").find(".accordion").accordion({collapsible:true,active:false,icons:icons});
              $('#donebtn').click(function () {$(".Tabs_Menu").tabs({active:1});});
              $(".Tabs_Menu").tabs();$(".Btn_Confirm").on("click",function(){ 
                  $('.ui-state-active').find('.done').addClass('Ans');//new code added 
                  $('.ui-accordion-header-active').css({'color':'#555'}); 
                  $('.ui-accordion-content-active').find('.Btn_Delegate').attr('disabled',true).css("cursor", "default");});             
              $("#Acc_Expand").click(function(){$(".ui-accordion-header-icon").addClass('iconClosed').removeClass('iconOpen');$(".ui-accordion-content").show();});
              $("#Acc_Collapse").click(function(){$(".ui-accordion-header-icon").addClass('iconOpen').removeClass('iconClosed');$(".ui-accordion-content").hide();});
              $("#tab-3 .Selection_CTA").click(function(){ $("#tab-3").hide(); $("#tab-2").show();}); 
                           $("#sol").click(function(){ $("#tab-solution").show();}); });
         </script>
       
      </head>
      <body >
         <div class="Tabs_Menu" id='mytabs'>
            <ul class="nav_tabs">
               <li><a id="ovrTab" href="#tab-1">OverView</a></li>
               <li><a href="#tab-3" id="qusTab">Questionnaire</a></li>
               <li><a href="#tab-4" id="sol">Solution</a></li>
            </ul>
            <apex:include pageName="SAFE_OverviewTab"/>
            <apex:include pageName="SAFE_QuestionaireTab"/>
            <apex:include pageName="SAFE_SolutionTab"/>
           
         </div>
      </body>
   </html>
</apex:page>