            <apex:actionFunction action="{!removeAsset}" name="removeAsset" rerender="assets_panel,swop_top_panel,addAssetButtonPanel,deleteTableDefinition,deleteTableHeader" oncomplete="$.unblockUI();$('#sowListTable').DataTable({
