                                                        <apex:image onclick="" url="{!URLFOR($Resource.Oblix_SwopAssets, 'Oblix_SwopAssets/images/redcross.png')}" width="12" height="12" alt="Delete" />
                                                    </a>

                                                </td>

                                            </apex:outputPanel>
                                        </tr>
                                    </apex:repeat>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </apex:outputPanel>
