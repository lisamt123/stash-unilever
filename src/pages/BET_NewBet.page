<apex:page showHeader="true" sidebar="false" standardController="uw_BET__c" extensions="BET_NewBetController">

	<apex:includeScript value="{!URLFOR($Resource.jscolor_1_4_1, 'jscolor/jscolor.js')}" />
	<apex:includeScript value="{!$Resource.BETApp}/js/jquery.min.js" />    
	<apex:includeScript value="{!$Resource.BETApp}/js/jquery.qtip.min.js" />    
	<apex:stylesheet value="{!$Resource.BETNewBetStyles}" />
    
	<style>
		.question-icon, .question-icon-left {
    		display: inline-block;
    		width: 13px;
    		height: 13px;
    		margin-left: 5px;
    		background: url({!$Resource.BETApp}/images/question-mark.png) center center/100% no-repeat;
    		cursor: pointer;
		}
	</style>

	<apex:form >
    	<apex:pageMessages escape="false"/>
    	<c:BET_NewBetComponent betobj="{!bet}" cdata="{!componentData}"/>
	</apex:form>

</apex:page>