                                    <div title="{!$ObjectType.Amr_Job_Details__c.fields.ProductCategory__c.inlineHelpText}"></div>
                                </apex:outputLabel>
                            </div>
                            <div class="form-field margin-left-minus30">
                                <apex:actionRegion >
                                    <select id="categoryList" size="1" onchange="transferCategoryValue(this.value)" reRender="brandPosPick" class="categorySelected"></select>
                                    <apex:actionSupport event="onchange" reRender="brandPosPick" status="counterStatus11" />
                                    <!-- <apex:selectList value="{!categoryName}" size="1">
                                <apex:selectOptions value="{!CategoryList}" />
                                </apex:selectList> -->
                                </apex:actionRegion>
                                <apex:actionstatus id="counterStatus11">
                                    <apex:facet name="start">
                                        <img src="/img/loading.gif" alt="loading" />
                                    </apex:facet>
                                </apex:actionstatus>
                                <div class="loading loading-category-image">
                                    <img src="/img/loading.gif" alt="loading" />
                                </div>
                            </div>
                            <div class="form-field">
                                <apex:actionRegion >
                                    <apex:selectList id="brandPosPick" value="{!brandPosName}" size="1">
                                        <apex:selectOptions value="{!listOfBrandPosition}" />
                                        <apex:actionSupport event="onchange" reRender="brandPick" status="actStatusId1" />
                                    </apex:selectList>
                                </apex:actionRegion>
                                <div class="w100ac">
                                    <br/>
                                    <apex:actionStatus id="actStatusId1">
                                        <apex:facet name="start">
                                            <img src="/img/loading.gif" alt="loading" />
                                        </apex:facet>
                                    </apex:actionStatus>
                                </div>
                            </div>
                            <div class="form-field">
                                <apex:actionRegion >
                                    <apex:selectList id="brandPick" value="{!brandNameFromPage}" size="1">
                                        <apex:selectOptions value="{!BrandList}" />
                                    </apex:selectList>
                                </apex:actionRegion>
                            </div>
