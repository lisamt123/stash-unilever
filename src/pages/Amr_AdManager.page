<apex:page controller="Amr_AdManagerController_CC" title="AdManager" sidebar="false" action="{!Redirect}" tabStyle="AD_Manager__tab" >    
    <head>
        <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"/>
        <apex:includeScript value="/support/console/31.0/integration.js"/>
        <script type="text/javascript">
            $j = jQuery.noConflict();
        </script>
        <script type="text/javascript">
            $j(function(){
                $j('.loadError').each(function(){ 
                    var lengthOfParent = $j(this).attr('data-width');
                    if(lengthOfParent) {
                        $j(this).closest('.form-field-parent').addClass('loadErrorParent width70percent');
                    } else {
                        $j(this).closest('.form-field-parent').addClass('loadErrorParent');
                    }                                       
                }); 
            });            
        </script>
        <apex:includeScript value="{!URLFOR($Resource.Amr_static_resources,'Amr-static-resource/js/menu-shift.js')}"/>
        <apex:includeScript value="{!URLFOR($Resource.Amr_static_resources,'Amr-static-resource/js/production-activity.js')}"/>
        <apex:includeScript value="{!URLFOR($Resource.Amr_static_resources,'Amr-static-resource/js/admanager-logging-form.js')}"/>    
            
        <style type="text/css">
            .errorM3 {
                border:3px solid red;
                border-radius:0px !important;
                margin:0 !important;
                color:red !important;
                background-color:#FFFFFF !important;
                width:40%;
            }
            .errorM3 span {
                color:red !important;
            }
            .errorM2 .messageText {
                color:red !important;
            }           
            .width70percent {
                width:70%;
            }
            #AllTab_Tab {
                display:none;
            }
            .loadError {
                color:red !important;
                position:absolute !important;
                top:0px !important;
                left: 0 !important;
            }
            .loadErrorParent {
                border:3px solid red;
                background:#EFE4B0;
                padding: 25px 0 10px 0;
                position:relative;
                margin: 12px 0;
            }
            .form-field-level1 {
                background:#FFFFFF;
            }         
            @font-face {
            font-family: 'dinOffPro';             
            src:url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro.eot')}")  format('embedded-opentype'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro.woff2')}") format('woff2'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro.woff')}") format('woff'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro.ttf')}") format('truetype'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro.svg#svgFontName')}") format('svg');            
            }
            @font-face {
            font-family: 'dinOffProBold';             
            src:url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Bold.eot')}")  format('embedded-opentype'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Bold.woff2')}") format('woff2'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Bold.woff')}") format('woff'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Bold.ttf')}") format('truetype'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Bold.svg#svgFontName')}") format('svg');            
            }
            @font-face {
            font-family: 'dinOffProBoldIta';             
            src:url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-BoldIta.eot')}")  format('embedded-opentype'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-BoldIta.woff2')}") format('woff2'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-BoldIta.woff')}") format('woff'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-BoldIta.ttf')}") format('truetype'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-BoldIta.svg#svgFontName')}") format('svg');            
            }
            @font-face {
            font-family: 'dinOffProIta';             
            src:url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Ita.eot')}")  format('embedded-opentype'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Ita.woff2')}") format('woff2'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Ita.woff')}") format('woff'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Ita.ttf')}") format('truetype'),
            url("{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/fonts/UnileverDINOffcPro-Ita.svg#svgFontName')}") format('svg');            
            }            
        </style>
        <style type="text/css">
            label div {
            background: url("{!URLFOR($Resource.Amr_static_resources,'Amr-static-resource/images/questionmark.png')}") no-repeat scroll center center rgba(0, 0, 0, 0);
            }           
            .star {
            background: url("../images/star.png") no-repeat scroll left center rgba(0, 0, 0, 0);                
            }
            label strong {
            background: url("{!URLFOR($Resource.Amr_static_resources,'Amr-static-resource/images/star.png')}") no-repeat scroll center center rgba(0, 0, 0, 0);             
            }            
            .starfield {
            background: url("{!URLFOR($Resource.Amr_static_resources,'Amr-static-resource/images/star.png')}") no-repeat scroll left center rgba(0, 0, 0, 0);               
            }
        </style>
        <apex:stylesheet value="{!URLFOR($Resource.Amr_static_resources, 'Amr-static-resource/css/logging-form.css')}" />
    </head>
    <body>
        <apex:form >        
              <apex:include pageName="amr_common_header" />
            <div class="container">             
