                                                    <apex:inputTextarea value="{!bosscard.IPM_Resources__c}" styleClass="countTarget" title="Please Enter Resources" disabled="{! if(OR(bosscard.IPM_Is_Accepted__c=true,bosscard.IPM_Bosscard_Status__c='Not Approved',isEditable=false),'true','false')}" richText="true" />
                                                   <!-- <apex:inputField value="{!bosscard.IPM_Resources__c}" rendered="{!if(OR(bosscard.IPM_Is_Accepted__c=false,bosscard.IPM_Bosscard_Status__c !='Not Approved',bosscard.IPM_Bosscard_Status__c !='Approved',isEditable=true),'true','false')}" />
                                                    <apex:outputField value="{!bosscard.IPM_Resources__c}" rendered="{!if(OR(bosscard.IPM_Is_Accepted__c=true,bosscard.IPM_Bosscard_Status__c='Not Approved',isEditable=false),'true','false')}" />-->       
