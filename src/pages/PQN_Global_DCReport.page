<apex:page controller="PQN_Global_DCReport" showHeader="false" readOnly="true">
<body>
    <div id="wrapper">
        <c:PQN_Navigation navigationName="Reports" subNavName="dcreport"/>
        <div id="page-wrapper">
            <div class="container-fluid">
 <apex:form >
 
      <apex:pageBlock >
          <apex:pageblockTable value="{!xSUWRAPLIST}" var="xD" columnsWidth="20%, 20%, 20%, 20%, 20%" >
              <apex:column headerValue="" >
                  <apex:outputText value="{!xD.Cluster}" style="font-weight:800" rendered="{!xD.child}" />
                  <apex:outputText value="{!empty}" rendered="{!xD.child}" />
              
                 &nbsp; &nbsp; &nbsp; &nbsp;<apex:outputText value="{!xD.ReportingDC }"  />
              </apex:column>              
              <apex:column headerValue="PY DC" value="{!xD.preYeardata}" /> 
              <apex:column headerValue="CM DC data" value="{!xD.cumnthdata }" />
              <apex:column headerValue="CY DC" value="{!xD.cuYeardata}" />
               
          </apex:pageblockTable>
      </apex:pageBlock>
      
</apex:form>
</div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
    </div>
</body>
</apex:page>