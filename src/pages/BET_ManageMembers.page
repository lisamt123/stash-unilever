   <apex:pageBlock id="requestpending" title="Pending Requests">
    <apex:pageBlockSection columns="1">
       <apex:pageBlockTable value="{!pendingRequests}" var="pending" style="width:100%;" columnsWidth="56px,105px,170px,109px,86px">
       <apex:column >
           <apex:facet name="header">Photo</apex:facet>  
           <apex:image url="{!pending.Team_Member__r.SmallPhotoUrl}"/>
         </apex:column>
       <apex:column >
           <apex:facet name="header">Name</apex:facet>  
           <apex:outputField value="{!pending.Team_Member__r.Name}"/>
         </apex:column>
         <apex:column >
           <apex:facet name="header">Title</apex:facet>  
           <apex:outputField value="{!pending.Team_Member__r.Title}"/>
         </apex:column>
         <apex:column rendered="{!hasModifyAccess}">
            <apex:facet name="header">Approve</apex:facet>  
            <!--
            <apex:outputField value="{!pending.id}" rendered="true"/>
            -->
            <!-- <apex:commandButton value="Approve" action="{!approveRequest}" styleClass="buttong2y" style="background: #e6007e; color:white;" rerender="showpage" > -->
            <!-- T1 Start -->   
             <apex:commandButton value="Approve" action="{!approveRequest}" styleClass="buttong2y" style="background: #e6007e; color:white;" rerender="showpage" onclick="this.value = 'Approving...'">
            <!-- T1 End -->
                <apex:param name="id" value="{!pending.id}" assignTo="{!recordId}" />
                
           </apex:commandButton>
            <!--
           <apex:inputCheckbox value="{!pending.Pending_Request__c}"/>
           -->
         </apex:column>
         <apex:column rendered="{!hasModifyAccess}">
            <apex:facet name="header">Deny</apex:facet>  
            <!--
            <apex:outputField value="{!pending.id}" rendered="true"/>
            -->
            <!--
           <apex:inputCheckbox value="{!pending.Request_Denied__c}"/>
           -->
          <!-- <apex:commandButton value="Deny"  action="{!denyRequest}"  styleClass="buttong2y" style="background: #e6007e; color:white;" rerender="showpage" > -->
            <!-- T1 Start -->  
              <apex:commandButton value="Deny"  action="{!denyRequest}"  styleClass="buttong2y" style="background: #e6007e; color:white;" rerender="showpage" onclick="this.value = 'Denying...'">
            <!-- T1 End -->  
              <apex:param name="id" value="{!pending.id}" assignTo="{!recordId}" />
           </apex:commandButton>
         </apex:column>
       </apex:pageBlockTable>
    </apex:pageBlockSection>
    </apex:pageBlock>
    <!--
    <apex:pageBlock id="hiddenBlock" rendered="false"></apex:pageBlock>
    -->
    
    <apex:pageBlock title="Browse Members" id="browsemembers">
    <apex:outputPanel layout="none" rendered="{!myPrivilegesChanged}">
        <script type="text/javascript">
            window.top.postMessage({action: 'checkPrivileges'}, document.location.protocol + '//' + document.domain);
        </script>
    </apex:outputPanel>
    <apex:pageBlockSection columns="1">
       <apex:pageBlockTable value="{!members}" var="member" style="width:100%;" columnsWidth="56px,105px,170px,109px,86px">
          <apex:column >
           <apex:facet name="header">Photo</apex:facet>  
           <apex:image url="{!member.Team_Member__r.SmallPhotoUrl}"/>
         </apex:column>
         <apex:column >
         <apex:facet name="header">Name</apex:facet>  
           <apex:outputField value="{!member.Team_Member__r.Name}" rendered="{!!ISBLANK(member.Team_Member__c)}"/>
           <apex:outputPanel layout="block" html-data-tooltip="{!member.email__c}" styleClass="uwtooltip" style="width:105px;text-overflow:ellipsis;overflow:hidden;" rendered="{!ISBLANK(member.Team_Member__c)}">
              {!member.email__c}
           </apex:outputPanel>
         </apex:column>
         <apex:column >
         <apex:facet name="header">Title</apex:facet>  
           <apex:outputField value="{!member.Team_Member__r.Title}"/>
         </apex:column>
         <apex:column > 
           <apex:facet name="header">Role</apex:facet> 
           <!--
           <apex:selectList id="roles" value="{!uw_BET_Team_Member__c.Role__c}"
                size="1" required="true" onchange="alert('Value Changed');">
           <apex:selectOptions value="{!roles}"/>
           </apex:selectList>
           -->
           <!--
           <apex:inputField id="memberRole" value="{!member.Role__c}" onchange="notifyUser('{!member.id}', '{!$Component.memberRole}')"/> 
             <apex:inputField id="memberRole" value="{!member.Role__c}" onchange="updateRole('{!member.id}', '{!$Component.memberRole}')"  rendered="{!member.Role__c != 'External Agency User'}"/>  
           -->
           <apex:inputField id="memberRole" value="{!member.Role__c}" onchange="updateRole('{!member.id}', '{!$Component.memberRole}')" required="{!member.Role__c != 'External Agency User'}" rendered="{! hasModifyAccess == true && member.Role__c != 'External Agency User'}"/> 

           <apex:outputField id="memberRoleExternal" value="{!member.Role__c}" rendered="{! hasModifyAccess==false || member.Role__c == 'External Agency User'}"/> 
    
         <!--
          <apex:inputField id="memberRoleInternal" value="{!member.Role__c}" onchange="updateRole('{!member.id}', '{!$Component.memberRole}')" rendered="{!member.Role__c != 'External Agency User'}"/> 
           <apex:outputField id="memberRoleExternal" value="{!member.Role__c}" rendered="{!member.Role__c == 'External Agency User'}"/> 
         -->
         </apex:column>      
         <apex:column rendered="{!hasModifyAccess && $User.Id != member.Team_Member__c}">
            <apex:facet name="header">Delete</apex:facet>  
            <!--
            <apex:outputField value="{!pending.id}" rendered="true"/>
            -->
            <!--<apex:commandButton value="Delete" action="{!deleteMember}" styleClass="buttong2y" style="background: #e6007e; color:white;" rerender="browsemembers" > -->
            <!-- T1 Start -->    
             <!-- T2 Start -->
              <!--<apex:commandButton value="Delete" action="{!deleteMember}" styleClass="buttong2y" style="background: #e6007e; color:white;" rerender="browsemembers" status="saveStatus4"> -->
              <apex:commandButton value="Delete" rendered="{!member.IsOwner__c == false}" action="{!deleteMember}" styleClass="buttong2y" style="background: #e6007e; color:white;" rerender="browsemembers" status="saveStatus4">
             <!-- T2 End -->
             <apex:actionStatus id="saveStatus4">
              <apex:facet name="start">
               <apex:image value="/img/loading.gif" height="16" width="16" />
              </apex:facet> 
             </apex:actionStatus>
            <!-- T1 End -->   
                <apex:param name="id" value="{!member.id}" assignTo="{!recordId}" />
           </apex:commandButton>
            <!--
           <apex:inputCheckbox value="{!pending.Pending_Request__c}"/>
           -->
         </apex:column>
       </apex:pageBlockTable>
    </apex:pageBlockSection>
  
    <apex:actionFunction name="changeRole" action="{!setRole}" rerender="browsemembers, requestpending">
        <apex:param id="memberId" name="memberId" value=""/>
        <apex:param id="role" name="role" value=""/>
    </apex:actionFunction>
  
  </apex:pageBlock>
  </apex:outputpanel>
