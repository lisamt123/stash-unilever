  <apex:pageBlock title="Add Members" id="addmembers" rendered="{!hasModifyAccess}">
    <apex:pageBlockSection columns="1"> 
     <apex:pageBlockSectionItem >
     <apex:pageBlockTable value="{!BETTeamMember}" var="teamMamber" headerClass="hide-header" columnsWidth="135px,220px">
         <apex:column >
    <apex:outputPanel layout="inline" styleClass="question-icon"
        html-data-tooltip="Members are only able to view the BET and upload into the BB Sharing tab. <br>They cannot access anything in the Settings menu.<br><br>Managers have the same permissions as the owner" />
       <apex:inputField label="Role" value="{!BETTeamMember.Role__c}" required="true" style="padding-right: 2px;margin-bottom: 0px;padding-left: 0;"/>
        </apex:column>
         <apex:column >
         <!--
       <apex:inputField label="User" value="{!BETTeamMember.Team_Member__c}" style="width:100%;"/>
       -->
       <apex:inputField label="User" type="text" value="{!BETTeamMember.Team_Member__c}" styleclass="hideDropdown" />
        </apex:column>
         <apex:column >
         <!-- <apex:commandButton value="Add Member" action="{!addMember}" styleClass="buttong2y" style="background: #e6007e; color:white;" rerender="showpage" /> -->
