<apex:page standardController="BET_Member_Request__c" showHeader="false" extensions="BET_ManageMembersController"  id="memberpage">

  <style>

.addMembersUserLabel {
  font-family: "ArialMT","Arial";
  font-weight: 400;
  font-style: normal;
  font-size: 18px;
  color: #333;
  text-align: left;
  line-height: normal;
  padding-left: 10px;
}

.addMembersDropdown {
    position: relative;
    left: 0px;
    top: 0px;
    width: 135px;
    height: 34px;
    font-family: "Calibri","Calibri";
    font-weight: 400;
    font-style: normal;
    font-size: 13px;
    text-decoration: none;
    color: #515151;
    text-align: left;
    line-height: normal;
    padding: 5px 5px 5px 5px;
}

.addMembersUserInput {
  position: absolute;
  padding-top: 32px;
  padding-left: 155px;
}

.hideOwner select {
    display: none;
}

body .lookupInput input {
    font-family: "Rockwell";
    font-size: 14px;
    border: 2px solid #DCDCDC;
    padding: 6px 10px 6px 10px;
    color: #4D5051;
    width: 297px;
}

.addmemberbutton{
  position: absolute;
  padding-left: 520px;
}

.addmemberEmailInput{
  width: 442px;
}

body .btn.buttong2y[value="Add Member"] {
  background-image: url('data:image/png;base64,') !important;
  background-repeat: no-repeat !important;
  background-position: 7px center !important;
  padding: 9px 10px 9px 30px;
}

body .lookupIcon,
body .lookupIconOn {
  width: 35px;
  height: 34px;
  position: absolute;
  margin-left: 0px;
  background-image: url('data:image/gif;base64,');
  background-position: center center;
  padding-bottom: 0px !important;
}

.addmemberEmailInput{
  width: 492px;
  height: 32px;
}

.rich-tabhdr-side-border {
  display: none;
}
.rich-tab-header {
  font-family: 'Rockwell';
  width: 208px;
  padding: 10px 0px;
  border: none;
  font-size: 17px;
  cursor: pointer;
}
.rich-tabhdr-cell-active .rich-tabhdr-side-cell {
  border: none;
}
.rich-tabhdr-cell-active .activeTab {
  color: #ffffff;
  background-position: center bottom;
  background-color: transparent;
  background-image: url('data:image/png;base64,');
}
.rich-tabhdr-cell-inactive {
  padding: 0px;
}
.rich-tabhdr-cell-inactive .rich-tabhdr-side-cell {
  border: none;
}
.rich-tabhdr-cell-inactive .inactiveTab {
  color: #be236d;
  background-position: center bottom;
  background-color: transparent;
  background-image: url('data:image/png;base64,');
}

.approveMembersheaderStyle{
    font-family: "ArialMT","Arial";
    font-weight: 400;
    font-style: normal;
    font-size: 18px;
    color: #F2F2F2;
    text-align: center;
    line-height: normal;
    background-color: #515151;
    height: 41px;
}

.browseMembersheaderStyle{
    font-family: "ArialMT","Arial";
    font-weight: 400;
    font-style: normal;
    font-size: 18px;
    color: #F2F2F2;
    text-align: center;
    line-height: normal;
    background-color: #515151;
    height: 41px;
}

.memberRequestRow td{
  height: 104px;
  font-size: 13px;
  color: #333;
  text-align: center;
  line-height: normal;
}

.memberRequestRow img{
  width: 70px;
  height: 82px;
}

.memberRequestRow a{
  color: #797979;
}

.memberRow td{
  height: 104px;
  font-size: 13px;
  color: #333;
  text-align: center;
  line-height: normal;
}

.memberRow img{
  width: 70px;
  height: 82px;
}

.memberRow a{
  color: #797979;
}

.headerButtons {
  padding-left: 657px;
  height: 40px;
}

.headerButtonsBrowsMembers{
  padding-left: 737px;
  height: 40px;
}

  </style>

  <script>

  function selectOrDeselectkAllBrowse(){
    var ch;
    var a = document.querySelectorAll(".browseMembersheaderStyle input[type=checkbox]");
    for (var i = 0; i < a.length; i++){
       ch = a[i].checked;
    }
    var aa = document.querySelectorAll(".memberRow input[type=checkbox]");
    for (var i = 0; i < aa.length; i++){
       aa[i].checked = ch;
    }
  }

  function selectOrDeselectkAll(){
    var ch;
    var a = document.querySelectorAll(".approveMembersheaderStyle input[type=checkbox]");
    for (var i = 0; i < a.length; i++){
       ch = a[i].checked;
    }
    var aa = document.querySelectorAll(".memberRequestRow input[type=checkbox]");
    for (var i = 0; i < aa.length; i++){
       aa[i].checked = ch;
    }
  }
  </script>

  <apex:form id="form">
  <apex:outputpanel id="showpage">
  <apex:pageMessages /> 

  <apex:tabPanel switchType="client" selectedTab="browseMembers" id="MembersTabPanel" tabClass="activeTab" 
                       inactiveTabClass="inactiveTab">

    <apex:tab label="Browse members" name="browseMembers" id="browseMembersid">
     
        <div>
          <apex:outputText value="Add Internal User" styleClass="addMembersUserLabel"/>
        </div>
        <table>
          <tr>
            <td>
              <apex:inputField value="{!BETTeamMember.Role__c}" styleClass="addMembersDropdown" required="false"/>
            </td>
            <td>
              <div class="hideOwner">   
                <apex:inputField value="{!BETTeamMember.UserId__c}"/>
              </div>
            </td>
            <td>
              <div style="padding-left:40px;">
                <apex:commandButton value="Add Member" action="{!addMember}" rendered="{!isBetOwnerOrManager}"
                                    styleClass="buttong2y" style="background: #e6007e; color:white;" />
              </div>
            </td>
          </tr>
        </table>
    
        <div>
          <apex:outputText value="External Agency User" styleClass="addMembersUserLabel"/>
        </div>
          <table>
            <tr>
              <td>
                  <apex:inputField value="{!BETTeamMember.External_User_Email__c}" styleClass="addmemberEmailInput" />
              </td>
              <td>
                <div style="padding-left:5px;">
                <apex:commandButton value="Add Member" action="{!addExternalMember}" rendered="{!isBetOwnerOrManager}"
                                    styleClass="buttong2y" style="background: #e6007e; color:white;"/> 
                </div>
              </td>
            </tr>
          </table>


        <div class="headerButtonsBrowsMembers">
        
          <apex:commandButton value="Delete selected" action="{!massDelete}"  rendered="{!isBetOwnerOrManager}"
            styleClass="buttong2y" style="background: #e6007e; color:white;height: 30px;"/> 
       
        </div>

        <table width="100%">
          <th class="browseMembersheaderStyle" style="{!IF(isBetOwnerOrManager,'','display:none')}">
            <apex:inputCheckbox value="{!selectAll}" id="globcheckboxbrowse" onchange="selectOrDeselectkAllBrowse();"/>
          </th>
          <th class="browseMembersheaderStyle">
            Photo
          </th>
          <th class="browseMembersheaderStyle">
            Name
          </th>
          <th class="browseMembersheaderStyle">
            Role
          </th>
          <th class="browseMembersheaderStyle" style="{!IF(isBetOwnerOrManager,'','display:none')}">
            Role
          </th>
          <th class="browseMembersheaderStyle" style="{!IF(isBetOwnerOrManager,'','display:none')}">
            Delete
          </th>
          <apex:repeat value="{!members}" var="member">
            <tr class="memberRow">
              <td style="{!IF(isBetOwnerOrManager,'','display:none')}">
                <apex:inputCheckbox value="{!member.isSelected}"/>
              </td>
              <td>
                <apex:image url="{!member.photo}"/>
              </td>
              <td>
                <apex:outputText value="{!member.firstName} {!member.lastName}"/>
              </td>
              <td>
                <apex:outputText value="{!member.role}"/>
              </td>
              <td style="{!IF(isBetOwnerOrManager,'','display:none')}">
                <apex:inputField value="{!member.obj.Role__c}"/>
              </td>
              <td style="{!IF(isBetOwnerOrManager,'','display:none')}">
                <apex:commandLink action="{!doDelete}" value="Delete">
                </apex:commandLink>
              </td>
            </tr>
          </apex:repeat>
        </table>

    </apex:tab>

    <apex:tab label="Approve members" name="approveMembers" id="approveMembersid">

      <div class="headerButtons">
        
        <apex:commandButton value="Approve selected" action="{!massApprove}" rendered="{!isBetOwnerOrManager}"
          styleClass="buttong2y" style="background: #e6007e; color:white;height: 30px;"/> 
       
        <apex:commandButton value="Deny selected" action="{!massDeny}" rendered="{!isBetOwnerOrManager}"
          styleClass="buttong2y" style="background: #e6007e; color:white;height: 30px;"/> 
       
      </div>

      <table width="100%">
        <th class="approveMembersheaderStyle" style="{!IF(isBetOwnerOrManager,'','display:none')}">
            <apex:inputCheckbox value="{!selectAll}" id="globcheckbox" onchange="selectOrDeselectkAll();"/>
        </th>
        <th class="approveMembersheaderStyle">
            Photo
        </th>
        <th class="approveMembersheaderStyle">
            Name
        </th>
        <th class="approveMembersheaderStyle">
            Project
        </th>
        <th class="approveMembersheaderStyle" style="{!IF(isBetOwnerOrManager,'','display:none')}">
            Approve
        </th>

        <apex:repeat value="{!memberRequests}" var="memberRequest">
          <tr class="memberRequestRow">
            <td style="{!IF(isBetOwnerOrManager,'','display:none')}">
              <apex:inputCheckbox value="{!memberRequest.isSelected}"/>
            </td>
            <td>
              <apex:image url="{!memberRequest.photo}"/>
            </td>
            <td>
              <apex:outputText value="{!memberRequest.firstName} {!memberRequest.lastName}"/>
            </td>
            <td>
              <apex:outputText value="{!memberRequest.projectName }"/>
            </td>
            <td style="{!IF(isBetOwnerOrManager,'','display:none')}">
              <apex:commandLink action="{!doApprove}" value="Approve">
                <apex:param name="obid" assignTo="{!selectedRequestId}" value="{!memberRequest.objectId}"/>
              </apex:commandLink>
              &nbsp;|&nbsp; 
              <apex:commandLink action="{!doDeny}" value="Deny">
                <apex:param name="obid" assignTo="{!selectedRequestId}" value="{!memberRequest.objectId}"/>
              </apex:commandLink>
            </td>
          </tr>
        </apex:repeat>

      </table>

  
    </apex:tab>

  </apex:tabPanel>
    
  </apex:outputpanel>
  </apex:form>
</apex:page>