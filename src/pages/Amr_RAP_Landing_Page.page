<apex:page id="thePage">
   <head>
      <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <title>Uniliver</title>
      <link href='http://fonts.googleapis.com/css?family=Open+Sans|Baumans' rel='stylesheet' type='text/css'/>
      <style>
         body{background:#ffffff;font-family: 'Open Sans', sans-serif;margin:0 auto;}
         /* Header css star */
         header {
         background: url("../images/header.jpg") no-repeat scroll center top #C0D4EC;
         height: 110px;
         }
         .right_section{margin:0 auto;width:1130px;}   
         /* account section css */
         .Container ul li{border: 1px solid #41719c;border-top-left-radius: 35px;border-top-right-radius: 35px;display: inline-block;font-size: 12px;line-height: 18px;list-style-type: none;margin: 0 0 0 6px;padding: 12px 30px;width: 50px;text-align: center;}
         .Container ul {text-align: right;}
         .Container2 > ul {margin: 0;padding: 0;}
         .Container2 .savings{padding-top: 25px;}
         .sub_section h1{font-size:20px;margin-left: 30px;}
         .Container2 ul li{border: 1px solid #41719c;border-top-left-radius: 35px;border-top-right-radius: 35px;display: inline-block;font-size: 12px;line-height: 18px;list-style-type: none;margin: 0 0 0 6px;padding: 10px 25px;width: 40px;text-align: center;}
         /* Alert section  css */
         #Alert_section{border: 1px solid #41719c;border-radius: 25px;width:1130px;display: inline-flex; margin: 55px 0 0;}
         #Alert_section .Aspchar{color:#a6a6a6;}
         #Alert_section .Anumber{color:#a6a6a6;}
         #Alert_section ul li{list-style-type:none;line-height: 28px;}
         #Alert_section ul li span{margin:0px 0px 0px 45px}
         #Alert_section .Alerts_text2{width:60%;}
         #Alert_section .Alerts_text2 ul li{list-style-type:none;}
         .Alerts_text2 ul li span{margin:0px 0px 0px 0px;}
         #Alert_section .Alerts_text h1{padding:0px 0px 0px 10px}
         .Alerts_text .alerttxtnew {width: 20%;}
         .Alerts_text {width:100%;}
         .Alerts_text .alerttxtnew h2{ margin: 10px 35px;}
         .Alerts_text .width20 select{width:95%;border: 1px solid #41719c;}
         .Alerts_text .Alerts_text2 ul {float: right;position: relative;right: 45px;top: 75px;}
         .Alerts_text .datedropdown{width:15%;}
         .Alerts_text .alerttxtarea{border: 1px solid #41719c;border-radius: 10px;padding: 18px 0px 0px 12px;width: 90%;}
         .btn{ background: none repeat scroll 0 0 #5b9bd5;border: 1px solid #41719c;border-radius: 12px;color: #fff;padding: 4px 30px;text-align: center;text-decoration: none;}
         .btn_section{text-align:right;width: 94%;margin-top: 20px;}
         .vaues_text {width:10%;}
         .vaues_text2 {width: 10%;}
         .vaues_text1 {width: 79%;}
         .errorsection{margin:20px 50px;}
         #comments_subsection .Aspchar{color:#a6a6a6;}
         #comments_subsection{border: 1px solid #41719c;border-radius: 25px;margin: 30px 0 0;width: 1130px;}
         .error_subsection{border: 1px solid #41719c;border-radius: 25px;width: 1000px;padding: 6px 0 0 30px;font-size: 25px;color: #a6a6a6;}
         .error_subsection h3{padding:0px 0px 0px 10px;}
         .commentssection{padding: 0 0 100px 50px;}
         #comments_subsection h1{padding:0px 0px 0px 10px;}
         .btnclick{text-align: right;width: 94%;margin-top:20px;}
         #creative_section{border: 1px solid #41719c;border-radius: 25px;margin: 30px 0 0;width: 1130px;}
         #creative_section .Aspchar{color:#a6a6a6;}
         .browsetxt{margin: 0 0 30px 70px;width:88%;}
         .browe_btn{background: none repeat scroll 0 0 #5b9bd5; border: 1px solid #41719c; border-radius: 12px;color: #fff;padding: 4px 20px;text-decoration: none;float:right;}
         .browsetxt input[type=text] {width: 30%;background:#f2f2f2;font-size: 20px;padding: 5px;width: 36%;border:1px solid #41719c;}
      </style>
   </head>
   <body>
    
      <header>
      </header>
     
