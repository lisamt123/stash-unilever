                    <apex:outputPanel rendered="{!showClearIcon}"> 
                        <div id="icon-area">
                            <apex:image width="40" height="40" rendered="{! bet.Icon_Location__c!=''  }" value="/servlet/servlet.FileDownload?file={!bet.Icon_Location__c }"/>
