<apex:component controller="IPM_Gate_RollOuts_Controller" allowDML="true">
    <table border="0" class="ipmTable table table-bordered" cellspacing="0">
        <thead>
            <tr>
                <th><div>Regional Rollouts</div></th>
                <th><div>Geography</div></th>
                <th><div>Status</div></th>
                <th><div>Target Launch Date</div></th>
                <th><div>Local Project Name</div></th>
            </tr>
        </thead>
        <tbody>
            <apex:repeat value="{!ipm_countries}" var="coun" id="theRepeat">
                <tr>
                    <td><apex:outputfield value="{!coun.Local_Rollout__r.Regional_Rollout__c}"/></td>
                    <td><apex:outputfield value="{!coun.Country_Name__c}"/></td>
                    <td><apex:outputfield value="{!coun.Local_Rollout__r.IPM_Status__c}"/></td>
                    <td><apex:outputfield value="{!coun.Local_Rollout__r.Target_Launch_Date__c}"/></td>
                    <td><apex:outputfield value="{!coun.Local_Rollout__c}"/></td>
                </tr>
            </apex:repeat>
        </tbody>
    </table>
</apex:component>