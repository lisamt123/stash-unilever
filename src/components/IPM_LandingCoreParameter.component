<apex:component controller="IPM_CoreParametersViewController" allowDML="true">
 <!--<apex:attribute name="ProId" assignTo="{!ProjectName}" type="String" required="false" description="Get project id"/>-->
    <style>
        .panel-label label{margin-bottom: 5px;font-weight: normal;color:#333333;}
        .panel-field-value{background-color: #f5f5f5;margin-bottom: 15px;padding: 5px !important;min-height: 30px;border: 1px #ddd solid;color:#666666;}  
        #strategicIntentTable.table-bordered>tbody>tr>th {border: none;white-space: initial;padding: 7px;text-align: right;color:#666666;font-weight: normal;font-size: 0.850em;}
        #strategicIntentTable.table-bordered>thead>tr>th {border: none;width: 10%;white-space: initial;padding: 4px;text-align:center;color:#666666;font-weight: normal;font-size: 0.850em;}
        #strategicIntentTable.table-bordered{border: none;}
        #strategicIntentTable.table-bordered>tbody>tr>td{text-align:center;}
        .panel-title{padding-bottom: 15px;} 
        .sectionDiv{margin:35px 15px 15px 15px; border:1px #ddd solid;}
        .sectionDiv .sectionHeading{margin-top: -1.7%;margin-left: 1.7%;background-color: #fff;width: 23%;margin-bottom:3%;}
        .sectionHeading#projectSpanTitle{width:21%;}
        .tableTitle{margin-left: 19%;font-weight:normal;color:#00B389;}
        .editBtn{float: right;background-color: #f5f5f5;padding: 5px 5px 5px 11px !important;text-align: center;margin-top: -5%;}
        .editBtn span{font-size:1.5em;}
        .tableIndicator{margin-top:15px;}
        .tableIndicator .box{height: 20px;width: 20px; float:left;}
        .orange.box{background-color: #ed8c23;}
        .yellow.box{background-color: #ffdc42;}
        .blue.box{background-color: #72c4e9;}
        .tableIndicator label{text-transform:uppercase; margin:2px 5px;font-weight:normal; color:#666;}
        .ipmPageHeader{padding-top:4.5%;}
        /*.ipmContainer div.container-fluid{padding-top:2%;}*/
        .sectionDiv div.ErrMsg{margin-bottom:10px;margin-top: -10px;padding-left: 0 !important;}
    
    /* About Project Styles */
       .coreParamTitle{font-size: 1.2em;display: block;float: left;padding: 10px 10px 10px 0px;border-right: 1px solid #B2B2B2;margin-top: 10px;margin-bottom: 10px;font-weight: bold;color: #3B516D;}  
       .coreDelete{padding: 15px 0px 0px 10px;}
       .aboutProContainer{border:1px solid #929292;padding-top:10px;border-radius:4px;}
       .coreProtitle{color:#3B516D;font-weight:bold;display:block;} 
       .coreProvalue{display:block;font-size:1.6em;color:#007DBB;}
       .coreTopContainer{padding:10px 0px;border-bottom:1px solid #C9CBC4;width:100%;float:left;}
       .coreMtitle{font-weight:bold;display:block;color:#555555;}
       .coreMvalue{color:#555555;display:block;}
       .custCoreparmborder{border-right:1px solid #7F7F7F;height: 60px;}
       .projectraLabel{padding-left:10px;color: #565656;position: relative;bottom: 3px;}
       .fastTrackcheck{margin-left: 15px !important;width: 30px;height: 17px;}
       .coreCheckrow{padding-top:10px;padding-bottom:10px;}
       .coreBg{background:#F5F9EC;}
       
     /* Core Parameters page Styles */
     
     .coreFieldContainer{margin-bottom:20px;}
     .coreSectionFtitle{display: block;float: left;margin-right: 30px;color: #555555;font-weight: bold;}
     .coreSectionFvalue{color:#565656;}  
     .custFieldsborder{border-right:0px solid #7F7F7F;}
     .noMarginBottom{margin-bottom:0px;}
     .fieldSection{border-bottom:1px solid #C9CBC4;float:left;padding:10px 0px 30px;}
     .graphContainer{padding:20px 0px;}
     .additionalMarbottom{margin-bottom:20px;}
     .tableIndicator label{text-transform:capitalize;}
     .graphHeading{display: block;color: #3B506B;margin-bottom:10px;font-size: 1.2em;font-weight: bold;}
     .graphCategory{display: block;color: #555555;margin-bottom:10px;font-weight: bold;}
     .graphContent{color:#676767;}
      #strategicIntentTable .fa{color:#fff;}
      
    </style>
        <apex:form >
            <div class="row">
                <div class="col-sm-12">
                    <span class="coreParamTitle">About Project</span>
                    <apex:outputPanel rendered="{!isEditable}">
                        <div class="SecButtonSet coreDelete editProject " value="/apex/ipmEditProject?Pid={!projectName}">
                            <a class="icoButton edit1" data-toggle="modal" data-target="#ipmEditProject" data-keyboard="false" data-backdrop="static" title="Edit"></a>
                        </div>
                    </apex:outputPanel>
                </div>
            </div>
            <div class="row coreMtop">
                <div class="col-sm-12">
                    <div class="col-sm-12 noPadleft noPadright aboutProContainer">
                        <div class="coreTopContainer">
                            <div class="col-sm-3 custCoreparmborder">
                                <span class="coreProtitle">Project Name</span>
                                <span class="coreProvalue coreCustmargin "><apex:outputtext value="{!ProjectCoreParemeters.name}"/></span>
                            </div>
                            <div class="col-sm-2 custCoreparmborder">    
                                <span class="coreMtitle">Category</span> 
                                <span class="coreMvalue"><apex:outputtext value="{!ProjectCoreParemeters.IPM_Category_Text__c}"/></span>   
                            </div>
                            <div class="col-sm-2 custCoreparmborder">
                                <span class="coreMtitle">Brand Positioning</span>
                                <span class="coreMvalue"><apex:outputtext value="{!ProjectCoreParemeters.IPM_Brand_Positioning__c}"/></span>    
                            </div>
                           <!-- <div class="col-sm-2 custCoreparmborder">
                                <span class="coreMtitle">Brand</span>
                                <span class="coreMvalue"><apex:outputtext value="{!ProjectCoreParemeters.IPM_Brand__c}"/></span>
                            </div>-->
                            <div class="col-sm-3">    
                                <span class="coreMtitle">Target Launch Date</span>
                                <span class="coreMvalue"><apex:outputField value="{!ProjectCoreParemeters.IPM_Target_Launch_Dates__c}"/></span>
                            </div>
                        </div>
                        <div class="coreTopContainer coreBg">
                            <div class="col-sm-12">
                                <span class="coreMtitle">Description</span>
                                <span class="coreMdesc coreCustmargin "><apex:outputtext value="{!ProjectCoreParemeters.IPM_Project_Description__c}"/></span>
                            </div>
                        </div>
                        <div class="coreTopContainer coreBg">
                            <div class="col-sm-12">
                                <div class="col-sm-6 noPadleft custCoreparmborder"> 
                                    <span class="coreMtitle"><apex:outputLabel value="Who will approve the Charter Gate?" /></span> 
                                    <span class="coreMvalue"><apex:outputtext value="{!ProjectCoreParemeters.IPM_Approver_of_Charter__c}"/></span>
                                </div>
                                <div class="col-sm-6">
                                    <span class="coreMtitle"><apex:outputLabel value="Who will approve the Contract and Market Ready Gate?" /></span>
                                    <span class="coreMvalue"><apex:outputtext value="{!ProjectCoreParemeters.IPM_GateKeeping_Model__c}"/> </span>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12 coreBg coreCheckrow noPadleft">
                               <apex:inputCheckbox styleClass="fastTrackcheck" disabled="true" value="{!ProjectCoreParemeters.IPM_Fast_Track_Project__c}"/>
                                <span class="projectraLabel">I want this project to be a Fast track Project</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <apex:pageMessages id="ValidationMsg" escape="false"></apex:pageMessages> 
                <div class="col-sm-12">
                    <span class="coreParamTitle">Core Parameters</span>
                    <apex:outputPanel rendered="{!isEditable}">
                    <div class="SecButtonSet coreDelete">
                        <a class="icoButton edit1 editcoreparam" data-toggle="modal" data-target="#ipmEditCoreparameters" data-keyboard="false" data-backdrop="static" title="Edit"></a>
                    </div>
                     </apex:outputPanel>
                    <apex:pagemessages />
                </div>
            </div>
            <div class="row coreMtop">
                <div class="col-sm-12">
                    <div class="col-sm-12 noPadleft noPadright aboutProContainer">
                        <div class="col-sm-12 noPadleft noPadright fieldSection">
                            <div class="col-sm-6 noPadright custFieldsborder">
                               <!-- <div class="col-sm-12 noPadleft noPadright coreFieldContainer">
                                    <div class="col-sm-6 noPadleft noPadright">
                                        <span class="coreSectionFtitle">Project Type</span>
                                    </div>
                                    <div class="col-sm-6 noPadleft noPadright">
                                        <span class="coreSectionFvalue">:  {!ProjectCoreParemeters.IPMProject_Type__c}</span>
                                    </div>
                                </div>-->
                                <div class="col-sm-12 noPadleft noPadright coreFieldContainer">
