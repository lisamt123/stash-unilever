     <script>
       function toggleSideBar(className){
          if(className =="navbar-toggle false"){   
              $("#navbar_toggle").removeClass( "navbar-toggle false" ).addClass("navbar-toggle true");
              $("#navbar_toggle").css({"transform": "rotate(-90deg)","margin-left": "1px"});
              $("#wrapper").css({"padding-left": "40px"});
              $(".side-nav").css({"left": "202px", "width": "58px"});
              
          }else{
              $("#navbar_toggle").removeClass( "navbar-toggle true" ).addClass("navbar-toggle false");
              $("#navbar_toggle").css({"transform": "rotate(0deg)","margin-left": "50px"});
              $("#wrapper").css({"padding-left": "175px"});
              $(".side-nav").css({"left": "210px", "width": "175px"});
          }
          
       } 
    
    </script>
    
