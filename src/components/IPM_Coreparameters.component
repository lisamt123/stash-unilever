<apex:component controller="IPM_Coreparameters" allowDML="true"
    id="IPM_Coreparameters">
    <apex:stylesheet value="{!URLFOR($Resource.IPM_Resource, 'css/IPM_Coreparameter_BET.css')}" />
    <div class='cust-overlay coreParm'></div>
    <div class="row coreParamContent" id="coreParamContent">

        <apex:pagemessages id="pgmsg"></apex:pagemessages>
        <div id="myCarousel" class="col-xs-12 negateMarginTop carousel myCarousel"
            data-interval="false" data-ride="carousel" data-wrap="false">
            <apex:actionStatus id="coreLoader" startStyleClass="proInit"></apex:actionStatus>
            <!--Carousel indicators -->
            <div class="col-xs-10 noPadleft noPadright carousel-inner">

                <div class="item fonequest active">
                    <div class="panel">
                        <div class="panel-body noPadleft noPadright">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-sm-12 noPadleft">
                                        <span class="coreParam marginT20">{!$Label.IPM_CoreParameter_Help_Guide}</span>
                                    </div>
                                    <div class="col-sm-12 noPadleft">
                                        <div class="questionNavigation">
                                            <div class="col-sm-4 noPadleft questName">
                                                <span class="coreTitle">{!$Label.IPM_CoreParameter_MPA}</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel-heading question">
                                <label id="Q1Description">{!$Label.IPM_MPA_Description}</label>
                            </div>
                            <apex:outputPanel id="questionone" layout="block">
                            <!--The below panel will be editable if isMPAEditable attribute is true -->
                                <apex:outputPanel layout="block" rendered="{!isMPAEditable}">
                                    <div
                                        class="slide-bar slideBar1 {! if(isOperational,operationalSlider_STR,'')}"
                                        id="slideBar1">
                                        <span class="slideIcon fa fa-circle" id="slideIcon1" />
                                        <div class="sliderTrack qnumone">
                                            <label for="{!$Label.IPM_Select_CoreParameter}"
                                                class="{! if(isOperational,operationaPointer_STR,'')} fstchild"><span>{!$Label.IPM_Select_CoreParameter}</span></label>
                                            <input type="radio" name="Question1" id="qone1"
                                                value="{!$Label.IPM_Select_CoreParameter}"
                                                title="{! if(answerone_STR == Null,answerChecked_STR,answerUnChecked_STR)}" />
                                            <apex:repeat value="{!questiononeList}" var="qone">
                                                <label for="{!qone}"
                                                    class="{! if(isOperational,operationaPointer_STR,'')}"><span>{!qone}</span></label>
                                                <input type="radio" name="Question1" id="{!qone}"
                                                    value="{!qone}"
                                                    title="{! if(qone==answerone_STR,answerChecked_STR,answerUnChecked_STR)}" />
                                            </apex:repeat>
                                        </div>
                                    </div>
                                </apex:outputPanel>
                            </apex:outputPanel>
                            <!--The below panel will be readonly if isMPAEditable attribute is false -->
                            <apex:outputtext value="{!IPM_Coreparameter.IPM_Answer_of_Q1__c}"
                                rendered="{!!isMPAEditable }" />
                        </div>
                    </div>
                </div>
                <div class="item ftwoquest" id="questionSlide3">
                    <div class="panel">
                        <div class="panel-body">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-sm-12 noPadleft">
                                        <span class="coreParam marginT20">{!$Label.IPM_CoreParameter_Help_Guide}</span>
                                    </div>
                                    <div class="col-sm-12 noPadleft">
                                        <div class="questionNavigation">
                                            <div class="col-sm-11 noPadleft questName">
                                                <span class="coreTitle">{!$Label.IPM_CoreParameter_CPV}</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <apex:outputPanel id="Quest2">
                                <div class="panel-heading question">
                                    <label id="Q2Description">{!$Label.IPM_CVP_Description}</label>
                                </div>
                                <!--The below panel will be editable if isCPVEditable attribute is true -->
                                <apex:outputPanel rendered="{!isCPVEditable}">
                                    <div class="slide-bar slideBar2" id="slideBar2">
                                        <span class="slideIcon fa fa-circle" id="slideIcon2" />
                                        <div class="sliderTrack qnumtwo">
                                            <label for="Select" class="fstchild"><span>{!$Label.IPM_Select_CoreParameter}</span></label>
                                            <input type="radio" name="Question2" id="qtwo2"
                                                value="Select" class="hide"
                                                title="{! if(answertwo_STR == Null,answerChecked_STR,answerUnChecked_STR)}" />
                                            <apex:repeat value="{!questiontwoList}" var="qtwo">
                                                <label for="{!qtwo}">{!qtwo}</label>
                                                <input type="radio" name="Question2" id="{!qtwo}"
                                                    value="{!qtwo}" class="hide"
                                                    title="{! if(qtwo==answertwo_STR,answerChecked_STR,answerUnChecked_STR)}" />
                                            </apex:repeat>
                                        </div>
                                    </div>
                                </apex:outputPanel>
                                <!--The below text will be readonly if isCPVEditable attribute is false -->
                                <apex:outputtext value="{!IPM_Coreparameter.IPM_Answer_of_Q2__c}"
                                    rendered="{!!isCPVEditable }" />
                            </apex:outputPanel>
                        </div>
                    </div>
                </div>
                <div class="item">
                    <div class="panel">
                        <div class="panel-body">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-sm-12 noPadleft">
                                        <span class="coreParam marginT20">{!$Label.IPM_CoreParameter_Help_Guide}</span>
                                    </div>
                                    <div class="col-sm-12 noPadleft">
                                        <div class="questionNavigation">
                                            <div class="col-sm-11 noPadleft questName">
                                                <span class="coreTitle">{!$Label.IPM_CoreParameter_ET}</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <apex:outputPanel id="QuestDesc">
                                <div class="panel-heading question">
                                    <label id="Q3Description">{!$Label.IPM_ET_Description}</label>
                                </div>
                                <!--The below panel will be editable if isETEditable attribute is true -->
                                <apex:outputPanel rendered="{!isETEditable}">
                                    <div class="slide-bar slideBar3" id="slideBar3">
                                        <span class="slideIcon fa fa-circle" id="slideIcon3" />
                                        <div class="sliderTrack qnumthree">
                                            <label for="Select" class="fstchild"><span>{!$Label.IPM_Select_CoreParameter}</span></label>
                                            <input type="radio" name="Question3" id="qthree3"
                                                value="Select" class="hide"
                                                title="{! if(answerthree_STR == Null,answerChecked_STR,answerUnChecked_STR)}" />
                                            <apex:repeat value="{!questionthreeList}" var="qthree">
                                                <label for="{!qthree}">{!qthree}</label>
                                                <input type="radio" name="Question3" id="{!qthree}"
                                                    value="{!qthree}" class="hide"
                                                    title="{! if(qthree==answerthree_STR,answerChecked_STR,answerUnChecked_STR)}" />
                                            </apex:repeat>
                                        </div>
                                    </div>
                                </apex:outputPanel>
                                <!--The below text will be readonly if isETEditable attribute is false -->
                                <apex:outputText value="{!IPM_Coreparameter.IPM_Answer_of_Q3__c}"
                                    rendered="{!!isETEditable}" />
                            </apex:outputPanel>
                        </div>
                    </div>
                    <apex:actionFunction status="coreLoader" action="{!answerAnswer}"
                        name="saveselectedAns"
                        reRender="sectorMarket,strategicIntent,MarketMsg,pgmsg"
                        oncomplete="sliderCP('.slide-bar');" />
                    <apex:actionFunction status="coreLoader" action="{!answerAnswer}"
                        name="saveselectedbdAns"
                        reRender="sectorMarket,strategicIntent,MarketMsg,pgmsg"
                        oncomplete="cpredirect();" />
                    <apex:actionFunction action="{!resetAnswer}" name="ResetAns"
                        status="loadcorep"
                        reRender="sectorMarket,helpTxt,helpTxt2,questionone,helpTxt3,QuestDesc,corejs,Quest2,Quest3,strategicIntent,QuestionthreeMsg,QuestiontwoMsg,QuestionOneMsg,MarketMsg" />
                    <apex:actionFunction action="{!resetAnswer}" name="ResetAns2"
                        status="loadcorep"
                        reRender="sectorMarket,helpTxt,helpTxt2,helpTxt3,QuestDesc,corejs2,Quest2,Quest3,strategicIntent,QuestionthreeMsg,QuestiontwoMsg,QuestionOneMsg,MarketMsg" />
                    <apex:actionFunction action="{!resetAnswer}" name="ResetAns3"
                        status="loadcorep"
                        reRender="sectorMarket,helpTxt,helpTxt2,helpTxt3,QuestDesc,corejs,Quest2,Quest3,strategicIntent,QuestionthreeMsg,QuestiontwoMsg,QuestionOneMsg,MarketMsg" />

                    <apex:actionFunction action="{!answerOne}" status="coreLoader"
                        name="Ans_parameter1"
                        reRender="sectorMarket,helpTxt,helpTxt2,helpTxt3,Quest2,Quest3,strategicIntent,QuestionthreeMsg,QuestiontwoMsg,QuestionOneMsg,MarketMsg">
                        <apex:param name="Ans" value="" id="SelectAns1"
                            assignTo="{!SelectedAnswer}" />
                    </apex:actionFunction>
                    <apex:actionFunction action="{!answerTwo}" status="coreLoader"
                        name="Ans_parameter2"
                        reRender="sectorMarket,helpTxt,helpTxt2,helpTxt3,strategicIntent,QuestionthreeMsg,QuestiontwoMsg,QuestionOneMsg,MarketMsg">
                        <apex:param name="Ans" value="" id="SelectAns2"
                            assignTo="{!SelectedAnswer}" />
                    </apex:actionFunction>
                    <apex:actionFunction action="{!answerThree}" status="coreLoader"
                        name="Ans_parameter3"
                        reRender="sectorMarket,helpTxt,helpTxt2,helpTxt3,strategicIntent,QuestionthreeMsg,QuestiontwoMsg,QuestionOneMsg,MarketMsg">
                        <apex:param name="Ans" value="" id="SelectAns3"
                            assignTo="{!SelectedAnswer}" />
                    </apex:actionFunction>
                </div>
                <div class="item fthreequest" id="slideStrategicIntent">
                    <div class="panel">
                        <div class="panel-body">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-sm-12 noPadleft">
                                        <span class="coreParam marginT20">{!$Label.IPM_Strategic_Help}</span>
                                    </div>
                                    <div class="col-sm-12 noPadleft">
                                        <div class="questionNavigation">
                                            <div class="col-sm-11 noPadleft questName">
                                                <span class="coreTitle">{!$Label.IPM_CoreParameter_Strategic_Intent}</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <apex:outputPanel id="strategicIntent">
                                <div class="col-sm-12 noPadleft noPadright">
                                    <div class="coreGreenbox">
                                        <apex:outputText value="{!$Label.IPM_Final_Stategic_Intent} " />
                                        <span><strong>
                                                {!IPM_Coreparameter.IPM_Strategic_Intent__c}</strong></span>
                                    </div>
                                    <div class="col-sm-5 noPadleft noPadright">
                                        <apex:outputText value="{!IPM_Coreparameter.IPM_Strategic_Intent__c}"
                                            styleClass="wizardCorecategory" />
                                        <apex:outputText styleClass="wizardCoreContent"
                                            value="{!strategicHelpText}" />
                                    </div>
                                    <div class="col-sm-7 noPadleft noPadright table-responsive"
                                        id="strategicIntentTableDiv">
                                        <table class="table-bordered strategicIntentTable" id="strategicIntentTable">
                                            <thead>
                                                <tr>
                                                    <th></th>
                                                    <th>{!$Label.IPM_New_Core_Product}</th>
                                                    <th>{!$Label.IPM_New_Benefit}</th>
                                                    <th>{!$Label.IPM_Improvement}</th>
                                                    <th>{!$Label.IPM_Variant}</th>
                                                    <th>{!$Label.IPM_No_Change}</th>
                                                    <th>{!$Label.IPM_Operational}</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <th>{!$Label.IPM_Radical}</th>
                                                    <!-- If IPM_Radical_New_Core_Product__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red1"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Radical_New_Core_Product__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Radical_New_Benefit__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red2"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Radical_New_Benefit__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Radical_Improvement__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red3"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Radical_Improvement__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Radical_Variant__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red4"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Radical_Variant__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Radical_No_Change__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green1"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Radical_No_Change__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Radical_Operational__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="oprtnGreenTable"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Radical_Operational__c }">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                </tr>
                                                <tr>
                                                    <th>{!$Label.IPM_Next_Generation}</th>
                                                    <!-- If IPM_Next_Generation_New_Core_Product__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red2"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Next_Generation_New_Core_Product__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Next_Generation_New_Benefit__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red3"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Next_Generation_New_Benefit__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Next_Generation_Improvement__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red4"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Next_Generation_Improvement__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Next_Generation_Variant__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green1"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Next_Generation_Variant__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Next_Generation_No_Change__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green2"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Next_Generation_No_Change__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Next_Generation_Operational__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="oprtnGreenTable"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Next_Generation_Operational__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                </tr>
                                                <tr>
                                                    <th>{!$Label.IPM_Incremental}</th>
                                                    <!-- If IPM_Incremental_New_Core_Product__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red3"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Incremental_New_Core_Product__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Incremental_New_Benefit__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red4"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Incremental_New_Benefit__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Incremental_Improvement__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green1"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Incremental_Improvement__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Incremental_Variant__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green2"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Incremental_Variant__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Incremental_No_Change__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green3"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Incremental_No_Change__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Incremental_Operational__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="oprtnGreenTable"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Incremental_Operational__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                </tr>
                                                <tr>
                                                    <th>{!$Label.IPM_Base}</th>
                                                    <!-- If IPM_Base_New_Core_Product__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="red4"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Base_New_Core_Product__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Base_New_Benefit__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green1"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Base_New_Benefit__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Base_Improvement__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green2"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Base_Improvement__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Base_Variant__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green3"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Base_Variant__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Base_No_Change__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="green4"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Base_No_Change__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_Base_Operational__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="oprtnGreenTable"><apex:outputPanel rendered="{!strategicGraphTick.IPM_Base_Operational__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                </tr>
                                                <tr>
                                                    <th class="blgHeader">{!$Label.IPM_BLG_No_change}</th>
                                                    <!-- If IPM_BLG_New_Core_Product__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="graphNoborder"><apex:outputPanel rendered="{!strategicGraphTick.IPM_BLG_New_Core_Product__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_BLG_New_Benefit__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="blue"><apex:outputPanel rendered="{!strategicGraphTick.IPM_BLG_New_Benefit__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_BLG_Improvement__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="blue"><apex:outputPanel rendered="{!strategicGraphTick.IPM_BLG_Improvement__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_BLG_Variant__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="blue"><apex:outputPanel rendered="{!strategicGraphTick.IPM_BLG_Variant__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    <!-- If IPM_BLG_No_Change__c field value is true based on Coreparemeter questions, display check mark -->
                                                    <td class="blue"><apex:outputPanel rendered="{!strategicGraphTick.IPM_BLG_No_Change__c}">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                    
                                                    <td class="graphNoborder oprtnNobgrnd"><apex:outputPanel rendered="">
                                                            <span class="fa fa-check-circle checkCircle"></span>
                                                        </apex:outputPanel></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <div class="pull-right txtAlignright tableIndicator">
                                            <div class="buttonmarRight graphLegend">
                                                <div class="box orange cust-Inputs"></div>
                                                <label>{!$Label.IPM_Innovation}</label>
                                            </div>
                                            <div class="buttonmarRight graphLegend">
                                                <div class="box yellow cust-Inputs"></div>
                                                <label>{!$Label.IPM_Renovation}</label>
                                            </div>
                                            <div class="buttonmarRight graphLegend">
                                                <div class="box blue cust-Inputs"></div>
                                                <label>{!$Label.IPM_BLG}</label>
                                            </div>
                                            <div class="graphLegend">
                                                <div class="box oprtnGreenTable cust-Inputs"></div>
                                                <label>{!$Label.IPM_Operational}</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="skipButtoncontainer txtAlignright pull-right test">
                                    <div class="buttonmarRight pull-left">
                                        <a href="#myCarousel" title="{!$Label.IPM_Back}"
                                            id="prevControl" class="ipmButton">{!$Label.IPM_Back}</a>
                                    </div>
                                    <div class="buttonmarRight pull-left ml5 testbtn">
                                    <!--The below panel will be editable if isEditCoreparameter attribute is true -->
                                        <apex:outputPanel rendered="{!isEditCoreparameter}"
                                            layout="block">
                                            <a href="#" title="{!$Label.IPM_SAVE_AND_CONTINUE}"
                                                onClick="skipValidation(),checkCoreParam();" class="ipmButton primary">{!$Label.IPM_SAVE_AND_CONTINUE}</a>
                                        </apex:outputPanel>
                                        <!--The below panel will be readonly if isEditCoreparameter attribute is false -->
                                        <apex:outputPanel rendered="{!!isEditCoreparameter}"
                                            layout="block">
                                            <a href="#myCarousel" data-slide="next"
                                                title="{!$Label.IPM_SAVE_AND_CONTINUE}" onClick="skipValidation(),checkTooltip();" id="nextControl"
                                                class="ipmButton primary">{!$Label.IPM_SAVE_AND_CONTINUE}</a>
                                            <!--The below panel will be readonly if isEditable attribute is false -->
                                            <apex:outputPanel rendered="{!!isEditable}">
                                                <a href="#myCarousel" data-slide="next"
                                                    title="{!$Label.IPM_Continue}" id="nextControl"
                                                    class="ipmButton primary">{!$Label.IPM_Continue}</a>
                                            </apex:outputPanel>
                                        </apex:outputPanel>
                                    </div>
                                </div>
                            </apex:outputPanel>
