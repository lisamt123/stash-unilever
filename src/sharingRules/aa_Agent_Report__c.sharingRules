<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Share_Published_Reports</fullName>
        <accessLevel>Read</accessLevel>
        <description>Share Publish reports with all Internal users</description>
        <label>Published Report Share</label>
        <sharedTo>
            <allInternalUsers></allInternalUsers>
        </sharedTo>
        <criteriaItems>
            <field>Status__c</field>
            <operation>equals</operation>
            <value>Published (Public)</value>
        </criteriaItems>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Competitor Report,Unilever Agent Published Report</value>
        </criteriaItems>
    </sharingCriteriaRules>
