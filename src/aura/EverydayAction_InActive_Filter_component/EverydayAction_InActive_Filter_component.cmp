     <style>
    .slds ul {
    list-style: none;
    margin-left: 0px;
	}

        .spacer-pull-to-load-more{
          height:100% !important;
        }

            </style>
	<aura:attribute name="themeName" type="string" ></aura:attribute>
    
     <div class="slds">
