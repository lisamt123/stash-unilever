<aura:component >
    <aura:attribute name="report" type="Object"/>
    <aura:attribute name="kpiTransaction" type="Object"/>
    <aura:attribute name="cssClass" type="String"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    <aura:handler event="c:OV_WizardFavouriteReportSelectedEvent" action="{!c.onFavChange}" />
    <aura:registerEvent name="OV_WizardFavouriteReportSelectedEvent" type="c:OV_WizardFavouriteReportSelectedEvent" />

    <div class="{! v.cssClass}"></div>
</aura:component>