<aura:component >
    <ltng:require styles="/resource/EAStaticResource_Ruma/assets/styles/salesforce-lightning-design-system-vf.css" />
 <style>
   .slds ul {
  list-style: none;
  margin-left: 0px;
}
       .spacer-pull-to-load-more{
          height:100% !important;
        }
     .slds .slds-list--vertical.slds-has-dividers > .slds-list__item {
  padding: 20px;
  border-bottom: 1px solid #d8dde6;
}
         </style>
	<aura:attribute name="themeName" type="string" ></aura:attribute>
 
   <div class="slds">
      <ul class="slds-list--vertical slds-has-dividers liststyle">
          <li class="slds-list__item">
            <div onclick="{!c.activeFilter}">
              <span class="activeFilter" >
      			<ui:outputText value="{!v.themeName}" />
   				</span>
              </div>  
                 <span>
                    <img  class="tick_img" src="/resource/EA_StaticResource/assets/images/sort_tick.svg" alt="back" />
                </span>
		  </li>
		</ul>
    </div>
</aura:component>