<aura:component Controller="EA_ControllerClass">
    <aura:attribute name="activity" type="EA_Wrapper_Class.EA_UserProfile_Wrapper"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="showdashboard" type="boolean" default="true"/>
    <aura:attribute name="showfeedback" type="boolean" default="false"/>
    <aura:attribute name="themecolor" type="string"/>
    <aura:attribute name="showInvitation" type="boolean" default="false"/>
    <aura:attribute name="showbody" type="boolean" default="true"/>
    <aura:attribute name="showDetailCard" type="boolean" default="false"></aura:attribute>
    <ltng:require styles="/resource/Lightning_designing_style/assets/styles/salesforce-lightning-design-system.min.css" />
    <div class="Container feedback-div"> 
        <div class="new-class" id="color1" style="{!'background:'+ v.themecolor +';'}">00</div>
        <ul class="home-page-container-ul">
            <li class="slds-list__item">
                <aura:if isTrue="{!v.showbody}">
                    <aura:if isTrue="{!v.showfeedback}">
                        <div class="regular_font slds-text-heading--label" onclick="{!c.gotoDetail}"><ui:outputText value="{!v.activity.theme_name}"/></div>
                        <div class="list_name" onclick="{!c.gotoDetail}"><ui:outputText value="{!v.activity.activity_name}"/></div>
                        
                        
                        <img  onclick="{!c.gotofeedback}" src="/resource/Lightning_designing_style/assets/icons/utility/share_post_60.png" class="image-ref" />
                    </aura:if> </aura:if>
            </li>
        </ul>
        
    </div>
</aura:component>