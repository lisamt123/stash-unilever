<aura:component >
    <aura:attribute name="activity" type="EA_Wrapper_Class.EA_UserProfile_Wrapper"/>
    <aura:handler name="init" value="{!this}" action="{!c.myAction}" />  
    <aura:attribute name="showdashboard" type="boolean" default="true"/>
    <aura:attribute name="showfeedback" type="boolean" default="false"/>
    <aura:attribute name="showInvitation" type="boolean" default="false"/>
    <aura:attribute name="showbody" type="boolean" default="true"/>
    <aura:attribute name="showDetailCard" type="boolean" default="false"></aura:attribute>
    <div class="Container inactive_act" style="margin-top: -6px;background-color: white;width: 95%;margin: 0 auto;border-radius:10px;">  
        <div class="new-class">01</div>
        <ul style="margin-bottom: 12px;" class="home-page-container-ul">
            <li class="slds-list__item">
                <div class="regular_font" onclick="{c.gotoDetail}"><ui:outputText value="{!v.activity.theme_name}" /></div>
                <div class="list_name" onclick="{c.gotoDetail}"><ui:outputText value="{!v.activity.activity_name}"/></div>
                <div class="invitation1" onclick="{!c.gotoInvitation}">
                    <img src="/resource/EA_StaticResource/images/icon-rsvp.png" style="width:21px;"></img>
                </div>
                <span class="right_arrow_img">
                </span>
            </li>
        </ul>
        <div class=""></div>
    </div>
</aura:component>