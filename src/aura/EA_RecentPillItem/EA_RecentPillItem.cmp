<aura:component >
    <aura:registerEvent name="updatePillListEvent" type="c:EA_RecentPill_Event" />
    <aura:attribute name="itemName" type="String"/>
    <aura:attribute name="itemId" type="String"/>
    <aura:attribute name="selectedUsers" type="String[]" />
    <aura:attribute name="selectedItem" type="String[]" description="To map selected details"/>
    <aura:attribute name="maxLimit" type="Integer" description="To limit selection" />
    <span  onclick="{!c.update}">
        <a href="javascript:void(0);"><span class="invt_lst">{!v.itemName}</span></a>
    </span>
       
</aura:component>