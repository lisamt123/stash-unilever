<aura:component implements="flexipage:availableForAllPageTypes,force:appHostable">
	<ltng:require styles="{!$Resource.Lightning_designing_style + '/assets/styles/salesforce-lightning-design-system.min.css'}" />
	<aura:handler name="init" value="{!this}" action="{!c.defaultPanel}" />
   <!-- <aura:handler event="c:LA_HandleEvent" action="{!c.navigateWithinComponent}" /> -->
