 <div class="parent" aura:id="parentDiv">
	<div class="slds-tabs--default">
        <!--Primary Tabs-->
			<ul class="slds-tabs--default__nav tabUi" role="tablist">
				<li aura:id="tab-profile" class="slds-tabs--default__item slds-text-title--caps " title="Item One" role="presentation"  onclick="{!c.profileDisplay}">
					<a class="slds-tabs--default__link" href="javascript:void(0);" role="tab" tabindex="0" aria-selected="true" aria-controls="tab-default-1" id="tab-default-1__item">MY PROFILE</a>
				</li>
				<li aura:id="tab-default" class="slds-tabs--default__item slds-text-title--caps " title="Item Two" role="presentation"  onclick="{!c.upcomingDisplay}">
					<a class="slds-tabs--default__link" href="javascript:void(0);" role="tab" tabindex="-1" aria-selected="false" aria-controls="tab-default-2" id="tab-default-2__item">UP-COMING</a>
				</li>
				<li aura:id="tab-past" class="slds-tabs--default__item slds-text-title--caps" title="Item Three" role="presentation" onclick="{!c.pastDisplay}">
