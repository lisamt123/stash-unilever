<aura:component >
<aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
<aura:attribute name="KPITransactionItem" type="Object" />
<aura:attribute name="itemFields" type="String[]" />
<aura:attribute name="KPItransactionsHeaders" type="Object" />
<aura:attribute name="KPItransactionsHeadersArray" type="String[]" />
    <aura:iteration items="{!v.itemFields}" var="fieldItem">
    <td><div>{! fieldItem.Value }<aura:if isTrue="{! !!fieldItem.Class}"><i class="{! fieldItem.Class}"></i></aura:if></div> </td>
    </aura:iteration>
</aura:component>