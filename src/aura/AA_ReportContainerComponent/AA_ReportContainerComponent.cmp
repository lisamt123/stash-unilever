<aura:component controller="AA_AgentAppService">
    <aura:attribute name="objPassAgentReportsToNG" type="AA_WrapperClass.agentReportDetail" />
    <aura:attribute name="totalReportCount" type="Integer" default="0"/>   
    <style>
        .cAA_ReportContainerComponent.slds-grid--align-end {
        -webkit-justify-content: flex-start;
        -ms-flex-pack: start;
        justify-content: flex-start;
        }
    </style>
    <aura:if isTrue="{!v.totalReportCount == 0}">
        <div style="text-align: center">
            No records were found.
        </div>
    </aura:if>
    <div class="slds-grid slds-wrap slds-grow slds-scrollable--y slds-grid--align-end reports-container">
        <aura:iteration items="{!v.objPassAgentReportsToNG}" var="objReport" > 
            <div class="slds-col slds-size--1-of-2 slds-small-size--1-of-2 slds-medium-size--1-of-3 slds-large-size--1-of-4">
                <c:AA_ReportDetailNavigation objReport="{!objReport}"/> 
            </div>
        </aura:iteration>
    </div>
</aura:component>