<aura:component implements="force:appHostable" controller="CORE_Feedback_App_Controller">
	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:handler event="c:MB_BaseEvent" action="{!c.getDetail}" />
    <aura:handler event="c:Feedback_Back_Event" action="{!c.gotoLandingpage}" />
    <aura:handler event="c:Feedback_Event" action="{!c.handleFeedback}" />
	<aura:handler event="c:MB_DetailSummary_Event" action="{!c.showDetail_charts}"/>
    <aura:handler event="c:MB_Detail_text_Event" action="{!c.showDetail_Text}"/>
     <aura:handler event="c:MB_gotoSummary_charts_Event" action="{!c.MB_gotoSummary_chart}"/>
    <aura:attribute name="showspinner" type="boolean"/>
     <aura:attribute name="Appname" type="string" default="TEM"/>
    <aura:attribute name="Pagename" type="string" default="MB_Devices"/>
    <aura:handler event="c:MB_ChangeMonth_Event" action="{!c.changeMonth}"/>
    <aura:handler event="c:MB_Back_Evt" action="{!c.handlebackButton}"/>
    <div aura:id="content"></div>
   
    <aura:if isTrue="{!v.showspinner}">
        <center>
            <img style="width:10%;position: absolute;top:50%;" src="/resource/EA_StaticResource/assets/images/spinners/slds_spinner.gif"></img>
        </center>
    </aura:if> 
    
</aura:component>