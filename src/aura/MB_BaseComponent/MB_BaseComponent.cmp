<aura:component implements="force:appHostable" controller="CORE_FB_Feedback_App_Controller">
    <ltng:require styles="/resource/Lightning_designing_style/assets/styles/salesforce-lightning-design-system.min.css" />
    <ltng:require styles="/resource/Lightning_designing_style/assets/styles/salesforce-lightning-design-system-ltng.min.css" />
    <ltng:require styles="/resource/Lightning_designing_style/assets/fonts/" />
    <ltng:require styles="/resource/Lightning_designing_style/assets/fonts/webfonts" />
   
    
	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:handler event="c:MB_BaseEvent" action="{!c.getDetail}" />
    <aura:handler event="c:CORE_FB_Feedback_Back_Event" action="{!c.gotoLandingpage}" />
    <aura:handler event="c:CORE_FB_Feedback_Event" action="{!c.handleFeedback}" />
	<aura:handler event="c:MB_DetailSummary_Event" action="{!c.showDetail_charts}"/>
    <aura:handler event="c:MB_Detail_text_Event" action="{!c.showDetail_Text}"/>
     <aura:handler event="c:MB_gotoSummary_charts_Event" action="{!c.MB_gotoSummary_chart}"/>
    <aura:attribute name="showspinner" type="boolean"/>
    <aura:attribute name="Appname" type="string" default="TEM"/>
    <aura:attribute name="Pagename" type="string" default="MB_Devices"/>
    <aura:handler event="c:MB_ChangeMonth_Event" action="{!c.changeMonth}"/>
    <aura:handler event="c:MB_Back_Evt" action="{!c.handlebackButton}"/>
    <aura:handler event="c:MB_Feedback_Event" action="{!c.gotoApp}"/>
    <aura:handler event="c:MB_Faq_Event" action="{!c.gotoFaqPage}"/>
    <aura:handler event="c:MB_UsageTypeSummary_Event" action="{!c.gotoUsageTypeDetail}"/>
    <aura:handler event="c:MB_SummarySpend_Event" action="{!c.gotoSummarySpend}"/>
    <aura:handler event="c:MB_IPassDevice_Event" action="{!c.gotoIpassCharts}"/>
    <aura:handler event="c:MB_iPass_Detail_Event" action="{!c.gotoIpassDetail}"/>
    <aura:handler event="c:MB_iPass_charts_Event" action="{!c.goBacktoIpassDetail}"/>
    <aura:handler event="c:MB_iPassUsageVarience_Event" action="{!c.gotoiPassVarience}"/>
    <div aura:id="content"></div>
   
    <aura:if isTrue="{!v.showspinner}">
        <div class="slds-spinner_container">
            <div class="slds-spinner slds-spinner--medium" aria-hidden="false" role="alert">
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
    </aura:if> 
    
</aura:component>