        <fullName>btn_Reason_for_Return</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Return from DocuSign</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_CR__c&apos;); 
p.id = &quot;{!CPA_CR__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_CR__c.pkl_Status__c }&quot;; 


try { 
	soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos;  and PermissionSet.Name in (&apos;CPA_SMT&apos;,&apos;CPA_ADM&apos;,&apos;CPA_ULPM&apos;)&quot;; 
	var result = sforce.connection.query(soql); 
	var records1 = result.getArray(&apos;records&apos;); 
	var permissionSetName = records1[0].PermissionSet.Name; 

	if( permissionSetName == &quot;CPA_SMT&quot;){ 
		if(p.pkl_Status__c == &quot;Signed&quot;){ 
			alert(&quot;Cant Return, as CR is already Signed&quot;); 
		} else if(p.pkl_Status__c == &quot;Cancelled&quot;){ 
			alert(&quot;Cant Return, as CR is already Cancelled&quot;); 
		}else if(p.pkl_Status__c ==&quot;Sent for Signature&quot;){ 
			window.open(&apos;/apex/CPA_CR_Reason_for_return?id=&apos;+&quot;{!CPA_CR__c.Id}&quot;,&quot;_self&quot;); 
		}else if(p.pkl_Status__c == &quot;Returned&quot;){ 
			alert(&quot;Cant Return, as CR is already Returned&quot;); 
		}else if(p.pkl_Status__c == &quot;Accepted&quot; ){ 
			alert(&quot;The CR should be sent for Signature before returning from DocuSign.&quot;); 
		}else if(p.pkl_Status__c ==&quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot;){
			alert(&quot;The CR should be sent for Signature before returning from DocuSign.&quot;); 
		}else{
			alert(&quot;The CR should be in &apos;sent for Signature&apos; before returning from DocuSign.&quot;); 
		}
	}
	else {
		alert(&quot;CR Return from DocuSign can be done by Supplier Management Team only.&quot;); 
	}
	

}catch (e) { 
	alert( &quot;An unexpected Error has Occurred. Error: \r\n&quot; + e ); 
}</url>
