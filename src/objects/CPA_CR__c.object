    <webLinks>
        <fullName>Submit_for_Vendor</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}

var p = new sforce.SObject(&apos;CPA_CR__c&apos;); 
p.id = &quot;{!CPA_CR__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_CR__c.pkl_Status__c }&quot;; 
p.isValidated__c = &quot;{!CPA_CR__c.isValidated__c }&quot;; 

soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos;  and PermissionSet.Name in (&apos;CPA_SMT&apos;,&apos;CPA_ADM&apos;,&apos;CPA_ULPM&apos;)&quot;; 
var result = sforce.connection.query(soql); 
var records1 = result.getArray(&apos;records&apos;); 
var permissionSetName = records1[0].PermissionSet.Name; 

if(permissionSetName == &quot;CPA_ADM&quot;){ 
	var isValidateFlag = sforce.apex.execute(&quot;CPA_CRInformationController&quot;,&quot;checkIsValidated&quot;,{crId:p.id});
	
	if(isValidateFlag == &apos;true&apos;){ 
		if(p.pkl_Status__c == &quot;Withhold&quot; || p.pkl_Status__c == &quot;Returned&quot;){ 
			p.pkl_Status__c = &quot;Resubmitted&quot;; 
			result = sforce.connection.update([p]); 

			location.reload(true); 

		} 
		else if(p.pkl_Status__c ==&quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; ){ 
			alert(&quot;CR is already submitted&quot;); 
		} 
		else if(p.pkl_Status__c ==&quot;Accepted&quot; ){ 
			alert(&quot;CR is already Accepted, you cant submit for Approval&quot;); 
		}
		else if(p.pkl_Status__c ==&quot;Sent for Signature&quot; || p.pkl_Status__c == &quot;Signed&quot; || p.pkl_Status__c == &quot;Delivered&quot;){
			alert(&quot;CR cant be submitted for Approval as it is Accepted and moved for further processing&quot;); 
		}
		else{ 
			p.pkl_Status__c = &quot;Submitted&quot;; 
			result = sforce.connection.update([p]); 
	//alert(result);
			location.reload(true); 
		} 
	} 
	else { 
		alert(&quot;CR cant be submitted, as required fields are missing&quot;); 
	}
}else{
	alert(&quot;You are not authorized to perform this action.&quot;);
}</url>
