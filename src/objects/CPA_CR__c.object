    <validationRules>
        <fullName>CPA_CR_Cant_be_created</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() ,RecordType.DeveloperName !=&apos;CPA_Standalone_CR&apos; ,OR( 

ISPICKVAL(CPA_Project_Work_Order__r.pkl_Status__c , &quot;Cancelled&quot;), 
ISPICKVAL(CPA_Project_Work_Order__r.pkl_Status__c , &quot;Terminated&quot;), 
ISPICKVAL(CPA_Project_Work_Order__r.pkl_Status__c , &quot;Sent For Termination&quot;), 
ISPICKVAL(CPA_Project_Work_Order__r.pkl_Status__c , &quot;Closed&quot;),
ISPICKVAL(CPA_Project_Work_Order__r.pkl_Status__c , &quot;Request For Termination&quot;),
ISPICKVAL(CPA_Project_Work_Order__r.pkl_Status__c , &quot;Sent For Closure&quot;)
))</errorConditionFormula>
        <errorMessage>CR cant be created, because PWO is in process of Closer/Termination/Cancellation/ Not Accepted by Supply Management Team</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CPA_CR_NOT_on_PWO_Saved</fullName>
        <active>true</active>
        <description>CPA_CR Can&apos;t be created if the associated  PWO is in SAVED only</description>
        <errorConditionFormula>AND( ISNEW() ,RecordType.DeveloperName !=&apos;CPA_Standalone_CR&apos; ,OR( 
ISPICKVAL(CPA_Project_Work_Order__r.pkl_Status__c , &quot;Saved&quot;)
))</errorConditionFormula>
        <errorMessage>CR cant be created, because associated  PWO is still in Saved Status.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Delivered</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Deliver</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_CR__c&apos;); 
p.id = &quot;{!CPA_CR__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_CR__c.pkl_Status__c }&quot;; 
p.dat_Actual_End_Date__c = &quot;{!CPA_CR__c.dat_Actual_End_Date__c }&quot;; 
PWO_ID__c = &quot;{!CPA_CR__c.CPA_Project_Work_Order__c}&quot;; 
profileName = &quot;{!$Profile.Name}&quot;; 
var today = new Date(); 
var dd = today.getDate(); 
var mm = today.getMonth(); 
var yyyy = today.getFullYear(); 
today = new Date(yyyy,mm,dd); 

result = sforce.connection.query(&quot;select Id,pkl_Status__c, TNF_Status__c from CPA_Project_Work_Order__c where Name = &apos;&quot;+PWO_ID__c+&quot;&apos;&quot;); 
var pwoRecords = result.getArray(&quot;records&quot;); 

soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos;&quot;; 
var result = sforce.connection.query(soql); 
var records1 = result.getArray(&apos;records&apos;); 
var permissionSetName = records1[0].PermissionSet.Name; 
//alert(&quot;permissionSetName -&gt;&quot; + permissionSetName); 

//alert(p.pkl_Status__c);
if( permissionSetName == &quot;CPA_ADM&quot;){ 
 
	if(p.pkl_Status__c == &quot;Signed&quot; &amp;&amp; (pwoRecords[0].pkl_Status__c != &quot;Terminated&quot; || pwoRecords[0].pkl_Status__c != &quot;Sent For Termination&quot; || pwoRecords[0].pkl_Status__c != &quot;Request For Termination&quot;)){ 
	p.pkl_Status__c = &quot;Delivered&quot;;	
	p.dat_Actual_End_Date__c = today; 
	result = sforce.connection.update([p]);
// alert(result);
	location.reload(true); 
	} 
	else if(p.pkl_Status__c == &quot;Delivered&quot;){ 
	alert(&quot;CR is already Delivered&quot;); 
	}
	else if(p.pkl_Status__c != &quot;Signed&quot;){ 
	alert(&quot;CR can not be Delivered as it is not yet signed&quot;); 
	} 
	else { 
	alert(&quot;CR can not be Delivered, Contact Administrator&quot;); 
	} 
}
else{
alert(&quot;You are not authorized to perform this action.&quot;); 
}</url>
