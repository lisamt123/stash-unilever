    <validationRules>
        <fullName>PWO_Canat_be_created</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
ISPICKVAL(PWORF_ID__r.pkl_Status__c , &quot;Submitted&quot;),
ISPICKVAL(PWORF_ID__r.pkl_Status__c , &quot;Cancelled&quot;),
ISPICKVAL(PWORF_ID__r.pkl_Status__c , &quot;Resubmitted&quot;),
ISPICKVAL(PWORF_ID__r.pkl_Status__c , &quot;Withhold&quot;),
ISPICKVAL(PWORF_ID__r.pkl_Status__c , &quot;Returned&quot;),
ISPICKVAL(PWORF_ID__r.pkl_Status__c , &quot;Rejected&quot;),
ISPICKVAL(PWORF_ID__r.pkl_Status__c , &quot;Saved&quot;)


)</errorConditionFormula>
        <errorMessage>PWO cant be created because, PWORF is not Answered / Accepted / Auto Accepted</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Delivered</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delivered</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_PWO__c&apos;); 
p.id = &quot;{!CPA_PWO__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_PWO__c.pkl_Status__c }&quot;;
p.dat_Actual_End_Date__c = &quot;{!CPA_PWO__c.dat_Actual_End_Date__c }&quot;; 
profileName = &quot;{!$Profile.Name}&quot;;
var today = new Date();
var dd = today.getDate();
var mm = today.getMonth();
var yyyy = today.getFullYear();
today = new Date(yyyy,mm,dd);

result = sforce.connection.query(&quot;select Id,PWO_ID__c,pkl_Status__c from CPA_CR__c where PWO_ID__c = &apos;&quot;+p.id+&quot;&apos; AND  pkl_Status__c !=&apos;Delivered&apos;&quot;); 
var crRecords = result.getArray(&quot;records&quot;); 

if (profileName != &quot;System Administrator&quot; &amp;&amp; profileName != &quot;CPA VDM&quot;){ 
	alert(&quot;Insufficient Privilege&quot;); 
}
else{
	if(p.pkl_Status__c == &quot;Signed&quot; &amp;&amp; crRecords.length == 0){ 
		p.pkl_Status__c = &quot;Delivered&quot;;	
		p.dat_Actual_End_Date__c = today;
		result = sforce.connection.update([p]);
		location.reload(true);
	}
	else if(p.pkl_Status__c != &quot;Signed&quot;){
		alert(&quot;PWO can not be Delivered as it is not yet signed&quot;); 	
	}
	else if(p.pkl_Status__c == &quot;Signed&quot; &amp;&amp; crRecords.length != 0){
		alert(&quot;PWO can not be Delivered as CR is not yet Delivered&quot;); 	
	}
}</url>
