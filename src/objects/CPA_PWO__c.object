        <fullName>Request_for_Termination</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Request for Termination</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 


var p = new sforce.SObject(&apos;CPA_PWO__c&apos;); 
p.id = &quot;{!CPA_PWO__c.Id}&quot;;
p.pkl_Status__c = &quot;{!CPA_PWO__c.pkl_Status__c }&quot;;

if(p.pkl_Status__c == &quot;Signed&quot;){
		var CPATR = new sforce.SObject(&quot;CPA_Termination_Requested__c&quot;);
	//accnt.Id = &apos;{!Account.Id}&apos;;
	CPATR.Reason_for_Termination__c  = &quot;{CPA_Termination_Requested__c.Reason_for_Termination__c}&quot;;
	CPATR.PWO__c = &quot;{CPA_Termination_Requested__c.PWO__c}&quot;;
	CPATR.Reason_for_Termination__c= prompt(&apos;Enter Reason for Termination&apos;);
	CPATR.PWO__c = p.id;

	var result = sforce.connection.create([CPATR]);
	if(result[0].getBoolean(&quot;success&quot;))
	{
	   
	   p.pkl_Status__c = &quot;Request For Termination&quot;;
	   //var result1 = sforce.connection.update([p]);
	   alert(&apos;Request for Termination submitted successfully&apos;);
	   window.location.reload();
	}
	else{
	  alert(&apos;Error : &apos;+result);
	}
	}else if(p.pkl_Status__c == &quot;Request For Termination&quot;){
		
		 alert(&apos;PWO is alreday Requested for Termination&apos;);
	} else if(p.pkl_Status__c == &quot;Closed&quot;){
		alert(&apos;Request for Termination cannot be raise, Because PWO is Closed&apos;);
	}
	else {
	alert(&apos;PWO is cannot be requested for Termination on this stage, Please raise query for Cancellation&apos;);
	}</url>
    </webLinks>
    <webLinks>
        <fullName>Submit_For_Unilever</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}

var p = new sforce.SObject(&apos;CPA_PWO__c&apos;); 
p.id = &quot;{!CPA_PWO__c.Id}&quot;; 

p.pkl_Status__c = &quot;{!CPA_PWO__c.pkl_Status__c }&quot;; 
p.isValidated__c = &quot;{!CPA_PWO__c.isValidated__c }&quot;; 

var isValidateFlag = sforce.apex.execute(&quot;CPA_PWOInformationController&quot;,&quot;checkIsValidated&quot;,{objId:p.id});



//if(p.isValidated__c == &quot;1&quot;){ 
if(isValidateFlag == &apos;true&apos;){ 

	if(p.pkl_Status__c == &quot;Withhold&quot; || p.pkl_Status__c == &quot;Returned&quot; || p.pkl_Status__c == &quot;Request For Termination&quot;){ 
		p.pkl_Status__c = &quot;Resubmitted&quot;; 
		result = sforce.connection.update([p]); 
		location.reload(true); 

	} 	else if(p.pkl_Status__c ==&quot;Saved&quot;){ 
		p.pkl_Status__c = &quot;Submitted&quot;; 
		result = sforce.connection.update([p]); 
		location.reload(true); 
	} 
	else if(p.pkl_Status__c ==&quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; ){ 
		alert(&quot;PWO is already submitted&quot;); 
	} 
	else if(p.pkl_Status__c == &quot;Delivered&quot;){ 
		alert(&quot;PWO can not be Submitted as it is already Delivered&quot;); 
	}
	else if(p.pkl_Status__c == &quot;Termination Requested&quot; || p.pkl_Status__c == &quot;Sent For Termination&quot; || p.pkl_Status__c == &quot;Terminated&quot;){ 
		alert(&quot;PWO can not be submitted as it is Terminated/Sent For Termination/Termination Requested&quot;); 
	}
	else	if(p.pkl_Status__c == &quot;Signed&quot;){ 
		alert(&quot;PWO can not be submitted as it is already Signed&quot;); 
	}
	else	if(p.pkl_Status__c == &quot;Sent for Signature&quot;){ 
		alert(&quot;PWO can not be submitted as it is already Sent for Signature&quot;); 
	}
	else if(p.pkl_Status__c == &quot;Sent For Closure&quot; || p.pkl_Status__c == &quot;Closed&quot;){ 
		alert(&quot;PWO can not be submitted as it is already Closed/Sent For Closure&quot;); 
	}
	else	if(p.pkl_Status__c == &quot;Cancelled&quot;){ 
		alert(&quot;PWO can not be submitted as it is already Cancelled&quot;); 
	}
	else	if(p.pkl_Status__c == &quot;Accepted&quot;){ 
		alert(&quot;PWO can not be submitted as it is already Accepted&quot;); 
	}else{
		alert(&quot;PWO can not be submitted, Concern Administrator&quot;); 
	}

} 
else { 
alert(&quot;PWO can not be submitted, as Required fields are missing.&quot;); 

}</url>
    </webLinks>
    <webLinks>
        <fullName>btn_Cancel_PWO</fullName>
        <availability>online</availability>
        <description>Popup new vf page to get cancellation reason.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_PWO__c&apos;); 
p.id = &quot;{!CPA_PWO__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_PWO__c.pkl_Status__c }&quot;; 
p.profileName = &quot;{!$Profile.Name}&quot;; 

if(p.profileName == &quot;System Administrator&quot; || p.profileName == &quot;CPA SMT&quot;){ 

	var result = sforce.connection.query(&quot;select id,pkl_Status__c from CPA_TNF__c where lkp_PWO_ID__c = &apos;&quot;+p.id+&quot;&apos; AND pkl_Status__c =&apos;Submitted&apos;&quot;); 
	var pwoRecords = result.getArray(&quot;records&quot;); 

	if(p.pkl_Status__c!= &apos;Delivered&apos; &amp;&amp; p.pkl_Status__c != &apos;Cancelled&apos; &amp;&amp; pwoRecords.length==0){ 
	window.open(&apos;/apex/CPA_PWO_Cancel_Reason?id=&apos;+&quot;{!CPA_PWO__c.Id}&quot;,&quot;_self&quot;); 
	} else{ 
	alert(&quot;Can not Cancel PWO as it is already Cancelled or Delivered or PWO is submitted&quot;); 
	} 
} else{ 
alert(&quot;Insufficient Privilege&quot;); 
}</url>
    </webLinks>
