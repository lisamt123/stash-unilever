    <validationRules>
        <fullName>FA_DateValidation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(   DATEVALUE(FA_Start_Date_Time__c) &gt;  DATEVALUE(  FA_End_Date_Time__c) , true, false)</errorConditionFormula>
        <errorDisplayField>FA_End_Date_Time__c</errorDisplayField>
        <errorMessage>Course End date cannot be before Course Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_Feedback_EndDate_Check</fullName>
        <active>true</active>
        <description>Feedback shouldnot be triggered if the course occurrence End date is &gt; today() and if Status is Cancelled.</description>
        <errorConditionFormula>If(OR(DATEVALUE(FA_End_Date_Time__c )   &gt;  TODAY(),  ISPICKVAL( FA_Status__c , &apos;Cancelled&apos;) ) ,  FA_Initiate_Feedback__c ==TRUE,False)</errorConditionFormula>
        <errorDisplayField>FA_Initiate_Feedback__c</errorDisplayField>
        <errorMessage>Cannot Initiate Feedback if Occurrence End date greater than today. and If Status is also cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_Initiate_Feedback_Check</fullName>
        <active>false</active>
        <description>Condition Feedback button once checked, cannot be further operated</description>
        <errorConditionFormula>IF(PRIORVALUE(FA_Initiate_Feedback__c)= TRUE,ISCHANGED(FA_Initiate_Feedback__c),False)</errorConditionFormula>
        <errorDisplayField>FA_Initiate_Feedback__c</errorDisplayField>
        <errorMessage>Feedback already Initiated, cannot Undo</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_NewOccStartDate_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>ISNEW() &amp;&amp; IF( DATEVALUE(FA_Start_Date_Time__c)  &lt;TODAY()  -0.5  , true , false )</errorConditionFormula>
        <errorMessage>Start Date of the new Course Occurrence cannot be less than Today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_Occ_Completion_Check</fullName>
        <active>false</active>
        <description>To check the End date to change the status to completed</description>
        <errorConditionFormula>IF( FA_End_Date_Time__c  &gt; Now(),  ISPICKVAL( FA_Status__c , &apos;Completed&apos;) ,False )</errorConditionFormula>
        <errorDisplayField>FA_Status__c</errorDisplayField>
        <errorMessage>Cannot change the Status to Completed if end day is greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_Occurance_for_Active_Course</fullName>
        <active>true</active>
        <description>This validation will restrict the User of creating new occurrence in active Course</description>
        <errorConditionFormula>ISNEW() &amp;&amp;  ISPICKVAL(FA_Course__r.FA_Status__c , &apos;Inactive&apos;)</errorConditionFormula>
        <errorMessage>Occurrence can be Associated with Active Courses.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_Record_Validation</fullName>
        <active>false</active>
        <description>Validation to ensure that Course Occurrence Start Date &lt;today() should not be able to be created / updated should be included</description>
        <errorConditionFormula>IF( today()  &gt; DATEVALUE(FA_Start_Date_Time__c) ,
OR( ISCHANGED( FA_Outlook_Invite_URL__c ) ,
         ISCHANGED(FA_Course__c),
         ISCHANGED(FA_Course_Occ_Name__c),
         ISCHANGED(FA_End_Date_Time__c),
         ISCHANGED(FA_Start_Date_Time__c),
         ISCHANGED(FA_Training_Venue__c),
         ISCHANGED(FA_Training_Room__c), 
         ISCHANGED(FA_Pre_Work_Start_Date__c) ,
         ISCHANGED(FA_Post_Prog_End_Date__c) ,
         ISCHANGED(FA_Mail_Content_1__c) ,
         ISCHANGED(FA_Mail_Content_2__c ) ,
         ISCHANGED(FA_Training_Room_Image__c ) ,
         ISCHANGED(FA_Training_Venue_Image__c )) ,
         False)</errorConditionFormula>
        <errorMessage>Record cannot be modified if start date is less than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_Status_Cancelled_Check</fullName>
        <active>true</active>
        <description>Status shouldnot be changed to Cancel if the course occurrence End date is &gt; today()</description>
        <errorConditionFormula>AND(ISPICKVAL(FA_Status__c,&apos;Cancelled&apos;),( NOW() &gt; FA_End_Date_Time__c ))</errorConditionFormula>
        <errorMessage>Cannot Cancel the Course Occurrence which has scheduled in Past.</errorMessage>
    </validationRules>
</CustomObject>
