        <externalId>false</externalId>
        <formula>FA_Course__r.FA_Course_Code__c+&apos;_&apos;  +
TEXT(DAY( DATEVALUE(FA_Start_Date_Time__c)))  

+ IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=1,&apos;JAN&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=2,&apos;FEB&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=3,&apos;MAR&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=4,&apos;APR&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=5,&apos;MAY&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=6,&apos;JUN&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=7,&apos;JUL&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=8,&apos;AUG&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=9,&apos;SEP&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=10,&apos;OCT&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=11,&apos;NOV&apos;,
IF(MONTH( DATEVALUE(FA_Start_Date_Time__c))=12,&apos;DEC&apos;,&apos;&apos;
))))))))))))
+
TEXT( YEAR( DATEVALUE(FA_Start_Date_Time__c) ))
+ &apos;_&apos; +  RIGHT( Name, 5)</formula>
        <label>Course Occurence Name</label>
        <required>false</required>
