    <validationRules>
        <fullName>ACCL__Check_if_Fund_Template_is_unique</fullName>
        <active>true</active>
        <errorConditionFormula>and( 
Name = VLOOKUP 
($ObjectType.ACCL__Fund_Template__c.Fields.Name , $ObjectType.ACCL__Fund_Template__c.Fields.Name, Name ), 

not( 
Id=VLOOKUP 
($ObjectType.ACCL__Fund_Template__c.Fields.Id , $ObjectType.ACCL__Fund_Template__c.Fields.Name, Name )))</errorConditionFormula>
        <errorMessage>Fund Template with same name already exists. Please use different name</errorMessage>
    </validationRules>
