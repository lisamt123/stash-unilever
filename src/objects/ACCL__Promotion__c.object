    <validationRules>
        <fullName>ACCL__Commit_Date_Validation</fullName>
        <active>false</active>
        <description>The Commit_Date must be less than or equal to DateThru.</description>
        <errorConditionFormula>ACCL__Commit_Date__c  &gt;  ACCL__Date_Thru__c</errorConditionFormula>
        <errorMessage>The Commit Date must be the same day or sooner than Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Date_From_Date_Thru_Validation</fullName>
        <active>true</active>
        <description>Date From must be less than or equal to Date Thru.</description>
        <errorConditionFormula>ACCL__Date_From__c  &gt;  ACCL__Date_Thru__c</errorConditionFormula>
        <errorMessage>Date From must be the same day or sooner than Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Delivery_Date_From_and_Thru_Validation</fullName>
        <active>true</active>
        <description>Check if Delivery Date from is lesser than delivery date thru.</description>
        <errorConditionFormula>ACCL__Delivery_Date_From__c  &gt;  ACCL__Delivery_Date_Thru__c</errorConditionFormula>
        <errorMessage>Delivery Date From cannot be later than Delivery Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Order_Date_From_and_Thru_Validation</fullName>
        <active>true</active>
        <description>Check if Order Date From is greater than Order Date Thru</description>
        <errorConditionFormula>ACCL__Order_Date_From__c  &gt;  ACCL__Order_Date_Thru__c</errorConditionFormula>
        <errorMessage>Order Date From cannot be later than Order Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Placement_Date_From_and_Thru_Validation</fullName>
        <active>true</active>
        <description>To Check if Placement Date From is lesser than Placement Date Thru</description>
        <errorConditionFormula>ACCL__Placement_Date_From__c  &gt;  ACCL__Placement_Date_Thru__c</errorConditionFormula>
        <errorMessage>Placement Date From cannot be later than Placement Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Prevent_Name_Duplication</fullName>
        <active>false</active>
        <description>Prevent name duplication</description>
        <errorConditionFormula>and(
Name = VLOOKUP 
($ObjectType.ACCL__Promotion__c.Fields.Name , $ObjectType.ACCL__Promotion__c.Fields.Name, Name ), 

not( 
Id=VLOOKUP 
      ($ObjectType.ACCL__Promotion__c.Fields.Id , $ObjectType.ACCL__Promotion__c.Fields.Name, Name )))</errorConditionFormula>
        <errorMessage>Promotion Name already exists, please enter different Name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Prevent_Promotion_Template_Change</fullName>
        <active>false</active>
        <description>This validation rule will prevents User to change/ update Promotion Template selection on Promotion</description>
        <errorConditionFormula>ISCHANGED( ACCL__Promotion_Template__c )</errorConditionFormula>
        <errorMessage>You cannot change the Promotion Template once the Promotion is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Read_Only_Name_Field</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  ISPICKVAL(ACCL__Phase__c , &apos;Committed&apos;),  ISCHANGED( Name )  )</errorConditionFormula>
        <errorMessage>Name should not be edited when the Phase is Committed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_Default_Sold_To_Mandatory_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF(  ACCL__Promotion_Template__r.UL_Calculate_Slogan_Name__c &amp;&amp; ISBLANK( ACCL__Anchor_Account__c) , true, false)</errorConditionFormula>
        <errorMessage>All promotions must have a sold-to assigned to them. If no sold-to appears in the list for your account, please contact your support team for guidance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_End_Date_Validation</fullName>
        <active>true</active>
        <description>Validates End Date workflow state conditions.</description>
        <errorConditionFormula>IF((ACCL__Sales_Org__c == &apos;AR01&apos; || ACCL__Sales_Org__c == &apos;CL01&apos; || ACCL__Sales_Org__c == &apos;CL03&apos;) 
&amp;&amp; TEXT( UL_Current_Status__c ) == &apos;End Date&apos; 
&amp;&amp; (ACCL__Date_Thru__c &lt; TODAY() || ACCL__Date_Thru__c &gt; PRIORVALUE(ACCL__Date_Thru__c )) 
,true 
,false )</errorConditionFormula>
        <errorMessage>For an End Dated promotion, Thru Date can&apos;t be a date in the past OR can&apos;t be extended beyond the original Thru Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_FromDate_Planning</fullName>
        <active>true</active>
        <description>Validates that user can set state from Approved to Planning only if Start date is in future.</description>
        <errorConditionFormula>IF((ACCL__Sales_Org__c == &apos;AR01&apos; || ACCL__Sales_Org__c == &apos;CL01&apos; || ACCL__Sales_Org__c == &apos;CL03&apos;)
&amp;&amp; ACCL__Date_From__c &gt;  TODAY()
&amp;&amp;  TEXT(PRIORVALUE(UL_Current_Status__c )) == &apos;Approved&apos; 
&amp;&amp;  TEXT( UL_Current_Status__c ) != &apos;Planning&apos; 
,true
,false )</errorConditionFormula>
        <errorMessage>You can set Workflow State from &apos;Approved&apos; to &apos;Planning&apos; only if the Promotion is not currently running</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_LTA_StartDate_Should_Not_Be_PastDate</fullName>
        <active>true</active>
        <description>The start date on LTAs should be greater than or equal to today</description>
        <errorConditionFormula>AND( ISPICKVAL( $User.ACCL__Sales_Org__c , &apos;UK SO&apos;), ISPICKVAL(ACCL__Promotion_Template__r.UL_Promo_Type_ControlView__c, &apos;LTA&apos;), ACCL__Date_From__c &lt; TODAY() )</errorConditionFormula>
        <errorDisplayField>ACCL__Active__c</errorDisplayField>
        <errorMessage>The start date on LTAs should be greater than or equal to today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_LTA_Time_Period_of_Longer_Than_a_Year</fullName>
        <active>false</active>
        <description>(TPM 1759) - If a time period of longer than a year is defined within the date fields then a validation should be returned and the user is warned that a LTA cannot be created which is longer than one year.</description>
        <errorConditionFormula>AND(VALUE(ACCL__Promotion_Template__r.LTA_Days_Counter__c)  &lt;&gt; 0, ISPICKVAL( $User.ACCL__Sales_Org__c , &apos;UK SO&apos;),ISPICKVAL(ACCL__Promotion_Template__r.UL_Promo_Type_ControlView__c, &apos;LTA&apos;), (ACCL__Date_Thru__c - ACCL__Date_From__c) + 1 &gt; VALUE(ACCL__Promotion_Template__r.LTA_Days_Counter__c))</errorConditionFormula>
        <errorDisplayField>ACCL__Active__c</errorDisplayField>
        <errorMessage>LTA duration is exceeding maximum permissible value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_LTA_Validation_Rule</fullName>
        <active>true</active>
        <description>Validation rule to restrict selection of top level Customer while creating an LTA promotion.</description>
        <errorConditionFormula>IF(AND(TEXT(ACCL__Promotion_Template__r.UL_Promo_Type_ControlView__c) == &apos;LTA&apos; 
, TEXT(ACCL__Anchor_Account__r.UL_Customer_Hierarchy_Level__c ) == &apos;Level 1&apos;
, OR(ACCL__Sales_Org__c == &apos;AR01&apos;,ACCL__Sales_Org__c == &apos;CL01&apos;,ACCL__Sales_Org__c == &apos;CL03&apos;))
, TRUE, FALSE)</errorConditionFormula>
        <errorMessage>An LTA Promotion can&apos;t be associated with a Customer at All Customers (top) level, please select other customer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_MarketingEventValidation</fullName>
        <active>true</active>
        <description>Validation rule to restrict selection of Customer at Level 1 while creating a Marketing Event.</description>
        <errorConditionFormula>IF(TEXT(ACCL__Promotion_Template__r.UL_Promo_Type_ControlView__c) == &apos;MarketingEvent&apos; &amp;&amp; TEXT(ACCL__Anchor_Account__r.UL_Customer_Hierarchy_Level__c ) != &apos;Level 1&apos; &amp;&amp; (ACCL__Anchor_Account__r.ACCL__Sales_Org__c == &apos;AR01&apos; || ACCL__Anchor_Account__r.ACCL__Sales_Org__c == &apos;CL01&apos; || ACCL__Anchor_Account__r.ACCL__Sales_Org__c == &apos;CL03&apos;), TRUE, FALSE)</errorConditionFormula>
        <errorMessage>A Marketing Event can only be associated with a Customer at All Customers (top) level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_MarketingEvent_Validation_Rule</fullName>
        <active>true</active>
        <description>Validation rule to restrict selection of Customer at Level 1 while creating a Marketing Event.</description>
        <errorConditionFormula>IF(TEXT(ACCL__Promotion_Template__r.UL_Promo_Type_ControlView__c) == &apos;MarketingEvent&apos; &amp;&amp; TEXT(ACCL__Anchor_Account__r.UL_Customer_Hierarchy_Level__c ) != &apos;Level 1&apos; &amp;&amp; (ACCL__Anchor_Account__r.ACCL__Sales_Org__c  == &apos;AR01&apos; || ACCL__Anchor_Account__r.ACCL__Sales_Org__c  == &apos;CL01&apos; || ACCL__Anchor_Account__r.ACCL__Sales_Org__c  == &apos;CL03&apos;), TRUE, FALSE)</errorConditionFormula>
        <errorMessage>A Marketing Event can only be associated with a Customer at All Customers (top) level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_PastDated_Urgent_Promotion_Creation</fullName>
        <active>true</active>
        <description>Past Dated promotion creation validation rule</description>
        <errorConditionFormula>IF(NOT(ISPICKVAL($User.ACCL__Sales_Org__c , &apos;UK SO&apos;)) 
&amp;&amp; ISPICKVAL(ACCL__Promotion_Template__r.UL_Promo_Type_ControlView__c, &apos;Past Dated/Urgent&apos;)
&amp;&amp; NOT(CONTAINS(CreatedBy.UserRole.DeveloperName, &apos;Account_Manager&apos;))
&amp;&amp; NOT(CONTAINS(CreatedBy.UserRole.DeveloperName, &apos;Finance_Admin&apos;))
,TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>ACCL__Active__c</errorDisplayField>
        <errorMessage>Past Dated promotions can only be created by Account Manager and Finance Manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_PastDated_Urgent_Promotion_Validation</fullName>
        <active>true</active>
        <description>Past Dated validation rule</description>
        <errorConditionFormula>IF( NOT(ISPICKVAL( $User.ACCL__Sales_Org__c , &apos;UK SO&apos;)) &amp;&amp; ISPICKVAL(ACCL__Promotion_Template__r.UL_Promo_Type_ControlView__c, &apos;Past Dated/Urgent&apos;)&amp;&amp;(ACCL__Date_From__c &gt;= TODAY() || ACCL__Date_Thru__c &lt;= TODAY()),TRUE,FALSE )</errorConditionFormula>
        <errorDisplayField>ACCL__Active__c</errorDisplayField>
        <errorMessage>From date must be in the past, Thru date must be in the future for Past Dated/Urgent Promotion</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_Promotion_Dates</fullName>
        <active>true</active>
        <description>Promotion dates should be with in from and thru dates.</description>
        <errorConditionFormula>IF(ACCL__Placement_Date_From__c	&lt; ACCL__Date_From__c ||ACCL__Placement_Date_Thru__c	&gt; ACCL__Date_Thru__c || ACCL__Delivery_Date_From__c &lt; ACCL__Date_From__c || ACCL__Delivery_Date_Thru__c &gt; ACCL__Date_Thru__c, true, false)</errorConditionFormula>
        <errorMessage>Dates(Shipment or Instore) are not with in Promotion dates range.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>in_store_date_from_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ACCL__Promotion_Template__r.Calculate_In_store_Dates__c = true,if( ISCHANGED(ACCL__Placement_Date_From__c )  &amp;&amp; ACCL__Placement_Date_From__c&lt;TODAY() ,true,false),false)</errorConditionFormula>
        <errorMessage>Date should be greater than today date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>in_store_date_thru_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ACCL__Promotion_Template__r.Calculate_In_store_Dates__c = true,if( ISCHANGED(ACCL__Placement_Date_Thru__c)  &amp;&amp; ACCL__Placement_Date_Thru__c&lt;TODAY() ,true,false), false)</errorConditionFormula>
        <errorMessage>Date should be greater than today date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validation_for_date_ranges</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ACCL__Promotion_Template__r.Calculate_In_store_Dates__c = true,
 IF((ACCL__Placement_Date_Thru__c - ACCL__Placement_Date_From__c)&gt;ACCL__Promotion_Template__r.UL_Calculate_for_Max_Min_Dates__c || 
 (ACCL__Placement_Date_Thru__c - ACCL__Placement_Date_From__c)&lt;0 , true, false), false) || 
 IF(ACCL__Promotion_Template__r.UL_Validate_for_Max_Min_Dates__c = true, 
 IF((ACCL__Date_Thru__c - ACCL__Date_From__c)&gt; ACCL__Promotion_Template__r.UL_Calculate_for_Max_Min_Dates__c || 
 (ACCL__Date_Thru__c - ACCL__Date_From__c)&lt;0 , true, false),false)</errorConditionFormula>
        <errorDisplayField>ACCL__Active__c</errorDisplayField>
        <errorMessage>Dates range should be more than 1 day or below 91 days</errorMessage>
    </validationRules>
