<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting contains data for: 
Chatter Groups that a user should be made a member of based on function/location 
Chatter Group Id for Salesforce Admin group
Value for XX i.e. no. of days of inactivity before deactivating a Chatter External User</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Chatter_Group_Id__c</fullName>
        <description>Contains the Chatter Group Id</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter the Chatter Group Id</inlineHelpText>
        <label>Chatter Group Id</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>External_User_Inactivity_Days__c</fullName>
        <description>This field stores the value for XX i.e. no. of days of inactivity before a Chatter External user should be deactivated.</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter the value for XX i.e. no. of days of inactivity before a Chatter External user should be deactivated.</inlineHelpText>
        <label>External User Inactivity Days</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Core Chatter User Administration</label>
    <visibility>Public</visibility>
</CustomObject>
