AND(NOT(ISBLANK(WU_Count_Avg_Current_Month_Utilization__c)),WU_Count_Avg_Current_Month_Utilization__c &lt;&gt; 0), 

WU_Total_Current_Month_Utilisation__c /WU_Count_Avg_Current_Month_Utilization__c,

