    <validationRules>
        <fullName>DateValidationForDailyUtilizationType</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( WU_Utilization_Type__c , &apos;Daily&apos;), WU_FromDate__c  &lt;&gt;  WU_ToDate__c )</errorConditionFormula>
        <errorDisplayField>WU_Utilization_Type__c</errorDisplayField>
        <errorMessage>Utilization Start Date and Utilization End Date must be same if Utilization Type has been chosen as Daily.</errorMessage>
    </validationRules>
	 <validationRules>
        <fullName>ValidationForCurrentUtilizationDetails</fullName>
        <active>true</active>
        <errorConditionFormula>if(not(isnew()),if(and(ispickval(WU_Utilization_Type__c,&apos;&apos;),isBlank(WU_FromDate__c),isBlank(WU_ToDate__c),isBlank(WU_Current_Utilization__c)),false,or(ispickval(WU_Utilization_Type__c,&apos;&apos;),isBlank(WU_FromDate__c),isBlank(WU_ToDate__c),isBlank(WU_Current_Utilization__c))),false)</errorConditionFormula>
        <errorMessage>In utilization section all fields should have values</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DateValidationForMonthlyUtilizationType</fullName>
        <active>true</active>
        <errorConditionFormula>/*AND( 
ISPICKVAL( WU_Utilization_Type__c , &apos;Monthly&apos;), OR(WU_FromDate__c = WU_ToDate__c, MONTH(WU_ToDate__c) &lt;&gt; MONTH(WU_FromDate__c),WU_ToDate__c-WU_FromDate__c &lt; 7))*/

AND(
ISPICKVAL( WU_Utilization_Type__c , &apos;Monthly&apos;),
OR(DAY(WU_FromDate__c) &lt;&gt; 1,
DAY(WU_ToDate__c) &lt;&gt; (IF( MONTH( WU_FromDate__c) = 12,31,DAY(DATE( YEAR( WU_FromDate__c), MONTH ( WU_FromDate__c) + 1, 1 ) - 1))))
)</errorConditionFormula>
        <errorDisplayField>WU_Utilization_Type__c</errorDisplayField>
        <errorMessage>Utilization Start Date and Utilization End Date should be for same month (with start date  1st day  and end date as last day of the Month)  for Utilization Type has been chosen as Monthly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DateValidationForWeeklyUtilizationType</fullName>
        <active>true</active>
        <errorConditionFormula>/*AND(
ISPICKVAL( WU_Utilization_Type__c , &apos;Weekly&apos;), OR(WU_FromDate__c  =  WU_ToDate__c, WU_ToDate__c-WU_FromDate__c &gt;=  7))*/

AND(

ISPICKVAL( WU_Utilization_Type__c , &apos;Weekly&apos;), 

OR(WU_FromDate__c  =  WU_ToDate__c, WU_ToDate__c-WU_FromDate__c &gt;= 7, 

MOD( WU_FromDate__c - DATE(1900, 1, 7), 7)&lt;&gt; 0,MOD( WU_ToDate__c - DATE(1900, 1, 7), 7)&lt;&gt;6)

)</errorConditionFormula>
        <errorDisplayField>WU_Utilization_Type__c</errorDisplayField>
        <errorMessage>Weekly Utilization should be entered  from Sunday to Saturday. Please Re- Enter the Utilization Start Date as Sunday and End Date as Saturday.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDate_NotGreaterThan_Enddate</fullName>
        <active>true</active>
        <description>Utilization Start date should not be greater than end date</description>
        <errorConditionFormula>WU_FromDate__c  &gt;  WU_ToDate__c</errorConditionFormula>
        <errorDisplayField>WU_FromDate__c</errorDisplayField>
        <errorMessage>Utilization Start  Date should not be greater than Utilization End Date.</errorMessage>
    </validationRules>
