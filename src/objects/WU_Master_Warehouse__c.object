     <fields>
        <fullName>WU_Average_Current_Utilization__c</fullName>
        <externalId>false</externalId>
        <formula>/*IF( WU_Approval_Status__c =&apos;Submitted&apos;,( WU_Current_Utilization__c / WU_Prior_CommittedPalletStorage__c)*100 ,(WU_Current_Utilization__c/ WU_Committed_Max_PalletStorage__c)*100 )*/
/*
IF(AND(WU_Count_Avg_Current_Month_Utilization__c = 0,WU_Approval_Status__c =&apos;Submitted&apos;),( WU_Current_Utilization__c / WU_Prior_CommittedPalletStorage__c)*100,IF(WU_Count_Avg_Current_Month_Utilization__c &gt; 0,( WU_Current_Utilization__c /  IF(ISBLANK(WU_Latest_Committed_Pallet__c ),WU_Committed_Max_PalletStorage__c,WU_Latest_Committed_Pallet__c ))*100,0) )*/

IF(
AND(NOT(ISBLANK(WU_Latest_Committed_Pallet__c)),NOT(WU_Latest_Committed_Pallet__c = 0)),
IF(   ((WU_Current_Utilization__c/WU_Latest_Committed_Pallet__c)*100  &gt; 100) , 100 , (WU_Current_Utilization__c/WU_Latest_Committed_Pallet__c)*100  ),
IF(
AND(NOT(ISBLANK(WU_Committed_Max_PalletStorage__c)),NOT(WU_Committed_Max_PalletStorage__c = 0)),
IF(  ((WU_Current_Utilization__c/WU_Committed_Max_PalletStorage__c)*100 &gt; 100) , 100 , (WU_Current_Utilization__c/WU_Committed_Max_PalletStorage__c)*100 ),
100
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
