    <validationRules>
        <fullName>ACCL__Customer_Product_Sales_Org_Mismatch</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() ,
ACCL__Anchor_Account__r.ACCL__Sales_Org__c &lt;&gt; ACCL__Anchor_Product__r.ACCL__Sales_Org__c,
 ISPICKVAL(ACCL__Fund_Template__r.ACCL__Anchor_Type__c ,&apos;Customer Product&apos;)
)</errorConditionFormula>
        <errorMessage>Sales Org of Anchor Customer and Anchor Product should be same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Template_Customer_Sales_Org_Mismatch</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),
TEXT( ACCL__Fund_Template__r.ACCL__Sales_Org__c ) &lt;&gt; ACCL__Anchor_Account__r.ACCL__Sales_Org__c
)</errorConditionFormula>
        <errorMessage>Sales org of Fund Template and Anchor Customer should be same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Template_Product_Sales_Org_Mismatch</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISNEW(),
TEXT( ACCL__Fund_Template__r.ACCL__Sales_Org__c ) &lt;&gt; ACCL__Anchor_Product__r.ACCL__Sales_Org__c,
ISPICKVAL(ACCL__Fund_Template__r.ACCL__Anchor_Type__c ,&apos;Customer Product&apos;) 
)</errorConditionFormula>
        <errorMessage>Sales org of Fund Template and Anchor Product should be same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_Fund_Close_validation</fullName>
        <active>true</active>
        <description>Fund cannot be closed till there a open tactic against it</description>
        <errorConditionFormula>AND(
ISPICKVAL(ACCL__Status__c, &quot;Closed&quot;) , 
OR(UL_Uncommitted__c &gt; 0, UL_Committed__c &gt; 0),  ACCL__Fund_Template__r.UL_Uncommitted_Amount_Validation__c =TRUE )</errorConditionFormula>
        <errorDisplayField>ACCL__Status__c</errorDisplayField>
        <errorMessage>Fund cannot be closed as it has a open tactic against it</errorMessage>
    </validationRules>
