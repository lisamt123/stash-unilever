    <validationRules>
        <fullName>PQN_Validation_ON_Date_Pallet_Received</fullName>
        <active>true</active>
        <description>This validation rule, will ensure that user will not enter a future date (Greater than today) for Date Pallet Received.</description>
        <errorConditionFormula>Date_Pallet_Received__c &gt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Date_Pallet_Received__c</errorDisplayField>
        <errorMessage>Date Pallet Received should not be greater than today&apos;s date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PQN_Validation_ON_SKU</fullName>
        <active>true</active>
        <description>This validation rule, will ensure that Product Name, Product Group and Product Category are having the correct values.</description>
        <errorConditionFormula>NOT (REGEX( SKU__c , &quot;[0-9]+&quot;))</errorConditionFormula>
        <errorDisplayField>SKU__c</errorDisplayField>
        <errorMessage>SKU entered should contain only numbers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PQN_Validation_ON_Supplier_Name</fullName>
        <active>false</active>
        <description>This validation rule, will ensure that Supplier Number and Supplier Type are having the correct values.</description>
        <errorConditionFormula>AND( ISBLANK( Supplier_Number__c  ) , ISBLANK( Supplier_type__c  )  )</errorConditionFormula>
        <errorDisplayField>Supplier__c</errorDisplayField>
        <errorMessage>Supplier Name entered is not valid.</errorMessage>
    </validationRules>
