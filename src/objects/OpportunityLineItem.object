    <fields>
        <fullName>ApprovalGranted__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Approval Granted</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ApprovalRequired__c</fullName>
        <defaultValue>false</defaultValue>
        <description>It is used in validating Approval if any discount is requested on Opportunity Product</description>
        <externalId>false</externalId>
        <label>Approval Required</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Description</fullName>
    </fields>
    <fields>
        <fullName>Discount</fullName>
    </fields>
    <fields>
        <fullName>Distributor__c</fullName>
        <externalId>false</externalId>
        <label>Distributor</label>
        <picklist>
            <picklistValues>
                <fullName>US Direct / CED</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>US Sysco</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>US Foods</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Reinhart</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>PFG</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>US GFS</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>US OTher</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>US Only (Food Buy, Aramark, Sodexho, Compass)</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>CA Sysco</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>CA GFS</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Colabor</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>CA Other</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>CA Direct</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>FS_BR3__c</fullName>
        <externalId>false</externalId>
        <label>BR3</label>
        <precision>7</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_DISTTMI__c</fullName>
        <externalId>false</externalId>
        <formula>FS_DSTMI__c * Quantity</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DIST TMI</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_DSTMI__c</fullName>
        <externalId>false</externalId>
        <formula>UnitPrice *((IF( ISPICKVAL( Distributor__c , &quot;US Direct / CED&quot;) , 0, IF( ISPICKVAL( Distributor__c , &quot;US Sysco&quot;) , 11.6, IF( ISPICKVAL( Distributor__c , &quot;US Foods&quot;) , 9.1, IF( ISPICKVAL( Distributor__c , &quot;Reinhart&quot;) , 9.1, IF( ISPICKVAL( Distributor__c , &quot;PFG&quot;) , 8.1, IF( ISPICKVAL( Distributor__c , &quot;US GFS&quot;) , 6, IF( ISPICKVAL( Distributor__c , &quot;US OTher&quot;) , 6.3, IF( ISPICKVAL( Distributor__c , &quot;US Only (Food Buy, Aramark, Sodexho, Compass)&quot;) , 6.1, IF( ISPICKVAL( Distributor__c , &quot;CA Sysco&quot;) , 12, IF( ISPICKVAL( Distributor__c , &quot;CA GFS&quot;) , 12.6, IF( ISPICKVAL( Distributor__c , &quot;Colabor&quot;) , 11.1, IF( ISPICKVAL( Distributor__c , &quot;CA Other&quot;) , 6.5, IF( ISPICKVAL( Distributor__c , &quot;CA Direct&quot;) , 0, 0))))))))))))))/100)*0.99</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DS TMI/Cs</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_DeltaBR1BR3__c</fullName>
        <externalId>false</externalId>
        <formula>FS_BR3__c  -    UnitPrice</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Delta BR1-BR3</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_FixedPrice__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Fixed Price</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>FS_HQRate__c</fullName>
        <externalId>false</externalId>
        <label>HQ Rate</label>
        <precision>6</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_Installation_charges__c</fullName>
        <externalId>false</externalId>
        <label>Installation charges</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_MarketTarget__c</fullName>
        <externalId>false</externalId>
        <formula>PricebookEntry.Product2.ProfitabilityTarget__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Market Target</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>FS_NSV__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Quantity &lt;&gt;0,  Quantity  *   UnitPrice   * 0.99, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>NSV</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_OpTMICs__c</fullName>
        <externalId>false</externalId>
        <formula>FS_Rate__c  +  FS_HQRate__c  +  Opportunity.FS_MSCS__c +  IF( FS_FixedPrice__c = TRUE,  FS_DeltaBR1BR3__c , 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Op TMI/Cs</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_OpTMI__c</fullName>
        <externalId>false</externalId>
        <formula>FS_OpTMICs__c  *  Quantity</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Op TMI</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_ProductMarket__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(PricebookEntry.Product2.MarketType__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Product Market</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>FS_Rate__c</fullName>
        <externalId>false</externalId>
        <label>Rate</label>
        <precision>8</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_ServicePerYear__c</fullName>
        <externalId>false</externalId>
        <label>Service Per Year</label>
        <picklist>
            <picklistValues>
                <fullName>0</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>1/4</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>1/2</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>1</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>2</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>3</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>4</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>GPPercent__c</fullName>
        <externalId>false</externalId>
        <formula>GrossProfit__c  /  Turnover__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gross Profit %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>GrossProfit__c</fullName>
        <description>To Calculate GrossProfit</description>
        <externalId>false</externalId>
        <formula>Turnover__c - TotalCOGS__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gross Profit</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>ListPrice</fullName>
    </fields>
    <fields>
        <fullName>OpportunityId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Product2Id</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>ProductCode</fullName>
    </fields>
    <fields>
        <fullName>Quantity</fullName>
    </fields>
    <fields>
        <fullName>ServiceDate</fullName>
    </fields>
    <fields>
        <fullName>Subtotal</fullName>
    </fields>
    <fields>
        <fullName>TotalCOGS__c</fullName>
        <externalId>false</externalId>
        <formula>PricebookEntry.Product2.COGS__c  *  Quantity</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total COGS</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>TotalPrice</fullName>
    </fields>
    <fields>
        <fullName>Turnover__c</fullName>
        <externalId>false</externalId>
        <formula>FS_NSV__c  -   FS_DISTTMI__c -  FS_OpTMI__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Turnover</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>UnitPrice</fullName>
    </fields>
    <validationRules>
        <fullName>FS_Inactive_Products</fullName>
        <active>true</active>
        <description>Check if inactive product is selected</description>
        <errorConditionFormula>Product2.Active__c  = FALSE</errorConditionFormula>
        <errorMessage>You can&apos;t select a product which is inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Products_Stage_5</fullName>
        <active>true</active>
        <description>An opportunity at stage 05 - Won/Closed Won can not be changed at the line item level.</description>
        <errorConditionFormula>OR(ISPICKVAL(Opportunity.StageName, &quot;Closed Won&quot;),ISPICKVAL(Opportunity.StageName, &quot;05 – Won&quot;))</errorConditionFormula>
        <errorMessage>An opportunity at stage 05 - Won/Closed Won can not be changed at the line item level.</errorMessage>
    </validationRules>
</CustomObject>
