        <description>To Calculate GrossProfit</description>
        <externalId>false</externalId>
        <formula>Turnover__c - TotalCOGS__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gross Profit</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>ListPrice</fullName>
    </fields>
    <fields>
        <fullName>OpportunityId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Product2Id</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>ProductCode</fullName>
    </fields>
    <fields>
        <fullName>Quantity</fullName>
    </fields>
    <fields>
        <fullName>ServiceDate</fullName>
    </fields>
    <fields>
        <fullName>Subtotal</fullName>
    </fields>
    <fields>
        <fullName>TotalCOGS__c</fullName>
        <externalId>false</externalId>
        <formula>PricebookEntry.Product2.COGS__c  *  Quantity</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total COGS</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>TotalPrice</fullName>
    </fields>
    <fields>
        <fullName>Turnover__c</fullName>
        <externalId>false</externalId>
        <formula>FS_NSV__c  -   FS_DISTTMI__c -  FS_OpTMI__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Turnover</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>UnitPrice</fullName>
    </fields>
    <validationRules>
        <fullName>FS_Inactive_Products</fullName>
        <active>true</active>
        <description>Check if inactive product is selected</description>
        <errorConditionFormula>Product2.Active__c  = FALSE</errorConditionFormula>
        <errorMessage>You can&apos;t select a product which is inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Products_Stage_5</fullName>
        <active>true</active>
        <description>An opportunity at stage 05 - Won/Closed Won can not be changed at the line item level.</description>
        <errorConditionFormula>OR(ISPICKVAL(Opportunity.StageName, &quot;Closed Won&quot;),ISPICKVAL(Opportunity.StageName, &quot;05 – Won&quot;))</errorConditionFormula>
        <errorMessage>An opportunity at stage 05 - Won/Closed Won can not be changed at the line item level.</errorMessage>
    </validationRules>
</CustomObject>
