    <validationRules>
        <fullName>FS_Distributor_TMI_Req</fullName>
        <active>true</active>
        <description>Distributor__c field is required for FS NA</description>
        <errorConditionFormula>AND($Profile.Name = &quot;Unilever Food Solution - NA&quot; , ISPICKVAL( Distributor__c,&apos;&apos;))</errorConditionFormula>
        <errorMessage>Distributor TMI is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FS_Inactive_Products</fullName>
        <active>true</active>
        <description>Check if inactive product is selected</description>
        <errorConditionFormula>Product2.IsActive  = FALSE</errorConditionFormula>
        <errorMessage>You can&apos;t select a product which is inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FS_OpportunityProductsStageWon</fullName>
        <active>true</active>
        <description>An opportunity at stage Won can not be changed at the line item level.</description>
        <errorConditionFormula>OR (IF($Profile.Name=&quot;Unilever Food Solution - Russia&quot;,ISPICKVAL(Opportunity.StageName, &quot;3a-Won&quot;),false),IF($Profile.Name=&quot;Unilever Food Solution - NA&quot;,
OR(ISPICKVAL(Opportunity.StageName, &quot;Closed Won&quot;),ISPICKVAL(Opportunity.StageName, &quot;05 â€“ Won&quot;)),false))</errorConditionFormula>
        <errorMessage>An opportunity at stage Won can not be changed at the line item level.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FS_Schedule_On_Existing_Opportunity</fullName>
        <active>true</active>
        <description>It is used to restrict user to do product scheduling on existing opportunity</description>
        <errorConditionFormula>AND(HasQuantitySchedule=true, Opportunity.RecordType.DeveloperName=&apos;FS_ExistingBusiness&apos;)</errorConditionFormula>
        <errorMessage>Product scheduling can not be done for Existing opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FS_TPRshouldnotbeanegativevalue</fullName>
        <active>true</active>
        <description>Purpose:&quot; TPR %&quot; shouldn&apos;t be a negative value and &quot;TPR %&quot; should be less 100.00%</description>
        <errorConditionFormula>AND(OR(
   FS_tprPercent__c&lt;0.0,
   FS_tprPercent__c&gt;1 
),$Profile.Name =&apos;Unilever Food Solution - Russia&apos;, Opportunity.RecordType.Name =&apos;UFS Opportunity Russia&apos;)</errorConditionFormula>
        <errorMessage>TPR% shouldn&apos;t be a negative value and should be less than 100.00%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FS_Valuenecessarynotgreaterthanlistpric</fullName>
        <active>true</active>
        <description>Purpose: Value necessary not greater than list price.</description>
        <errorConditionFormula>AND(( FS_valueNecessaryWithVAT__c &gt; ListPrice), $Profile.Name =&apos;Unilever Food Solution - Russia&apos;, Opportunity.RecordType.Name =&apos;UFS Opportunity Russia&apos;)</errorConditionFormula>
        <errorMessage>Value necessary (w/ VAT) not greater than list price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Margin_Market_Code_Matches_on_Opportunit</fullName>
        <active>true</active>
        <description>The Opportunity Margin Market Code must match the Product Margin Market Code</description>
        <errorConditionFormula>FSNAMarginMarketCodeHelper__c  &lt;&gt;  TEXT(Opportunity.FSNA_Margin_Market_Code__c)</errorConditionFormula>
        <errorMessage>Opportunity Product Margin Market Code must match the Opportunity Margin Market Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_Locked_Validation</fullName>
        <active>true</active>
        <description>Validation for read only opportunity</description>
        <errorConditionFormula>IF($Permission.FS_Admin, 
   false, 
   IF( Opportunity.RecordType.DeveloperName = &apos;FS_OpportunityTPRReadOnly&apos; 
      &amp;&amp; ( ISCHANGED(Quantity)
      || ISCHANGED( UnitPrice )
      || ISCHANGED ( FS_tprPercent__c) 
      || ISCHANGED( FS_valueNecessaryWithVAT__c ) 
      || ISCHANGED( FS_DistributorMarkup__c )
      || ISCHANGED(  FS_Sample__c )
      || ISCHANGED(  FS_TPRCalculationFlag__c ) 
          ),
      true,
      false
     )
)</errorConditionFormula>
        <errorMessage>Opportunity is already locked for update.</errorMessage>
    </validationRules>
</CustomObject>