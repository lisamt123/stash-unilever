    <validationRules>
        <fullName>ACCL__I_date_from_less_or_equal_i_date_thru</fullName>
        <active>true</active>
        <description>Instore Date From must be less than or equals Instore Date Thru</description>
        <errorConditionFormula>ACCL__Instore_Date_From__c  &gt;  ACCL__Instore_Date_Thru__c</errorConditionFormula>
        <errorDisplayField>ACCL__Instore_Date_From__c</errorDisplayField>
        <errorMessage>Instore Date From cannot be later than Instore Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Prevent_Tactic_Template_Change</fullName>
        <active>true</active>
        <description>This validation rule will prevent User to change/ update Tactic Template selection on Tactic</description>
        <errorConditionFormula>ISCHANGED( ACCL__Tactic_Template__c )</errorConditionFormula>
        <errorDisplayField>ACCL__Tactic_Template__c</errorDisplayField>
        <errorMessage>You cannot change a Tactic Template once a Tactic is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__S_date_from_less_or_equal_s_date_thru</fullName>
        <active>true</active>
        <description>Shipment Date From &lt;= Shipment Date Thru</description>
        <errorConditionFormula>ACCL__Shipment_Date_From__c  &gt;  ACCL__Shipment_Date_Thru__c</errorConditionFormula>
        <errorDisplayField>ACCL__Shipment_Date_From__c</errorDisplayField>
        <errorMessage>Shipment Date From cannot be later than Shipment Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__date_from_less_or_equal_date_thru</fullName>
        <active>true</active>
        <errorConditionFormula>ACCL__Date_From__c  &gt;   ACCL__Date_Thru__c</errorConditionFormula>
        <errorDisplayField>ACCL__Date_From__c</errorDisplayField>
        <errorMessage>Date From cannot be later than Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_CreateInvestmentOnPromotionRule</fullName>
        <active>true</active>
        <errorConditionFormula>if( ISPICKVAL(ACCL__Promotion__r.UL_Promotion_Type__c, &apos;Volume Only&apos;) ,true,false )</errorConditionFormula>
        <errorMessage>you can&apos;t able to create on this promotion type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_OrderDateFromLessthanThrudate</fullName>
        <active>true</active>
        <errorConditionFormula>UL_Order_Date_From__c &gt; UL_Order_Date_Thru__c</errorConditionFormula>
        <errorDisplayField>UL_Order_Date_From__c</errorDisplayField>
        <errorMessage>Order Date From cannot be later than Order Date Thru</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ul_Validate_Promotion_Dates</fullName>
        <active>true</active>
        <description>This is used to validate Tactic dates with in the promotion dates range.</description>
        <errorConditionFormula>iF(ACCL__Tactic_Template__r.UL_Validate_Promotion_Dates__c&amp;&amp;NOT( (ACCL__Date_From__c&gt;= ACCL__Promotion__r.ACCL__Date_From__c &amp;&amp; ACCL__Date_From__c&lt;= ACCL__Promotion__r.ACCL__Date_Thru__c)|| (ACCL__Date_Thru__c&gt;=ACCL__Promotion__r.ACCL__Date_From__c &amp;&amp;ACCL__Date_Thru__c&lt;=ACCL__Promotion__r.ACCL__Date_Thru__c   ) || (ACCL__Instore_Date_From__c&gt;=ACCL__Promotion__r.ACCL__Date_From__c &amp;&amp; ACCL__Instore_Date_From__c&lt;=ACCL__Promotion__r.ACCL__Date_Thru__c) || (ACCL__Instore_Date_Thru__c&gt;=ACCL__Promotion__r.ACCL__Date_From__c &amp;&amp; ACCL__Instore_Date_Thru__c&lt;=ACCL__Promotion__r.ACCL__Date_Thru__c) || (UL_Order_Date_From__c&gt;=ACCL__Promotion__r.ACCL__Date_From__c &amp;&amp; UL_Order_Date_From__c&lt;=ACCL__Promotion__r.ACCL__Date_Thru__c) ||  (UL_Order_Date_Thru__c&gt;=ACCL__Promotion__r.ACCL__Date_From__c &amp;&amp; UL_Order_Date_Thru__c&lt;=ACCL__Promotion__r.ACCL__Date_Thru__c) || (ACCL__Shipment_Date_From__c&gt;=ACCL__Promotion__r.ACCL__Date_From__c &amp;&amp; ACCL__Shipment_Date_From__c&lt;=ACCL__Promotion__r.ACCL__Date_Thru__c) || (ACCL__Shipment_Date_Thru__c&gt;=ACCL__Promotion__r.ACCL__Date_From__c &amp;&amp; ACCL__Shipment_Date_Thru__c&lt;=ACCL__Promotion__r.ACCL__Date_Thru__c)), true, false)</errorConditionFormula>
        <errorMessage>Tactic Dates(Shipment or Order or Instore) are not with in Promotion dates range.</errorMessage>
    </validationRules>
</CustomObject>
