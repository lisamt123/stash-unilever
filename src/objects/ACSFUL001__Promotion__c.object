    <validationRules>
        <fullName>ACSFUL001__Commit_Date_Validation</fullName>
        <active>true</active>
        <description>The Commit_Date must be less than or equal to DateThru.</description>
        <errorConditionFormula>ACSFUL001__Commit_Date__c  &gt;  ACSFUL001__Date_Thru__c</errorConditionFormula>
        <errorMessage>The Commit Date must be the same day or sooner than Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACSFUL001__Date_From_Date_Thru_Validation</fullName>
        <active>true</active>
        <description>Date From must be less than or equal to Date Thru.</description>
        <errorConditionFormula>ACSFUL001__Date_From__c  &gt;  ACSFUL001__Date_Thru__c</errorConditionFormula>
        <errorMessage>Date From must be the same day or sooner than Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACSFUL001__Delivery_Date_From_and_Thru_Validation</fullName>
        <active>true</active>
        <description>Check if Delivery Date from is lesser than delivery date thru.</description>
        <errorConditionFormula>ACSFUL001__Delivery_Date_From__c  &gt;  ACSFUL001__Delivery_Date_Thru__c</errorConditionFormula>
        <errorMessage>Delivery Date From cannot be later than Delivery Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACSFUL001__Order_Date_From_and_Thru_Validation</fullName>
        <active>true</active>
        <description>Check if Order Date From is greater than Order Date Thru</description>
        <errorConditionFormula>ACSFUL001__Order_Date_From__c  &gt;  ACSFUL001__Order_Date_Thru__c</errorConditionFormula>
        <errorMessage>Order Date From cannot be later than Order Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACSFUL001__Placement_Date_From_and_Thru_Validation</fullName>
        <active>true</active>
        <description>To Check if Placement Date From is lesser than Placement Date Thru</description>
        <errorConditionFormula>ACSFUL001__Placement_Date_From__c  &gt;  ACSFUL001__Placement_Date_Thru__c</errorConditionFormula>
        <errorMessage>Placement Date From cannot be later than Placement Date Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACSFUL001__Prevent_Name_Duplication</fullName>
        <active>false</active>
        <description>Prevent name duplication</description>
        <errorConditionFormula>and(
Name = VLOOKUP 
($ObjectType.ACSFUL001__Promotion__c.Fields.Name , $ObjectType.ACSFUL001__Promotion__c.Fields.Name, Name ), 

not( 
Id=VLOOKUP 
      ($ObjectType.ACSFUL001__Promotion__c.Fields.Id , $ObjectType.ACSFUL001__Promotion__c.Fields.Name, Name )))</errorConditionFormula>
        <errorMessage>Promotion Name already exists, please enter different Name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACSFUL001__Prevent_Promotion_Template_Change</fullName>
        <active>false</active>
        <description>This validation rule will prevents User to change/ update Promotion Template selection on Promotion</description>
        <errorConditionFormula>ISCHANGED( ACSFUL001__Promotion_Template__c )</errorConditionFormula>
        <errorMessage>You cannot change the Promotion Template once the Promotion is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACSFUL001__Read_Only_Name_Field</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  ISPICKVAL(ACSFUL001__Phase__c , &apos;Committed&apos;),  ISCHANGED( Name )  )</errorConditionFormula>
        <errorMessage>Name should not be edited when the Phase is Committed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_PastDated_Urgent_Promotion_Validation</fullName>
        <active>true</active>
        <description>Past Dated validation rule</description>
        <errorConditionFormula>IF(ISPICKVAL(ACSFUL001__Promotion_Template__r.UL_Promo_Type_ControlView__c, &apos;Past Dated/Urgent&apos;),
	IF(ACSFUL001__Date_From__c  &gt;=  TODAY() || ACSFUL001__Date_Thru__c  &lt;  TODAY(),TRUE,FALSE ),
	ACSFUL001__Date_From__c  &lt;  TODAY())</errorConditionFormula>
        <errorMessage>Past\Urgent promotion template should be used to create past promotions only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>in_store_date_from_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ACSFUL001__Promotion_Template__r.Calculate_In_store_Dates__c = true,if(ACSFUL001__Placement_Date_From__c&lt;TODAY() ,true,false),false)</errorConditionFormula>
        <errorMessage>Date should be greater then today date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>in_store_date_thru_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ACSFUL001__Promotion_Template__r.Calculate_In_store_Dates__c = true,if(ACSFUL001__Placement_Date_Thru__c&lt;TODAY() ,true,false), false)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Date should be greater then today date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validation_for_date_ranges</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ACSFUL001__Promotion_Template__r.Calculate_In_store_Dates__c  = true, IF( (ACSFUL001__Placement_Date_Thru__c -  ACSFUL001__Placement_Date_From__c)&gt;90 || (ACSFUL001__Placement_Date_Thru__c -  ACSFUL001__Placement_Date_From__c)&lt;1 , true, false), false)</errorConditionFormula>
        <errorMessage>In store dates range should be more than 1 day or below 91 days</errorMessage>
    </validationRules>
