    <validationRules>
        <fullName>Lock_Campaign_Record_if_Campaign_Sync</fullName>
        <active>true</active>
        <description>Lock Campaign Record if Campaign Sync  in Progress.</description>
        <errorConditionFormula>IF($Setup.Oblix_Job_Orchestration__c.Job_Is_Running__c == true &amp;&amp; $Profile.Name &lt;&gt; &apos;System Administrator&apos; &amp;&amp; (TEXT( Sync_Status__c ) == &apos;Sync In Progress&apos; ||  Sync_Required_NFY__c  == &apos;Sync In Progress&apos; || TEXT( Sync_Status__c ) == &apos;Sync Requested&apos; ||  Sync_Required_NFY__c  == &apos;Sync Requested&apos;),true,false)</errorConditionFormula>
        <errorMessage>Sync in Progress. Record Locked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Oblix_Campaign_Splits_Should_Add_To_100</fullName>
        <active>false</active>
        <description>Total number of agency hub splits should add up to 100%</description>
        <errorConditionFormula>ISCHANGED(Campaign_Splits_Total__c) &amp;&amp; (Campaign_Splits_Total__c &gt; 1 || (Campaign_Splits_Total__c &gt;0 &amp;&amp; Campaign_Splits_Total__c &lt; 1))</errorConditionFormula>
        <errorMessage>Total % for Agency Hub splits should add up to 100</errorMessage>
    </validationRules>
