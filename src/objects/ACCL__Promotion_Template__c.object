    <validationRules>
        <fullName>ACCL__AnchorType_CalculationMode_Combination</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(ACCL__Anchor_Type__c, &apos;CustomerSet&apos;) &amp;&amp; ISPICKVAL(ACCL__Calculation_Mode__c, &apos;Writeback&apos;))</errorConditionFormula>
        <errorDisplayField>ACCL__Calculation_Mode__c</errorDisplayField>
        <errorMessage>The selected Calculation Mode is not valid in combination with CustomerSet-Promotions</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Prevent_Name_Duplication</fullName>
        <active>true</active>
        <errorConditionFormula>and(
      Name = VLOOKUP
      ($ObjectType.ACCL__Promotion_Template__c.Fields.Name , $ObjectType.ACCL__Promotion_Template__c.Fields.Name, Name ),

      not(
      Id=VLOOKUP
      ($ObjectType.ACCL__Promotion_Template__c.Fields.Id , $ObjectType.ACCL__Promotion_Template__c.Fields.Name, Name )))</errorConditionFormula>
        <errorMessage>Promotion Template Name already exists, please enter different Name.</errorMessage>
    </validationRules>
