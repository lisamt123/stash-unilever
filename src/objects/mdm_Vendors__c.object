    <fields>
        <fullName>VPM_FullDetails__c</fullName>
        <description>Provides a text summary of the important SAP matching fields for display within the Advanced form. Not interfaced with SAP</description>
        <externalId>false</externalId>
        <formula>mdm_Name1__c &amp; &quot;, &quot; &amp;  mdm_Street__c &amp; &quot;, &quot; &amp; mdm_City__c &amp; &quot;, &quot; &amp; mdm_Postcode__c  &amp; &quot;, &quot; &amp; TEXT(mdm_Country__c)  &amp;&quot; | ECC: &quot;&amp; mdm_ecc__c &amp;&quot; | Vendor Code: &quot;&amp; 
IF(CONTAINS(mdm_ecc__c,&quot;Cordillera&quot;), mdm_vCodeCordillera__c,
IF(CONTAINS(mdm_ecc__c,&quot;Fusion&quot;), mdm_vCodeFusion__c,
IF(CONTAINS(mdm_ecc__c,&quot;Sirius&quot;), mdm_vCodeSirius__c,
IF(CONTAINS(mdm_ecc__c,&quot;U2K2&quot;), mdm_vCodeU2K2__c,
&quot;&quot;))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Full Details</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
