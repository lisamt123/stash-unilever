    <webLinks>
        <fullName>Submit_for_Approval</fullName>
        <availability>online</availability>
        <description>Custom button to alert user if no planner has been selected, redirect otherwise</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>/**
 * 04-DEC-2013 sebastian@tquila.com
 * as per CR - alert user if no planner selected
 */
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
 
// query the record
var qr = sforce.connection.query(&quot;SELECT Id, Track_Categories__c, Track_Cluster__c, Track_Country__c, Track_Expertise__c, Track_Functions__c, Track_MCO__c  FROM cp_Event__c where Id=&apos;&quot; + &quot;{!cp_Event__c.Id}&quot; + &quot;&apos;&quot;);
var records = qr.getArray(&quot;records&quot;);
var record = records[0];

// Define return url
var fromUrl = &apos;{!cp_Event__c.Id}&apos;; 
if (self != top) { 
fromUrl = &apos;apex%2Fcp_RecordDetail%3FId={!cp_Event__c.Id}&apos;; 
}
 
// redirect to approval submission
if (record.Track_Categories__c|| record.Track_Cluster__c || record.Track_Country__c || record.Track_Expertise__c || record.Track_Functions__c || record.Track_MCO__c){
    window.location = &apos;/p/process/Submit?retURL=%2F&apos;+fromUrl+&apos;&amp;id={!cp_Event__c.Id}&amp;saveURL=%2F&apos;+fromUrl;
}
else {
    alert(&apos;Don\&apos;t forget to select the Planner you are representing&apos;);
}</url>
