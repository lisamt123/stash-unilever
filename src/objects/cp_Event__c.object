    <validationRules>
        <fullName>Activity_Description_Other_not_Included</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( ActivityDescriptionOther__c ) &amp;&amp;  (INCLUDES( ActivityDescriptionInternal__c , &quot;Other&quot;) || INCLUDES( ActivityDescriptionExternal__c , &quot;Other&quot;))</errorConditionFormula>
        <errorDisplayField>ActivityDescriptionOther__c</errorDisplayField>
        <errorMessage>Please specify how you are activating this event.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Additional_Status_Details</fullName>
        <active>true</active>
        <description>Additional information on the present Delivery Status is required when the status is Amber or Red.</description>
        <errorConditionFormula>ISPICKVAL(Event_Delivery_Status__c, &quot;Red = issues arisen&quot;) &amp;&amp;
ISBLANK( Delivery_Status_Details__c )</errorConditionFormula>
        <errorDisplayField>Delivery_Status_Details__c</errorDisplayField>
        <errorMessage>Why is this event on red status?</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_cannot_be_before_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>EndDate__c &lt;  StartDate__c</errorConditionFormula>
        <errorDisplayField>EndDate__c</errorDisplayField>
        <errorMessage>End date must be after start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GlobalEvent_NoLocalApproverNeeded</fullName>
        <active>false</active>
        <description>You don&apos;t need a local approver for a global event.</description>
        <errorConditionFormula>NOT(ISBLANK( Local_Approver__c )) &amp;&amp;  ISPICKVAL(Local_or_Global__c, &quot;Global&quot;)</errorConditionFormula>
        <errorDisplayField>Local_Approver__c</errorDisplayField>
        <errorMessage>You do not need a Local Approver for a Global Event, this will default to Emma &amp; Leena.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Key_Audience_Other_Not_Entered</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Key_Audience_Other__c) &amp;&amp;  ( INCLUDES( KeyAudienceInternal__c, &quot;Other&quot;) || INCLUDES( KeyAudienceExternal__c, &quot;Other&quot;))</errorConditionFormula>
        <errorDisplayField>Key_Audience_Other__c</errorDisplayField>
        <errorMessage>Please specify ‘other’ key audience.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LocalEvent_NeedsLocalApprover</fullName>
        <active>false</active>
        <description>User will be prompted to enter a local approver if the event is local</description>
        <errorConditionFormula>ISBLANK( Local_Approver__c ) &amp;&amp;  ISPICKVAL(Local_or_Global__c, &quot;Local&quot;)</errorConditionFormula>
        <errorDisplayField>Local_Approver__c</errorDisplayField>
        <errorMessage>You need to specify a &quot;Local Approver&quot; for a local event</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_markets_selected_on_Local_Event</fullName>
        <active>false</active>
        <errorConditionFormula>(
ISBLANK( Clusters__c) &amp;&amp;
ISBLANK( MCO__c ) &amp;&amp;
ISBLANK( Country__c )
)
&amp;&amp; ISPICKVAL(  Local_or_Global__c, &quot;Local&apos; 70%&quot;)</errorConditionFormula>
        <errorMessage>You need to specify a Market for Local Events</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stop_changes_on_Approved_Event</fullName>
        <active>true</active>
        <description>This validation rule will stop a user from changing the Local/Global type if an event has been approved as Tentative/Confirmed.</description>
        <errorConditionFormula>ISCHANGED( Local_or_Global__c ) &amp;&amp;
   ( 
     ISPICKVAL(Activity_Status__c, &quot;Tentative&quot;) ||
     ISPICKVAL(Activity_Status__c, &quot;Confirmed&quot;)
    )</errorConditionFormula>
        <errorMessage>Oops…
You can’t switch your event from Local to Global (or vice versa). Please get in contact with the Planning team via the Chatter group @Communications Planner Help.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>You_Must_Select_a_Market_Func_Cat_Exp</fullName>
        <active>false</active>
        <description>A user must select at least a:
Market, Function, Category or Expertise</description>
        <errorConditionFormula>ISBLANK( TEXT(Categories__c)) &amp;&amp;
ISBLANK( TEXT (Function__c )) &amp;&amp;
ISBLANK( TEXT(Expertise__c )) &amp;&amp;
(CONTAINS($Profile.Name, &quot;Central&quot;))</errorConditionFormula>
        <errorMessage>You haven&apos;t selected a Function, Category or Expertise</errorMessage>
    </validationRules>
