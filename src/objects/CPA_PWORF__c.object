        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}

var p = new sforce.SObject(&apos;CPA_PWORF__c&apos;);
p.id = &quot;{!CPA_PWORF__c.Id}&quot;;
p.pkl_Status__c = &quot;{!CPA_PWORF__c.pkl_Status__c }&quot;;


if(p.pkl_Status__c == &quot;Withhold&quot; || p.pkl_Status__c == &quot;Returned&quot;||p.pkl_Status__c ==&quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; || p.pkl_Status__c == &quot;Saved&quot;|| p.pkl_Status__c == &quot;Returned&quot;){
p.pkl_Status__c = &quot;Canceled&quot;;
result = sforce.connection.update([p]);
location.reload(true);
} else if(p.pkl_Status__c == &quot;Auto Accepted&quot; || p.pkl_Status__c == &quot;Manually Accepted&quot;){
