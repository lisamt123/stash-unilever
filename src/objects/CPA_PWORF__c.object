    <webLinks>
        <fullName>btn_Cancel_PWORF</fullName>
        <availability>online</availability>
        <description>Popup new vf page to get cancellation reason.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_PWORF__c&apos;); 
p.id = &quot;{!CPA_PWORF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_PWORF__c.pkl_Status__c }&quot;; 

var pwoIdList = new Array(); 
var doFlag= true; 
var errMsg =&quot;&quot;; 

soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos;&quot;;
var result = sforce.connection.query(soql);
var records1 = result.getArray(&apos;records&apos;); 
var permissionSetName = records1[0].PermissionSet.Name;

if( permissionSetName == &quot;CPA_ULPM&quot; || permissionSetName == &quot;CPA_SMT&quot;){ 

	if(p.pkl_Status__c != &quot;Cancelled&quot;){ 

	var result = sforce.connection.query(&quot;select id,txta_Reason_for_Cancellation__c,pkl_Status__c,PWORF_ID__c from CPA_project_work_order__c where PWORF_ID__c =&apos;&quot;+p.id+&quot;&apos;&quot;); 
	var pwoRecords = result.getArray(&quot;records&quot;); 

		for(i=0;i&lt; pwoRecords.length;i++){ 
			if(pwoRecords[i].pkl_Status__c != &apos;Delivered&apos; &amp;&amp; pwoRecords[i].pkl_Status__c != &apos;Closed&apos; &amp;&amp; pwoRecords[i].pkl_Status__c != &apos;Terminated&apos;) 
				pwoIdList.push(pwoRecords[i].Id); 
			else{ 
				doFlag= false; 
			break; 
			} 
		}//for 


		if(doFlag == true &amp;&amp; pwoIdList.length &gt;0 ){ 
			for (i=0; i &lt; pwoIdList.length; i++) { 
				result = sforce.connection.query(&quot;select id,ltxta_Reason_for_Cancellation__c,pkl_Status__c from CPA_CR__c where CPA_Project_Work_Order__c = &apos;&quot;+pwoIdList[i] +&quot;&apos; AND (pkl_Status__c = &apos;Delivered&apos; )&quot;); 
				var crRecords = result.getArray(&quot;records&quot;); 
			//	alert(crRecords);
				if(crRecords.length &gt;0){ 
					doFlag= false; break; 
				}	
			}//for 
		} //if(doFlag == true &amp;&amp; pwoIdList.length &gt;0 )	

	// Status check Starts here 
		if(doFlag == true ){ 
			if(permissionSetName == &quot;CPA_ULPM&quot; &amp;&amp; (p.pkl_Status__c != &quot;Accepted&quot; &amp;&amp; p.pkl_Status__c != &quot;Answered&quot; &amp;&amp; p.pkl_Status__c != &quot;Auto Accepted&quot; &amp;&amp; p.pkl_Status__c != &quot;Sent For Signature&quot; &amp;&amp; p.pkl_Status__c != &quot;Signed&quot;)){ 
				// redirect to cancel 
				window.open(&apos;/apex/CPA_PWORF_Cancel_Reason?id=&apos;+&quot;{!CPA_PWORF__c.Id}&quot;,&quot;_self&quot;); 
			}else if( permissionSetName == &quot;CPA_SMT&quot;){ 
				// redirect to cancel 
				window.open(&apos;/apex/CPA_PWORF_Cancel_Reason?id=&apos;+&quot;{!CPA_PWORF__c.Id}&quot;,&quot;_self&quot;); 
			}else{ 
				alert(&apos;PWORF is already Accepted/Answered&apos;); 
			} 

		}else{ 
			alert(&apos;Can not Cancel PWORF as one of the PWO/CR are Delivered/Closed/Terminated&apos;); 
		} 

	// Status check Ends here 
	}
	else{alert(&quot;Can not Cancel PWO as it is already Cancelled&quot;); 
	} 

}else{ 
alert(&quot;Insufficient Privilege&quot;); 
}</url>
