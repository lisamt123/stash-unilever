    <webLinks>
        <fullName>Submit_For_VDM</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}

var p = new sforce.SObject(&apos;CPA_PWORF__c&apos;); 
p.id = &quot;{!CPA_PWORF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_PWORF__c.pkl_Status__c }&quot;; 
p.isValidated__c = &quot;{!CPA_PWORF__c.isValidated__c }&quot;; 

//if(p.isValidated__c == &quot;1&quot;){ 
soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos; and PermissionSet.Name in (&apos;CPA_SMT&apos;,&apos;CPA_ADM&apos;,&apos;CPA_ULPM&apos;)&quot;;
var result = sforce.connection.query(soql);
var records1 = result.getArray(&apos;records&apos;);
var permissionSetName = records1[0].PermissionSet.Name;
//alert(permissionSetName);

if( permissionSetName == &quot;CPA_SMT&quot; || permissionSetName == &quot;CPA_ULPM&quot;){ 

	if(p.pkl_Status__c ==&quot;Cancelled&quot;){ 
		alert(&quot;PWORF is already Cancelled, you cant submit for Approval&quot;); 
	}
	else{
		
		var isValidateFlag = sforce.apex.execute(&quot;CPA_PWORFInformationController&quot;,&quot;checkIsValidated&quot;,{objId:p.id});

		if(isValidateFlag == &apos;true&apos;){ 

			if(p.pkl_Status__c == &quot;Withhold&quot; || p.pkl_Status__c == &quot;Returned&quot;){ 
				p.pkl_Status__c = &quot;Resubmitted&quot;; 
				result = sforce.connection.update([p]); 
		//alert(result);
				location.reload(true); 

			} 
			else if(p.pkl_Status__c ==&quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; ){ 
				alert(&quot;PWORF has already been submitted&quot;); 
			} 
			else if(p.pkl_Status__c ==&quot;Answered&quot; ){ 
				alert(&quot;PWORF is already Answered, you cant submit for Approval&quot;); 
			} 
			else if(p.pkl_Status__c ==&quot;Accepted&quot; || p.pkl_Status__c ==&quot;Auto Accepted&quot;){ 
				alert(&quot;PWORF has already been accepted by Vendor and you cannot resubmit it&quot;); 
			}
			else{ 
			p.pkl_Status__c = &quot;Submitted&quot;; 
			result = sforce.connection.update([p]); 
			//alert(result);
			location.reload(true); 
			} 

		} 
		else { 
			alert(&quot; PWORF cannot be submitted since mandatory data is not present.&quot;); 
		}
	}
}else{
	alert(&quot;You are not authorized to perform this action&quot;);
	
}</url>
    </webLinks>
    <webLinks>
        <fullName>bnt_Withhold</fullName>
        <availability>online</availability>
        <description>This button will work for SMT only.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Withhold</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_PWORF__c&apos;); 
p.id = &quot;{!CPA_PWORF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_PWORF__c.pkl_Status__c }&quot;; 

soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos; and PermissionSet.Name in (&apos;CPA_SMT&apos;,&apos;CPA_ADM&apos;,&apos;CPA_ULPM&apos;)&quot;; 
var result = sforce.connection.query(soql); 
var records1 = result.getArray(&apos;records&apos;); 
var permissionSetName = records1[0].PermissionSet.Name; 


sql = &quot;select id,pkl_Status__c from CPA_LOI__c where pkl_Status__c !=&apos;Saved&apos; AND  pkl_Status__c !=&apos;Cancelled&apos; AND lkp_PWORF__c =&apos;&quot;+ p.id+&quot;&apos;&quot;;
var result = sforce.connection.query(sql); 
var loiRec = result.getArray(&apos;records&apos;); 



if( permissionSetName == &quot;CPA_SMT&quot;){ 
	if(p.pkl_Status__c == &quot;Saved&quot;){ 
	alert(&quot;Record should first be submitted before withholding it.&quot;); 
	} 
	else if(loiRec != null &amp;&amp; loiRec.length &gt; 0){
		alert(&quot;PWORF cannot be Withheld as it has LOI in Process.&quot;); 
	}
	else if(p.pkl_Status__c != &quot;Answered&quot; &amp;&amp; p.pkl_Status__c != &quot;Cancelled&quot; &amp;&amp; p.pkl_Status__c != &quot;Withhold&quot;){ 
	window.open(&apos;/apex/CPA_PWORF_Reason_for_Withhold?id=&apos;+&quot;{!CPA_PWORF__c.Id}&quot;,&quot;_self&quot;);	
	}else{ 
	alert(&quot;PWORF cant be Withhold as it is already Answered/Cancelled/Withhold.&quot;); 
	} 
} 
else if(permissionSetName == &quot;CPA_ULPM&quot;){ 
	if(loiRec != null &amp;&amp; loiRec.length &gt; 0){
		alert(&quot;PWORF cannot be Withheld as it has LOI in Process.&quot;); 
	}
	else if(p.pkl_Status__c == &quot;Saved&quot;){ 
	alert(&quot;PWORF cannot be withheld since it is yet to be submitted.&quot;); 
	} /*
	else if(p.pkl_Status__c == &quot;Returned&quot;){ 
	//window.open(&apos;/apex/CPA_PWORF_Reason_for_Withhold?id=&apos;+&quot;{!CPA_PWORF__c.Id}&quot;,&quot;_self&quot;); 
	alert(&quot;PWORF has been submitted to the Vendor for acceptance. Please recall the record to withhold.&quot;); 
	} */
	else if(p.pkl_Status__c == &quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; || p.pkl_Status__c == &quot;Returned&quot;){ 
	//alert(&quot;PWORF has been submitted to the Vendor for acceptance. Please recall the record to withhold.&quot;); 
	window.open(&apos;/apex/CPA_PWORF_Reason_for_Withhold?id=&apos;+&quot;{!CPA_PWORF__c.Id}&quot;,&quot;_self&quot;); 
	} 
	else if(p.pkl_Status__c == &quot;Answered&quot; || p.pkl_Status__c == &quot;Accepted&quot; || p.pkl_Status__c == &quot;Auto Accepted&quot;){ 
	alert(&quot;You cannot withhold the PWORF since it has been accepted by the vendor. Please request SMT.&quot;); 
	}
	else if(p.pkl_Status__c == &quot;Cancelled&quot;){ 
	alert(&quot;You cannot withhold the PWORF since it has been cancelled&quot;); 
	}
	else if(p.pkl_Status__c == &quot;Withhold&quot;){ 
	alert(&quot;You cannot withhold the PWORF since it is already withhold&quot;); 
	}
	else{ 
	alert(&quot;PWORF cannot be withheld since it has been accepted by the vendor. Please contact SMT.&quot;); 
	} 
} 
else{ 
	alert(&quot;You are not authorized to perform this action.&quot;); 
}</url>
