    <webLinks>
        <fullName>bnt_Withhold</fullName>
        <availability>online</availability>
        <description>This button will work for SMT only.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Withhold</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_PWORF__c&apos;); 
p.id = &quot;{!CPA_PWORF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_PWORF__c.pkl_Status__c }&quot;; 

soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos;&quot;;
var result = sforce.connection.query(soql);
var records1 = result.getArray(&apos;records&apos;); 
var permissionSetName = records1[0].PermissionSet.Name;

if( permissionSetName == &quot;CPA_SMT&quot;){
	if(p.pkl_Status__c != &quot;Answered&quot; &amp;&amp; p.pkl_Status__c != &quot;Cancelled&quot; &amp;&amp; p.pkl_Status__c != &quot;Withhold&quot;){
		p.pkl_Status__c = &quot;Withhold&quot;;	

		result = sforce.connection.update([p]); 
//alert(result);
		location.reload(true);
	}else{
		alert(&quot;PWORF Can not be Withhold as it is already Answered/Cancelled/Withhold&quot;); 
	}
}
else if(permissionSetName == &quot;CPA_ULPM&quot;){
		if(p.pkl_Status__c == &quot;Saved&quot; || p.pkl_Status__c == &quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; || p.pkl_Status__c == &quot;Returned&quot;){
			p.pkl_Status__c = &quot;Withhold&quot;;	
			result = sforce.connection.update([p]); 
//alert(result);
			location.reload(true);
		}else if(p.pkl_Status__c == &quot;Cancelled&quot; || p.pkl_Status__c != &quot;Answered&quot; || p.pkl_Status__c != &quot;Withhold&quot;){
			alert(&quot;PWORF Can not be Withhold as it is already Answered/Cancelled/Withhold&quot;); 
		}else{
			alert(&quot;PWORF Can not be Withhold, Please request SMT to withhold it&quot;); 
		}
}else{
	alert(&quot;Insufficient Privilege&quot;); 
}</url>
