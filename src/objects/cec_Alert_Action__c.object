    <validationRules>
        <fullName>Alert_Type_Mandatory_Validation</fullName>
        <active>true</active>
        <description>Alert_Type_Mandatory_Validation</description>
        <errorConditionFormula>TEXT(Type__c) = null</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>Please enter an Alert Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Template_Name_Validation</fullName>
        <active>true</active>
        <description>Email Template Name is required when type is &apos;Email&apos;</description>
        <errorConditionFormula>AND( ISPICKVAL(Type__c, &apos;Email&apos;) ,  Email_Template_Name__c=null )</errorConditionFormula>
        <errorDisplayField>Email_Template_Name__c</errorDisplayField>
        <errorMessage>Please enter an Email Template name as the type is &apos;Email&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Popup_Message_Validation</fullName>
        <active>true</active>
        <description>Popup Label Name cannot be left blank as  the type is &apos;Popup&apos;</description>
        <errorConditionFormula>AND(TEXT(Type__c) = &apos;Popup&apos;, Popup_Message__c =null )</errorConditionFormula>
        <errorDisplayField>Popup_Message__c</errorDisplayField>
        <errorMessage>Please enter value for Popup message as the type is &apos;Popup&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Questionnaire_URL_Validation</fullName>
        <active>true</active>
        <description>Questionaire URL cannot be left blank when the type is &apos;Questionnaire&apos;</description>
        <errorConditionFormula>AND(TEXT(Type__c) = &apos;Questionnaire&apos;, Questionnaire_URL__c= null )</errorConditionFormula>
        <errorDisplayField>Questionnaire_URL__c</errorDisplayField>
        <errorMessage>Please enter value for Questionnaire URL as the type is &apos;Questionnaire&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>To_field_validation</fullName>
        <active>true</active>
        <description>To field cannot be left blank, when the type is &apos;Email&apos;</description>
        <errorConditionFormula>AND(TEXT(Type__c) = &apos;Email&apos;, To__c =null )</errorConditionFormula>
        <errorDisplayField>To__c</errorDisplayField>
        <errorMessage>Please enter value for &apos;To&apos; field as the type is &apos;Email&apos;</errorMessage>
    </validationRules>
