        <description>Target launch month value</description>
        <externalId>false</externalId>
        <formula>CASE(MONTH( IPM_Completed_On__c ), 

1, &quot;January&quot;, 

2, &quot;February&quot;, 

3, &quot;March&quot;, 

4, &quot;April&quot;, 

5, &quot;May&quot;, 

6, &quot;June&quot;, 

7, &quot;July&quot;, 

8, &quot;August&quot;, 

9, &quot;September&quot;, 

10, &quot;October&quot;, 

11, &quot;November&quot;, 

