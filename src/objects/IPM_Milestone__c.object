        <fullName>IPM_Phase_Order__c</fullName>
        <description>To sort according to phase</description>
        <externalId>false</externalId>
        <formula>IF(text(IPM_Phase__c)=&apos;Ideas&apos;,
1, 
IF(text(IPM_Phase__c)=&apos;Feasibility&apos;, 
2, 
IF(text(IPM_Phase__c)=&apos;Capability&apos;, 
3, 
IF(text(IPM_Phase__c)=&apos;Market Ready&apos;, 
4, 
IF(text(IPM_Phase__c)=&apos;Market Deployment&apos;,
5,
IF(text(IPM_Phase__c)=&apos;Post Launch Evaluation&apos;, 
6, 
-1) 
) 
) 
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
