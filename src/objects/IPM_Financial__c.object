        <fullName>GM_Y1_bps_vs_DNDS_Global__c</fullName>
        <description>GM Y1 bps vs DNDS Global</description>
        <externalId>false</externalId>
        <formula>(
  IF(Gross_Y1_Turnover_Global__c = 0, 0, Gross_Y1_Gross_Profit_Global__c / Gross_Y1_Turnover_Global__c)
    - 
  IF((Gross_Y1_Turnover_Global__c - Incremental_Y1_Turnover_Global__c) = 0, 0, (Gross_Y1_Gross_Profit_Global__c - Incremental_Y1_Gross_Profit_Global__c)/(Gross_Y1_Turnover_Global__c - Incremental_Y1_Turnover_Global__c))
) * 10000</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>GM Y1 bps vs DNDS Global</label>
