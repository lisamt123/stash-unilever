        <fullName>IPM_CPP_Local__c</fullName>
        <description>Project Priority Local</description>
        <externalId>false</externalId>
        <formula>IF(OR( 
   Local_Project__r.IPM_Company_Card__r.IPM_Total_OpCo_Turnover__c/1000&gt;200,
   AND(Local_Project__r.IPM_Company_Card__r.IPM_Total_OpCo_Turnover__c/1000=200,
   Year_1_iTO_Local__c/1000 &gt;=10),
   AND(Local_Project__r.IPM_Company_Card__r.IPM_Total_OpCo_Turnover__c/1000=200,   
   Year_1_iTO_Local__c &gt;=          
  (Local_Project__r.IPM_Company_Card__r.IPM_Total_OpCo_Turnover__c *0.005) ) ), 
&quot;Gold&quot;,
IF(AND(
  Local_Project__r.IPM_Company_Card__r.IPM_Total_OpCo_Turnover__c/1000=200, 
  Year_1_iTO_Local__c &gt;= (OpCo_Local__c *0.001), Year_1_iTO_Local__c &lt; (OpCo_Local__c *0.005) ), 
&quot;Silver&quot;,
IF(AND(
  Local_Project__r.IPM_Company_Card__r.IPM_Total_OpCo_Turnover__c/1000=200,
  Year_1_iTO_Local__c &lt; (OpCo_Local__c *0.001) ), 
&quot;Bronze&quot;,
IF(AND(
  Local_Project__r.IPM_Company_Card__r.IPM_Total_OpCo_Turnover__c/1000&lt;200,
  Year_1_iTO_Local__c &gt;= (OpCo_Local__c *0.001) ),
  &quot;Silver&quot;,
IF(AND(
  Local_Project__r.IPM_Company_Card__r.IPM_Total_OpCo_Turnover__c/1000&lt;200,
  Year_1_iTO_Local__c &lt; (OpCo_Local__c *0.001) ),
  &quot;Bronze&quot;,
&quot;NA&quot;
)
) 
)
)
)</formula>
