    <validationRules>
        <fullName>Approval_required</fullName>
        <active>false</active>
        <description>Force approval on next edit</description>
        <errorConditionFormula>AND(ISPICKVAL(Issue_Status__c, &quot;New&quot;), NOT(ISNEW())) &amp;&amp;  Submitted__c = TRUE</errorConditionFormula>
        <errorDisplayField>Issue_Status__c</errorDisplayField>
        <errorMessage>The Issue Stage is currently &apos;New&apos;. You must change the Issue Stage to &apos;Reviewed&apos; before any further updates can be made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Issue_type_can_be_either_Global_or_RandD</fullName>
        <active>true</active>
        <description>Issue type can only be either Global or R&amp;D.</description>
        <errorConditionFormula>AND( Global_Issue__c , R_D_Issue__c )</errorConditionFormula>
        <errorMessage>You can only select one of the check boxes. Global issue or R&amp;D Issue</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Submitted_To_Unilever_Person</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RIGHT(Report_Issue_To_Email__c, 13) &lt;&gt; &quot;@unilever.com&quot;,  Report_Issue_To_Email__c  &lt;&gt; &quot;&quot;)</errorConditionFormula>
        <errorMessage>You can only submit to a Unilever colleague with a &apos;@Unilever.com&apos; email address</errorMessage>
    </validationRules>
