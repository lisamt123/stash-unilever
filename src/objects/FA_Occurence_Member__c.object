    <validationRules>
        <fullName>FA_Occurrence_Member_Creation_Check</fullName>
        <active>true</active>
        <description>Needs to create Occurrence Member only for Course Occurrence that are active</description>
        <errorConditionFormula>IF (NOT( ISPICKVAL(FA_Course_Occurence__r.FA_Status__c, &apos;Active&apos;)) ,ISNEW() ,False)</errorConditionFormula>
        <errorMessage>Status of Course Occurrence is not Active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_Participant_Check</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( FA_Course_Occurence__r.FA_End_Date_Time__c   &lt;  NOW()  ,ISNEW() ,False)</errorConditionFormula>
        <errorMessage>Cannot Add Participant to a date post the end date of the course occurrence</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_RistrictOwnerAsMember</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; IF ( $User.Id== FA_Participant_Instructor__c,true,false)</errorConditionFormula>
        <errorDisplayField>FA_Participant_Instructor__c</errorDisplayField>
        <errorMessage>Owner cannot be added as the Occurence Member
Owner will be added automatically 
Please choose another Participant/Instructor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_cannotChangeStatusIfCancelled</fullName>
        <active>true</active>
        <description>Status cannot be changed if the Status is already Cancelled</description>
        <errorConditionFormula>ISCHANGED(FA_Participation_Status__c) &amp;&amp;   TEXT( PRIORVALUE( FA_Participation_Status__c ))==&apos;Cancelled&apos;</errorConditionFormula>
        <errorDisplayField>FA_Participation_Status__c</errorDisplayField>
        <errorMessage>Cancelled Status cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_mustEnterParticipantInstructor</fullName>
        <active>true</active>
        <description>Participant/Instructor must be entered</description>
        <errorConditionFormula>ISBLANK(FA_Participant_Instructor__c)</errorConditionFormula>
        <errorDisplayField>FA_Participant_Instructor__c</errorDisplayField>
        <errorMessage>Please select a Participant/Instructor</errorMessage>
    </validationRules>
