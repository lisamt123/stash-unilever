    <validationRules>
        <fullName>DO_detail_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL(pkl_SAM_team_Status__c , &apos;Closed&apos;), rol_DO_detail_validation_count__c &gt;0 )</errorConditionFormula>
        <errorMessage>Please provide the DO details in the respective Quotation details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Master_Clarity_Alphanumeric_Data</fullName>
        <active>true</active>
        <description>It accepts any thing between A-Z, a-z, 0-9 and space (no other special characters allowed).</description>
        <errorConditionFormula>AND(( NOT( ISBLANK(txt_Master_Clarity_Id__c ) ) ),(NOT( REGEX( txt_Master_Clarity_Id__c , &quot;[a-zA-Z0-9 ]+&quot;))))</errorConditionFormula>
        <errorDisplayField>txt_Master_Clarity_Id__c</errorDisplayField>
        <errorMessage>Only Alphanumeric data allowed. For ex. Characters between A-Z, a-z, 0-9 and space.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Action_Comments_Validation</fullName>
        <active>true</active>
        <description>Provide comment on rejection and close actions</description>
        <errorConditionFormula>AND( 
ISBLANK(ltxt_Comments__c), 
OR(
TEXT(pkl_SAM_Finance_Status__c)=$Label.SAM_Charging_Rejected ,
TEXT(pkl_SAM_Requester_Status__c)=$Label.SAM_Answer_Rejected ,
TEXT(pkl_SAM_Requester_Status__c)=$Label.SAM_Quote_Rejected ,
TEXT(pkl_SAM_team_Status__c)=$Label.SAM_Closed, 
TEXT(pkl_SAM_team_Status__c)=$Label.SAM_Answer_Rejected ,
TEXT(pkl_SAM_team_Status__c)=$Label.SAM_Quote_Rejected ,
TEXT(pkl_SAM_team_Status__c) =  $Label.SAM_Details_Pending ,
TEXT(pkl_SAM_team_Status__c) =  $Label.SAM_Awaiting_Information ),	
OR(
txt_Request_Type__c = &quot;SAM_RW_Project_Request&quot;,
txt_Request_Type__c = &quot;SAM_RW_CrossCharge_Request&quot;,
txt_Request_Type__c = &quot;SAM_RW_Non_Project_Request&quot;,
txt_Request_Type__c = &quot;SAM_RW_Query_Request&quot;)
)</errorConditionFormula>
        <errorDisplayField>ltxt_Comments__c</errorDisplayField>
        <errorMessage>Action Comment Mandatory for selected action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Any_Infrastructural_Req_Validation</fullName>
        <active>false</active>
        <description>Any Infrastructural Requirement field validation.</description>
        <errorConditionFormula>AND( ISBLANK( TEXT( pkl_Any_infrastructural_requirements__c ) ) ,ISPICKVAL(pkl_Status__c , &quot;Open&quot;) , RecordType.DeveloperName =&quot;SAM_RW_Project_Request&quot; )</errorConditionFormula>
        <errorDisplayField>pkl_Any_infrastructural_requirements__c</errorDisplayField>
        <errorMessage>Any Infrastructural Requirement is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_AwaitingCCDetailsPendEmp_Validation</fullName>
        <active>true</active>
        <description>cannot select &quot;Awaiting CC details (Pending with ULEMP)&quot; if none of the quote LI is &quot;X&quot;</description>
        <errorConditionFormula>AND( 
TEXT(pkl_SAM_team_Status__c) =  $Label.SAM_Awaiting_CC_details_Pending_with_ULEMP, 
rol_Product_detail_count_having_cross_ch__c =0, 
txt_Request_Type__c = &quot;SAM_RW_Project_Request&quot;)</errorConditionFormula>
        <errorDisplayField>pkl_SAM_team_Status__c</errorDisplayField>
        <errorMessage>Cannot select &quot;Awaiting CC details (Pending with ULEMP)&quot; if none of the quote Line Item is &quot;X&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Awaiting_Charge_Validation</fullName>
        <active>true</active>
        <description>cannot select &quot;Awaiting charge&quot; if none of the quote LI is &quot;X&quot;</description>
        <errorConditionFormula>AND( 
OR(TEXT(pkl_SAM_team_Status__c) = $Label.SAM_Awaiting_Charge, TEXT(pkl_SAM_team_Status__c) =  $Label.SAM_Awaiting_CC_details_Pending_with_ULEMP ),
rol_Product_detail_count_having_cross_ch__c =0, 
txt_Request_Type__c = &quot;SAM_RW_Project_Request&quot;)</errorConditionFormula>
        <errorDisplayField>pkl_SAM_team_Status__c</errorDisplayField>
        <errorMessage>You can not select &quot;Awaiting charge&quot; or &quot;Awaiting CC details (Pending with ULEMP)&quot; if none of the quote Line Item is &quot;X&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Budget_Owner_Email_Validation</fullName>
        <active>true</active>
        <description>Budget Owner Email Validation.</description>
        <errorConditionFormula>AND(
  ISBLANK(   eml_Budget_Owner_Email__c  ) ,
 OR( 
   AND(rol_Product_detail_count_having_cross_ch__c &gt; 0 ,
     (TEXT(pkl_SAM_Requester_Status__c)= $Label.SAM_Quote_Approved || TEXT(pkl_SAM_team_Status__c) =$Label.SAM_Quote_Approved 
