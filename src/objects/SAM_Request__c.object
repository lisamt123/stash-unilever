OR(TEXT(pkl_SAM_team_Status__c)=$Label.SAM_Closed ,TEXT(pkl_SAM_team_Status__c)=  $Label.SAM_Query_Answered ), 
txt_Request_Type__c =&quot;SAM_RW_Query_Request&quot; )</errorConditionFormula>
        <errorDisplayField>ltxt_Query_SAM_Response__c</errorDisplayField>
        <errorMessage>SAM Response is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Query_StartDate_Mandatory_Validation</fullName>
        <active>true</active>
        <description>Start Date Mandatory for all states after Saved</description>
        <errorConditionFormula>AND( 
ISBLANK( date_Query_Start_Date__c), 
txt_Request_Type__c =&quot;SAM_RW_Query_Request&quot; ,
CONTAINS(ltxt_User_Public_Groups__c ,&apos;SAM_Team&apos;),
Text( pkl_Status__c )&lt;&gt; $Label.SAM_SaveStatus
)</errorConditionFormula>
        <errorDisplayField>date_Query_Start_Date__c</errorDisplayField>
        <errorMessage>Start Date is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Query_StartDt_LTE_EndDt_Validation</fullName>
        <active>true</active>
        <description>Query Start Date should be less than equal to End date Validation</description>
        <errorConditionFormula>AND( 
(date_Query_Start_Date__c)&gt;(date_Query_End_Date__c),
txt_Request_Type__c =&quot;SAM_RW_Query_Request&quot; )</errorConditionFormula>
        <errorDisplayField>date_Query_Start_Date__c</errorDisplayField>
        <errorMessage>Start Date cannot be greater than the End date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Query_Start_Date_Modify_Validation</fullName>
        <active>true</active>
        <description>Start Date cannot be modified by Non-creator of the request</description>
        <errorConditionFormula>AND( 
ISCHANGED(date_Query_Start_Date__c ), 
 CreatedById  &lt;&gt;  OwnerId,
txt_Request_Type__c =&quot;SAM_RW_Query_Request&quot; 
)</errorConditionFormula>
        <errorDisplayField>date_Query_Start_Date__c</errorDisplayField>
        <errorMessage>Cannot modify</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Quotation_Count_Validation</fullName>
        <active>true</active>
        <description>Cannot do Draft Quote Provided or Quote Provided if Quotation Count is zero</description>
        <errorConditionFormula>AND( 
Quotation_Record_Count__c = 0,  
OR(
TEXT(pkl_SAM_team_Status__c)= $Label.SAM_Draft_Quote_Provided,
TEXT(pkl_SAM_team_Status__c)= $Label.SAM_Quote_Provided 
)
)</errorConditionFormula>
        <errorDisplayField>pkl_SAM_team_Status__c</errorDisplayField>
        <errorMessage>No Quotation line item in this request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Quotation_SW_Purchase_No_Validation</fullName>
        <active>true</active>
        <description>There can be no quotation line item if  Software Purchase Required is NOT Yes</description>
        <errorConditionFormula>AND(
OR(ISPICKVAL(pkl_Software_Purchase_Req__c,&quot;No&quot;),
ISPICKVAL(pkl_Software_Purchase_Req__c,&quot;&quot;)),
Quotation_Record_Count__c  &gt; 0
)</errorConditionFormula>
        <errorMessage>No Quotation Line items can be created when Software Purchase Required is No or None</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Quotation_SW_Purchase_Yes_Validation</fullName>
        <active>true</active>
        <description>Quotation should be added when Software Purchase Required is Yes</description>
        <errorConditionFormula>AND(
ISPICKVAL(pkl_Software_Purchase_Req__c,&quot;Yes&quot;),
Quotation_Record_Count__c  = 0,
ISCHANGED(pkl_SAM_team_Status__c),
Text(pkl_SAM_team_Status__c) &lt;&gt; $Label.SAM_Awaiting_Information ,
Text(pkl_SAM_team_Status__c) &lt;&gt; $Label.SAM_Information_Provided  ,
Text(pkl_SAM_team_Status__c) &lt;&gt; &apos;In Progress&apos;,
NOT(ISPICKVAL(pkl_SAM_team_Status__c, &quot;&quot;)) 
)</errorConditionFormula>
        <errorMessage>At least one quotation line item should be added when Software Purchase Required is Yes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Quote_Provided_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Quotation_Record_Count__c &lt;&gt; rus_Completed_Quotes__c, TEXT(pkl_SAM_team_Status__c) =&quot;Quote Provided&quot; )</errorConditionFormula>
        <errorMessage>Status of all Quote line items should be complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Reason_For_Close_Reverse_Validation</fullName>
        <active>true</active>
        <description>Reason for Close should be empty when Next Action(SAM) is NOT closed</description>
        <errorConditionFormula>AND(
NOT(ISPICKVAL( pkl_Reason_for_closure__c ,&quot;&quot;)),
OR( txt_Request_Type__c = &apos;SAM_RW_Non_Project_Request&apos;,txt_Request_Type__c = &apos;SAM_RW_Project_Request&apos; ),
Text( pkl_SAM_team_Status__c)&lt;&gt; $Label.SAM_Closed,
Text( pkl_Status__c)&lt;&gt; $Label.SAM_Closed
)</errorConditionFormula>
        <errorDisplayField>pkl_Reason_for_closure__c</errorDisplayField>
        <errorMessage>Reason For Closure NOT applicable when Next Action is not Closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Reason_For_Close_Validation</fullName>
        <active>true</active>
        <description>Reason for Close is mandatory before close</description>
        <errorConditionFormula>AND(
  Text( pkl_SAM_team_Status__c)= $Label.SAM_Closed,
  OR( txt_Request_Type__c = &apos;SAM_RW_Non_Project_Request&apos;,txt_Request_Type__c = &apos;SAM_RW_Project_Request&apos; ),
  ISPICKVAL( pkl_Reason_for_closure__c ,&quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>pkl_Reason_for_closure__c</errorDisplayField>
        <errorMessage>Reason For Closure is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Run_cost_detail_validation_on_close</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(  pkl_SAM_team_Status__c  , &apos;Closed&apos;) ,rol_Run_Cost_Detail_Validation_count__c &gt;0)</errorConditionFormula>
        <errorMessage>Please provide the run cost details in the respective Quotation details.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_SAAS_Validation</fullName>
        <active>true</active>
        <description>SAAS field validation</description>
        <errorConditionFormula>AND( 
ISBLANK( TEXT(pkl_SAAS_Yes_No__c)),
OR(TEXT(pkl_SAM_Requester_Status__c)= $Label.SAM_Information_Provided , TEXT(pkl_SAM_team_Status__c)= $Label.SAM_Information_Provided),
NOT(ISNULL(txt_Request_Type__c   )),
txt_Request_Type__c   = &quot;SAM_RW_Project_Request&quot;
)</errorConditionFormula>
        <errorDisplayField>pkl_SAAS_Yes_No__c</errorDisplayField>
        <errorMessage>SAAS is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_SA_Clarity_Id_Validation</fullName>
        <active>true</active>
        <description>SA Clarity Id Validation.</description>
        <errorConditionFormula>AND( 
ISBLANK(txt_SA_Clarity_Id__c),
OR(TEXT(pkl_SAM_Requester_Status__c)= $Label.SAM_Information_Provided , TEXT(pkl_SAM_team_Status__c)= $Label.SAM_Information_Provided),
NOT(ISNULL(txt_Request_Type__c  
)),txt_Request_Type__c  
 = &quot;SAM_RW_Project_Request&quot;
)</errorConditionFormula>
        <errorDisplayField>txt_SA_Clarity_Id__c</errorDisplayField>
        <errorMessage>SA Clarity Id is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Scope_of_the_Project_Validation</fullName>
        <active>true</active>
        <description>Scope of the Project  field Validation</description>
        <errorConditionFormula>AND( 
ISBLANK(txt_Scope_of_the_Project__c),
NOT(ISNULL(txt_Request_Type__c )),
 OR(TEXT(pkl_SAM_Requester_Status__c)= $Label.SAM_Information_Provided,TEXT(pkl_SAM_team_Status__c)= $Label.SAM_Information_Provided),
   OR(   txt_Request_Type__c   =  &quot;SAM_RW_Project_Request&quot; , txt_Request_Type__c=&quot;SAM_RW_Non_Project_Request&quot;))</errorConditionFormula>
        <errorDisplayField>txt_Scope_of_the_Project__c</errorDisplayField>
        <errorMessage>Scope of Project  is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Software_Purchase_Req_Validation</fullName>
        <active>true</active>
        <description>Cannot proceed to next stage if  Software Purchase Required is No</description>
        <errorConditionFormula>AND(
ISPICKVAL(pkl_Software_Purchase_Req__c,&quot;No&quot;),  
OR(
TEXT(pkl_SAM_team_Status__c) =  $Label.SAM_Draft_Quote_Provided ,
TEXT(pkl_SAM_team_Status__c) = $Label.SAM_Quote_Provided 
)
)</errorConditionFormula>
        <errorDisplayField>pkl_SAM_team_Status__c</errorDisplayField>
        <errorMessage>Cannot proceed for Quotation if Software Purchase Required is No</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_cost_code_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  ISBLANK( txt_CrossCharge_Cost_Code__c  ) ,
 OR( 
   AND(rol_Product_detail_count_having_cross_ch__c &gt; 0 ,
     (TEXT(pkl_SAM_Requester_Status__c)= $Label.SAM_Quote_Approved || TEXT(pkl_SAM_team_Status__c) =$Label.SAM_Quote_Approved 
     ||TEXT    (pkl_SAM_Requester_Status__c)=  $Label.SAM_CC_Details_Provided || TEXT(pkl_SAM_team_Status__c)=$Label.SAM_Awaiting_Charge
     ),txt_Request_Type__c =&quot;SAM_RW_Project_Request&quot; ), 
     AND(TEXT(pkl_SAM_team_Status__c)=$Label.SAM_Awaiting_Charge ,txt_Request_Type__c =&quot;SAM_RW_CrossCharge_Request&quot;) 
    ) 
  )</errorConditionFormula>
        <errorDisplayField>txt_CrossCharge_Cost_Code__c</errorDisplayField>
        <errorMessage>Cost Code is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SA_Clarity_Alphanumeric_Data</fullName>
        <active>true</active>
        <description>It accepts any thing between A-Z, a-z, 0-9 and space (no other special characters allowed).</description>
        <errorConditionFormula>AND(( NOT( ISBLANK(txt_SA_Clarity_Id__c) ) ),(NOT( REGEX( txt_SA_Clarity_Id__c, &quot;[a-zA-Z0-9 ]+&quot;))))</errorConditionFormula>
        <errorDisplayField>txt_SA_Clarity_Id__c</errorDisplayField>
        <errorMessage>Only Alphanumeric data allowed. For ex. Characters between A-Z, a-z, 0-9 and space.</errorMessage>
    </validationRules>
</CustomObject>
