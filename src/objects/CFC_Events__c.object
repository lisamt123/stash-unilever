    <validationRules>
        <fullName>Check_Event_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>From__c  &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>From__c</errorDisplayField>
        <errorMessage>Event Start Date Must be greater than today...</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Start_And_End_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(NOT(ISBLANK(From__c)),ISBLANK(To__c)), AND(NOT(ISBLANK(To__c)), ISBLANK(From__c)))</errorConditionFormula>
