    <fields>
        <fullName>Link_Output__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
OR((CONTAINS( Link_URL__c, &quot;http&quot;) &amp;&amp; CONTAINS(Link_URL__c, &quot;unilever&quot;)),
(CONTAINS( Link_URL__c, &quot;https&quot;) &amp;&amp; CONTAINS(Link_URL__c, &quot;unilever&quot;)),
(CONTAINS( Link_URL__c, &quot;https&quot;) &amp;&amp; CONTAINS(Link_URL__c, &quot;force&quot;)),
(CONTAINS( Link_URL__c ,&quot;www&quot;) &amp;&amp; CONTAINS(Link_URL__c,&quot;http&quot;)) ) , 
Link_URL__c, 
if(CONTAINS(Link_URL__c, &quot;sfc&quot;) , substitute(Link_URL__c,&quot;http://&quot;,&quot;&quot;),  
&quot;http://&quot; +Link_URL__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Link Output</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
