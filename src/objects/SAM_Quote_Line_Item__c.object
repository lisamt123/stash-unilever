        <errorConditionFormula>AND( ISNEW(), ISPICKVAL( md_Request__r.pkl_Software_Purchase_Req__c , &apos;Yes&apos;)  , ISPICKVAL( pkl_Charging_Type__c , &apos;DO/PO Applicable (O)&apos;)  ,ISPICKVAL(pkl_Line_Status__c , &apos;Completed&apos;))</errorConditionFormula>
