        <errorConditionFormula>AND(ISPICKVAL( pkl_Publisher__c , &apos;Other Product&apos;) ,
 ISBLANK(txt_Publishers_Details__c  ) )</errorConditionFormula>
        <errorDisplayField>txt_Publishers_Details__c</errorDisplayField>
        <errorMessage>Mandatory when Publisher is Other Product</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Publishers_Details_Reverse_Validation</fullName>
        <active>true</active>
        <description>User should not set this field when Publisher is not  &apos;Other Product&apos;</description>
        <errorConditionFormula>AND(NOT(ISPICKVAL( pkl_Publisher__c , &apos;Other Product&apos;)) ,
 NOT(ISBLANK(txt_Publishers_Details__c  )) )</errorConditionFormula>
        <errorDisplayField>txt_Publishers_Details__c</errorDisplayField>
        <errorMessage>Cannot set when Publisher is Not &apos;Other Product&apos;</errorMessage>
