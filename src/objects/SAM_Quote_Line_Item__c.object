    <validationRules>
        <fullName>Name_field</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Name),TEXT( pkl_Request_status__c )&lt;&gt; $Label.SAM_Open ,TEXT( pkl_Request_status__c )&lt;&gt; $Label.SAM_In_Progress  ,TEXT( pkl_Request_status__c )&lt;&gt; $Label.SAM_Draft_Quote_Provided ,TEXT( pkl_Request_status__c )&lt;&gt; $Label.SAM_Quote_Rejected ,TEXT( pkl_Request_status__c )&lt;&gt; $Label.SAM_Information_Provided )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>This cannot be updated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_CC_Charging_Type_Validation</fullName>
        <active>true</active>
        <description>For non-project request type, don’t allow user to select cross charge (x) option for charging type</description>
        <errorConditionFormula>AND(Text(pkl_Charging_Type__c)= $Label.SAM_Cross_Charge_Applicable ,
 md_Request__r.txt_Request_Type__c  = &apos;SAM_RW_Non_Project_Request&apos;)</errorConditionFormula>
        <errorDisplayField>pkl_Charging_Type__c</errorDisplayField>
        <errorMessage>Cross Charge Applicable (X) charging type not applicable for Non-Project requests</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Dis_Percentage_shd_be_positive</fullName>
        <active>true</active>
        <description>Discount percentage should be +ve</description>
        <errorConditionFormula>num_Discount_Percentage__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>num_Discount_Percentage__c</errorDisplayField>
        <errorMessage>Enter only positive value here</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Discount_Percentage</fullName>
        <active>true</active>
        <description>Mandatory when discount percentage is applicable.</description>
        <errorConditionFormula>IF(AND(ISPICKVAL( pkl_Discount_Applicable__c , &apos;Yes&apos;), ISBLANK(  num_Discount_Percentage__c )),true,false )</errorConditionFormula>
        <errorDisplayField>num_Discount_Percentage__c</errorDisplayField>
        <errorMessage>Mandatory when discount percentage is applicable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Discount_Percentage_reverse</fullName>
        <active>true</active>
        <description>Value require only when discount percentage is applicable.</description>
        <errorConditionFormula>IF(AND(ISPICKVAL( pkl_Discount_Applicable__c , &apos;No&apos;),Not(ISBLANK(  num_Discount_Percentage__c ) )),true,false )</errorConditionFormula>
        <errorDisplayField>num_Discount_Percentage__c</errorDisplayField>
        <errorMessage>Value require only when discount percentage is applicable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_End_date_greater_than_Start_Date</fullName>
        <active>true</active>
        <description>Start Date should be less than End Date</description>
        <errorConditionFormula>dt_Contract_Start_Date__c &gt; dt_Contract_End_Date__c</errorConditionFormula>
        <errorMessage>Contract Start Date should be less than Contract End Date OR Contract End Date should be greater than Contract Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_List_Price_shd_be_positive</fullName>
        <active>true</active>
        <errorConditionFormula>num_List_Price__c &lt;=0</errorConditionFormula>
        <errorDisplayField>num_List_Price__c</errorDisplayField>
        <errorMessage>Enter only positive value here</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Maintainence_Cost</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISPICKVAL(Maintainence_Applicable__c , &apos;Yes&apos;), ISPICKVAL( pkl_Maintainence_Type__c , &apos;Amount&apos;), ISBLANK(   num_Maintainence_Cost__c )),true,false )</errorConditionFormula>
        <errorDisplayField>num_Maintainence_Cost__c</errorDisplayField>
        <errorMessage>Mandatory when maintenance amount is applicable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Maintainence_Cost_reverse</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(OR(AND(ISPICKVAL(Maintainence_Applicable__c , &apos;Yes&apos;), 
ISPICKVAL( pkl_Maintainence_Type__c , &apos;Percentage&apos;)),ISPICKVAL(Maintainence_Applicable__c , &apos;No&apos;)), 
NOT(ISBLANK(num_Maintainence_Cost__c) )),
true,false )</errorConditionFormula>
        <errorDisplayField>num_Maintainence_Cost__c</errorDisplayField>
        <errorMessage>Value require only when maintenance amount is applicable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Maintainence_Percentage</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISPICKVAL(Maintainence_Applicable__c , &apos;Yes&apos;), ISPICKVAL( pkl_Maintainence_Type__c , &apos;Percentage&apos;), ISBLANK(  num_Maintainence_Percentage__c )),true,false )</errorConditionFormula>
        <errorDisplayField>num_Maintainence_Percentage__c</errorDisplayField>
        <errorMessage>Mandatory when maintenance percentage is applicable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Maintainence_Percentage_reverse</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(OR(AND(ISPICKVAL(Maintainence_Applicable__c , &apos;Yes&apos;),
ISPICKVAL( pkl_Maintainence_Type__c , &apos;Amount&apos;)),ISPICKVAL(Maintainence_Applicable__c , &apos;No&apos;)), 
Not(ISBLANK( num_Maintainence_Percentage__c) )),
true,false )</errorConditionFormula>
        <errorDisplayField>num_Maintainence_Percentage__c</errorDisplayField>
        <errorMessage>Value require only when maintenance percentage is applicable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Maintainence_Type</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISPICKVAL( Maintainence_Applicable__c , &apos;Yes&apos;),ISPICKVAL(pkl_Maintainence_Type__c,&apos;&apos; )),true,false)</errorConditionFormula>
        <errorDisplayField>pkl_Maintainence_Type__c</errorDisplayField>
        <errorMessage>Mandatory if maintenance is applicable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Maintainence_Type_reverse</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISPICKVAL( Maintainence_Applicable__c , &apos;No&apos;), NOT( ISPICKVAL(pkl_Maintainence_Type__c,&apos;&apos; ) )),true,false)</errorConditionFormula>
        <errorDisplayField>pkl_Maintainence_Type__c</errorDisplayField>
        <errorMessage>Value require only if maintenance is applicable.</errorMessage>
    </validationRules>
