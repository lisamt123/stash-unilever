    <fields>
        <fullName>AF_Adjusted_estimate_1__c</fullName>
        <description>Calculate the value for qtr based on which qtr we are in and total value</description>
        <externalId>false</externalId>
        <formula>IF(INCLUDES(AF_OOPS_Actual__r.Quarter__c, &quot;Q4&quot;), 1,
	IF(INCLUDES(AF_OOPS_Actual__r.Quarter__c, &quot;Q3&quot;), 0.75,
		IF(INCLUDES(AF_OOPS_Actual__r.Quarter__c, &quot;Q2&quot;), 0.5,
			IF(INCLUDES(AF_OOPS_Actual__r.Quarter__c, &quot;Q1&quot;), 0.25, 0))))
* AF_Estimate__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Adjusted estimate 1</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AF_Adjusted_estimate_2__c</fullName>
        <description>Calculate value based on previous value and current value</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK( AF_Previous_Quarter_Estimate__c),null, AF_Adjusted_estimate_1__c -  AF_Previous_Quarter_Estimate__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
