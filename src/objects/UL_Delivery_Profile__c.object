    <validationRules>
        <fullName>UL_Validation_for_date_duration</fullName>
        <active>false</active>
        <errorConditionFormula>UL_Instore_End_days__c &lt; UL_Shipment_End_days__c</errorConditionFormula>
        <errorMessage>Shipment End should be less than or equal to In-Store Duration</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_Validation_for_date_ranges</fullName>
        <active>true</active>
        <errorConditionFormula>UL_Valid_From__c &gt; UL_Valid_Thru__c</errorConditionFormula>
        <errorMessage>Valid From Thru Date cannot be less than Valid From Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ul_Total_Shipment_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(UL_Total_of_Phasing__c &lt;1, true, false)</errorConditionFormula>
        <errorMessage>Shipment Phasing cannot be less than 100%</errorMessage>
    </validationRules>
