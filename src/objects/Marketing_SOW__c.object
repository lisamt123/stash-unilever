    <validationRules>
        <fullName>Lock_Record_if_Sync_in_Progress</fullName>
        <active>true</active>
        <description>Lock the SWOP record if a roll over sync is in progress</description>
        <errorConditionFormula>IF(
NOT(ISCHANGED(Sync_Status__c)) &amp;&amp; 
NOT(ISCHANGED(Sync_Status_PFY__c)) &amp;&amp; 
$Profile.Name &lt;&gt; &apos;System Administrator&apos; 

&amp;&amp; (TEXT( Sync_Status__c ) == &apos;Sync In Progress&apos; || Sync_Status_PFY__c == &apos;Sync In Progress&apos; || TEXT( Sync_Status__c ) == &apos;Sync Requested&apos; || Sync_Status_PFY__c == &apos;Sync Requested&apos;),true,false)</errorConditionFormula>
        <errorDisplayField>Sync_Status__c</errorDisplayField>
        <errorMessage>Sync in Progress. Record Locked. SOW Locked.</errorMessage>
    </validationRules>
