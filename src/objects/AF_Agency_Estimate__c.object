        <fullName>AF_Adjusted_estimate_1__c</fullName>
        <description>Calculate the value for qtr based on which qtr we are in and total value</description>
        <externalId>false</externalId>
        <formula>IF(INCLUDES( AF_Brand_Estimate__r.AF_Basefee_Estimate_Initial_Quarter__c , &quot;Q4&quot;), 1, 
IF(INCLUDES(AF_Brand_Estimate__r.AF_Basefee_Estimate_Initial_Quarter__c, &quot;Q3&quot;), 0.75, 
IF(INCLUDES(AF_Brand_Estimate__r.AF_Basefee_Estimate_Initial_Quarter__c, &quot;Q2&quot;), 0.5, 
IF(INCLUDES(AF_Brand_Estimate__r.AF_Basefee_Estimate_Initial_Quarter__c, &quot;Q1&quot;), 0.25, 0)))) 
* AF_Total__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Adjusted estimate 1</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
