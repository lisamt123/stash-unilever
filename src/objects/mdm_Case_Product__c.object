        <externalId>false</externalId>
        <formula>IF( LOWER(Product_Status__c) = &apos;inactive&apos; , 
IF(ISBLANK(Other__c) &amp;&amp; ISBLANK(Last_Sold_Manufactured__c) , Name &amp; &quot; *DL&quot;, 
IF(ISBLANK(Other__c), Name &amp; &quot; *DL&quot; &amp; &quot; LS:&quot; &amp; Last_Sold_Manufactured__c , 
IF(NOT(ISBLANK(Other__c))&amp;&amp; ISBLANK(Last_Sold_Manufactured__c),Name &amp; &quot; (&quot; &amp; Other__c &amp; &quot;)&quot; &amp; &quot; *DL&quot;, 
Name &amp; &quot; (&quot; &amp; Other__c &amp; &quot;)&quot; &amp; &quot; *DL&quot; &amp; &quot; LS:&quot; &amp; Last_Sold_Manufactured__c ))), 
IF(ISBLANK(Other__c), Name , Name &amp; &quot; (&quot; &amp; Other__c &amp; &quot;)&quot;))</formula>
