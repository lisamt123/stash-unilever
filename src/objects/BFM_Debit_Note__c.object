    <validationRules>
        <fullName>BFM_DN_Definitive_with_Validation_Error</fullName>
        <active>true</active>
        <errorConditionFormula>Debit_Note_Definitive__c  = true &amp;&amp;  (DN_Vendor_Code_Check_Status__c != &apos;Vendor Ok&apos; || ISBLANK(Amazon_File_Link__c) )</errorConditionFormula>
        <errorMessage>Debit Note cannot be Definitive without passing Validations</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BFM_DebitNote_Invoice_Posted_Edit_Err</fullName>
        <active>true</active>
        <errorConditionFormula>!ISNEW() &amp;&amp; UPPER(PRIORVALUE( Debit_Note_Status__c )) = &apos;INVOICE POSTED&apos; &amp;&amp; $Profile.Name != &apos;System Administrator&apos;</errorConditionFormula>
        <errorMessage>Record is not editable if status is Invoice Posted</errorMessage>
    </validationRules>
