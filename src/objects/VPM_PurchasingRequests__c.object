    <webLinks>
        <fullName>VPM_AdvancedFormFLS</fullName>
        <availability>online</availability>
        <description>Button to send the user to the Advanced form. Checks if the approval has been completed first.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Advanced Form</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)} 

var criteria1 = &quot;{!VPM_PurchasingRequests__c.VPM_Status__c}&quot;; 

if(criteria1==&quot;FLS Review&quot;) 
{alert(&quot;Please Approve / Reject the Request before completing the Advanced Form&quot;); 
} 
else { 
navigateToUrl(&apos;flow/VPM_PurchasingRequestAdvancedForm?VPM_PurchasingRequestID={!VPM_PurchasingRequests__c.Id}&apos;); 
}</url>
    </webLinks>
    <webLinks>
        <fullName>VPM_BusinessReqSubmit</fullName>
        <availability>online</availability>
        <description>Button to trigger the record updates via Process Builder necessary when the Business Requestor has submitted the form</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/34.0/connection.js&apos;)} 
{ 
var pur = new sforce.SObject(&quot;VPM_PurchasingRequests__c&quot;); 
pur.id=&quot;{!VPM_PurchasingRequests__c.Id}&quot;; 
var currentstatus=&quot;{!VPM_PurchasingRequests__c.VPM_Status__c}&quot;; 
if(currentstatus.indexOf(&apos;Draft&apos;) &gt;= 0 ) 
{ 
pur.VPM_BusinessRequestorSubmitted__c=&apos;Yes&apos;; 
result = sforce.connection.update([pur]); 
window.location.reload(); 
} 
else if (currentstatus.indexOf(&apos;FLS Review&apos;)&gt;=0)
{

var result = sforce.connection.query(&quot;Select  QueueId, Queue.Name From QueueSobject where Queue.Name = &apos;FLS Queue&apos; Limit 1&quot;); 
if (result[&apos;size&apos;] == 1){ 
	var queue_records = result[&apos;records&apos;]; 
	pur.OwnerId=queue_records[&apos;QueueId&apos;];
        result = sforce.connection.update([pur]); 
        window.location.reload(); 
}
}
}</url>
    </webLinks>
