    <validationRules>
        <fullName>VPM_VendorEmailorVendorPhoneMandatory</fullName>
        <active>false</active>
        <description>VPM - Validation rule to make Vendor Phone or Vendor Email mandatory .</description>
        <errorConditionFormula>AND(
ISBLANK(VPM_VendorContactDetail__c),
ISBLANK(VPM_VendorPhone__c)
)</errorConditionFormula>
        <errorDisplayField>VPM_AccountGroup__c</errorDisplayField>
        <errorMessage>Please Enter either Vendor Email or Phone Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VPM_VendorPhoneOnlyNumeric</fullName>
        <active>true</active>
        <description>VPM - Validate to check only  Numeric character allowed</description>
        <errorConditionFormula>AND(NOT(ISBLANK(VPM_VendorPhone__c)), 
NOT(REGEX(VPM_VendorPhone__c, &quot;\\D*?(\\d\\D*?){10}&quot;)))</errorConditionFormula>
        <errorMessage>Please enter only Numeric character</errorMessage>
    </validationRules>
