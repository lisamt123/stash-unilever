        <availability>online</availability>
        <description>This is the generic button for rework. When this button is clicked it will assign the record to last worked queue.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Re Work</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/34.0/connection.js&apos;)} 
{ 
	var pur = new sforce.SObject(&quot;VPM_PurchasingRequests__c&quot;); 
	pur.id=&quot;{!VPM_PurchasingRequests__c.Id}&quot;; 
	var currentstatus=&quot;{!VPM_PurchasingRequests__c.VPM_Status__c}&quot;; 
	if(currentstatus.indexOf(&apos;Draft&apos;) &gt;= 0 ) 
	{ 
		pur.VPM_BusinessRequestorSubmitted__c=&apos;Yes&apos;; 
		result = sforce.connection.update([pur]); 
		window.location.reload();	
	} 
	if (VPM_RequestLastWithGroup__c == &apos;FLS&apos; &amp;&amp; VPM_Status__c &lt;&gt; &apos;FLS Requested Re-Work&apos;)
	{
		var result = sforce.connection.query(&quot;Select  QueueId, Queue.Name From QueueSobject where Queue.Name = &apos;FLS Queue&apos; Limit 1&quot;); 
		if (result[&apos;size&apos;] == 1){ 
			var queue_records = result[&apos;records&apos;]; 
			pur VPM_Status__c = &apos;FLS Requested Re-Work&apos;;
			pur.OwnerId=queue_records[&apos;QueueId&apos;];
				result = sforce.connection.update([pur]); 
				window.location.reload(); 
		}
	}
	if (VPM_RequestLastWithGroup__c == &apos;Procurement&apos; &amp;&amp; VPM_Status__c &lt;&gt; &apos;Procurement Requested Re-Work&apos;)
	{
		var result = sforce.connection.query(&quot;Select  QueueId, Queue.Name From QueueSobject where Queue.Name = &apos;Procurement&apos; Limit 1&quot;); 
		if (result[&apos;size&apos;] == 1){ 
			var queue_records = result[&apos;records&apos;]; 
			pur VPM_Status__c = &apos;Procurement Requested Re-Work&apos;;
			pur.OwnerId=queue_records[&apos;QueueId&apos;];
				result = sforce.connection.update([pur]); 
				window.location.reload(); 
		}
	}
	if (VPM_RequestLastWithGroup__c == &apos;Finance&apos; &amp;&amp; VPM_Status__c &lt;&gt; &apos;Finance Requested Re-Work&apos;)
	{
		var result = sforce.connection.query(&quot;Select  QueueId, Queue.Name From QueueSobject where Queue.Name = &apos;Finance Queue&apos; Limit 1&quot;); 
		if (result[&apos;size&apos;] == 1){ 
			var queue_records = result[&apos;records&apos;]; 
			pur VPM_Status__c = &apos;Finance Requested Re-Work&apos;;
			pur.OwnerId=queue_records[&apos;QueueId&apos;];
				result = sforce.connection.update([pur]); 
				window.location.reload(); 
		}
	}
}</url>
