    <fields>
        <fullName>VPM_UserSurveyComments__c</fullName>
        <description>Used to track the user survey score for question 3 -Comments for Vendor Request tool, free text entry. Not displayed on the Vendor Request page layout and used only for reporting by the VMD team.</description>
        <externalId>false</externalId>
        <label>User Survey Comments</label>
        <length>20000</length>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>5</visibleLines>
    </fields>
    <fields>
        <fullName>VPM_UserSurveyReminderEmailSubject__c</fullName>
        <description>Used to store the dynamic text for the email subject line for the User Survey</description>
        <externalId>false</externalId>
        <formula>CASE( VPM_UserSurveyStatus__c , 
&quot;Not sent - Request Incomplete&quot;, &quot;Please complete survey related to your recent Vendor Request&quot;, 
&quot;Reminder 1 sent&quot;, &quot;Reminder 2 - Please complete survey related to your recent Vendor Request&quot;,
&quot;Reminder 2 sent&quot;, &quot;Reminder 3 - Please complete survey related to your recent Vendor Request&quot;,
&quot;N/A&quot;)</formula>
        <label>User Survey Reminder Email Subject</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>VPM_UserSurveyStatus__c</fullName>
        <description>Used to record the status of the User Survey; hidden system field updated by Process Builder as the user survey process completes</description>
        <externalId>false</externalId>
        <label>User Survey Status</label>
        <picklist>
            <picklistValues>
                <fullName>Not sent - Request Incomplete</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Reminder 1 sent</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Reminder 2 sent</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Reminder 3 sent</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Survey Completed</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Survey Ignored</fullName>
                <default>false</default>
            </picklistValues>
            <restrictedPicklist>true</restrictedPicklist>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>VPM_VendorName1__c</fullName>
        <description>VPM - Free text entry for the Business Requestor to enter on the Rapid form the Vendor name in mind. Can be amended by Advanced Users and used to match existing SAP vendors. Interfaces with SAP.</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter the Vendor&apos;s legal name in Latin characters. To add in non-Latin character set, use &apos;International Version&apos; section in the Advanced Form.</inlineHelpText>
        <label>Vendor Name</label>
        <length>40</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <listViews>
        <fullName>VPM_MDMOps_VPM_PurchasingRequests</fullName>
        <booleanFilter>1 AND 2 AND 3 AND (4 OR 5 OR 6)</booleanFilter>
        <columns>NAME</columns>
        <columns>VPM_VendorName1__c</columns>
        <columns>CREATEDBY_USER</columns>
        <columns>VPM_VendorType__c</columns>
        <columns>VPM_Status__c</columns>
        <columns>VPM_MDMInsertUpdateStatus__c</columns>
        <columns>VPM_ECC_InsertUpdateStatus__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>OWNER.ALIAS</field>
            <operation>equals</operation>
            <value>MDM Ops</value>
        </filters>
        <filters>
            <field>VPM_MDMInsertUpdateStatus__c</field>
            <operation>notEqual</operation>
            <value>Failed to Submit to BPM,Approval Rejected in BPM</value>
        </filters>
        <filters>
            <field>VPM_ECC_InsertUpdateStatus__c</field>
            <operation>notEqual</operation>
            <value>Failed to Submit to ECC</value>
        </filters>
        <filters>
            <field>VPM_Status__c</field>
            <operation>equals</operation>
            <value>MDM Ops Review,MDM Ops Review - Unblock - Undelete</value>
        </filters>
        <filters>
            <field>VPM_Status__c</field>
            <operation>equals</operation>
            <value>MDM Ops Review - Delete,MDM Ops Review - Undelete,MDM Ops Review - Unblock</value>
        </filters>
        <filters>
            <field>VPM_Status__c</field>
            <operation>equals</operation>
            <value>MDM Ops Review - Block,MDM Ops Review - Block - Delete</value>
        </filters>
        <label>MDM Ops Queue</label>
        <language>en_US</language>
        <sharedTo>
            <group>VPM_MDMOps</group>
        </sharedTo>
    </listViews>
</CustomObject>
