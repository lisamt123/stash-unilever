    <webLinks>
        <fullName>Add_Issue</fullName>
        <availability>online</availability>
        <description>Cloning the parent case</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Issue</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>/**********************************************************************
 Name:  Add Issue Button
 Copyright © 2013  Unilever
======================================================================
======================================================================
Purpose:  To create a child case by cloning the existing case                                                
1. To populate the case fields from the original case
======================================================================
======================================================================
History                                                            
-------                                                            
VERSION  AUTHOR       DATE             DETAIL                  Description
   1.0 - Vaishnavi       29/01/2015       INITIAL DEVELOPMENT     Add Issue to a case. 
   
****************************************************************************/
{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)}


var result = sforce.connection.query(&quot;Select Id,AccountId,Origin,OwnerId,Reason,ParentId,ContactId,Description,Priority,Subject,SuppliedCompany,SuppliedEmail,SuppliedName,SuppliedPhone,Action_Taken__c,Sapient_Campaign__c,Case_Brand_Mapping__c,Sapient_Corporate__c,Market__c,Sapient_Micro_Site_ID__c,Out_of_Hours__c,Sapient_DCS_ID__c,Set_Assignment_Fields__c,CEC_Product__c,Date_of_Manufacture__c,Date_of_Manufacture_str__c,DayCode__c,Expiry_Date__c,isFormKnown__c,isPacksizeKnown__c,isVariantKnown__c,Production_Code__c,Purchase_Date__c,Reason_Code__c,Store__c,Store_Location__c,UPC_Code_Detail__c, Country__c,CEC_Product_Hierarchy__c From Case Where Id = &apos;{!Case.Id}&apos;&quot;);

var records = result.getArray(&quot;records&quot;);
var caseRecord = records[0];
var caseId;
var clonedCaseRecord = new sforce.SObject(&quot;Case&quot;);
if(caseRecord.ParentId==null){
clonedCaseRecord.ParentId = caseRecord.Id;
} else if(caseRecord.ParentId !=null){
clonedCaseRecord.ParentId = caseRecord.ParentId;
}

clonedCaseRecord.AccountId=caseRecord.AccountId;
clonedCaseRecord.Origin=caseRecord.Origin;
clonedCaseRecord.OwnerId=caseRecord.OwnerId;
clonedCaseRecord.Reason=caseRecord.Reason;
clonedCaseRecord.ContactId=caseRecord.ContactId;
clonedCaseRecord.Description=caseRecord.Description;
clonedCaseRecord.Priority=caseRecord.Priority;
clonedCaseRecord.Subject=caseRecord.Subject;
clonedCaseRecord.SuppliedCompany=caseRecord.SuppliedCompany;
clonedCaseRecord.SuppliedEmail=caseRecord.SuppliedEmail;
clonedCaseRecord.SuppliedName=caseRecord.SuppliedName;
clonedCaseRecord.SuppliedPhone=caseRecord.SuppliedPhone;
clonedCaseRecord.Action_Taken__c=caseRecord.Action_Taken__c;
clonedCaseRecord.Sapient_Campaign__c=caseRecord.Sapient_Campaign__c;
clonedCaseRecord.Case_Brand_Mapping__c=caseRecord.Case_Brand_Mapping__c;
clonedCaseRecord.Sapient_Corporate__c=caseRecord.Sapient_Corporate__c;
clonedCaseRecord.Market__c=caseRecord.Market__c;
clonedCaseRecord.Sapient_Micro_Site_ID__c=caseRecord.Sapient_Micro_Site_ID__c;
clonedCaseRecord.Out_of_Hours__c=caseRecord.Out_of_Hours__c;
clonedCaseRecord.Set_Assignment_Fields__c=caseRecord.Set_Assignment_Fields__c;
clonedCaseRecord.CEC_Product__c=caseRecord.CEC_Product__c;

/* Product hierarchy was earlier updated on Page load 
This was creating an issue that, the case was getting modified even if the case was simply viewed by the user 
To fix the incident, page loading code is removed from controller and product hierarchy is updated here while creating a child case */

clonedCaseRecord.CEC_Product_Hierarchy__c=caseRecord.CEC_Product_Hierarchy__c; 
clonedCaseRecord.Date_of_Manufacture__c=caseRecord.Date_of_Manufacture__c;
clonedCaseRecord.Date_of_Manufacture_str__c=caseRecord.Date_of_Manufacture_str__c;
clonedCaseRecord.DayCode__c=caseRecord.DayCode__c;
clonedCaseRecord.Expiry_Date__c=caseRecord.Expiry_Date__c;
clonedCaseRecord.isFormKnown__c=caseRecord.isFormKnown__c;
clonedCaseRecord.isPacksizeKnown__c=caseRecord.isPacksizeKnown__c;
clonedCaseRecord.isVariantKnown__c=caseRecord.isVariantKnown__c;
clonedCaseRecord.Production_Code__c=caseRecord.Production_Code__c;
clonedCaseRecord.Purchase_Date__c=caseRecord.Purchase_Date__c;
//clonedCaseRecord.Reason_Code__c=caseRecord.Reason_Code__c;
clonedCaseRecord.Country__c=caseRecord.Country__c;
clonedCaseRecord.Store_Location__c=caseRecord.Store_Location__c;
clonedCaseRecord.UPC_Code_Detail__c=caseRecord.UPC_Code_Detail__c;

sforce.connection.create([clonedCaseRecord],successCreate);

/*This method is a call back method on successfully creating a child case. This calls the Tabs refresh method, inorder to have the related child cases in the related list of parent case record.*/
//after creating
function successCreate(result) {

caseId = result[0].id;

if(caseRecord.Store__c != null &amp;&amp; caseRecord.Country__c != null){
 
 var updateCaseRecord = new sforce.SObject(&quot;Case&quot;);
 updateCaseRecord.Id = caseId;
 updateCaseRecord.Store__c = caseRecord.Store__c;
 sforce.connection.update([updateCaseRecord],successUpdate);

}else{
 

 sforce.console.getPrimaryTabIds(refreshPrimaryCase);
 //Refresh finished

}

}

//after updating
function successUpdate(result) {

 sforce.console.getPrimaryTabIds(refreshPrimaryCase);
 //Refresh finished

}



/* This method Refreshes the first case Primary tab assuming that is the master for all.
Even if this master case tab is closed by the advisor it is still not an issue in refreshing other Primary tabs */
function refreshPrimaryCase(result) {
var primaryCaseIds = result.ids;
sforce.console.refreshPrimaryTabById(primaryCaseIds[0], true);
sforce.console.openPrimaryTab(null, &apos;/&apos;+caseId, true);

}</url>
