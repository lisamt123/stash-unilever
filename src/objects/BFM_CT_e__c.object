    <validationRules>
        <fullName>BFM_CTE_Definitive_with_Validation_Error</fullName>
        <active>true</active>
        <errorConditionFormula>DEFINITIVE__c = true &amp;&amp;   UPPER(CT_e_Compatible__c) != &apos;COMPATIBLE&apos;</errorConditionFormula>
        <errorMessage>CT-e cannot be Definitive without passing Validations</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BFM_CTe_Button_Disable</fullName>
        <active>false</active>
        <errorConditionFormula>IF( DATEVALUE(Cte_Emission_date_time__c) &lt;=Today()-7,true,false &amp;&amp;  CT_e_Request_for_Cancel__c = True)</errorConditionFormula>
        <errorDisplayField>Annulment_Reason__c</errorDisplayField>
        <errorMessage>CT-e issued before 168h or 7 days</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BFM_CTe_invoice_posted_error</fullName>
        <active>true</active>
        <errorConditionFormula>!ISNEW() &amp;&amp; UPPER(PRIORVALUE( Cte_Status__c )) = &apos;INVOICE POSTED&apos; &amp;&amp; $Profile.Name != &apos;System Administrator&apos; &amp;&amp;  CT_e_Request_for_Cancel__c != True</errorConditionFormula>
        <errorMessage>Record is not editable if status is Invoice Posted</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>CT_e_Annulment</fullName>
        <availability>online</availability>
        <description>Sends annulment request for this CT-e to SAP.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>CT-e Annulment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot;)} 
var reason = prompt(&quot;What is the annullment reason?&quot;, &quot;&quot;);
var c = new sforce.SObject(&quot;BFM_CT_e__c&quot;); 
c.id =&quot;{!BFM_CT_e__c.Id}&quot;; 
c.Annulment_Reason__c= reason; 
var newRecords = [];
newRecords.push(c); 
result = sforce.connection.update(newRecords); 
alert(&apos;CT-e annulled&apos;); 

window.location.reload();</url>
    </webLinks>
