    <validationRules>
        <fullName>ACCL__Call_Start_Time_Format_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISBLANK( ACCL__Call_Start_Time__c)), 
NOT(REGEX(ACCL__Call_Start_Time__c, &quot;([01]?[0-9]|2[0-3]):[0-5][0-9]&quot;)) 
)</errorConditionFormula>
        <errorDisplayField>ACCL__Call_Start_Time__c</errorDisplayField>
        <errorMessage>Please enter the correct format for Call Start time (hh:mm &amp; valid range: 00:00 to 23:59).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_If_Name_Is_Unique</fullName>
        <active>true</active>
        <errorConditionFormula>and( 
Name = VLOOKUP 
($ObjectType.ACCL__Sales_Organization__c.Fields.Name , $ObjectType.ACCL__Sales_Organization__c.Fields.Name, Name ), 

not( 
Id=VLOOKUP 
($ObjectType.ACCL__Sales_Organization__c.Fields.Id , $ObjectType.ACCL__Sales_Organization__c.Fields.Name, Name )))</errorConditionFormula>
        <errorMessage>A Sales Organization with the same name already exists, please specify a different name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Language_Unique_Validation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF(OR((NOT(ISBLANK(Text(ACCL__Language_1__c))) &amp;&amp; OR(Text(ACCL__Language_1__c)== Text(ACCL__Language_2__c), Text(ACCL__Language_1__c)== Text(ACCL__Language_3__c), Text(ACCL__Language_1__c)== Text(ACCL__Language_4__c))),(NOT(ISBLANK(TEXT(ACCL__Language_2__c))) &amp;&amp; OR(Text(ACCL__Language_2__c)== Text(ACCL__Language_3__c), Text(ACCL__Language_2__c)== Text(ACCL__Language_4__c))),(NOT(ISBLANK(Text(ACCL__Language_3__c))) &amp;&amp; (Text(ACCL__Language_3__c)== Text(ACCL__Language_4__c) ))),true,false)</errorConditionFormula>
        <errorMessage>Same language cannot be selected  twice for a sales organisation.</errorMessage>
    </validationRules>
