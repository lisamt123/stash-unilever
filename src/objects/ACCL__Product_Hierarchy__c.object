    <validationRules>
        <fullName>ACCL__Child_unequal_parent</fullName>
        <active>true</active>
        <description>Child product has to be different than the parent product</description>
        <errorConditionFormula>ACCL__Child_Product__c == ACCL__Parent_Product__c</errorConditionFormula>
        <errorMessage>Child and Parent Products cannot be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Mandatory_StructureType_is_set</fullName>
        <active>true</active>
        <description>Enforces that the Hierarchy-StrucutureType is set in all cases</description>
        <errorConditionFormula>ISPICKVAL( ACCL__Structure_Type__c , &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>ACCL__Structure_Type__c</errorDisplayField>
        <errorMessage>Please select a Structure Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Valid_From_and_Valid_Thru</fullName>
        <active>true</active>
        <description>Valid Thru must be greater or equal than valid from.</description>
        <errorConditionFormula>ACCL__Valid_From__c  &gt; ACCL__Valid_Thru__c</errorConditionFormula>
        <errorDisplayField>ACCL__Valid_Thru__c</errorDisplayField>
        <errorMessage>Valid From cannot be later than Valid Thru.</errorMessage>
    </validationRules>
