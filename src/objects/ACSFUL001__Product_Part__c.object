        <fullName>ACSFUL001__Same_Product_Added</fullName>
        <active>true</active>
        <description>This validation rule checks that the same product is not added as parent and child</description>
        <errorConditionFormula>ACSFUL001__Child_Product__c =  ACSFUL001__Parent_Product__c</errorConditionFormula>
        <errorMessage>Parent and Child Product cannot be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACSFUL001__Valid_From_Valid_Thru_Validation</fullName>
        <active>true</active>
        <description>This validates that Valid From cannot be greater than Valid Thru</description>
