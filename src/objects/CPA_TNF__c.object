    <webLinks>
        <fullName>Cancel_TNF</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_TNF__c&apos;); 
p.id = &quot;{!CPA_TNF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_TNF__c.pkl_Status__c }&quot;; 
p.profileName = &quot;{!$Profile.Name}&quot;; 

if (p.profileName == &quot;System Administrator&quot; || p.profileName == &quot;CPA SMT&quot;){ 
	if(p.pkl_Status__c == &quot;Signed&quot;){ 
		alert(&quot;Can not Cancel as TNF is already Signed&quot;); 
	} 
	else if(p.pkl_Status__c == &quot;Cancelled&quot;){ 
		alert(&quot;Can not Cancel as TNF is already Cancelled&quot;); 
	}else { 
		window.open(&apos;/apex/CPA_TNF_Cancel_Reason?id=&apos;+&quot;{!CPA_TNF__c.Id}&quot;,&quot;_self&quot;); 
	}
}else{
		alert(&quot;Insufficient Privilege&quot;);  
}</url>
    </webLinks>
    <webLinks>
        <fullName>Submit_For_Vendor</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_TNF__c&apos;); 
p.id = &quot;{!CPA_TNF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_TNF__c.pkl_Status__c }&quot;; 
//p.isValidated__c = &quot;{!CPA_TNF__c.isValidated__c }&quot;; 

var isValidateFlag = sforce.apex.execute(&quot;CPA_TNF_InformationController&quot;,&quot;checkIsValidated&quot;,{objId:p.id}); 

//if(p.isValidated__c == &quot;1&quot;){ 

if(isValidateFlag == &apos;true&apos;){ 
if(p.pkl_Status__c == &quot;Withhold&quot; || p.pkl_Status__c == &quot;Returned&quot;){ 
p.pkl_Status__c = &quot;Resubmitted&quot;; 
result = sforce.connection.update([p]); 
location.reload(true); 

} 
else if(p.pkl_Status__c ==&quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; ){ 
alert(&quot;TNF is already submitted&quot;); 
} 
else if(p.pkl_Status__c ==&quot;Accepted&quot; ){ 
alert(&quot;TNF is already Accepted, you cant submit for Approval&quot;); 
} 
else if(p.pkl_Status__c ==&quot;Sent for Signature&quot; || p.pkl_Status__c == &quot;Signed&quot; || p.pkl_Status__c == &quot;Delivered&quot;){
		alert(&quot;TNF cant be submitted for Approval as it is Accepted and moved for further processing&quot;); 
	}
else{ 
p.pkl_Status__c = &quot;Submitted&quot;; 
result = sforce.connection.update([p]); 

location.reload(true); 
} 
} 
else { 
alert(&quot;TNF can not be submitted, Due to Required fields are missing&quot;); 
}</url>
    </webLinks>
