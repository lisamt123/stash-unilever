    <validationRules>
    	<fullName>IPM_Cluster_not_allowed_Rollout_Projects</fullName>
	<active>true</active>
	<description>User are not allowed to edit IPM Project Cluster when project type is Rollout</description>
	<errorConditionFormula>ISPICKVAL(IPM_Project_Type__c, &apos;Rollout&apos;)&amp;&amp; NOT(IPM_Cluster__c = PRIORVALUE(IPM_Cluster__c)) &amp;&amp;  !Is_Archieved__c</errorConditionFormula>
	<errorMessage>IPM Project Cluster not allowed  to edit when Project type is Rollout</errorMessage>
    </validationRules>
    <validationRules>
	<fullName>IPM_exclude_stppd_prjcts_from_cluster</fullName>
	<active>true</active>
	<description>This ensures that the stopped projects are not added to a cluster.</description>
	<errorConditionFormula>Is_Archieved__c &amp;&amp;  IPM_Cluster__c !=Null</errorConditionFormula>
	<errorMessage>The stopped projects  can not be included  in a cluster.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IPM_Cluster_not_allowed_for_Operational</fullName>
        <active>true</active>
        <description>IPM Cluster not allowed for Operational Project type</description>
        <errorConditionFormula>ISPICKVAL(IPM_Project_Type__c, &apos;Operational&apos;)&amp;&amp; NOT(ISBLANK(IPM_Cluster__c))</errorConditionFormula>
        <errorMessage>Operational Projects cannot be added to IPM Project Clusters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ideas_create_Global_Original_Projects</fullName>
        <active>false</active>
        <description>Global Original Project can only be created in Ideas Phase</description>
        <errorConditionFormula>AND($Profile.Name != &apos;IPM Data Migration&apos;,ISPICKVAL( IPM_Project_Type__c,&apos;Original&apos; ),ISNEW(),ISPICKVAL ( IPMProject_Span__c,&apos;Global&apos;),NOT(ISPICKVAL(IPM_Temporary_Phase__c,&apos;Ideas&apos;)))</errorConditionFormula>
        <errorMessage>Global Original Project must begin with Ideas Phase</errorMessage>
    </validationRules>
