        <fullName>New_Resource_Requirement</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Resource Requirement</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos;  and PermissionSet.Name in (&apos;CPA_SMT&apos;,&apos;CPA_ADM&apos;,&apos;CPA_ULPM&apos;)&quot;;
var result = sforce.connection.query(soql);
var records1 = result.getArray(&apos;records&apos;);
var permissionSetName = records1[0].PermissionSet.Name;

if(permissionSetName == &quot;CPA_ADM&quot;){
alert(&quot;Resource Requirement cannot be created by Vendor Team.&quot;);
}else if(permissionSetName == &quot;CPA_ULPM&quot;){

if(&quot;{!CPA_PWORF__c.pkl_Status__c}&quot; == &quot;Auto Accepted&quot; ||&quot;{!CPA_PWORF__c.pkl_Status__c}&quot; == &quot;Accepted&quot; || &quot;{!CPA_PWORF__c.pkl_Status__c}&quot; == &quot;Answered&quot;)
{
alert(&quot;New Resource Requirement can be created before Associated PWORF is Accepted.&quot;);
}else if(&quot;{!CPA_PWORF__c.pkl_Status__c}&quot; == &quot;Cancelled&quot;)
{
alert(&quot;New Resource Requirement cannot be created as Associated PWORF is Cancelled.&quot;);
}else{
window.open(&quot;/&quot;+&quot;{!$Label.CPA_ResourceSummary_Object_Id}/e?{!$Label.CPA_PWORF_ResourceSummary_Name_Filed_Id}={!CPA_PWORF__c.Name}&amp;{!$Label.CPA_PWORF_ResourceSummary_Name_Filed_Id}_lkid={!CPA_PWORF__c.Id}&amp;retURL=%2F{!CPA_PWORF__c.Id}&quot;,&quot;_parent&quot;);
}

}else if(permissionSetName == &quot;CPA_SMT&quot;){
if(&quot;{!CPA_PWORF__c.pkl_Status__c}&quot; == &quot;Cancelled&quot;)
{
alert(&quot;New Resource Requirement cannot be created as Associated PWORF is Cancelled.&quot;);
}else{
window.open(&quot;/&quot;+&quot;{!$Label.CPA_ResourceSummary_Object_Id}/e?{!$Label.CPA_PWORF_ResourceSummary_Name_Filed_Id}={!CPA_PWORF__c.Name}&amp;{!$Label.CPA_PWORF_ResourceSummary_Name_Filed_Id}_lkid={!CPA_PWORF__c.Id}&amp;retURL=%2F{!CPA_PWORF__c.Id}&quot;,&quot;_parent&quot;);
}
}</url>
