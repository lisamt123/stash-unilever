    <fields>
        <fullName>FS_List_Price_including_VAT__c</fullName>
        <externalId>false</externalId>
        <formula>UnitPrice  * (1  +  FS_VAT__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>List Price including VAT</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_Price_per_piece_with_VAT__c</fullName>
        <externalId>false</externalId>
        <formula>FS_List_Price_including_VAT__c / Product2.FS_ZSU__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Price per piece with VAT</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_Price_per_piece_without_VAT__c</fullName>
        <externalId>false</externalId>
