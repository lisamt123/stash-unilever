    <fields>
        <fullName>FS_Unit_conversion_factor__c</fullName>
        <description>Purpose : Unit conversion factor from Sales Unit to Base UOM from Product</description>
        <externalId>false</externalId>
        <formula>Product2.FS_ZSU__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Unit conversion factor</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>FS_VAT__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( CurrencyIsoCode,
 &apos;RUB&apos;,  
  CASE( Product2.FS_VATRU__c, 
        1,  $Setup.FSVATRU__c.FS_Value_1__c , 
        2,  $Setup.FSVATRU__c.FS_Value_2__c , 
        0) , 
 &apos;UAH&apos;,
  CASE( Product2.FS_VATUA__c, 
        2,  $Setup.FSVATUA__c.FS_Value_2__c , 
        0) , 
 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>VAT</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>FS_Weight_Value__c</fullName>
        <description>Purpose: Corresponding NetWeight value from product</description>
        <externalId>false</externalId>
