    <fields>
        <fullName>FS_VAT__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( CurrencyIsoCode,
 &apos;RUB&apos;,  
  CASE( Product2.FS_VATRU__c, 
        1,  $Setup.FSVATRU__c.FS_Value_1__c , 
        2,  $Setup.FSVATRU__c.FS_Value_2__c , 
        0) , 
 &apos;UAH&apos;,
  CASE( Product2.FS_VATUA__c, 
        2,  $Setup.FSVATUA__c.FS_Value_2__c , 
        0) , 
 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
