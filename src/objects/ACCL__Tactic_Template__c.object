    <validationRules>
        <fullName>ACCL__Prevent_Name_Duplication</fullName>
        <active>true</active>
        <description>Name must be unique for Tactic Templates</description>
        <errorConditionFormula>and( 
UPPER(Name) = UPPER(VLOOKUP 
($ObjectType.ACCL__Tactic_Template__c.Fields.Name , $ObjectType.ACCL__Tactic_Template__c.Fields.Name, Name )), 

not( 
Id=VLOOKUP 
($ObjectType.ACCL__Tactic_Template__c.Fields.Id , $ObjectType.ACCL__Tactic_Template__c.Fields.Name, Name )))</errorConditionFormula>
        <errorMessage>Tactic Template Name already exists, please enter different Name.</errorMessage>
    </validationRules>
