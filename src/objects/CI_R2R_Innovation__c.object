        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(R2R_SME_Area__c, &apos;Account Reconciliation&apos;), ( text(YEAR(today())) +&apos;.AR.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;FX Revaluation&apos;), (text(YEAR(today())) +&apos;.FX.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Intercompany&apos;), (text(YEAR(today())) +&apos;.IC.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Period-End&apos;), (text(YEAR(today())) +&apos;.PE.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Project&apos;), (text(YEAR(today())) +&apos;.PT.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;SAP Roles&apos;), (text(YEAR(today())) +&apos;.SR.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Tax&apos;), (text(YEAR(today())) +&apos;.TX.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Tools&apos;), (text(YEAR(today())) +&apos;.PT.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Continuous Improvement&apos;), (text(YEAR(today())) +&apos;.CI.&apos;+ Name),
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Fixed Assets&apos;), (text(YEAR(today())) +&apos;.FA.&apos;+ Name),
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Financial Reporting&apos;), (text(YEAR(today())) +&apos;.FR.&apos;+ Name),
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Journal Entries&apos;), (text(YEAR(today())) +&apos;.FA.&apos;+ Name),
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;GL MDM&apos;), (text(YEAR(today())) +&apos;.MDM.&apos;+ Name), 
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Direct Tax&apos;), (text(YEAR(today())) +&apos;.DT.&apos;+ Name),
IF ( ISPICKVAL(R2R_SME_Area__c, &apos;Indirect Tax&apos;), (text(YEAR(today())) +&apos;.IDT.&apos;+ Name),
(text(YEAR(today())) +&apos;.&apos;+ text( R2R_SME_Area__c ) +&apos;.&apos;+ Name))))))))))))))))</formula>
