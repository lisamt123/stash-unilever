    <webLinks>
        <fullName>Submit_For_ULPM</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

var obj = new sforce.SObject(&apos;CPA_CNF__c&apos;); 
obj.id = &quot;{!CPA_CNF__c.Id}&quot;; 
obj.pkl_Status__c = &quot;{!CPA_CNF__c.pkl_Status__c}&quot;;
var isValidateFlag = sforce.apex.execute(&quot;CPA_CNF_InformationController&quot;,&quot;checkIsValidated&quot;,{objId:obj.id}); 


if(isValidateFlag == &apos;true&apos;){ 
if(obj.pkl_Status__c == &quot;Returned&quot;){
obj.pkl_Status__c = &quot;Resubmitted&quot;; 
result = sforce.connection.update([obj]); 
location.reload(true); 

} 
else if(obj.pkl_Status__c ==&quot;Submitted&quot; || obj.pkl_Status__c == &quot;Resubmitted&quot; ){ 
alert(&quot;CNF is already submitted&quot;); 
} 
else if(obj.pkl_Status__c ==&quot;Cancelled&quot; ){ 
alert(&quot;CNF is already Cancelled, you can&apos;t submit for Approval&quot;); 
} 
else if(obj.pkl_Status__c ==&quot;Accepted&quot; ){ 
alert(&quot;CNF is already Accepted, you can&apos;t submit for Approval&quot;); 
} 
else if(obj.pkl_Status__c ==&quot;Delivered&quot; ){ 
alert(&quot;CNF is already Delivered, you can&apos;t submit for Approval&quot;); 
} 
else if(obj.pkl_Status__c ==&quot;Answered&quot; ){ 
alert(&quot;CNF is already Answered, you can&apos;t submit for Approval&quot;); 
} 
else if(obj.pkl_Status__c ==&quot;Sent for Signature&quot; || obj.pkl_Status__c == &quot;Signed&quot; || obj.pkl_Status__c == &quot;Delivered&quot;){
		alert(&quot;CNF cant be submitted for Approval as it is Accepted and moved for further processing&quot;); 
	}
else{ 
obj.pkl_Status__c = &quot;Submitted&quot;; 
result = sforce.connection.update([obj]); 
location.reload(true); 
} 

} 
else { 
alert(&quot;CNF can not be submitted, Due to Required fields are missing&quot;); 
}</url>
    </webLinks>
    <webLinks>
        <fullName>btn_Cancel_CNF</fullName>
        <availability>online</availability>
        <description>Popup new vf page to get cancellation reason.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_CNF__c&apos;); 
p.id = &quot;{!CPA_CNF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_CNF__c.pkl_Status__c }&quot;; 
p.profileName = &quot;{!$Profile.Name}&quot;; 


if(p.profileName == &quot;System Administrator&quot; || p.profileName == &quot;CPA SMT&quot;){ 
	if(p.pkl_Status__c == &apos;Cancelled&apos; ){
		alert(&quot;Can not Cancel as CNF is already Cancelled&quot;);
	} else if(p.pkl_Status__c == &apos;Signed&apos; ){
		alert(&quot;Can not Cancel as CNF is already Signed&quot;);
	} else{
		open(&apos;/apex/CPA_CNF_CancelReason?id=&apos;+&quot;{!CPA_CNF__c.Id}&quot;,&quot;_self&quot;);		 
	}
} else{
	alert(&quot;Insufficient Privilege&quot;); 
}</url>
    </webLinks>
    <webLinks>
        <fullName>btn_Sent_for_signature</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Sent for Signature</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_CNF__c&apos;); 
p.id = &quot;{!CPA_CNF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_CNF__c.pkl_Status__c }&quot;; 
profileName = &quot;{!$Profile.Name}&quot;; 

if(profileName == &quot;System Administrator&quot; || profileName == &quot;CPA SMT&quot;){ 
if(p.pkl_Status__c == &quot;Accepted&quot;){  
p.pkl_Status__c = &quot;Sent for Signature&quot;;	
result = sforce.connection.update([p]); 
location.reload(true); 
}else if(p.pkl_Status__c == &quot;Saved&quot; || p.pkl_Status__c == &quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot;){ 
alert(&quot;CNF can not be Sent for Signature as it is not yet Accepted&quot;); 
}else if(p.pkl_Status__c == &quot;Cancelled&quot;){ 
alert(&quot;CNF can not be Sent for Signature as it is Cancelled&quot;); 
}else if(p.pkl_Status__c == &quot;Returned&quot;){ 
alert(&quot;CNF can not be Sent for Signature as it is Returned&quot;); 
}else if(p.pkl_Status__c == &quot;Sent for Signature&quot;){ 
alert(&quot;CNF is already Sent for Signature&quot;); 
}else if(p.pkl_Status__c == &quot;Signed&quot; || p.pkl_Status__c == &quot;Delivered&quot;){ 
alert(&quot;CNF is can not be Signed as it is yet to Sent for Signature&quot;); 
}
}else{ 
alert(&quot;Insufficient Privilege&quot;); 
}</url>
    </webLinks>
    <webLinks>
        <fullName>btn_Signed</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Signed</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

var p = new sforce.SObject(&apos;CPA_CNF__c&apos;); 
p.id = &quot;{!CPA_CNF__c.Id}&quot;; 
p.pkl_Status__c = &quot;{!CPA_CNF__c.pkl_Status__c }&quot;; 
profileName = &quot;{!$Profile.Name}&quot;; 

if(profileName == &quot;System Administrator&quot; || profileName == &quot;CPA SMT&quot;){ 
if(p.pkl_Status__c == &quot;Sent for Signature&quot;){ 
p.pkl_Status__c = &quot;Signed&quot;;	
result = sforce.connection.update([p]); 
location.reload(true); 
}else if(p.pkl_Status__c == &quot;Saved&quot; || p.pkl_Status__c == &quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; || p.pkl_Status__c == &quot;Accepted&quot;){ 
alert(&quot;CNF can not be Signed as it is not yet Sent for Signature&quot;); 
}else if(p.pkl_Status__c == &quot;Cancelled&quot;){ 
alert(&quot;CNF can not be Signed as it is Cancelled&quot;); 
}else if(p.pkl_Status__c == &quot;Returned&quot;){ 
alert(&quot;CNF can not be Signed as it is Returned&quot;); 
}else if(p.pkl_Status__c == &quot;Signed&quot;){ 
alert(&quot;CNF is already Signed&quot;); 
}else if(p.pkl_Status__c == &quot;Delivered&quot;){ 
alert(&quot;CNF is already Delivered&quot;); 
}
}else{ 
alert(&quot;Insufficient Privilege&quot;); 
}</url>
