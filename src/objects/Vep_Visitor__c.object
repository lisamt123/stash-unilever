    <validationRules>
        <fullName>VEP_Contact_should_be_number</fullName>
        <active>true</active>
        <errorConditionFormula>!ISNUMBER( Contact__c ) &amp;&amp;  !ISBLANK(Contact__c)</errorConditionFormula>
        <errorDisplayField>Contact__c</errorDisplayField>
        <errorMessage>Please Enter a Valid Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEP_Date_To_is_grater_than_Date_From</fullName>
        <active>true</active>
        <errorConditionFormula>Date_To__c &lt; Date_From__c</errorConditionFormula>
        <errorDisplayField>Date_To__c</errorDisplayField>
        <errorMessage>Must be greater than or equal to From Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEP_Greyout_ShopFloor_if_Asthma_Checked</fullName>
