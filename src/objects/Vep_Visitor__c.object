        <active>true</active>
        <errorConditionFormula>RecordType.Name = &apos;Request Readonly&apos; &amp;&amp;  PRIORVALUE(Name) !=Name</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Name should not be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEP_Reason_For_Other_should_be_Filled</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL( Reason_for_Visit__c , &apos;Other&apos;) &amp;&amp;  ISBLANK( Reason_for_Visit_Other__c ) || (!ISPICKVAL( Reason_for_Visit__c , &apos;Other&apos;) &amp;&amp; !ISBLANK(Reason_for_Visit_Other__c)))</errorConditionFormula>
        <errorDisplayField>Reason_for_Visit_Other__c</errorDisplayField>
        <errorMessage>Field is required if Reason for Visit is &quot;Other&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEP_Visit_Entry_date_less_than_Exit_date</fullName>
        <active>true</active>
        <errorConditionFormula>Visitor_Exit_Date__c  &lt;  Visitor_Entry_Date__c</errorConditionFormula>
        <errorDisplayField>Visitor_Exit_Date__c</errorDisplayField>
        <errorMessage>Must be greater than or equal to Visit Entry Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VEP_Visit_Must_be_a_Future_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Date_From__c &lt; Today() || Date_To__c &lt; Today()</errorConditionFormula>
        <errorMessage>Visit Must be a Future Date</errorMessage>
    </validationRules>
</CustomObject>
