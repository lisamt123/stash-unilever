    <fields>
        <fullName>AF_Active_Rejection_Comment__c</fullName>
        <description>Reject comment shown on Email Template</description>
        <externalId>false</externalId>
        <formula>If(
 	AF_Rejected_Quarter__c = &apos;Q1&apos;,  AF_Q1_Rejection_Comment__c,
		If(
 			AF_Rejected_Quarter__c = &apos;Q2&apos;,  AF_Q2_Rejection_Comment__c,
	
				If(
