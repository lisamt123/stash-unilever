    <validationRules>
        <fullName>ACCL__Amount_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>ACCL__Amount__c &lt;=0</errorConditionFormula>
        <errorMessage>Transaction Amount should be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_Mismatch_Source_Target</fullName>
        <active>true</active>
        <errorConditionFormula>AND
          (
             ACCL__Transaction_Type__c = &quot;Transfer&quot;,
             ACCL__Source_Fund__r.ACCL__Sales_Org__c &lt;&gt; ACCL__Target_Fund__r.ACCL__Sales_Org__c
          )</errorConditionFormula>
        <errorMessage>Sales Org of Source Fund and Target Fund must be same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_Mismatch_Template_Source</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
          (
            ACCL__Transaction_Type__c = &quot;Transfer&quot;,
            ACCL__Source_Fund__r.ACCL__Sales_Org__c &lt;&gt; TEXT(ACCL__Fund_Transaction_Template__r.ACCL__Sales_Org__c ) 
          )</errorConditionFormula>
        <errorMessage>Sales Org of Transaction Template and Source Fund should be same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_Mismatch_Template_Target</fullName>
        <active>true</active>
        <errorConditionFormula>ACCL__Target_Fund__r.ACCL__Sales_Org__c &lt;&gt; TEXT(ACCL__Fund_Transaction_Template__r.ACCL__Sales_Org__c )</errorConditionFormula>
        <errorMessage>Sales Org of Transaction Template and Target Fund should be same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Fund_Validation</fullName>
        <active>true</active>
        <description>validation  to restrict money transfer to and from closed fund</description>
        <errorConditionFormula>(ISPICKVAL( ACCL__Target_Fund__r.ACCL__Status__c , &quot;Closed&quot;)) ||  ISPICKVAL( ACCL__Source_Fund__r.ACCL__Status__c , &quot;Closed&quot;)</errorConditionFormula>
        <errorMessage>Cannot transfer money to or from closed fund</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_Adjustment_transaction_validation</fullName>
        <active>true</active>
        <description>Validation to check if the transaction amount is more than the available amount in the target fund</description>
        <errorConditionFormula>AND( ISPICKVAL( ACCL__Fund_Transaction_Template__r.ACCL__Transaction_Type__c , &quot;Adjustment&quot;) , ACCL__Amount__c &gt;  ACCL__Target_Fund__r.UL_Available_to_Spend__c )</errorConditionFormula>
        <errorDisplayField>ACCL__Amount__c</errorDisplayField>
        <errorMessage>Adjustment amount is more than the available to spend amount in the target fund</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UL_Transfer_transaction_validation</fullName>
        <active>true</active>
        <description>Validation to check if the transaction amount is more than the available amount in the source fund</description>
        <errorConditionFormula>AND( ISPICKVAL( ACCL__Fund_Transaction_Template__r.ACCL__Transaction_Type__c , &quot;Transfer&quot;)  ,  ACCL__Amount__c &gt;  ACCL__Source_Fund__r.UL_Available_to_Spend__c  )</errorConditionFormula>
        <errorDisplayField>ACCL__Amount__c</errorDisplayField>
        <errorMessage>Transfer amount is more than the available to spend amount in the source fund</errorMessage>
    </validationRules>
</CustomObject>
