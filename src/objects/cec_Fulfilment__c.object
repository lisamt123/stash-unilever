        <description>CEC New Retrieval Request  will create New Retrieval Request and populate Fulfilment: Retrieval Id &amp; 	Retrieval: Fulfilment Id</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Retrieval Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;
var fulfillmentQuery;
if ({!$Permission.CEC_Retrieval_Process}) {
    fulfillmentQuery = sforce.connection.query(&quot;Select Id,case__r.id,case__r.AccountId,case__r.Account.Phone,case__r.Account.PersonMobilePhone,case__r.Account.PersonOtherPhone,case__r.Account.PersonHomePhone,case__r.Account.PersonMailingCity,case__r.Account.PersonMailingCountry,case__r.Account.PersonBirthdate, case__r.Account.PersonMailingPostalCode, case__r.Account.PersonMailingState, case__r.Account.PersonMailingStreet,case__r.Account.Name,case__r.Country_Name__c,Retrieval_Id__c,isApproved__c From cec_fulfilment__c Where Id = &apos;{!cec_Fulfilment__c.Id}&apos;&quot;);
    var retrivalRequestId;
    var retrivalRequestName;
    var records = fulfillmentQuery.getArray(&quot;records&quot;);
    var fulfillmentRecord = records[0];
    var caseRecord = fulfillmentRecord.getArray(&quot;Case__r&quot;);
    var accountRecord = caseRecord[0].getArray(&quot;Account&quot;);
    if (fulfillmentRecord != null &amp;&amp; caseRecord != null &amp;&amp; accountRecord != null &amp;&amp; (accountRecord[0].Phone == null &amp;&amp; accountRecord[0].PersonMobilePhone == null &amp;&amp; accountRecord[0].PersonOtherPhone == null &amp;&amp; accountRecord[0].PersonHomePhone == null)) {
        alert(&apos;{!$Label.CEC_CaseAccountPersonPhoneMissing}&apos;);
    } else if (fulfillmentRecord.isApproved__c == &apos;true&apos;) {
		alert(&apos;{!$Label.CEC_ApprovedFulfilmentError}&apos;);
	} else if (fulfillmentRecord.Retrieval_Id__c != null) {
		alert(&apos;{!$Label.CEC_DuplicateRetrievalRequestError}&apos;);
	}else {
        var newRetrievelRequest = new sforce.SObject(&quot;CEC_Retrieval_Request__c&quot;);
        newRetrievelRequest.Fulfilment_Id__c = fulfillmentRecord.Id;
        newRetrievelRequest.Case__c = caseRecord[0].Id;
        newRetrievelRequest.Case_Country_Name__c = caseRecord[0].Country_Name__c;
        sforce.connection.create([newRetrievelRequest], updateFulfilment);
    }
} else {
    alert(&apos;{!$Label.CEC_NoAccessForRetrievalProcess}&apos;);
}
function updateFulfilment(result) {
    retrivalRequestId = result[0].id;
	var fulfillmentProductQuery;
	fulfillmentProductQuery = sforce.connection.query(&quot;Select Id,Retrieval_Id__c From CEC_Fulfilment_Product__c Where Fulfilment_Id__c = &apos;{!cec_Fulfilment__c.Id}&apos;&quot;);
	var recordsFP = fulfillmentProductQuery.getArray(&quot;records&quot;);
	var updateFP = [];
	if (recordsFP !=null ){
		for(var i=0;i&lt;recordsFP.length;i++){
			var FulfilmentProduct=new sforce.SObject(&quot;CEC_Fulfilment_Product__c&quot;);
			FulfilmentProduct.Id=recordsFP[i].Id;
			FulfilmentProduct.Retrieval_Id__c = retrivalRequestId;
			updateFP.push(FulfilmentProduct);
		}
		var result = sforce.connection.update(updateFP);	
	}
    var fulfilment = new sforce.SObject(&quot;cec_fulfilment__c&quot;);
    fulfilment.id = &apos;{!cec_Fulfilment__c.Id}&apos;;
    fulfilment.Retrieval_Id__c = retrivalRequestId; // Update Retrieval Request Id on 
    sforce.connection.update([fulfilment], tabOperations);
}
function tabOperations(result) {
    sforce.console.getFocusedSubtabId(refreshFulfilmentSubTab);
    sforce.console.getFocusedPrimaryTabId(openRetrievalRequestSubtab);
}
function refreshFulfilmentSubTab(result) {
    sforce.console.refreshSubtabById(result.id);
}
function openRetrievalRequestSubtab(result) {
    //Now that we have the primary tab ID, we can open a new subtab in it
    var primaryTabId = result.id;
	// Refresh the Case details
    sforce.console.refreshPrimaryTabById(primaryTabId, false);
    sforce.console.openSubtab(primaryTabId, &apos;/&apos; + retrivalRequestId, true, null, null);
};</url>
    </webLinks>
    <webLinks>
        <fullName>New_Fulfilment</fullName>
        <availability>online</availability>
        <description>CEC_Custom New fulfilment button</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Fulfilment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}

sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;

var caseQuery;
if ({!$Permission.CEC_Debit_Card_Process}) {
    caseQuery = sforce.connection.query(&quot;Select Id,AccountId,Account.PersonMailingCity,Account.PersonEmail,Account.PersonMailingCountry,Case_Brand__c,Account.PersonBirthdate, Account.PersonMailingPostalCode, Account.PersonMailingState, Account.PersonMailingStreet,Account.Salutation, Account.Local_Identifier__c,Account.Debit_Card_Number__c,Account.Debit_Card_Number__r.Active__c,Account.Debit_Card_Number__r.Name,Account.Name,Country_Name__c , L0_Product_Category__c,L2_Product_Category__c,L5_Code__c,CEC_Product__c From Case Where Id = &apos;{!Case.Id}&apos;&quot;);
} else {
    caseQuery = sforce.connection.query(&quot;Select Id,AccountId,Account.PersonMailingCity,Account.PersonEmail,Account.PersonMailingCountry,Case_Brand__c,Account.PersonBirthdate, Account.PersonMailingPostalCode, Account.PersonMailingState, Account.PersonMailingStreet,Account.Salutation, Account.Local_Identifier__c,  Account.Name,Country_Name__c , L0_Product_Category__c,L2_Product_Category__c,L5_Code__c,CEC_Product__c From Case Where Id = &apos;{!Case.Id}&apos;&quot;);
}
var fulfilmentId;
var fulfilmentName;
var records = caseQuery.getArray(&quot;records&quot;);
var caseRecord = records[0];


if (caseRecord != null &amp;&amp; caseRecord.AccountId == null) {
    alert(&apos;{!$Label.CEC_CaseAccountMissing}&apos;);
} else if (caseRecord != null &amp;&amp;
    caseRecord.Account.PersonMailingCity == null &amp;&amp; caseRecord.Account.PersonMailingCountry == null &amp;&amp; caseRecord.Account.PersonMailingPostalCode == null &amp;&amp; caseRecord.Account.PersonMailingState == null &amp;&amp; caseRecord.Account.PersonMailingStreet == null) {
    alert(&apos;{!$Label.CEC_CaseAccountPersonDetailsError}&apos;);
} else {

    var newFulfilment = new sforce.SObject(&quot;cec_Fulfilment__c&quot;);
    newFulfilment.Case__c = caseRecord.Id;
    newFulfilment.Country__c = caseRecord.Account.PersonMailingCountry;


    var objFulfilment = sforce.connection.describeSObject(&quot;cec_fulfilment__c&quot;);
    var result = &apos;false&apos;;
    var field = &apos;&apos;;
    for (var i = 0; i &lt; objFulfilment.fields.length; i++) {
        field = objFulfilment.fields[i];
        if (field.name == &apos;Payable_To__c&apos;) {
            result = &apos;true&apos;;
            break;
        }
    }


    if (result == &apos;true&apos; &amp;&amp; caseRecord.Account.Salutation != null) {
        newFulfilment.Payable_To__c = caseRecord.Account.Salutation + &quot; &quot; + caseRecord.Account.Name;
    } else if (result == &apos;true&apos;) {
        newFulfilment.Payable_To__c = caseRecord.Account.Name;
    }

    //This Code will default the values only if advisor has debit card Process custom permission.
    //This Code is used to default the values of DOB,Local identifier,Debit Card Number and Debit Card Status
    if ({!$Permission.CEC_Debit_Card_Process} || {!$Permission.CEC_E_Voucher_Process} ) {

        if (caseRecord.Account.PersonBirthdate != null) {
            newFulfilment.Date_of_Birth__c = caseRecord.Account.PersonBirthdate;
        }
        if (caseRecord.Account.PersonEmail != null) {
            newFulfilment.PersonEmail__c = caseRecord.Account.PersonEmail;
        }

        if (caseRecord.Account.Local_Identifier__c != null) {
            newFulfilment.Local_Identifier__c = caseRecord.Account.Local_Identifier__c;
        }		

        if (caseRecord.Account.Debit_Card_Number__c != null &amp;&amp; caseRecord.Account.Debit_Card_Number__r.Active__c != null &amp;&amp; caseRecord.Account.Debit_Card_Number__r.Name != null) {

            newFulfilment.Account_Card_Status__c = caseRecord.Account.Debit_Card_Number__r.Active__c;
            newFulfilment.Account_Card_Number__c = caseRecord.Account.Debit_Card_Number__r.Name;
        }

    }

    var result = sforce.apex.execute(&quot;CEC_AlertFulfilmentEnclosureHelper&quot;, &quot;populateEnclosureDetails&quot;, {
        CaseId: caseRecord.Id,
        CountryName: newFulfilment.Country__c
    });

    if (result[0].status == &apos;S&apos;) {
        if (result[0].enclosuresList != null) {

            if (result[0].enclosuresList[0]) {
                newFulfilment.Enclosure1__c = result[0].enclosuresList[0];

            }

            if (result[0].enclosuresList[1]) {
                newFulfilment.Enclosure2__c = result[0].enclosuresList[1];

            }

            if (result[0].enclosuresList[2]) {
                newFulfilment.Enclosure3__c = result[0].enclosuresList[2];

            }

            if (result[0].enclosuresList[3]) {
                newFulfilment.Enclosure4__c = result[0].enclosuresList[3];

            }
            if (result[0].enclosuresList[4]) {
                newFulfilment.Enclosure5__c = result[0].enclosuresList[4];

            }

        }
    } else if (result[0].status == &apos;E&apos;) {
        alert(result[0].message);
    }
    
    sforce.connection.create([newFulfilment], success);
}

function success(result) {
    
    fulfilmentId = result[0].id;
    fulfilmentName = result[0].Name;
    sforce.console.getFocusedPrimaryTabId(openSubtab);
}

var openSubtab = function openSubtab(result) {
    //Now that we have the primary tab ID, we can open a new subtab in it
    var primaryTabId = result.id;
    sforce.console.refreshPrimaryTabById(primaryTabId, false);
    sforce.console.openSubtab(primaryTabId, &apos;/&apos; + fulfilmentId, true, fulfilmentName, null);
};</url>
