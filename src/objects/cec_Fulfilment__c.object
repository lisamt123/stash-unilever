    <validationRules>
        <fullName>Case_Information_Vlidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK(Case__r.Account.PersonMailingCity),
ISBLANK(Case__r.Account.PersonMailingCountry),
ISBLANK(Case__r.Account.PersonMailingPostalCode),
ISBLANK(Case__r.Account.PersonMailingState),
ISBLANK(Case__r.Account.PersonMailingStreet)
)</errorConditionFormula>
        <errorMessage>Fulfilment cannot be created until the consumer address has been added</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DebitCard_Value_Validation</fullName>
        <active>true</active>
        <description>CEC Debit Card value validation rule is used to disable the updation of debit card details on approved fulfillment.</description>
        <errorConditionFormula>(ISCHANGED( Debit_Card_Value__c ) ||  ISCHANGED( Debit_Card_Number__c ) ) &amp;&amp;  isApproved__c ==true</errorConditionFormula>
        <errorMessage>The fulfillment is already approved, cannot update debit card information</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>E_Voucher_Value_Validation</fullName>
        <active>true</active>
        <description>CEC E-Voucher Value Validation rule is used to disable the updation of E-Voucher details on  approved fulfillment.</description>
        <errorConditionFormula>(ISCHANGED( E_Voucher_Value__c ) || ISCHANGED( E_Voucher_Number__c ) ) &amp;&amp; isApproved__c ==true</errorConditionFormula>
        <errorMessage>The fulfilment is already approved, cannot update E Voucher details information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enclosure_Quantity_Check</fullName>
        <active>true</active>
        <description>CEC : To mandate population of Enclosure Quantity field when corresponding Enclosure field is populated</description>
        <errorConditionFormula>OR(
NOT(ISBLANK(Enclosure1__c)) &amp;&amp; ISPICKVAL( Enclosure1_Quantity__c , &quot;&quot;),
NOT(ISBLANK(Enclosure2__c)) &amp;&amp; ISPICKVAL( Enclosure2_Quantity__c , &quot;&quot;),
NOT(ISBLANK(Enclosure3__c)) &amp;&amp; ISPICKVAL( Enclosure3_Quantity__c , &quot;&quot;),
NOT(ISBLANK(Enclosure4__c)) &amp;&amp; ISPICKVAL( Enclosure4_Quantity__c , &quot;&quot;),
NOT(ISBLANK(Enclosure5__c)) &amp;&amp; ISPICKVAL( Enclosure5_Quantity__c , &quot;&quot;)
)</errorConditionFormula>
        <errorMessage>Enclosure quantity is mandatory for the selected enclosure.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FulfilmentType_MandatoryCheck</fullName>
        <active>true</active>
        <description>cec - to check the Type field mandatory</description>
        <errorConditionFormula>ISPICKVAL( Type__c , &quot;&quot;)</errorConditionFormula>
        <errorMessage>Unable to create Fulfilment with empty &apos;Type&apos; information. Select appropriate &apos;Country&apos; value to proceed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Max_Refund_value_validation</fullName>
        <active>true</active>
        <description>CEC: Restrict advisor from entering/updating the refund value with the value greater than max refund limit</description>
        <errorConditionFormula>ISCHANGED(Refund_Value__c) &amp;&amp; (Refund_Value__c  &gt;   $User.Max_Refund_Amount__c ||  (ISBLANK($User.Max_Refund_Amount__c) &amp;&amp; NOT(ISBLANK(Refund_Value__c ))))</errorConditionFormula>
        <errorDisplayField>Refund_Value__c</errorDisplayField>
        <errorMessage>Refund value exceeded Max refund value  limit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RefundValue_Not_Editable_ChequeSent</fullName>
        <active>true</active>
        <description>CEC: Restrict advisor from entering/updating the refund value if cheque sent flag is set to true</description>
        <errorConditionFormula>Cheque_Sent__c  &amp;&amp; (  PRIORVALUE( Refund_Value__c)  &lt;&gt;  Refund_Value__c )</errorConditionFormula>
        <errorDisplayField>Refund_Value__c</errorDisplayField>
        <errorMessage>Refund value cannot be updated as cheque has already been sent.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Add_Cheque_Details</fullName>
        <availability>online</availability>
        <description>CEC_Custom Add Cheque_Details button.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Cheque Details</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}

sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 

if ({!$Permission.CEC_Cheque_Process}) {
	var fulfilment = new sforce.SObject(&quot;cec_fulfilment__c&quot;); 
	fulfilment.Id = &apos;{!cec_Fulfilment__c.Id}&apos;; 
	fulfilment.Brand__c = &apos;{!JSENCODE(cec_Fulfilment__c.CaseBrand__c)}&apos;; 
	fulfilment.Case_Product_Category__c = &apos;{!JSENCODE(cec_Fulfilment__c.Case_Product_Category__c)}&apos;;
	fulfilment.Cheque_Number__c = &apos;{!cec_Fulfilment__c.Cheque_Number__c}&apos;;

	fulfilment.Country__c = &apos;{!cec_Fulfilment__c.Country__c}&apos;; 
	 
	//alert(&apos;my alert===&apos;+fulfilment.Id + &apos; &apos; + fulfilment.Brand__c + &apos; &apos; +fulfilment.Country__c + &apos; &apos;+ fulfilment.Case_Product_Category__c); 

	if (fulfilment.Cheque_Number__c != &quot;&quot;){
		alert(&apos;{!$Label.CEC_ChequeBook_ChequeNo_Already_Added}&apos;);
	}	
	else if (fulfilment.Id ==&apos;&apos; || fulfilment.Brand__c== &apos;&apos; || fulfilment.Country__c == &apos;&apos; || fulfilment.Case_Product_Category__c == &apos;&apos;){
		alert(&apos;{!$Label.CEC_ChequeBook_input_fields_missing}&apos;);
	} else  {
		var result =sforce.apex.execute(&quot;CEC_ChequeProcess&quot;,&quot;generateChequeNumber&quot;,{fulfilmentId:fulfilment.Id});
	//alert (&apos;my result===&apos;+result);
		if (result[0].status == &apos;S&apos;) { 
			window.location.reload(); 
		} else if (result[0].status == &apos;E&apos;) { 
			alert(result[0].message);
		} 
	}
} else {
    alert(&apos;{!$Label.CEC_NoAccessForChequeProcess}&apos;);
}</url>
