        <fullName>Add_Cheque_Details</fullName>
        <availability>online</availability>
        <description>CEC_Custom Add Cheque_Details button.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Cheque Details</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}

sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 

var fulfilment = new sforce.SObject(&quot;cec_fulfilment__c&quot;); 
fulfilment.Id = &apos;{!cec_Fulfilment__c.Id}&apos;; 
fulfilment.Brand__c = &apos;{!JSENCODE(cec_Fulfilment__c.CaseBrand__c)}&apos;; 
fulfilment.Case_Product_Category__c = &apos;{!JSENCODE(cec_Fulfilment__c.Case_Product_Category__c)}&apos;;

fulfilment.Country__c = &apos;{!cec_Fulfilment__c.Country__c}&apos;; 
 
//alert(&apos;my alert===&apos;+fulfilment.Id + &apos; &apos; + fulfilment.Brand__c + &apos; &apos; +fulfilment.Country__c + &apos; &apos;+ fulfilment.Case_Product_Category__c); 

if (fulfilment.Id ==&apos;&apos; || fulfilment.Brand__c== &apos;&apos; || fulfilment.Country__c == &apos;&apos; || fulfilment.Case_Product_Category__c == &apos;&apos;){
	alert(&apos;{!$Label.CEC_ChequeBook_input_fields_missing}&apos;);
} else  {
	var result =sforce.apex.execute(&quot;CEC_ChequeProcess&quot;,&quot;generateChequeNumber&quot;,{fulfilmentId:fulfilment.Id});
//alert (&apos;my result===&apos;+result);
	if (result[0].status == &apos;S&apos;) { 
		window.location.reload(); 
	} else if (result[0].status == &apos;E&apos;) { 
		alert(result[0].message);
	} 
}</url>
    </webLinks>
    <webLinks>
        <fullName>New_Fulfilment</fullName>
        <availability>online</availability>
        <description>CEC_Custom New fulfilment button</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Fulfilment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}

sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;
var caseQuery = sforce.connection.query(&quot;Select Id,AccountId,Account.PersonMailingCity,Account.PersonMailingCountry,Case_Brand__c, Account.PersonMailingPostalCode, Account.PersonMailingState, Account.PersonMailingStreet,Account.Salutation, Account.Name From Case Where Id = &apos;{!Case.Id}&apos;&quot;); 

var fulfilmentId;
var fulfilmentName;
var records = caseQuery.getArray(&quot;records&quot;); 
var caseRecord = records[0]; 


if(caseRecord != null &amp;&amp; caseRecord.AccountId == null) {
	alert(&apos;{!$Label.CEC_CaseAccountMissing}&apos;);
} else if (caseRecord != null &amp;&amp; 
caseRecord.Account.PersonMailingCity == null &amp;&amp; caseRecord.Account.PersonMailingCountry == null &amp;&amp; caseRecord.Account.PersonMailingPostalCode == null &amp;&amp; caseRecord.Account.PersonMailingState == null &amp;&amp; caseRecord.Account.PersonMailingStreet == null) {
	alert(&apos;{!$Label.CEC_CaseAccountPersonDetailsError}&apos;);
} else {
	var newFulfilment = new sforce.SObject(&quot;cec_Fulfilment__c&quot;);
	newFulfilment.Case__c = caseRecord.Id;
	newFulfilment.Country__c = caseRecord.Account.PersonMailingCountry;
        //var result =sforce.apex.execute(&quot;CEC_ChequeProcess&quot;,&quot;checkFieldAccess&quot;,{sObjectName:&apos;cec_fulfilment__c&apos;,sFieldName:&apos;Payable_TO__c&apos;}); 

        var objFulfilment= sforce.connection.describeSObject(&quot;cec_fulfilment__c&quot;); 
        var result = &apos;false&apos;; 
        var field = &apos;&apos;;
        for (var i=0; i&lt;objFulfilment.fields.length; i++) { 
            field = objFulfilment.fields[i]; 
            if(field.name == &apos;Payable_To__c&apos;) { 
                result= &apos;true&apos;; 
                break;
            } 
        }


        if (result == &apos;true&apos; &amp;&amp; caseRecord.Account.Salutation != null){
	        newFulfilment.Payable_To__c = caseRecord.Account.Salutation + &quot; &quot;+ caseRecord.Account.Name;
	} else if(result == &apos;true&apos;){
		newFulfilment.Payable_To__c = caseRecord.Account.Name;
	}
	
	sforce.connection.create([newFulfilment], success);
}

function success(result) {
	fulfilmentId = result[0].id;
	fulfilmentName = result[0].Name;
	sforce.console.getFocusedPrimaryTabId(openSubtab);
}

var openSubtab = function openSubtab(result) {
	//Now that we have the primary tab ID, we can open a new subtab in it
	var primaryTabId = result.id;
        sforce.console.refreshPrimaryTabById(primaryTabId, false);
	sforce.console.openSubtab(primaryTabId, &apos;/&apos;+fulfilmentId, true, fulfilmentName,null);
};</url>
