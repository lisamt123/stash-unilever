    <fields>
        <fullName>AP_of_TO_Difference__c</fullName>
        <description>Advertising and Promotions percent of turnover difference between regional and local misalignment.</description>
        <externalId>false</externalId>
        <formula>IF(AP_of_TO_Regional__c &lt;&gt; 0, 
  IF((ABS((AP_of_TO_Regional__c - AP_of_TO_Local__c)/AP_of_TO_Regional__c)) &gt; 0.05, 
    ABS(AP_of_TO_Regional__c - AP_of_TO_Local__c), 
    0
  ), 
  ABS(AP_of_TO_Regional__c - AP_of_TO_Local__c)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>A&amp;P (% of T/O) Difference</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>AP_of_TO_Global__c</fullName>
        <description>Advertising and Promotions percent of turnover value when project Span was global</description>
        <externalId>false</externalId>
        <formula>IF(Turnover_Global__c &gt; 0,  Advertising_Promotions_Global__c / Turnover_Global__c, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>A&amp;P (% of T/O) Global</label>
        <precision>18</precision>
