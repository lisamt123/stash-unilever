if( permissionSetName == &quot;CPA_ADM&quot;){


	var isValidateFlag = sforce.apex.execute(&quot;CPA_ContractInformationController&quot;,&quot;checkIsValidated&quot;,{objId:obj.id}); 
	if(isValidateFlag == &apos;true&apos;){ 
	if(obj.CNF_Status__c == &quot;Returned&quot;){
	obj.CNF_Status__c = &quot;Resubmitted&quot;; 
	result = sforce.connection.update([obj]); 
	//alert(result );
	location.reload(true); 

	} 
	else if(obj.CNF_Status__c ==&quot;Submitted&quot; || obj.CNF_Status__c == &quot;Resubmitted&quot; ){ 
	alert(&quot;CNF is already submitted&quot;); 
	} 
	else if(obj.CNF_Status__c ==&quot;Cancelled&quot; ){ 
