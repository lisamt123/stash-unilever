{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

var obj = new sforce.SObject(&apos;CPA_project_work_order__c&apos;); 
obj.id = &quot;{!CPA_project_work_order__c.Id}&quot;; 
obj.CNF_Status__c = &quot;{!CPA_project_work_order__c.CNF_Status__c}&quot;;


soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos;&quot;;
