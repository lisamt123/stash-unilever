        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}

var p = new sforce.SObject(&apos;CPA_project_work_order__c&apos;); 
p.id = &quot;{!CPA_project_work_order__c.Id}&quot;; 
p.chk_isStandalone_PWO__c = &quot;{!CPA_project_work_order__c.chk_isStandalone_PWO__c }&quot;; 
p.pkl_Status__c = &quot;{!CPA_project_work_order__c.pkl_Status__c }&quot;; 
PWORF_ID = &quot;{!CPA_project_work_order__c.PWORF_ID__c}&quot;;


soql = &quot;SELECT Id,Assignee.Name, PermissionSet.Name FROM PermissionSetAssignment WHERE AssigneeId = &apos;{!$User.Id}&apos;&quot;;
var result = sforce.connection.query(soql);
var records1 = result.getArray(&apos;records&apos;);
var permissionSetName = records1[0].PermissionSet.Name;
//alert(&quot;permissionSetName -&gt;&quot; + permissionSetName);
//alert(&quot;p.chk_isStandalone_PWO__c&gt;&gt; &quot;+p.chk_isStandalone_PWO__c);

if((permissionSetName == &quot;CPA_ADM&quot; &amp;&amp; p.chk_isStandalone_PWO__c != 1)||(permissionSetName == &quot;CPA_SMT&quot; &amp;&amp; p.chk_isStandalone_PWO__c == 1)){ 


	var parentFlag = &apos;true&apos;;
	if(p.chk_isStandalone_PWO__c != 1){
		soql = &quot;select Id,pkl_Status__c from CPA_PWORF__c where name =&apos;&quot;+PWORF_ID+&quot;&apos;&quot;;
		//alert(soql);;
		result = sforce.connection.query(soql); 
		var recPworf = result.getArray(&quot;records&quot;); 
		if(recPworf != null &amp;&amp; recPworf.length &gt; 0){ 
			if(recPworf[0].pkl_Status__c == &apos;Returned&apos; || recPworf[0].pkl_Status__c == &apos;Withhold&apos; || recPworf[0].pkl_Status__c == &apos;Cancelled&apos;){
				parentFlag = &apos;false&apos;;
			}
		}	
	}
	//alert(parentFlag);
	
	if(parentFlag == &apos;true&apos;){	
		//alert(&quot;in parentFlag :&quot;+parentFlag);	
		var isValidateFlag = sforce.apex.execute(&quot;CPA_ContractInformationController&quot;,&quot;checkIsValidated&quot;,{objId:p.id});
		if(isValidateFlag == &apos;true&apos;){ 

			if(p.pkl_Status__c == &quot;Withhold&quot; || p.pkl_Status__c == &quot;Returned&quot; || p.pkl_Status__c == &quot;Request For Termination&quot;){ 
				p.pkl_Status__c = &quot;Resubmitted&quot;; 
				result = sforce.connection.update([p]); 
				location.reload(true); 

			} 	else if(p.pkl_Status__c ==&quot;Saved&quot;){ 
				p.pkl_Status__c = &quot;Submitted&quot;; 
				result = sforce.connection.update([p]); 
				//alert(result);
				location.reload(true); 
			} 
			else if(p.pkl_Status__c ==&quot;Submitted&quot; || p.pkl_Status__c == &quot;Resubmitted&quot; ){ 
				alert(&quot;PWO is already submitted&quot;); 
			} 
			else if(p.pkl_Status__c == &quot;Delivered&quot;){ 
				alert(&quot;PWO cannot be Submitted as it is already Delivered&quot;); 
			}
			else if(p.pkl_Status__c == &quot;Termination Requested&quot; || p.pkl_Status__c == &quot;Sent For Termination&quot; || p.pkl_Status__c == &quot;Terminated&quot;){ 
				alert(&quot;PWO cannot be submitted as it is Terminated/Sent For Termination/Termination Requested&quot;); 
			}
			else	if(p.pkl_Status__c == &quot;Signed&quot;){ 
				alert(&quot;PWO cannot be submitted as it is already Signed&quot;); 
			}
			else	if(p.pkl_Status__c == &quot;Sent for Signature&quot;){ 
				alert(&quot;PWO cannot be submitted as it is already Sent for Signature&quot;); 
			}
			else if(p.pkl_Status__c == &quot;Sent For Closure&quot; || p.pkl_Status__c == &quot;Closed&quot;){ 
				alert(&quot;PWO cannot be submitted as it is already Closed/Sent For Closure&quot;); 
			}
			else	if(p.pkl_Status__c == &quot;Cancelled&quot;){ 
				alert(&quot;PWO cannot be submitted as it is already Cancelled&quot;); 
			}
			else	if(p.pkl_Status__c == &quot;Accepted&quot;){ 
				alert(&quot;PWO cannot be submitted as it is already Accepted&quot;); 
			}else{
				alert(&quot;PWO cannot be submitted, Concern Administrator&quot;); 
			}
		} 
		else { 
			alert(&quot;PWO cannot be submitted, as Required fields are missing.&quot;); 
		}
	}else{
		alert(&quot;PWO cannot be submitted, as associated PWORF is in Returned/Withhold/Cancelled status.&quot;); 
	}
}else{
alert(&quot;You are not authorized to perform this action&quot;);
}</url>
