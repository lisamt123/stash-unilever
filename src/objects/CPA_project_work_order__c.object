	else if(obj.CNF_Status__c ==&quot;Sent for Signature&quot; || obj.CNF_Status__c == &quot;Signed&quot; || obj.CNF_Status__c == &quot;Delivered&quot;){
			alert(&quot;CNF cant be submitted for Approval as it is Accepted and moved for further processing&quot;); 
		}
	else{ 
	obj.CNF_Status__c = &quot;Submitted&quot;; 
	result = sforce.connection.update([obj]); 
	//alert(result );
	location.reload(true); 
	} 

	} 
	else { 
	alert(&quot;CNF cannot be submitted, Due to Required fields are missing&quot;); 
	}
	
