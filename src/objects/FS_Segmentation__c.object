    <fields>
        <fullName>FS_FoodPriceValue__c</fullName>
        <description>Calculating based on Turnover and Food cost</description>
        <externalId>false</externalId>
        <formula>FS_FoodTurnover__c  *  FS_FoodCost__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Food Price Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>FS_FoodTurnover__c</fullName>
        <description>Created for calculating food turnover based of week per year,days per week,meal per day and average meal price</description>
        <externalId>false</externalId>
        <formula>(FS_DaysPerWeek__c  *  FS_WeeksPerYear__c  *  FS_MealsPerDay__c  *  FS_AveragePricePerMeal__c)/66.66265</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Food Turnover</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
