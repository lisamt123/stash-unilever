        <fullName>FS_FinalPotential__c</fullName>
        <description>Created for calculating final potential based on Potential UFS Share Of Food Cost Euro and Convenience Factor  (ABC Segmentation)</description>
        <externalId>false</externalId>
        <formula>(FS_PotentialUFSShareOfFoodCostEuro__c  *  FS_ConvenienceFactor__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Final Potential</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
