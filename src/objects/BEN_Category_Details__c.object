    <validationRules>
        <fullName>BEN_CUC_Code_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK( lkp_CS_GTIN__c ), ISBLANK( txt_CUC_Code__c ) , ISBLANK( lkp_CU_GTIN__c )  )</errorConditionFormula>
        <errorMessage>Kindly add receipe on at-least one aspects from CU-GTIN,CS-GTIN or CUC Code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BEN_CUC_Validation</fullName>
        <active>true</active>
        <description>To validate CU_GTN and CS_GTN against Product Category</description>
        <errorConditionFormula>OR
(
IF(NOT(ISBLANK(lkp_CU_GTIN__c)) ,IF(lkp_CU_GTIN__r.lkp_Product_Category__r.Name == Product_Category__r.Name ,false,true),false),
IF(NOT(ISBLANK(lkp_CS_GTIN__c)) ,IF(lkp_CS_GTIN__r.lkp_Product_Category__r.Name == Product_Category__r.Name ,false,true),false)
)</errorConditionFormula>
        <errorMessage>CU_GTN or CS_GTN Value should be Same as Product Category</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BEN_Claim_Mandatory</fullName>
        <active>true</active>
        <description>Claim is mandatory to select if &apos;&apos;Launched Status&quot; is &apos;will launch&apos;.</description>
        <errorConditionFormula>TEXT(pkl_Launch_Not_Launched_information__c) == &apos;Will launch&apos; &amp;&amp;  ((ISPICKVAL ( pkl_Claims__c , &quot;&quot;)) || (ISPICKVAL(pkl_Claims__c , &quot;NONE&quot;)))</errorConditionFormula>
        <errorDisplayField>pkl_Claims__c</errorDisplayField>
        <errorMessage>Claim is mandatory as &apos;Launched Status&apos; is &apos;will launch&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BEN_Deletion_Reason_Mandatory</fullName>
        <active>true</active>
        <description>When the record is marked for deletion, reason has to be entered</description>
        <errorConditionFormula>AND( bln_Mark_for_Deletion__c=True, ISBLANK( txt_Reason_for_Deletion__c))</errorConditionFormula>
        <errorDisplayField>txt_Reason_for_Deletion__c</errorDisplayField>
        <errorMessage>Please enter the Reason for Deletion</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BEN_Key_Attribute_Mandatory</fullName>
        <active>true</active>
        <description>Validation rule to make any 2 key attributes mandatory out of 3</description>
        <errorConditionFormula>OR(AND(ISPICKVAL(pkl_Key_Attribute_1_Appraisal__c,&quot;&quot;) ,ISPICKVAL(pkl_Key_Attribute_2_Consumer_Test__c,&quot;&quot;)) ,AND(ISPICKVAL(pkl_Key_Attribute_2_Consumer_Test__c,&quot;&quot;),ISPICKVAL(pkl_Key_Attribute_3_Sensory__c,&quot;&quot;))
,AND(ISPICKVAL(pkl_Key_Attribute_3_Sensory__c,&quot;&quot;) ,ISPICKVAL(pkl_Key_Attribute_1_Appraisal__c,&quot;&quot;)))</errorConditionFormula>
        <errorMessage>Kindly add brand&apos;s performance on at-least on two aspects from Appraisal, Sensory or Consumer Test</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BEN_Mandate_Fields_post_approval</fullName>
        <active>true</active>
        <description>Mandate_Fields_post_approval</description>
        <errorConditionFormula>AND(ISPICKVAL( pkl_Status__c,&apos;Approved&apos;),
   ( OR( ISCHANGED( Product_Category__c ), 
         ISCHANGED( pkl_Year__c ), 
         ISCHANGED( pkl_Quarter__c )
        )
    )
    )</errorConditionFormula>
        <errorMessage>Product Category OR Year OR Quarter cannot be modified once the test results are Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BEN_MarginMandatory</fullName>
        <active>true</active>
        <description>Margin is mandatory to select if &apos;&apos;Launched Status&quot; is &apos;will launch&apos;.</description>
        <errorConditionFormula>TEXT(pkl_Launch_Not_Launched_information__c) == &apos;Will launch&apos; &amp;&amp;  ((ISPICKVAL ( pkl_Margin__c , &quot;&quot;)) || (ISPICKVAL(pkl_Margin__c , &quot;NONE&quot;)))</errorConditionFormula>
        <errorDisplayField>pkl_Margin__c</errorDisplayField>
        <errorMessage>Margin is mandatory as &apos;Launched Status&apos; is &apos;will launch&apos;</errorMessage>
    </validationRules>
