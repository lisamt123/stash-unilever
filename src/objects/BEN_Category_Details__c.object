    <fields>
        <fullName>fml_Overall_Score__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(pkl_Logic__c,&apos;WIN on either OO or KA; Parity on other&apos;),(IF(OR(ISPICKVAL(pkl_Blind_OO__c,&apos;Loss&apos;),
ISPICKVAL(pkl_Key_Attributes__c,&apos;Loss&apos;)),
&apos;Loss&apos;,
IF(OR(ISPICKVAL(pkl_Blind_OO__c,&apos;Win&apos;),
ISPICKVAL(pkl_Key_Attributes__c,&apos;Win&apos;)),
&apos;Win&apos;,
IF(AND(ISPICKVAL(pkl_Blind_OO__c,&apos;Parity&apos;),
ISPICKVAL(pkl_Key_Attributes__c,&apos;Parity&apos;)),
&apos;Parity&apos;,null)
)
)),(IF(ISPICKVAL(pkl_Logic__c,&apos;WIN on OO &amp; KA&apos;),(IF(OR(ISPICKVAL(pkl_Blind_OO__c,&apos;Loss&apos;),
ISPICKVAL(pkl_Key_Attributes__c,&apos;Loss&apos;)),
&apos;Loss&apos;,
IF(OR(ISPICKVAL(pkl_Blind_OO__c,&apos;Parity&apos;),
ISPICKVAL(pkl_Key_Attributes__c,&apos;Parity&apos;)),
&apos;Parity&apos;,
IF(AND(ISPICKVAL(pkl_Blind_OO__c,&apos;Win&apos;),
ISPICKVAL(pkl_Key_Attributes__c,&apos;Win&apos;)),
&apos;Win&apos;,null)
)
)),(IF(ISPICKVAL(pkl_Logic__c,&apos;PARITY on OO &amp; KA&apos;),(IF(OR(ISPICKVAL(pkl_Blind_OO__c,&apos;Loss&apos;),
ISPICKVAL(pkl_Key_Attributes__c,&apos;Loss&apos;)),
&apos;Loss&apos;,
IF(AND(NOT(ISPICKVAL(pkl_Blind_OO__c,&apos;&apos;)),
NOT(ISPICKVAL(pkl_Key_Attributes__c,&apos;&apos;))),
&apos;Win&apos;,null)
)),null)))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Overall Score</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
