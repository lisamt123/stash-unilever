    <fields>
        <fullName>Reporting_Period__c</fullName>
        <description>Formula field based upon the Value captured in the Frequency and Value Fields on the Report Object.
This value will be displayed on the Report Tile.</description>
        <externalId>false</externalId>
        <formula>CASE(TEXT(Frequency__c), 
&quot;Yearly&quot;,    &quot;Year - &quot;   &amp; TEXT(Value__c), 
&quot;Monthly&quot;,   &quot;Month - &quot;  &amp; TEXT(Value__c),
&quot;Quarterly&quot;, &quot;Q&quot;         &amp; LEFT(TEXT(Value__c), 1)  &amp; &quot;-&quot; &amp; RIGHT(TEXT(Value__c), 4)  ,
&quot;BI-Weekly&quot;, &quot;Week - &quot;   &amp; TEXT(Value__c),
&quot;Weekly&quot;,    &quot;Week - &quot;   &amp; TEXT(Value__c),
&quot;Daily&quot;,     LEFT(TEXT(Value__c), 4) &amp; &quot;-&quot; &amp;     
             Left( RIGHT(TEXT(Value__c), 4) ,2) &amp; &quot;-&quot; &amp; 
             RIGHT(TEXT(Value__c), 2) ,
&quot;Other&quot;,     LEFT(TEXT(Value__c), 4) &amp; &quot;-&quot; &amp;     
             Left( RIGHT(TEXT(Value__c), 4) ,2) &amp; &quot;-&quot; &amp; 
             RIGHT(TEXT(Value__c), 2) ,
&quot;Period&quot;,    &quot;P&quot; &amp; RIGHT(TEXT(Value__c), 2) &amp; &quot;-&quot; &amp; LEFT(TEXT(Value__c), 4),
&quot;Dynamic&quot;,   &quot;Dynamic&quot;,
&quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Derived value based upon the Frequency and Value Fields on the Report , for display on the Report Tile.</inlineHelpText>
        <label>Reporting Period</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
