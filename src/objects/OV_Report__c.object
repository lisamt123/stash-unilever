    <validationRules>
        <fullName>Frequency_is_Dynamic</fullName>
        <active>true</active>
        <description>Validation for where Frequency is Dynamic the Value is not required</description>
        <errorConditionFormula>ISPICKVAL( Frequency__c , &quot;Dynamic&quot;)
  &amp;&amp;  NOT(ISBLANK(TEXT(Value__c )) )</errorConditionFormula>
        <errorMessage>Frequency is Dynamic. Please remove Value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_is_a_Day_Other</fullName>
        <active>true</active>
        <description>Validation for where Frequency is Daily or Other  the Value is  8 digits</description>
        <errorConditionFormula>( ISPICKVAL( Frequency__c , &quot;Daily&quot;)  || 
  ISPICKVAL( Frequency__c , &quot;Other&quot;))

&amp;&amp; ((LEN( TEXT(Value__c )) &lt;&gt; 8 ) 

|| ((VALUE((RIGHT( TEXT(Value__c ) , 2))) &lt; 1 ) 

|| (VALUE((RIGHT( TEXT(Value__c ) , 2))) &gt; 31 )
|| (VALUE(  LEFT((RIGHT( TEXT(Value__c ) , 4)),2)) &gt; 12 )
|| (VALUE(  LEFT((RIGHT( TEXT(Value__c ) , 4)),2)) &lt; 01 ) )

)</errorConditionFormula>
        <errorMessage>Frequency is Daily or Other. Please enter an 8 digit Date in the following format YYYYMMDD e.g. 20151118 = 2015-11-18
Values: Month 01  - 12 , Day 01 -31</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_is_a_Month</fullName>
        <active>true</active>
        <description>Validation for where Frequency is Monthly the Value is in range  1 - 12</description>
        <errorConditionFormula>( ISPICKVAL( Frequency__c , &quot;Monthly&quot;)
  &amp;&amp; ISBLANK(TEXT(Value__c )) )
 || 
( ISPICKVAL( Frequency__c , &quot;Monthly&quot;)
  &amp;&amp; ((LEN( TEXT(Value__c ))   &gt;  2 )
  ||   ABS(Value__c)  &lt;   1 
  ||    ABS(Value__c)  &gt;   12 ))</errorConditionFormula>
        <errorMessage>Frequency is Monthly. Please enter a 2 digit Month value between 1 and 12</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_is_a_Period</fullName>
        <active>true</active>
        <description>Ensure the Value field is a Period and Year where the Frequency is set to Period</description>
        <errorConditionFormula>ISPICKVAL( Frequency__c , &quot;Period&quot;) 

  &amp;&amp; ((LEN( TEXT(Value__c ))  &lt;&gt; 6 )

 ||   ((VALUE((RIGHT( TEXT(Value__c ) , 2))) &lt; 1 )

 ||   (VALUE((RIGHT( TEXT(Value__c ) , 2))) &gt; 13 ))

)</errorConditionFormula>
        <errorMessage>Frequency is Period. Please enter a 4 digit Year and 2 digit Period e.g. 201513.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_is_a_Quarter</fullName>
        <active>true</active>
        <description>Validation for where Frequency is Quarterly the Value is in range  1 - 4 + Year</description>
        <errorConditionFormula>( ISPICKVAL( Frequency__c , &quot;Quarterly&quot;)
  &amp;&amp; ISBLANK(TEXT(Value__c )) )
 || 

(ISPICKVAL( Frequency__c , &quot;Quarterly&quot;)
  &amp;&amp; ((LEN( TEXT(Value__c ))   &lt;  5 )
  ||   VALUE( LEFT(TEXT(Value__c ), 1) )  &lt;   1 
 ||    VALUE( LEFT(TEXT(Value__c ), 1) )  &gt;   4 ))</errorConditionFormula>
        <errorMessage>Frequency is Quarterly. Please enter a 1 digit value for Quarter between 1 and 4, and 4 digit value for Year.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_is_a_Week</fullName>
        <active>true</active>
        <description>Validation for where Frequency is Weekly or BI Weekly the Value is in range  1 - 52</description>
        <errorConditionFormula>(( ISPICKVAL( Frequency__c , &quot;Weekly&quot;)  || 
   ISPICKVAL( Frequency__c , &quot;BI-Weekly&quot;) )
  &amp;&amp; ((LEN( TEXT(Value__c ))   &lt;  1 )))

 || 

(( ISPICKVAL( Frequency__c , &quot;Weekly&quot;)  || 
   ISPICKVAL( Frequency__c , &quot;BI-Weekly&quot;) )
  &amp;&amp; ((LEN( TEXT(Value__c ))   &gt;  2 )
  ||   ABS(Value__c)  &lt;   1 
  ||    ABS(Value__c)  &gt;   52 ))</errorConditionFormula>
        <errorMessage>Frequency is Weekly/BI-Weekly. Please enter a 2 digit Week value between 1 and 52</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_is_a_Year</fullName>
        <active>true</active>
        <description>Ensure the Value field is a Year where the Frequency is set to Yearly</description>
        <errorConditionFormula>ISPICKVAL( Frequency__c , &quot;Yearly&quot;) 

  &amp;&amp; ((LEN( TEXT(Value__c ))  &lt;&gt; 4 )
  ||   (ABS(Value__c)  &lt;   (YEAR( TODAY())-5 )
  ||   ABS(Value__c)  &gt;    (YEAR( TODAY())+1 )))</errorConditionFormula>
        <errorMessage>Frequency is Yearly. Please enter a 4 digit Year e.g. 2015.</errorMessage>
    </validationRules>
</CustomObject>
