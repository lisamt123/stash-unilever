    f.fieldsToNull = [];
    f.fieldsToNull.push(&quot;Feed_ID__c&quot;,&quot;Feed_URL__c&quot;,&quot;Feed_Thumbnail_URL__c&quot;);
    var result = sforce.connection.update([f]);
    location.reload();
} catch (e) {
