        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>OOH_Address__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Address</label>
        <referenceTo>OOH_Address__c</referenceTo>
        <relationshipLabel>Point of Sale Outlets</relationshipLabel>
        <relationshipName>Point_of_Sale_Outlets</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>OOH_Chain_outlet_flag__c</fullName>
        <externalId>false</externalId>
        <formula>if(OR(ISNULL(OOH_POS_Outlet_chain__c ),ISBLANK(OOH_POS_Outlet_chain__c )),false,true)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Chain outlet flag</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>OOH_Channel__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Channel</label>
        <referenceTo>OOH_Channel__c</referenceTo>
        <relationshipLabel>Point of Sale Outlets</relationshipLabel>
        <relationshipName>Point_of_Sale_Outlets</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>OOH_Delayed_Activation__c</fullName>
        <externalId>false</externalId>
        <formula>IF( OOH_Activition_Date__c + 386 &lt; TODAY(), true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Delayed Activation</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>OOH_GSVLostFlag__c</fullName>
