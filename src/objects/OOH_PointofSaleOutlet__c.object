        <externalId>false</externalId>
        <label>GSV Lost Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>OOH_ImageDocumentID__c</fullName>
        <externalId>false</externalId>
        <label>Image Document ID</label>
        <length>20</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OOH_Image__c</fullName>
        <externalId>false</externalId>
        <formula>IMAGE(&quot;/servlet/servlet.FileDownload?file=&quot;+OOH_ImageDocumentID__c,&quot;Point of Sale Image&quot; , 100, 60)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Image</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OOH_Indicator__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Agg_Gsv__c  &gt;= 400, &apos;High&apos;,  IF( AND(Agg_Gsv__c  &lt; 400,Agg_Gsv__c &gt;= 1), &apos;Low&apos;, &apos;New&apos;) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Indicator</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OOH_Is_New__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
