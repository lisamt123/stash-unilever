        <externalId>false</externalId>
        <formula>IF(Agg_Gsv__c &gt;= 400,true,false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Performence</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Upcoming_Activations__c</fullName>
        <externalId>false</externalId>
        <formula>if(AND( (OOH_Activition_Date__c + 386) &lt;= (TODAY() + 7),(OOH_Activition_Date__c + 386) &gt;= TODAY()  ),true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Upcoming Activations</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
