    <fields>
        <fullName>Agg_Gsv__c</fullName>
        <externalId>false</externalId>
        <label>Agg Gsv</label>
        <summarizedField>OOH_SellOut__c.OOH_GSV__c</summarizedField>
        <summaryForeignKey>OOH_SellOut__c.OOH_PointOFSaleOutlet__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>No_of_Days__c</fullName>
        <externalId>false</externalId>
        <formula>TODAY() - OOH_Activition_Date__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>No of Days</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OOH_Activition_Date__c</fullName>
