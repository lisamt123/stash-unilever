    <validationRules>
        <fullName>BFM_POD_Definitive_with_Validation_Error</fullName>
        <active>true</active>
        <errorConditionFormula>POD_Definitive__c = true &amp;&amp;  POD_Link_Check_Status__c  != &apos;Link Ok&apos; &amp;&amp;  RecordType.Name =&apos;POD&apos;</errorConditionFormula>
        <errorMessage>POD cannot be Definitive without passing Validations</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BFM_POD_Invoice_Posted_Edit_Err</fullName>
        <active>false</active>
        <errorConditionFormula>!ISNEW() &amp;&amp; (UPPER(PRIORVALUE( POD_Status__c )) = &apos;INVOICE POSTED&apos; || UPPER( NF_e__r.CT_e__r.Cte_Status__c ) = &apos;INVOICE POSTED&apos; || UPPER( NF_e__r.NFS__r.NFS_Status__c  ) = &apos;INVOICE POSTED&apos;)
&amp;&amp; $Profile.Name != &apos;System Administrator&apos;</errorConditionFormula>
        <errorMessage>Record is not editable if status is Invoice Posted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BFM_POD_and_NFe_Date_Check</fullName>
        <active>true</active>
        <description>NF-e date should be previous to POD date</description>
        <errorConditionFormula>IF(NF_e__r.NF_e_creation_date__c &gt; POD_Date__c,true,false )</errorConditionFormula>
        <errorDisplayField>POD_Date__c</errorDisplayField>
        <errorMessage>NF-e date should be previous to POD date</errorMessage>
    </validationRules>
