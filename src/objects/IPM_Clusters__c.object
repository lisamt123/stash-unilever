    <validationRules>
        <fullName>IPM_Cluster_Name_length</fullName>
        <active>true</active>
        <description>Cluster name should not more than 14 characters</description>
        <errorConditionFormula>if( LEN(Name)&gt;14,TRUE,FALSE )</errorConditionFormula>
        <errorMessage>IPM Project Cluster Name can not be more the 14 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IPM_Special_Characters_not_allowed</fullName>
        <active>true</active>
        <description>system should not allow any of the following special characters : _!@#$%^&amp;*()+=-][;&apos;/.,\ 
system should allow spaces in the name. 
system should not allow underscores.</description>
        <errorConditionFormula>NOT(REGEX(Name,&apos;([A-Z, ]+)&apos;))</errorConditionFormula>
        <errorMessage>Special characters are not allowed/Project Cluster name should always be captured in Capitals</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IPM_Unique_Cluster_Name</fullName>
        <active>false</active>
        <description>Only Unique cluster name is allowed in the system</description>
        <errorConditionFormula>AND(CONTAINS(VLOOKUP( $ObjectType.IPM_Clusters__c.Fields.Name , $ObjectType.IPM_Clusters__c.Fields.Name, Name), Name), OR(ISNEW(), ISCHANGED(Name)) )</errorConditionFormula>
        <errorMessage>Project Cluster Name already exist. Please create a Project Cluster with different Name</errorMessage>
    </validationRules>
