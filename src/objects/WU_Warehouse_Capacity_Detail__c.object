<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <enableActivities>true</enableActivities>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>CommitedPalletFromMaster__c</fullName>
        <formula>Master_Warehouse__r.WU_Committed_Max_PalletStorage__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>CommitedPalletFromMaster</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>Is_In_Current_Month__c</fullName>
        <description>Returns whether a warehouse detail record falls in current month based on Utilization Start and End date.</description>
        <formula>IF(

(WU_Utilization_Start_Date__c &gt;=
DATE( YEAR( TODAY()), MONTH ( TODAY()), 1 )
)
&amp;&amp;
(
WU_Utilization_End_Date__c &lt;= IF( MONTH( TODAY()) = 12,  
DATE( YEAR( TODAY()), 12, 31 ),  
DATE( YEAR( TODAY()), MONTH ( TODAY()) + 1, 1 ) - 1)
),
true,
false
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is In Current Month</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Master_Warehouse__c</fullName>
        <label>Master Warehouse</label>
        <referenceTo>WU_Master_Warehouse__c</referenceTo>
        <relationshipLabel>Warehouse Utilization History</relationshipLabel>
        <relationshipName>Warehouse_Capacity_Utilizaion</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <type>MasterDetail</type>
    </fields>
    <fields>
        <fullName>Next_Update_Date__c</fullName>
        <formula>DATE(YEAR(WU_Utilization_Start_Date__c),MONTH(WU_Utilization_Start_Date__c),1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Next Update Date</label>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Test_Time_Based_WF_Checkbox__c</fullName>
        <formula>IF(Now() &lt; Test_Time_Based_WF__c, true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Test Time Based WF Checkbox</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Test_Time_Based_WF__c</fullName>
        <label>Test Time Based WF</label>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>WFUpdateCheckbox__c</fullName>
        <defaultValue>false</defaultValue>
        <label>WFUpdateCheckbox</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>WU_Committed_Max_PalletStorage__c</fullName>
        <label>Committed/Max PalletStorage</label>
        <precision>18</precision>
        <scale>0</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>WU_CurrentMonth__c</fullName>
        <label>Current Month</label>
        <length>10</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>WU_Current_Pallet_Utilization_Week__c</fullName>
        <formula>IF( WU_Committed_Max_PalletStorage__c = 0,
    0,
    ( WU_Current_Pallet_Utilization__c / WU_Committed_Max_PalletStorage__c ) * 100)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Average Pallet Utilization (%)</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>WU_Current_Pallet_Utilization__c</fullName>
        <label>Current Pallet Utilization Count</label>
        <precision>18</precision>
        <scale>0</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>WU_No_Of_Days_In_Month__c</fullName>
        <formula>DAY(DATE(YEAR(TODAY()),MONTH(TODAY())+1,1)-1)</formula>
        <label>No. Of Days In Month</label>
        <precision>18</precision>
        <scale>0</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>WU_SAP_Code__c</fullName>
        <formula>Master_Warehouse__r.WU_SAPCode_s__c</formula>
        <label>SAP Code</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>WU_Uncheck_Current_Month__c</fullName>
        <label>Uncheck Current Month</label>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>WU_Utilization_End_Date__c</fullName>
        <label>Utilization End Date</label>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>WU_Utilization_Start_Date__c</fullName>
        <label>Utilization Start Date</label>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>WU_Utilization_Type__c</fullName>
        <label>Utilization Type</label>
        <picklist>
            <picklistValues>
                <fullName>Daily</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Weekly</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Monthly</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <label>Warehouse Capacity Detail</label>
    <nameField>
        <displayFormat>WC{0000}</displayFormat>
        <label>Warehouse Capacity SF Id</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Warehouse Capacity Detail</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>DateValidationForDailyUtilizationType</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( WU_Utilization_Type__c,&apos;Daily&apos; ), WU_Utilization_Start_Date__c  &lt;&gt;  WU_Utilization_End_Date__c )</errorConditionFormula>
        <errorDisplayField>WU_Utilization_Type__c</errorDisplayField>
        <errorMessage>Utilization Start Date and Utilization End Date must be same if Utilization Type has been chosen as Daily.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DateValidationForMonthlyUtilizationType</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( WU_Utilization_Type__c , &apos;Monthly&apos;), OR(WU_Utilization_Start_Date__c = WU_Utilization_End_Date__c, MONTH(WU_Utilization_End_Date__c) &lt;&gt; MONTH(WU_Utilization_Start_Date__c),WU_Utilization_End_Date__c-WU_Utilization_Start_Date__c &lt; 7))</errorConditionFormula>
        <errorDisplayField>WU_Utilization_Type__c</errorDisplayField>
        <errorMessage>Utilization Start Date and Utilization End Date should be for same month for Utilization Type has been chosen as Monthly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DateValidationForWeeklyUtilizationType</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( WU_Utilization_Type__c , &apos;Weekly&apos;), OR(WU_Utilization_Start_Date__c = WU_Utilization_End_Date__c, WU_Utilization_End_Date__c-WU_Utilization_Start_Date__c &gt; 7))</errorConditionFormula>
        <errorDisplayField>WU_Utilization_Type__c</errorDisplayField>
        <errorMessage>Utilization Start Date and Utilization End Date cannot be same or their difference cannot exceed 7 days if Utilization Type has been chosen as Weekly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDate_NotGreaterThan_Enddate</fullName>
        <active>true</active>
        <errorConditionFormula>WU_Utilization_Start_Date__c &gt; WU_Utilization_End_Date__c</errorConditionFormula>
        <errorDisplayField>WU_Utilization_Start_Date__c</errorDisplayField>
        <errorMessage>Utilization Start Date should not be greater than Utilization End Date.</errorMessage>
    </validationRules>
