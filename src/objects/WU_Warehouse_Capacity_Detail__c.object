    <fields>
        <fullName>Is_In_Current_Month__c</fullName>
        <description>Returns whether a warehouse detail record falls in current month based on Utilization Start and End date.</description>
        <externalId>false</externalId>
        <formula>IF(

(WU_Utilization_Start_Date__c &gt;=
DATE( YEAR( TODAY()), MONTH ( TODAY()), 1 )
)
&amp;&amp;
(
WU_Utilization_Start_Date__c &lt;= IF( MONTH( TODAY()) = 12,  
DATE( YEAR( TODAY()), 12, 31 ),  
DATE( YEAR( TODAY()), MONTH ( TODAY()) + 1, 1 ) - 1)
),
true,
false
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is In Current Month</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
