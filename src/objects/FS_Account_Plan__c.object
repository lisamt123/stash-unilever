    <validationRules>
        <fullName>CheckNoofDefineObjective</fullName>
        <active>true</active>
        <description>This validation rule is used to check if account plan record is associated with any defined objective</description>
        <errorConditionFormula>AND(ISPICKVAL(FS_Status__c,&apos;Approval Requested&apos;) ,FS_No_of_Objectives__c =0)</errorConditionFormula>
        <errorMessage>Please create Defined Objectives prior to submission for approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FS_End_Date_Before_Start_Date</fullName>
        <active>true</active>
        <description>It is used to restrict user to put closed date before start date.</description>
        <errorConditionFormula>FS_End_Date__c &lt; FS_Start_Date__c</errorConditionFormula>
        <errorMessage>End date can not be before start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_approved_Account_Plan_change</fullName>
        <active>true</active>
        <description>Sales rep are not allowed to make certain changes in account plan once it is approved</description>
        <errorConditionFormula>And(OR(ISPICKVAL(PriorValue(FS_Status__c),&apos;Approved&apos;),
ISPICKVAL(FS_Status__c,&apos;Approved&apos;)),
OR( ISCHANGED(FS_Status__c), ISCHANGED( FS_Start_Date__c), ISCHANGED(FS_End_Date__c),ISCHANGED(FS_PlanDollars__c), ISCHANGED(FS_PlanPercent__c), ISCHANGED(FS_Plan_Type__c), ISCHANGED( FS_Plan_Statement__c)),$Profile.Name &lt;&gt; &quot;System Administrator&quot;)</errorConditionFormula>
        <errorMessage>You are not allowed to make changes to account plan once it is approved</errorMessage>
    </validationRules>
