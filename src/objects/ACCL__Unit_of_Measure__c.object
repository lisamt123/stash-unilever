    <validationRules>
        <fullName>ACCL__Order_Ability_depend_on_IsOrderUnit_is_c</fullName>
        <active>true</active>
        <errorConditionFormula>ACCL__Is_Order_Unit__c = true  &amp;&amp;  ACCL__Order_Ability__c  &lt;&gt; true</errorConditionFormula>
        <errorDisplayField>ACCL__Order_Ability__c</errorDisplayField>
        <errorMessage>If the &quot;Is Order Unit&quot; is checked the &quot;Order Ability&quot; must be checked as well.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Rounding_Rule_Validation</fullName>
        <active>true</active>
        <description>Validation Rule for detail object &quot;Unit of Measure&quot; that the highest value of Sort (sort__c - integer field) must have in the Picklist field  Rounding_Rule__c the value=„Never“.</description>
        <errorConditionFormula>(ACCL__Product__r.ACCL__Rounding_Rule_Validation__c &lt;=  ACCL__Sort__c  ||  ISBLANK( ACCL__Product__r.ACCL__Rounding_Rule_Validation__c )  )  &amp;&amp;  NOT(ISPICKVAL(ACCL__Rounding_Rule__c, &apos;Never&apos;))</errorConditionFormula>
        <errorMessage>Unit of Measure with the highest value of Sort must have a Rounding Rule „Never“.</errorMessage>
    </validationRules>
