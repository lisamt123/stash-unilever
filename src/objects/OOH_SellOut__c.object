    <fields>
        <fullName>OOH_Week__c</fullName>
        <externalId>false</externalId>
        <formula>Case(
MOD( DATE(YEAR( OOH_SellOutDate__c ), 1, 1) - DATE (1900, 1, 7), 7),
0, ( OOH_SellOutDate__c- DATE(YEAR(OOH_SellOutDate__c),1,1) + 3) /7, 
1, ( OOH_SellOutDate__c- DATE(YEAR(OOH_SellOutDate__c),1,1) + 4) /7, 
2, ( OOH_SellOutDate__c- DATE(YEAR(OOH_SellOutDate__c),1,1) + 5) /7, 
3, ( OOH_SellOutDate__c- DATE(YEAR(OOH_SellOutDate__c),1,1) + 6) /7, 
4, ( OOH_SellOutDate__c- DATE(YEAR(OOH_SellOutDate__c),1,1) + 7) /7, 
5, ( OOH_SellOutDate__c- DATE(YEAR(OOH_SellOutDate__c),1,1) + 1) /7, 
6, ( OOH_SellOutDate__c- DATE(YEAR(OOH_SellOutDate__c),1,1) + 2) /7, 
99)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Week</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OOH_YTD_Flag__c</fullName>
        <description>Used to filter for sellout YTD reports</description>
        <externalId>false</externalId>
        <formula>IF(OR(AND(year(OOH_SellOutDate__c)=YEAR(TODAY()), OOH_SellOutDate__c&lt;=TODAY()),DATE(year(OOH_SellOutDate__c) + 1,MONTH( OOH_SellOutDate__c ),DAY(OOH_SellOutDate__c)) &lt;= TODAY()), true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>YTD Flag</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
