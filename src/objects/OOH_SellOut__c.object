        <externalId>false</externalId>
        <formula>if(and(OOH_ULProduct__r.product_sold_in_current_year__c =true,(year(OOH_SellOutDate__c)=year(today()))),0,1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
