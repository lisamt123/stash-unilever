        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Referenced_URL__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
    BEGINS ( Full_Story_URL__c , &quot;http&quot;),  Full_Story_URL__c , IF(ISBLANK(Full_Story_URL__c), &quot;&quot;, &quot;http://&quot;+ Full_Story_URL__c
    ))</formula>
        <label>Referenced URL</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
